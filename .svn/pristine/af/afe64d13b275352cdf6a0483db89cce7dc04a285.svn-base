<h3>
    Visitor Management
</h3>
<div ng-controller="MstVisitorSummaryController as dp">

    <div class="panel">
        <br />
        <div class="row mb-lg">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-3 col-xs-6 br pv">
                        <div class="row">
                            <div class="col-md-2 text-center visible-md visible-lg">
                                <em class="fa fa-calendar fa-2x text-muted"></em>
                            </div>
                            <div class="col-md-10">
                                <div style="font-size:13px;"><b>Today's Visitor Count</b></div>
                                <div  style="font-size:18px;color:brown;"><b> {{todayvisitor_count}} </b>
                                    <span style="font-size:12px;color:brown;"><b> ({{txtvisit_date}}) </b></span> </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-xs-6 br pv">
                        <div class="row">
                            <div class="col-md-2 text-center visible-md visible-lg">                               
                                <em class="fa fa-calendar fa-2x text-muted"></em>
                            </div>
                            <div class="col-md-10">
                                <div style="font-size:13px;"><b>Tagged Visitor Count</b></div>
                                <div style="font-size:18px;color:brown;"><b> {{taggedvisitor_count}} </b></div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-xs-6 br pv">
                        <div class="row">
                            <div class="col-md-2 text-center visible-md visible-lg">
                                <em class="fa fa-calendar fa-2x text-muted"></em>
                            </div>
                            <div class="col-md-10">
                                <div style="font-size:13px;"><b> Visitor History Count</b></div>
                                <div style="font-size:18px;color:brown;"><b> {{visitorhistory_count}} </b></div>
                                </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-xs-6 pv">
                        <div class="row">
                            <div class="col-md-2 text-center visible-md visible-lg">
                                <em class="fa fa-calendar fa-2x text-muted"></em>
                            </div>
                            <div class="col-md-10">
                                <div style="font-size:13px;"><b> Total Visitor's Count </b></div>
                                <div style="font-size:18px;color:brown;"><b> {{lstotalcount}} </b></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
      
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="row">
                        <div class="col-lg-10 col-md-10 col-sm-10">
                            <div id="panelDemo20" class="panel">
                                <div class="panel-heading" style="background-color:rgb(19,14,89);background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);color:#ffffff;height:46px;font-size:15px;"><b>Today's Visitor Details</b></div>
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <button type="button" ng-hide="lsoneapipage=='OneApi'"class="btn btn-info button-sm" ng-click="addvisitor()">
                                                <span class="btn-label">
                                                    <i class="fa fa-plus"></i>
                                                </span>
                                                Add Visitor
                                            </button>
                                            <br />
                                            </div>
                                        </div>
                                    <br/>
                                    <div class="table-responsive">
                                        <table datatable="ng" class="table table-hover table-striped">
                                            <thead>
                                                <tr class="text-white" style="background-color:#09385f;">
                                                    <th> S.No </th>
                                                    <th> Visit ID</th>
                                                    <th> Visiting Official Name </th>
                                                    <th> Visitor Name</th>
                                                    <th> Branch Name</th>
                                                    <th> Visit Type</th>
                                                    <!--<th> Visit Date</th>-->
                                                    <th ng-hide="lsoneapipage=='OneApi'"> Created Date </th>
                                                    <!--<th> Created By</th>-->
                                                    <th class="text-center" ng-hide="lsoneapipage=='OneApi'">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="list in visitor_list">
                                                    <td data-title="'S.No'"> {{$index+1}} </td>
                                                    <td data-title="'Visit ID'">{{list.visit_id}}</td>
                                                    <td data-title="'Visiting Official Name'">{{list.visitingofficial_name}}</td>
                                                    <td data-title="'Visitor Name'">{{list.visitor_name}}</td>
                                                    <td data-title="'Branch Name'"> {{list.branch_name}}</td>
                                                    <td data-title="'Visit Type'"> {{list.visiting_type}}</td>
                                                    <!--<td data-title="'Visit Date'"> {{list.visit_date}}</td>-->
                                                    <td data-title="'Created Date'" ng-hide="lsoneapipage=='OneApi'"> {{list.created_date}} </td>
                                                    <!--<td data-title="'Created By'"> {{list.created_by}} </td>-->
                                                    <td align="center" ng-hide="lsoneapipage=='OneApi'"ng-switch="list.generate_status" class="nowrap">
                                                        <button title="View" ng-hide="lsoneapipage=='OneApi'"ng-click="visitor_view(list.visitor_gid)" class="btn btn-info">
                                                            <em class="fa fa-eye"></em>
                                                        </button>
                                                        <button title="Edit" ng-hide="lsoneapipage=='OneApi'"ng-click="edit(list.visitor_gid)" class="btn btn-green">
                                                            <em class="fa fa-edit "></em>
                                                        </button>
                                                        <!--<button title="PDF" ng-switch-when='Y' class="btn bg-brown-400 btn-sm"
                                                                ng-click="downloadpdf(list.visitor_gid)">
                                                            <em class="fa fa-file-pdf-o"></em>
                                                        </button>
                                                        <button title="PDF" ng-switch-when='N' class="btn bg-brown-400 btn-sm" disabled>
                                                            <em class="fa fa-file-pdf-o"></em>
                                                        </button>-->
                                                        <button title="Delete" ng-hide="lsoneapipage=='OneApi'"ng-click="delete(list.visitor_gid)" class="btn btn-danger">
                                                            <em class="icon icon-trash "></em>
                                                        </button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-2 col-md-2 col-sm-2" style="padding-left:0px;">
                            <div id="panelDemo21" class="panel" style="background-color:#ffce9d;height:140px;color:#e07d16;"  ng-click="tagged_visitor()">                               
                                <div class="panel-body">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                              <br/>
                                                <div class="text-center">
                                                    <em style="text-align:center;" class="fa fa-tags fa-2x"></em>
                                                </div>
                                                    <h4 style="text-align:center;font-size:16px;"><b> Tagged Visitor  </b></h4>
                                                </div>
                                        </div>
                                   
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-2 col-md-2 col-sm-2" style="padding-left:0px;">
                            <div id="panelDemo21" class="panel" style="background-color:#b8f4b8;color:black;height:140px;color:#167815;" ng-click="history_visitor()">                                
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <br />
                                            <div class="text-center">
                                                <em style="text-align:center;" class="fa fa-history fa-2x"></em>
                                            </div>
                                            <p style="text-align:center;font-size:16px;"><b> Visitor History  </b> </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

</div>