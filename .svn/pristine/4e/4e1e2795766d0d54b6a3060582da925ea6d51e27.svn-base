                <div class="form-wizard wizard-horizontal">
                    <!-- START wizard steps indicator-->

                    <ol class="row text-center">
                        <li ng-click="clickstep1()" class="col-md-3 col-lg-2 col-sm-3 btn-primary">
                            <label style="font-size:16px;color:black;">A.	Limit Information<br />&nbsp;</label>

                        </li>
                        <li ng-click="clickstep2()" class="col-md-3 col-lg-2 col-sm-3 btn-primary">
                            <label style="font-size:16px;color:black;">B.	Repayment Details <br />&nbsp; </label>

                        </li>
                        <li ng-click="clickstep3()" class="col-md-3 col-lg-2 col-sm-3  btn-primary">
                            <label style="font-size:16px;color:black;">C.	Customer Bank A/c. Details</label>

                        </li>
                        <li ng-click="clickstep4()" class="col-md-3 col-lg-2 col-sm-3 btn-primary">
                            <label style="font-size:16px;color:black;">D.	Processing Fee / Commission</label>
                        </li>
                        <li ng-click="clickstep5()" class="col-md-3 col-lg-2 col-sm-3 btn-primary">
                            <label style="font-size:16px;color:black;">E.	Documentation / Client Visit Charges</label>

                        </li>
                        <li ng-click="clickstep6()" class="col-md-3 col-lg-2 col-sm-3 btn-primary">
                            <label style="font-size:16px;color:black;">CAD Internal Usage <br />&nbsp;</label>
                        </li>
                    </ol>
                    
                    <!-- END wizard steps indicator-->
                    <!-- START Wizard Step inputs -->
                    <div ng-hide="stepone">
                        <fieldset>
                            <legend>Limit Information for setting in system</legend>
                            <!-- START row -->
                            <table id="statictable" class="row-border hover">
                                <thead>
                                    <tr class="bg-success text-white">
                                        <th width="05%">Limit Ref No</th>
                                        <th width="05%">ODLIM</th>
                                        <th width="05%">Interchangeability (If Applicable)</th>
                                        <th width="05%">Report Structure</th>
                                        <th width="10%">Nature of Facility</th>
                                        <th width="5%">Margin</th>
                                        <th width="10%">Documented Limit to be set in System    (in Rs.)</th>
                                        <th width="12%">Limit To be Released to be set in System (in Rs.)</th>
                                        <th width="5%">Tenure </th>
                                        <th width="10%">Revolving/ Non Revolving</th>
                                        <th width="07%">ROI in % P.A</th>
                                        <th width="07%">Penal Interest in % P.A</th>
                                        <th width="07%">Document Info</th>
                                        <th width="07%">Date of Expiry</th>
                                      
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="limit in limitinfo_limit">
                                        <td data-title="Limit Ref No" style="text-align:center;">{{limit.lslimitref_no}}</td>
                                        <td data-title="ODLIM" style="text-align:right;">{{limit.odlim}}</td>
                                        <td data-title="Interchangeability" style="text-align:center;">{{limit.interchangeability}}</td>
                                        <td data-title="Report Structure" style="text-align:center;">{{limit.report_structure}}</td>
                                        <td data-title="Facility Type">{{limit.facility_type}}</td>
                                        <td data-title="Margin" style="text-align:right;">{{limit.margin}}</td>
                                        <td data-title="Document Limit" style="text-align:right;">{{limit.document_limit}}</td>
                                        <td data-title="Limit Released" style="text-align:right;">{{limit.limit_released}}</td>
                                        <td data-title="Tenure" style="text-align:center;">{{limit.tenure}}</td>
                                        <td data-title="Revolving Type" style="text-align:center;">{{limit.revolving_type}}</td>
                                        <td data-title="ROI in %">{{limit.rate_interest}}</td>
                                        <td data-title="Penal Interest in %">{{limit.penal_interest}}</td>
                                        <td data-title="document Name">{{limit.document_name}}
                                         <a ng-hide="limit.document_name==''" class="btn" download="" href="" id="btnpdf"  ng-click="downloads(limit.document_path,limit.document_name)">
    <span class="fa fa-download"></span>
</a></td>
                                        <td data-title="Expiry Date" style="text-align:center;">{{limit.expiry_date}}</td>
                                      
                                    </tr>
                                    <tr>
                                        <td style="color:darkred" class="text-bold">Total</td>
                                        <td></td>
                                        <td> </td>

                                        <td></td>

                                        <td> </td>
                                        <td></td>
                                        <td style="color:darkred;text-align:right;" class="text-bold">{{total_document_limit}}</td>
                                        <td style="color:darkred;text-align:right;" class="text-bold">{{totol_limit_released}}</td>
                                        <td> </td>
                                        <td> </td>
                                        <td> </td>
                                        <td> </td>
                                        <td> </td>
                                        <td> </td>
                                    </tr>
                                </tbody>
                            </table>
                            <br />
                            <label> &nbsp;&nbsp;&nbsp;Note:</label>
                            <div ng-repeat="limit in limitinfo_limit">
                                &nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;  {{limit.limitinfo_remarks}}
                            </div>
                            <br />
                         
                            <!-- END row -->
                        </fieldset>
                        <div class="text-center">
                            <button ng-click="lsaback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                        </div>
                    </div>
                    <!-- END Wizard Step inputs -->
                    <!-- START Wizard Step inputs -->
                    <div ng-hide="steptwo">
                        <fieldset>
                            <legend>Repayment Details</legend>
                            <!-- START row -->
                            <div class="panel-body">
                                <div class="table-responsive table-bordered">
                                    <table class="table" style="padding: 0px 0px">
                                        <thead class="bg-indigo-400">
