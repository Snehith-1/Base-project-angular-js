
<h3>
    Raise Legal SR
</h3>
<div ng-controller="editLegalSRcontroller as dpick">
    <div class="panel panel-success">
       
        <div class="panel-body">

            <div class="modal-body ng-scope">
                <form novalidate="" name="userform" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row" style="display:none;">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">

                            <div class="col-lg-8 col-md-8 col-sm-8 ">
                                <label style="font-size:5px;" ng-model="lawyer_gid" class="col-form-label w3-text-indigo"><strong></strong></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-1 col-md-1 col-sm-1">
                    </div>
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtsrref_no" tabindex="0" required="" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>SR Ref No *</label>

                                </div>
                            </div>
                            <div class="col-lg-1 col-md-1 col-sm-1">

                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txturn" tabindex="0" required="" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>URN *</label>

                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <label class="col-form-label control-label"><strong>Customer Name *</strong></label>
                                <select id="customername" name="customer" ng-change="onselectedchangecustomer(customer)" dropzone="" ng-model="customer" required="" class=" mda-form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched" size="1">
                                    <option value="">---Select Customer---</option>
                                    <option data-ng-repeat="data in customer_list" value="{{data.customer_gid}}">
                                        {{data.customername}}
                                    </option>
                                </select>
                            </div>
                            <div class="col-lg-1 col-md-1 col-sm-1">

                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">

                                <label class="col-form-label control-label"><strong>Loan *</strong></label>
                                <select id="loan" name="loan" ng-change="onselectedchangecustomer(customer)" dropzone="" ng-model="customer" required="" class=" mda-form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched" size="1">
                                    <option value="">---Select Loan---</option>
                                    <option data-ng-repeat="data in customer_list" value="{{data.customer_gid}}">
                                        {{data.customername}}
                                    </option>
                                </select>

                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtraised_by" tabindex="0" aria-required="true" aria-invalid="true"
                                           class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Raised By </label>

                                </div>
                            </div>
                            <div class="col-lg-1 col-md-1 col-sm-1">

                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <label>Requested Date</label>
                                    <p class="input-group">
                                        <input type="text" id="requested_date" name="date" ng-model="requested_date" datepicker-popup="{{dpick.format}}"
                                               style="height:25px;" is-open="dpick.opened" min-date="dpick.minDate" datepicker-options="dpick.dateOptions"
                                               close-text="Close" class="form-control" />
                                        <span class="input-group-btn">
                                            <button type="button" ng-click="dpick.open($event)" style="height:25px;" class="btn btn-default">
                                                <em class="fa fa-calendar"></em>
                                            </button>
                                        </span>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtdocument_type" type="number" required="" maxlength="12" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Document Type * </label>

                                </div>
                            </div>
                            <div class="col-lg-1 col-md-1 col-sm-1">

                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtcase_severitys" type="email" required="" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Case Severity * </label>

                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                    <label for="input_13">Case details *</label>
                                    <textarea ng-model="txtcase" rows="3" md-maxlength="1000" class="ng-valid md-input ng-valid-md-maxlength ng-touched ng-dirty ng-valid-parse"
                                              id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                </md-input-container>

                            </div>

                            <div class="col-lg-1 col-md-1 col-sm-">
                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <strong>Upload Document</strong>
                                    <input type="file" id="addupload" ng-model="test_document" onchange="angular.element(this).scope().upload(this.files, angular.element(this).scope().documentupload, this.name)"
                                           data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                </div>
                            </div>
                        </div>
                    </div>



                </form>
            </div>
            <div>
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="userform.$invalid" ng-click="lawyerSubmit()"><i class="fa fa-arrow-circle-up"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                    <button ng-click="srback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                </div>
            </div>
        </div>
    </div>
</div>
