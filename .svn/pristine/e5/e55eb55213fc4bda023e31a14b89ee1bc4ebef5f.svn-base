<h3>
    Create Sanction
</h3>
<div ng-controller="idasMstCreateSanction as dp" class="ball-clip-rotate">
    <div id="panelDemo8" class="panel panel-primary ">
        <div class="panel-heading">
            Create Sanction
        </div>

        <div>
            <div class="row" style="display:none;">
                <div class="col-lg-12 col-md-12 col-sm-12 ">
                    <div class="col-lg-6 col-md-6 col-sm-6 ">
                        <input name="customer_gid" id="customer_gid" style="font-size:12px" placeholder="Enter Customer gid"
                               ng-model="customer_gid" class="form-control">
                    </div>
                </div>
            </div>

            <div class="modal-body ng-scope">
                <div class="pd-sm-10">
                    <form novalidate="" name="userform5" ng-submit="form.submitForm1()" autocomplete="off" class="form-validate form-horizontal">

                        <div class="row ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <label class="col-form-label control-label">Customer <span style="color:red">*</span></label>
                                    <input type="text" name="customer" id="customer" ng-model="customer" required
                                           ng-keyup="complete(customer)" class="form-control" placeholder="Search Customer Name Here..." />

                                    <ul class="list-group panel-scroll" style="height:100%; word-wrap: break-word;">
                                        <li class="list-group-item" ng-repeat="m in customer_list"
                                            ng-click="fillTextbox(m.customer_gid,m.customername)">{{m.customername}}</li>
                                    </ul>

                                    <label style="color:red;">{{message}}</label>
                                </div>


                            </div>

                        </div>

                        <div class="row " ng-show="customer_pnl">
                            <br />
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">


                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label class="col-form-label control-label"><strong>Zonal Head</strong></label>
                                    <select id="zonal_name" name="vertical" dropzone="" ng-model="zonalHead"
                                            class="form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched"
                                            tabindex="0" aria-label="State" aria-expanded="false" size="1" ng-disabled="true">
                                        <option value=""> Select Zonal Head </option>
                                        <option data-ng-repeat="data in employee_list" value="{{data.employee_gid}}">
                                            {{data.employee_name}}
                                        </option>
                                    </select>
                                </div>

                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label class="col-form-label control-label"><strong>Business Head</strong></label>
                                    <select id="businesshead_name" name="vertical" dropzone="" ng-model="businessHead"
                                            class="form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched"
                                            tabindex="0" aria-label="State" aria-expanded="false" size="1" ng-disabled="true">
                                        <option value=""> Select Business Head </option>
                                        <option data-ng-repeat="data in employee_list" value="{{data.employee_gid}}">
                                            {{data.employee_name}}
                                        </option>
                                    </select>
                                </div>
                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label class="col-form-label control-label"><strong>Cluster Manager</strong></label>
                                    <select id="cluster_manager_name" name="vertical" dropzone="" ng-model="clustermanager"
                                            class="form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched"
                                            tabindex="0" aria-label="State" aria-expanded="false" size="1" ng-disabled="true">
                                        <option value=""> Select Cluster Manager </option>
                                        <option data-ng-repeat="data in employee_list" value="{{data.employee_gid}}">
                                            {{data.employee_name}}
                                        </option>
                                    </select>
                                </div>
                            </div>

                        </div>

                        <div class="row " ng-show="customer_pnl">
                            <br />
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">


                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label class="col-form-label control-label"><strong>Relationship Manager</strong></label>
                                    <select id="relationshipmgmt_name" name="vertical" dropzone="" ng-model="relationshipMgmt"
                                            class="form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched"
                                            tabindex="0" aria-label="State" aria-expanded="false" size="1" ng-disabled="true">
                                        <option value=""> Select Relationship Manager </option>
                                        <option data-ng-repeat="data in employee_list" value="{{data.employee_gid}}">
                                            {{data.employee_name}}
                                        </option>
                                    </select>
                                </div>
                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label class="col-form-label control-label"><strong>Credit Manager</strong></label>
                                    <select id="creditmanager_name" name="creditmanager" dropzone="" ng-model="creditmgmt_name"
                                            class="form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched"
                                            tabindex="0" aria-label="State" aria-expanded="false" size="1" ng-disabled="true">
                                        <option value=""> Select Credit Manager </option>
                                        <option data-ng-repeat="data in employee_list" value="{{data.employee_gid}}">
                                            {{data.employee_name}}
                                        </option>
                                    </select>
                                </div>
                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <br />&nbsp;
                                    <label><strong>Vertical</strong></label>

                                    &nbsp;
                                    <p>  &nbsp; &nbsp;{{vertical_code}}</p>
                                </div>
                            </div>

                        </div>
                        <br />

                        <div class="row ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">

                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <label>Sanction Date <span style="color:red">*</span></label>
                                    <p class="input-group">
                                        <input type="text" datepicker-popup="{{dp.format}}" ng-model="txtSanctionDate" is-open="dp.open1"
                                               datepicker-options="dp.dateOptions" ng-required="true"
                                               close-text="Close" class="form-control" />
                                        <span class="input-group-btn">
                                            <button type="button" ng-click="dp.calender1($event)" class="btn btn-default">
                                                <em class="fa fa-calendar"></em>
                                            </button>
                                        </span>
                                    </p>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <label>Sanction Amount (ODLIM)<span style="color:red">*</span></label>
                                    <input id="txtInput" ng-model="txtSanctionAmount" required type="text" ng-change="amountschange()"
                                           class="form-control ng-pristine ng-valid ng-touched" style="text-align:right;">
                                    <span style="font-style:italic;">Number in words:</span>
                                    <span id="words_requestedloan" style="color:#003c96fc;" class="text-bold"></span>
                                </div>
                            </div>
                        </div>

                        <div class="row ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">

                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <label>Entity <span style="color:red">*</span></label>
                                    <select name="approval_authority" ng-model="cboentity_type" required="" ng-options="m.entity_name for m in entity_list"
                                            class="form-control ng-valid ng-touched ng-valid-parse" size="1">
                                        <option value="">--- Select Entity ---</option>

                                    </select>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <label><strong>Entity Branch </strong></label> <span style="color:red">*</span>
                                    <select name="branch_name" chosen="" ng-options="m.branch_name for m in branch_list" required ng-click="ngclickevent()"
                                            ng-model="branch_gid" class="chosen-select form-control">
                                        <option value=""> ---Select Branch--- </option>
                                    </select>
                                </div>

                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <label><strong>Entity State</strong> </label> <span style="color:red"> *</span>

                                    <select name="state_name" chosen="" ng-options="m.state_name for m in state_list" required ng-click="ngclickevent()"
                                            ng-model="state_gid" class="chosen-select form-control">
                                        <option value=""> ---Select State ---</option>
                                    </select>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <label>Colending (If Applicable) <span style="color:red">*</span></label>
                                    <div class="radio c-radio">
                                        <label>
                                            &nbsp;&nbsp; <input type="radio" name="rdbcolanding" value="Yes" ng-model="rdbcolanding" required ng-click="rdbcolanding_yes()" />
                                            &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                        </label>
                                        <label>
                                            <input type="radio" name="rdbcolanding" value="No" ng-model="rdbcolanding" required ng-click="rdbcolanding_no()" />
                                            <span class="fa fa-circle"></span>No
                                        </label>
                                    </div>
                                    <div ng-show="colandingyes">
                                        <label>Name of the Colending </label>
                                        <select name="txtcolander_name" ng-model="txtcolander_name" ng-click="ngclickevent()"
                                                class="form-control ng-valid ng-touched ng-valid-parse" size="1">
                                            <option value="">--- Select Colending Name ---</option>
                                            <option value="DBS"> DBS</option>
                                            <option value="AARYANDHAN"> AARYANDHAN</option>
                                            <option value="LIVFIN"> LIVFIN</option>
                                            <option value="VisageHoldings"> VisageHoldings</option>
                                        </select>

                                    </div>
                                </div>
                            </div>

                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">

                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <label>E & S Declaration Available <span style="color:red">*</span></label>
                                    <div class="radio c-radio">
                                        <label>
                                            &nbsp;&nbsp; <input type="radio" name="rdbdeclaration" value="Yes" ng-model="rdbdeclaration" required ng-click="rdbdeclaration_yes()" />
                                            &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                        </label>
                                        <label>
                                            <input type="radio" name="rdbdeclaration" value="No" ng-model="rdbdeclaration" required ng-click="rdbdeclaration_no()" />
                                            <span class="fa fa-circle"></span>No
                                        </label>
                                    </div>

                                </div>
                             </div>
                                <div ng-show="esdeclarationyes">
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="col-sm-4 col-md-4 col-lg-4">
                                                <br />
                                                <strong class="ng-binding w3-text-indigo">
                                                    <em class="fa fa-file text-primary fa-lg"></em>&emsp;
                                                    Upload E & S Declaration Document <span style="color:red">*</span>
                                                </strong>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <div class="col-lg-5 col-md-5 col-sm-5">
                                                        <div class="mda-form-group float-label">
                                                            <input ng-model="es_declarationdocument_type" tabindex="0" maxlength="45" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                                            <label>Document Title </label>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-1 col-md-1 col-sm-1">
                                                    </div>
                                                    <div class="col-lg-5 col-md-5 col-sm-5">
                                                        <div class="mda-form-group float-label">
                                                            <strong>Upload Documents</strong>
                                                            <input type="file" id="adduploades_declaration" ng-model="test_document" onchange="angular.element(this).scope().uploades_declaration(this.files, angular.element(this).scope().documentupload, this.name)"
                                                                   data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                        <div class="table-responsive">
                                                            <table class="table table-striped table-secondary">
                                                                <thead class="bg-gray text-white">
                                                                    <tr width="100%">
                                                                        <th scope="col">Document Title</th>
                                                                        <th scope="col">Document Name</th>
                                                                        <th scope="col">Uploaded Date</th>
                                                                        <th scope="col">Uploaded By</th>
                                                                        <th scope="col">Action</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr ng-repeat="data in uploadesfilename_list" width="100%">
                                                                        <td title="Document Title">{{data.document_type}}</td>
                                                                        <td title="Document Name">{{data.document_name}}</td>
                                                                        <td title="Uploaded Date">{{data.updated_date}}</td>
                                                                        <td title="Uploaded By">{{data.uploaded_by}}</td>
                                                                        <td data-title="Action">
                                                                            <button title="Download" class="btn btn-default btn-sm"
                                                                                    ng-click="esdownloaddocument(data.document_path,data.document_name)">
                                                                                <em class="fa fa-download"></em>
                                                                                <a download="" href="" id="btnpdf"></a>
                                                                            </button>
                                                                            <a class="btn btn-xs btn-danger" ng-click="esdocument_cancelclick(data.document_gid)">
                                                                                <span class="icon icon-trash "></span></a>
                                                                        </td>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div ng-show="esdeclarationno">
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="col-sm-4 col-md-4 col-lg-4">
                                                <br />
                                                <strong class="ng-binding w3-text-indigo">
                                                    <em class="fa fa-file text-primary fa-lg"></em>&emsp;
                                                    Upload Deviation Mail Document
                                                </strong>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <div class="col-lg-5 col-md-5 col-sm-5">
                                                        <div class="mda-form-group float-label">
                                                            <input ng-model="deviationmaildocument_type" tabindex="0" maxlength="45" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                                            <label>Document Title </label>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-1 col-md-1 col-sm-1">
                                                    </div>
                                                    <div class="col-lg-5 col-md-5 col-sm-5">
                                                        <div class="mda-form-group float-label">
                                                            <strong>Upload Documents</strong>
                                                            <input type="file" id="addmailupload" ng-model="test_document" onchange="angular.element(this).scope().deviationmailupload(this.files, angular.element(this).scope().documentupload, this.name)"
                                                                   data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                        <div class="table-responsive">
                                                            <table class="table table-striped table-secondary">
                                                                <thead class="bg-gray text-white">
                                                                    <tr width="100%">
                                                                        <th scope="col">Document Title</th>
                                                                        <th scope="col">Document Name</th>
                                                                        <th scope="col">Uploaded Date</th>
                                                                        <th scope="col">Uploaded By</th>
                                                                        <th scope="col">Action</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr ng-repeat="data in mailfilename_list" width="100%">
                                                                        <td title="Document Title">{{data.document_type}}</td>
                                                                        <td title="Document Name">{{data.document_name}}</td>
                                                                        <td title="Uploaded Date">{{data.updated_date}}</td>
                                                                        <td title="Uploaded By">{{data.uploaded_by}}</td>
                                                                        <td data-title="Action">
                                                                            <button title="Download" class="btn btn-default btn-sm"
                                                                                    ng-click="downloadmail(data.document_path,data.document_name)">
                                                                                <em class="fa fa-download"></em>
                                                                                <a download="" href="" id="btnpdf"></a>
                                                                            </button>
                                                                            <a class="btn btn-xs btn-danger" ng-click="maildocument_cancelclick(data.document_gid)">
                                                                                <span class="icon icon-trash "></span></a>
                                                                        </td>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                        </div>
                        <br/>
                        <div class="row" ng-show="vertical_code=='FPO'">
                            <div class="col-lg-12 col-sm-12 col-md-12">

                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <label>Paycard Solutions <span style="color:red">*</span></label>
                                    <div class="radio c-radio">
                                        <label>
                                            &nbsp;&nbsp; <input type="radio" name="rdbpaycard" value="applicable" ng-model="rdbpaycard" />
                                            &nbsp;&nbsp;<span class="fa fa-circle"></span>Applicable
                                        </label>
                                        <label>
                                            <input type="radio" name="rdbpaycard" value="notapplicable" ng-model="rdbpaycard" />
                                            <span class="fa fa-circle"></span>Not Applicable
                                        </label>
                                    </div>

                                </div>
                            </div>

                        </div>
                        <br />
                        <div class="row" ng-show="mandatorycolending">

                            <div class="col-sm-12 col-md-12 col-lg-12 text-center">

                                <label style="color:red;"> Select Colending Name</label>
                                <br />
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12 col-lg-12 col-sm-12">
                                <strong class="ng-binding w3-text-indigo">
                                    <em class="fa fa-file text-primary fa-lg"></em>&emsp;
                                    Upload CAM Document
                                </strong>
                                <br />

                                <div class="panel">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-5 col-md-5 col-sm-5">
                                                    <div class="mda-form-group float-label">
                                                        <input ng-model="CAMdocument_type" tabindex="0" maxlength="45" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                                        <label>Document Title </label>
                                                    </div>
                                                </div>
                                                <div class="col-lg-1 col-sm-1 col-md-1">

                                                </div>
                                                <div class="col-lg-5 col-md-5 col-sm-5">
                                                    <div class="mda-form-group float-label">
                                                        <strong>Upload Documents</strong>
                                                        <input type="file" id="addCAMupload" ng-model="test_document"
                                                               onchange="angular.element(this).scope().uploadCAM_doc(this.files, angular.element(this).scope().documentupload, this.name)"
                                                               data-classbutton="btn btn-default" accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />

                                                    </div>
                                                </div>
                                                <!--<div class="col-lg-6 col-md-6 col-sm-6">
                                <br />

                                <input type="file" id="addCAMupload" ng-model="test_document"
                                       onchange="angular.element(this).scope().uploadCAM_doc(this.files, angular.element(this).scope().documentupload, this.name)" class="form-control"
                                       data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />

                            </div>-->

                                            </div>
                                        </div>
                                        <br />
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class="col-lg-12">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-secondary">
                                                            <thead class="bg-gray text-white">
                                                                <tr>
                                                                    <th>Document Title</th>
                                                                    <th>Document Name</th>
                                                                    <th>Uploaded Date</th>
                                                                    <th>Uploaded By</th>
                                                                    <th>Action</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="document in CAMfilename_list">
                                                                    <td>{{document.document_type}}</td>
                                                                    <td>{{document.document_name}}</td>
                                                                    <td>{{document.updated_date}}</td>
                                                                    <td>{{document.uploaded_by}}</td>
                                                                    <td class="text-nowrap">
                                                                        <button title="Download" class="btn btn-default btn-sm"
                                                                                ng-click="downloadsCAM(document.document_path,document.document_name)">
                                                                            <em class="fa fa-download"></em>
                                                                            <a download="" href="" id="btnpdf"></a>
                                                                        </button>
                                                                        <a class="btn btn-sm btn-danger" ng-click="deleteCAM(document.document_gid)">
                                                                            <span class="icon icon-trash "></span>
                                                                        </a>

                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <br/>
                        <div class="row">

                            <div class="col-md-12 col-lg-12 col-sm-12">
                                <strong class="ng-binding w3-text-indigo">
                                    <em class="fa fa-file text-primary fa-lg"></em>&emsp;
                                    Upload MOM Document
                                </strong>
                                <br />
                                <div class="panel">

                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-5 col-md-5 col-sm-5">
                                                    <div class="mda-form-group float-label">
                                                        <input ng-model="MOMdocument_type" tabindex="0" maxlength="45" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                                        <label>Document Title </label>
                                                    </div>
                                                </div>
                                                <div class="col-lg-1 col-sm-1 col-md-1">

                                                </div>
                                                <div class="col-lg-5 col-md-5 col-sm-5">
                                                    <strong>Upload Documents</strong>
                                                    <input type="file" id="addMOMupload" ng-model="test_document"
                                                           onchange="angular.element(this).scope().uploadMOM_doc(this.files, angular.element(this).scope().documentupload, this.name)" 
                                                           data-classbutton="btn btn-default" accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />

                                                </div>

                                            </div>
                                        </div>
                                        <br />
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class="col-lg-12">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-secondary">
                                                            <thead class="bg-gray text-white">
                                                                <tr>
                                                                    <th>Document Title</th>
                                                                    <th>Document Name</th>
                                                                    <th>Uploaded Date</th>
                                                                    <th>Uploaded By</th>
                                                                    <th>Action</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="document in MOMfilename_list">
                                                                    <td>{{document.document_type}}</td>
                                                                    <td>{{document.document_name}}</td>
                                                                    <td>{{document.updated_date}}</td>
                                                                    <td>{{document.uploaded_by}}</td>
                                                                    <td class="text-nowrap">
                                                                        <button title="Download" class="btn btn-default btn-sm"
                                                                                ng-click="downloadsMOM(document.document_path,document.document_name)">
                                                                            <a download="" href="" id="btnpdf"></a>
                                                                            <em class="fa fa-download"></em>
                                                                        </button>
                                                                        <a class="btn btn-sm btn-danger" ng-click="deleteMOM(document.document_gid)">
                                                                            <span class="icon icon-trash "></span>
                                                                        </a>

                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>

                        </div>

                        <br />

                        <div class="ng-scope">
                            <div class="text-center">
                                <button type="button" class="btn btn-success btn-md" ng-click="sanctionSubmit()" ng-disabled="userform5.$invalid"><i class="fa fa-arrow-circle-right"></i> &nbsp;&nbsp;Submit</button>
                                <button ng-click="sanctionback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                            </div>
                        </div>

                    </form>
                </div>

            </div>



        </div>
    </div>

</div>


