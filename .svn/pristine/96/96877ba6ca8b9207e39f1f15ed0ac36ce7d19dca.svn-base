                                                                <span class="fa-stack">
                                                                    <em class="fa fa-circle fa-stack-2x text-green"></em>
                                                                    <em class="fa fa-check-squarefa-stack-1x fa-inverse text-white"></em>
                                                                </span></a>

                                                            <a  ng-show="data.proposed_roi==''" title="Update ROI" style="height:20px;" 
                                                               ng-click="btnupdate(data.sanction2loanfacilitytype_gid,data.loanfacilityref_no,data.loanfacility_type)">
                                                           <span class="fa-stack">
                                <em class="fa fa-circle fa-stack-2x text-green"></em>
                                <em class="fa fa-check-square fa-stack-1x fa-inverse text-white"></em>
                            </span></a>
                                                            <a  title="Delete" ng-click="deleteloanfacility(data.sanction2loanfacilitytype_gid)">
