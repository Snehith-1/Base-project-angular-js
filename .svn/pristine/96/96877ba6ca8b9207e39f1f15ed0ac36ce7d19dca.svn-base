                                            <table class="table table-striped table-secondary">
                                                <thead class="bg-gray text-white">
                                                    <tr width="100%">
                                                        <th scope="col">Ref No</th>
                                                        <th scope="col">Loan Facility Type</th>
                                                        <th scope="col">Loan Facility Amount</th>
                                                        <th scope="col">Interchangeability</th>
                                                        <th scope="col">Report Structure</th>
                                                        <th scope="col">Document Limit</th>
                                                        <th scope="col">Margin</th>
                                                        <th scope="col">Tenure</th>
                                                        <th scope="col">Date of Expiry</th>
                                                        <th scope="col">Proposed ROI</th>
                                                        <th scope="col">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in loanfacilitytype_list" width="100%">
                                                        <td title="Ref No">{{data.loanfacilityref_no}}</td>
                                                        <td title="Facility Type">{{data.loanfacility_type}}</td>
                                                        <td title="Facility Amount" style="text-align:right;">{{data.loanfacility_amount}}</td>
                                                        <td title="Facility Type">{{data.interchangeability}}</td>
                                                        <td title="Facility Amount">{{data.report_structure}}</td>
                                                        <td title="Facility Type" style="text-align:right;">{{data.document_limit}}</td>
                                                        <td title="Facility Amount">{{data.margin}}</td>
                                                        <td title="Facility Type">{{data.tenure}}</td>
                                                        <td title="Facility Amount">{{data.expiry_date}}</td>
                                                        <td title="Facility Amount">{{data.proposed_roi}}</td>
                                                        <td data-title="Action" class="text-nowrap" style="text-align:center;">
