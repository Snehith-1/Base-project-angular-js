                                                            <td align="left" scope="col">
                                                                {{data.created_date}}
                                                            </td>                                                            
                                                            <td align="center">
                                                                <em class="fa fa-info-circle" ng-click=view_remarks(data.remarks) style="font-size:24px;align-content:center;"></em>
                                                            </td>           
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="dd-handle ng-scope">
                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Tag Details </b></p>
