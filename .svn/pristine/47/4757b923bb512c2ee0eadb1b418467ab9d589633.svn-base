<h3>
    My Leads
</h3>
<div ng-controller="MstMarketingMBDRejectedCallSummaryController">
    <div class="panel">
        <br>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <!--<div class="col-lg-2 col-md-6 col-sm-12">
                    <p style="color:#09385f;font-size: 17px;"><b>Completed Calls Summary</b></p>
                    <hr />
                </div>-->
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="myassigned_calls()" style="width:100%;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;"> <b> {{assignedcall_count}}</b></div>
                        <b style="font-size: 14px;">Assigned Leads</b>
                    </button>
                    <br />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="work_inprogress()" style="width:100%;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;"> <b>{{inprogresscall_count}}</b></div>
                        <b style="font-size: 14px;">Work In Progress</b>
                    </button>
                    <br />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="tag_member()" style="width:100%;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;"> <b>{{tagmember_count}}</b></div>
                        <b style="font-size: 14px;">Tagged Member</b>
                    </button>
                    <br />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="transfer_calls()" style="width:100%;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;"> <b> {{transfercall_count}}</b></div>
                        <b style="font-size: 14px;">Transfer Leads</b>
                    </button>
                    <br />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="followup_call()" style="width:100%;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;"> <b>{{followupcall_count}}</b></div>
                        <b style="font-size: 14px;">Follow-up Leads</b>
                    </button>
                    <br />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="completed_calls()" style="width:100%;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;"> <b> {{completedcall_count}}</b></div>
                        <b style="font-size: 14px;">Converted Leads</b>
                    </button>
                    <br />
                </div>
            </div>
        </div>
        <br />
        <div class="row">
            <div class="col-lg-12 col-sm-12 col-md-12">
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="mbdrejected_calls()" style="width:100%;height:50px;border:1px solid rgb(179, 177, 177);background-color: #84cbdb;">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;"> <b> {{rejectedcall_count}}</b></div>
                        <b style="font-size: 14px;">Rejected Leads</b>
                    </button>
                    <br />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="closedcall()" style="width:100%;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;"> <b> {{closedcall_count}}</b></div>
                        <b style="font-size: 14px;">Closed Leads</b>
                    </button>
                    <br />
                </div>
            </div>
        </div>
        <div class="row">
            <br>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="table-responsive">
                        <table datatable="ng" class="table table-hover table-striped">
                            <thead>
                                <tr class="text-white" style="background-color:#09385f;">
                                    <th> S.No </th>
                                    <th> Lead Number</th>
                                    <th> Lead Name</th>
                                    <th> Lead Request Type</th>
                                    <th> Lead Received Date</th>
                                    <th> Origination</th>
                                    <th> Rejected by</th>
                                    <th> Rejected date </th>
                                    <th> Status </th>
                                    <th class="text-center">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in marketingcallcompleted_list" ng-switch="data.callclosure_status">
                                    <td data-title="'S.No'"> {{$index+1}} </td>
                                    <td data-title="'Lead Number'"> {{data.ticket_refid}}</td>
                                    <td data-title="'Lead Name'"> {{data.caller_name}}</td>
                                    <td data-title="'Lead Request Type'"> {{data.leadrequesttype_name}}</td>
                                    <td data-title="'Call Received Date'"> {{data.callreceived_date}}</td>   
                                    <td data-title="'Origination'"> {{data.origination}} </td>                             
                                    <td data-title="'Rejected by'"> {{data.rejected_by}} </td>
                                    <td data-title="'Rejected date'"> {{data.rejected_date}}</td>

                                    <td data-title="'Status'">
                                        <label ng-switch-when="Rejected">
                                            <a title="Completed" style="color:white;" class="blinking ng-binding ng-scope label label-success">
                                                <b>Rejected</b>
                                            </a>
                                        </label>
                                        
                                    </td>
                                    <td align="center">
                                        <button title="View" ng-click="view(data.marketingcall_gid)" class="btn btn-info">
                                            <em class="fa fa-retweet"></em>
                                        </button>
                                    </td>
                                </tr>
                                <tr ng-repeat="data in ibcallcompleted_list" ng-switch="data.callclosure_status">

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <br />
    </div>
    <script type="text/ng-template" id="/transferContent.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="icon icon-share-alt"></em>&nbsp;Transfer</h2>
                <span flex></span>

            </div>
        </md-toolbar>
        <div class="modal-body ng-scope">
            <form novalidate="" name="transferformsubmit" ng-submit="form.submitForm1()" autocomplete="off" class="form-validate form-horizontal">
                <div class="pd-sm-10">

                    <div class="row ">
                        <div class="col-sm-12 col-lg-12 col-md-12">
                            <label>Ticket Ref No</label>
                            <br />
                            {{ticketref_no}}
                        </div>
                    </div>
                    <br>
                    <div class="row">
                        <div class="col-sm-12 col-lg-12 col-md-12">
                            <label>Assign To </label><br>
                            {{assigned_to}}
                        </div>
                    </div>
                    <br>
                    <div class="row">
                        <div class="col-sm-12 col-lg-12 col-md-12">
                            <label>Transfer To </label><br>
                            <select name="transfer_to" id="transfer_to" chosen="" ng-options="m.employee_gid as m.employee_name for m in TransferEmployeeList"
                                    ng-model="transfer_to" class="chosen-select form-control" required></select>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-lg-12 col-md-12">
                            <label>Remarks </label>
                            <textarea ng-model="transfer_remarks" placeholder="Enter Remarks" rows="3" class="form-control"
                                      id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                        </div>
                    </div>
                </div>
                <br />
                <div>
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="transferformsubmit.$invalid" ng-click="transfercall()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                        <button ng-click="close()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
            </form>
            <br>
            <div class="table-responsive">
                <div style="max-height:300px;overflow:auto;">
                    <table class="table table-striped table-secondary">
                        <thead class="bg-gray text-white">
                            <tr width="100%">
                                <th align="center" scope="col">S.No</th>
                                <th align="center" scope="col">Transfer From</th>
                                <th align="center" scope="col">Transfer To</th>
                                <th align="center" scope="col">Allotted By/On</th>
                                <th align="center" scope="col">Transfer Date</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="data in TransferLog_list" width="100%">
                                <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                <td align="left" scope="col" title="Transfer From">{{data.assignedby_name}}</td>
                                <td align="left" scope="col" title="Transfer To'">{{data.transferby_name}}</td>
                                <td align="left" scope="col" title="Allotted By/On'">{{data.created_by}}/{{data.created_date}}</td>
                                <td align="left" scope="col" title="Transfer Date'">{{data.transfered_date}}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </script>
</div>