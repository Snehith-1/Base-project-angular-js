<h3>
    Document Conversation
</h3>
<style>
    .blinking {

animation:20s blinker linear infinite;
-webkit-animation:20s blinker linear infinite;
-moz-animation:20s blinker linear infinite;
}

@-moz-keyframes blinker {  
 0% { opacity: 1.0; }
 50% { opacity: 0.0; }
 100% { opacity: 1.0; }
 }

@-webkit-keyframes blinker {  
 0% { opacity: 1.0; }
 50% { opacity: 0.0; }
 100% { opacity: 1.0; }
 }

@keyframes blinker {  
 0% { opacity: 1.0; }
 50% { opacity: 0.0; }
 100% { opacity: 1.0; }
 }
</style>
<div ng-controller="idasTrnDocCoversationChkr as dp">
    <div id="panelDemo13" class="panel panel-default">
        <div class="panel-body table-responsive">
            <p class="lead bb text-primary text-bold text-center">
                {{documentrecord_id}}
                <label style="font-weight:bold">/</label>{{document_name}}
            </p>
          
            <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <strong>  Document Comments : </strong>&nbsp;&nbsp;
                      
                            <label style="background-color:deeppink;color:white ;font-size:14px;" class="blinking ng-binding ng-scope">  {{doc_comments}} </label>
                      
                    </div>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <label>Document Date <span>*</span></label>
                        <p class="input-group">
                            <input type="text" datepicker-popup="{{dp.format}}" ng-model="document_date" is-open="dp.open1"
                                   datepicker-options="dp.dateOptions" ng-required="true" ng-change="update()" style="height:35px;"
                                   close-text="Close" class="form-control" />
                            <span class="input-group-btn">
                                <button type="button" ng-click="dp.calender1($event)" class="btn btn-default" style="height:35px;">
                                    <em class="fa fa-calendar"></em>
                                </button>
                            </span>
                        </p>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3">

                    </div>

                    <div class="col-lg-6 col-md-6 col-sm-6">
                        <label class="col-form-label control-label"><strong> Types Of Document </strong></label><br />
                     
                            <div class="radio c-radio">
                                <label>
                                    <input type="radio" required name="types_of_copy" value="Scan" ng-change="onchangecopy(types_of_copy)" ng-model="types_of_copy" />
                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Scan
                                </label>
                                <label>
                                    <input type="radio" required name="types_of_copy"  ng-change="onchangecopy(types_of_copy)" value="Original" ng-model="types_of_copy" />
                                    <span class="fa fa-circle"></span>Original
                                </label>
                             
                            </div>
                       
                    </div>

                </div>

            </div>
          
            <br />
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-sm-6 col-md-6 col-lg-6">
                        <div class="panel panel-primary ">
                            <h4 class="panel-heading" align="left">
                                <label class="text-white">Document Conversation - Maker & Checker</label><br />
                            </h4>

                        </div>
                        <div ng-show="valueInternal">

                            <div id="panelDemo1" class="panel" ng-repeat="data in docconversationlistInternal">
                                <div ng-class="{'panel-heading-collapsed':panelDemo1}" class="panel-heading bg-grey-100">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="col-lg-12">
                                                <div class="pull-left">
                                                    <lable class="text-purple"><strong>Maker Query #{{data.query_no}}:({{data.docconversationref_no}})</strong></lable>
                                                </div>

                                                <div class="pull-right ">
                                                    <span class="text-sm"> {{data.cad_name}}/{{data.cadquery_on}}</span>
                                                    <br />
                                                    &nbsp;
                                                    <button title="View Documents" ng-if="data.uploaddocument_count != '0'" ng-click="PopupDownload(data.docconversation_gid)" class="btn btn-purple">
                                                        <em class="icon icon-link"></em>
                                                    </button>


                                                </div>
                                            </div>

                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="col-lg-12">
                                                <p ng-bind-html="data.cad_query"></p>
                                            </div>
                                        </div>
                                    </div>
                                    <paneltool tool-collapse="tool-collapse"></paneltool>
                                </div>
                                <div collapse="panelDemo1" class="panel-wrapper">

                                    <div ng-switch="data.flag">
                                        <div class="panel-heading" ng-switch-when="Y">
                                            <div class="row">
                                                <div class="col-lg-12">
                                                    <div class="col-lg-12">
                                                        <div class="pull-left">
                                                            <lable class="text-purple"><strong>Checker Response #{{data.query_no}}:</strong></lable>
                                                        </div>

                                                        <div class="pull-right">
                                                            <span class="text-sm">
                                                                {{data.relationshipmgr_name}}/{{data.relationshipmgrquery_on}}
                                                            </span>
                                                        </div>
                                                    </div>


                                                </div>

                                            </div>
                                            <div class="row">
                                              
                                                <div class="col-lg-12">
                                                    <div class="col-lg-12">

                                                        <p ng-bind-html="data.rm_response"></p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div ng-switch-when="N">
                                            <form novalidate="" name="formReply" autocomplete="off" class="form-validate form-horizontal">
                                                <div ng-show="data.hide">
                                                   
                                                    <div class="row">
                                                        <div class="col-lg-12">
                                                            <div class="col-lg-1"">
                                                                <button type="button" title="Copy Query" ng-click="btncopyinternal($index,data.cad_query)">
                                                                    <em class="fa fa-copy text-primary fa-sm"></em>
                                                                </button>
                                                            </div>
                                                            <div class="col-lg-12">
                                                                <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                                                    <label for="input_13">Reply to Maker * </label>
                                                                    &nbsp;&nbsp;
                                                                   
                                                                    <textarea ng-model="docconversationlistInternal[$index].reply" required rows="2" maxlength="1000" class="ng-valid md-input ng-valid-md-maxlength ng-touched ng-dirty ng-valid-parse"
                                                                              id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                                                </md-input-container>
                                                            </div>
                                                        </div>
                                                    </div>


                                                    <br />

                                                    <div class="ng-scope row">
                                                        <div class="text-left col-lg-12">
                                                            <div class="col-lg-12">
                                                                <button ng-click="raiseResponse(data.docconversation_gid,docconversationlistInternal[$index].reply)" ng-disabled="formReply.$invalid" class="btn btn-purple"><i class="fa fa-send (alias)"></i>&nbsp;&nbsp;Send</button>
                                                                <button ng-click="data.hide=false" class="btn btn-danger"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Cancel</button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />

                                                <div class="row">
                                                    <div class="col-lg-12">

                                                        <div class="col-lg-12">



                                                                <div ng-switch="data.forwarded_flag" ng-hide="data.hide">
                                                                    <button title="Query Confirm" class="btn btn-info" ng-click="confirmQuery(data.docconversation_gid,data.noquery_flag)">
                                                                        <em class="fa fa-check"></em>&nbsp;
                                                                    </button>
                                                                    <span class="text-info">Query Confirm</span>
                                                                    &nbsp;&nbsp;
                                                                    <button title="Reply Maker" class="btn-pink" ng-click="data.hide=true">
                                                                        <em class="fa fa-mail-reply (alias) w3-text-pink"></em>&nbsp;
                                                                    </button>
                                                                    <span class="text-pink">Reply to Maker</span>
                                                                    &nbsp;&nbsp;
                                                                    <button title="Forward to RM" ng-switch-when="N" ng-click="forwardQuery(data.docconversation_gid)" class="btn btn-success">
                                                                        <em class="fa fa-mail-forward (alias)"></em>
                                                                    </button>
                                                                    <span class="text-success">Forward to RM</span>

                                                                    &nbsp;
                                                                    <button type="button" title="Copy Query" ng-click="btncopy(data.cad_query)">
                                                                        <em class="fa fa-copy text-primary fa-sm"></em>
                                                                    </button>
                                                                    <span class="text-gray-dark">Copy Query</span>
                                                                </div>
                                                            
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                            </form>
                                        </div>


                                    </div>

                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-6">
                        <div class="panel panel-primary ">
                            <h4 class="panel-heading" align="left">
                                <label class="text-white">Document Conversation - RM</label><br />
                            </h4>

                        </div>

                        <div id="panelDemo1" class="panel" ng-repeat="data in docconversationlistExternal">
                            <div ng-class="{'panel-heading-collapsed':panelDemo1}" class="panel-heading bg-grey-100">
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                            <div class="pull-left">
                                                <lable class="text-purple"><strong>CAD Query #{{data.query_no}}:({{data.docconversationref_no}})</strong></lable>

                                            </div>
                                            <div class="pull-right " ng-switch="data.forwarded_flag">
                                                <span class="text-sm"> {{data.cad_name}}/{{data.cadquery_on}}</span>
                                                &nbsp;
                                                <button title="View Documents" ng-if="data.uploaddocument_count != '0'" ng-click="PopupDownload(data.docconversation_gid)" class="btn btn-purple">
                                                    <em class="icon icon-link"></em>
                                                </button>

                                                <i class="fa fa-mail-forward (alias)" title="Fowarded By : {{data.forwarded_by_name}}&#013;Fowarded On:{{data.forwarded_on}}" ng-switch-when="Y"></i>

                                            </div>
                                        </div>

                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                            <p ng-bind-html="data.cad_query"></p>
                                        </div>
                                    </div>
                                </div>
                                <paneltool tool-collapse="tool-collapse"></paneltool>
                            </div>
                            <div collapse="panelDemo1" class="panel-wrapper">

                                <div ng-switch="data.flag">
                                    <div class="panel-heading" ng-switch-when="Y">
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class="col-lg-12">
                                                    <div class="pull-left">
                                                        <lable class="text-pink"><strong>RM Response #{{data.query_no}}:</strong></lable>

                                                    </div>
                                                    <div class="pull-right ">
                                                        <span class="text-sm">
                                                            {{data.relationshipmgr_name}}/{{data.relationshipmgrquery_on}}
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class="col-lg-12">

                                                    <p ng-bind-html="data.rm_response"></p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div ng-switch-when="N">
                                        <form novalidate="" name="formReply" autocomplete="off" class="form-validate form-horizontal">
                                            <div ng-show="data.hide">
                                                <div class="row">
                                                    <div class="col-lg-12">
                                                        <div class="col-lg-12">
                                                            <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                                                <label for="input_13">Reply From RM * </label>
                                                                <textarea ng-model="docconversationlistExternal[$index].reply" required rows="2" md-maxlength="1000" class="ng-valid md-input ng-valid-md-maxlength ng-touched ng-dirty ng-valid-parse"
                                                                          id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                                            </md-input-container>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />

                                                <div class="ng-scope row">
                                                    <div class="text-left col-lg-12">
                                                        <div class="col-lg-12">
                                                            <button ng-click="raiseResponse(data.docconversation_gid,docconversationlistExternal[$index].reply)" ng-disabled="formReply.$invalid" class="btn btn-purple"><i class="fa fa-send (alias)"></i>&nbsp;&nbsp;Send</button>
                                                            <button ng-click="data.hide=false" class="btn btn-danger"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Cancel</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                        </form>
                                        <div class="row">
                                            <div class="col-lg-12">

                                                <div class="col-lg-12" ng-hide="data.hide">
                                                    <button title="Reply Conversation" class="btn-pink" ng-click="data.hide=true">
                                                        <em class="fa fa-mail-reply (alias) w3-text-pink"></em>&nbsp;
                                                    </button>
                                                    <span class="text-pink">RM Reply</span>
                                                </div>
                                            </div>
                                        </div>
                                        <br />
                                    </div>


                                </div>

                            </div>

                        </div>

                    </div>
                </div>
            </div>


            <div class="panel panel-primary ">
                <h4 class="panel-heading" align="left">
                    <label class="text-white">Raise Query to RM</label><br />
                </h4>

            </div>
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-lg-2 col-md-2 col-sm-2">
                        <button type="button" ng-click="FileShow()" class="btn btn-info btn-xs">
                            <span class="icon icon-cloud-upload"></span>&nbsp;&nbsp;Upload File
                        </button>
                    </div>
                </div>
            </div>
            <br />
            <div ng-show="DivFile">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-2 col-md-2 col-sm-2">
                            <label>Document Title</label>&nbsp;<span style="color:red">*</span>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3">
                            <input type="text" ng-model="txtdocument_title" rows="1" tabindex="0" min="0" aria-required="true"
                                   aria-invalid="true" class="form-control" maxlength="256" />
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-2 col-md-2 col-sm-2">
                            <label>File Upload</label>
                        </div>

                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <input type="file" id="addupload" ng-model="test_document" ng-disabled="!txtdocument_title"
                                   onchange="angular.element(this).scope().uploadallocation(this.files, angular.element(this).scope().documentupload, this.name)"
                                   data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select"
                                   accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />
                        </div>
                    </div>
                </div>
                <br />
            </div>

            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">

                    <div ng-repeat="data in uploaddocument" class="panel bg-grey-200">
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <label>{{data.document_title}}</label><br />
                            <span>{{data.document_name}}</span>
                            &nbsp;&nbsp;
                            <button title="Un-Tag" ng-click="UploadDocCancel(data.conversationdocument_gid)" class="btn btn-danger">
                                <em class="fa fa-remove (alias)"></em>
                            </button>

                        </div>
                    </div>
                </div>

            </div>
            <br />
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <form novalidate="" name="formRaiseQuery" autocomplete="off" class="form-validate form-horizontal">

                            <textarea ng-model="content" required rows="4" maxlength="1000" placeholder="Raise Query to RM"
                                      class="form-control ng-valid "
                                      id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>

                            <br />
                            <button type="button" ng-click="raiseQuery()" ng-disabled="formRaiseQuery.$invalid" class="btn btn-primary btn-sm mb">Send to RM</button>
                        </form>
                    </div>

                </div>

            </div>

            <div class="panel panel-primary ">
                <h4 class="panel-heading" align="left">
                    <label class="text-white">Final Remarks</label><br />
                </h4>

            </div>

            <div class="row">
                <div class="col-lg-12">
                    <div class="col-lg-12">
                        <form novalidate="" name="formfinalremarks" autocomplete="off" class="form-validate form-horizontal">
                            <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <div class="col-sm-3 col-md-3 col-lg-3">
                                        <select name="txtfinalremarks" ng-model="txtfinalremarks" class="form-control ng-valid ng-touched ng-valid-parse" required
                                                size="1" ng-change="onchangeremarks(txtfinalremarks)">
                                            <option value="">--- Select Final Remarks ---</option>
                                            <option value="Query Cleared">Query Cleared </option>
                                            <option value="Deferral Taken">Deferral Taken </option>
                                            <option value="Others">Others </option>
                                        </select>
                                    </div>
                                    <div class="col-sm-9 col-md-9 col-lg-9" ng-show="other_remarks">
                                        <textarea ng-model="scanfinal_remarks" placeholder="Final Remarks - Conversation" rows="2" maxlength="1000"
                                                  class="form-control" aria-required="true" style="resize:vertical;"
                                                  id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                    </div>
                                </div>
                            </div>
                            <br /><br />
                            <button ng-click="updateFinalRemarks()" ng-disabled="formfinalremarks.$invalid" class="btn btn-success">&nbsp;&nbsp;Update</button>
                        </form>
                    </div>

                </div>
            </div>
            <br />

            <div class="row">
                <div class="text-center" ng-switch="maker_status">

                    <button ng-click="ChkrVerify()" ng-disabled="txtfinalremarks==''"
                            ng-if="checker_status=='Checker Pending'" class="btn btn-success">
                        <i class="icon icon-check"></i>&nbsp;&nbsp;Document Confirm
                    </button>
                    <button ng-click="docconback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                </div>
            </div>
            <br /><br />
        </div>
    </div>


    <script type="text/ng-template" id="/mailconversation.html">


        <div class="modal-body ng-scope">
            <h4 id="myModalLabel" class="modal-title">
                <p class="lead bb text-primary text-bold text-center">
                    View Documents
                </p>
            </h4>

            <div class="text-center">
                <div class="dd-handle ng-scope">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="table-responsive">
                                <table class="table table-striped table-secondary">
                                    <thead class="bg-gray text-white">
                                        <tr width="100%">
                                            <th align="center" scope="col">S.No</th>
                                            <th align="center" scope="col">Document Name</th>
                                            <th align="center" scope="col">Document Title</th>
                                            <th align="center" scope="col">Upload By/On</th>
                                            <th align="center" scope="col">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="data in viewDocumentList" width="100%">
                                            <td scope="col" title="'S.No'">{{$index+1}}</td>
                                            <td align="left" scope="col" title="'Document Name'">{{data.document_name}}</td>
                                            <td align="left" scope="col" title="'Document Title'">{{data.document_title}}</td>
                                            <td scope="col" title="'Upload By/On'">{{data.created_by}}/{{data.created_date}}</td>
                                            <td scope="col" data-title="'Action'">
                                                <span class="fa fa-download" ng-click="downloads(data.document_path,data.document_name)" />
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>

                </div>
                <br />
                <button ng-click="ok()" class="btn btn-danger">
                    <i class="fa fa-close (alias)">
                    </i>&nbsp;Close
                </button>
            </div>
        </div>
    </script>
</div>

