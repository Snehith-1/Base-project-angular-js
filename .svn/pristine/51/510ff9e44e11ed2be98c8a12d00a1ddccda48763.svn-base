                                                        <td data-title="'Query Description & RM Response'">
                                                            <button title="View Query Description'" ng-click="view_campaignquerydesc(data.query_description,data.queryresponse_remarks,data.query_responseby)" class="btn btn-info">
                                                                <em class="fa fa-eye"></em>
                                                            </button>
                                                        </td>
                                                        <td data-title="'Entity Name'" style="text-align:left;">{{data.created_by}}</td>
