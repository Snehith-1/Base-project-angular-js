                                        <span ng-if="multichoice.importance == 'No'" style="color:red"></span>
                                    </div>
                                    <br />
                                    <div class="col-sm-12">
                                        <input type="text" tabindex="0" required maxlength="10" ng-if="multichoice.importance == 'Yes'" ng-model="multichoice.name" ng-pattern="/^[0-9]*$/"
                                               aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
