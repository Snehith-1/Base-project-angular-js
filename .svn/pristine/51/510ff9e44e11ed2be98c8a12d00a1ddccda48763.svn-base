                                                            <div class="col-sm-12">
                                                                <div ng-repeat="radioOption in choice.answerdesc_list">
                                                                    <label for="rbvalue_{{radioOption.id}}">
                                                                        <input id="rbvalue_{{radioOption.id}}" type="radio" ng-model="$parent.SelectedId"
                                                                               value="{{radioOption.name}}" ng-checked=radioOption.check ng-change="onSingleTaskSelect(radioOption.name,radioOption.id,choice.question,'Radio Button')" />&nbsp;{{radioOption.name}}
