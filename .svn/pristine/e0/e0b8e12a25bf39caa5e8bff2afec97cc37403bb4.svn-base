  
<h3>
    Template Summary
</h3>
<div ng-controller="AgrMstContrateTemplateSummaryController">
    <button title="Add Template" class="btn btn-info btn-sm" ng-click="popupTemplate()">
        <em class="fa fa-plus"></em>&nbsp;Add Template
    </button>
    <br>
    <br>
    <div class="panel"> 
        <div class="row"> 
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12 text-center">  
                            <br/>
                            <label style="font-size:14px;" class="col-form-label"> Vertical Name: </label> 
                            <span style="font-size:14px;"class="col-form-label text-primary"><b>{{lblvertical_name}}</b></span>
                        </div>
                    </div>
                    <br>
                    <br/>
                    <div class="table-responsive">
                        <table datatable="ng" class="table table-hover table-striped">
                            <thead>
                                <tr class="text-white" style="background-color:#09385f;">
                                    <th>S.No</th>
                                    <th>Program Name</th> 
                                    <th>Template Name</th> 
                                    <th>Created By</th>
                                    <th class="text-center">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in templatelist">
                                    <td data-title="'S.No'">{{$index+1}}</td>
                                    <td data-title="'Program Name'">{{data.program_name}}</td> 
                                    <td data-title="'Template Name'">{{data.template_name}}</td> 
                                    <td data-title="'Created By'">{{data.created_by}}</td>
                                    <td class="nowrap" data-title="'Action'" style="text-align: center;">
                                        <!-- <button title="View" ng-click="viewtemplatedtl(data.template_gid)" class="btn btn-info btn-view btn-sm">
                                            <em class="fa fa-eye"></em>
                                        </button> -->
                                        <button title="Edit" ng-click="edittemplatedtl(data.template_gid)" class="btn btn-green btn-edit btn-sm">
                                            <em class="fa fa-edit"></em>
                                        </button>
                                    </td>
                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="text-center"> 
            <button ng-click="back()" class="btn btn-info"><i class="fa fa-arrow-circle-left fa-1x"></i>&nbsp;&nbsp;Back</button>
        </div>
        <br>
    </div>
</div>