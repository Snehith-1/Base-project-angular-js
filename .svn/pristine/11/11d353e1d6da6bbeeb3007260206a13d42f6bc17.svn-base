<h3>
    Completed View
</h3>
<div ng-controller="MstTeleCallingCompletedViewController">
    <div class="panel">
        <div class="panel-heading bg-primary">
            <h5><i class="fa fa-eye"></i> &nbsp; View Completed Call </h5>
        </div>
        <div class="modal-body ng-scope">
            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Completed Details </b></p>
                <hr class="space" />
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Completed By</label><br>
                            {{txtcompleted_by}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Completed Date</label><br>
                            {{txtcompleted_date}}
                        </div>
                        <div class="col-md-6 col-lg-6 col-sm-6">
                            <label>Completed Remarks</label><br>
                            {{txtcompleted_remarks}}
                        </div>
                    </div>
                </div>
                <br>
            </div>
            <div class="dd-handle ng-scope">
                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Assigned Details </b></p>
                <hr class="space" />
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Ticket Reference ID</label><br>
                            {{txtticket_refid}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Enquiry Status</label><br>
                            {{txtcallclosure_status}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Assigned Employee</label><br>
                            {{txtassignemployee_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Entity</label><br>
                            {{txtentity_name}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Source of Contact</label><br>
                            {{txtsourceofcontact_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Call Received Number</label><br>
                            {{txtcallreceivednumber_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Customer Type</label><br>
                            {{txtcustomer_type}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Call Received Date</label><br>
                            {{txtcallreceived_date}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Enquiry Name</label><br>
                            {{txtcaller_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Internal Reference</label> <br>
                            {{txtinternalreference_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Office Landline Number</label><br>
                            {{txtoffice_landlineno}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Company</label><br>
                            {{txtcallerassociate_company}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Enquiry Type</label><br>
                            {{txtcalltype_name}}
                        </div>
                        
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Tagged Employee</label><br>
                            {{txttagemployee_name}}
                        </div>
                       
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Allocated TAT (days)</label><br>
                            {{txttat_days}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Allocated TAT Date</label><br>
                            {{txttat_date}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>TAT in Hrs</label><br>
                            {{txttat_hours}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Function</label><br>
                            {{txtfunction_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Function Remarks</label><br>
                            {{txtfunction_remarks}}
                        </div>
                    </div>
                </div>
                <br>
            </div>



                    <div class="dd-handle ng-scope">
                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Tag Details </b></p>
                        <hr class="space" />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="table-responsive table-bordered">
                                        <div style="max-height:200px;overflow:auto;">
                                            <table class="table">
                                                <thead class="bg-gray text-white">
                                                    <tr width="100%">
                                                        <th align="center" scope="col">S.No</th>
                                                        <th align="center" scope="col">Member Name</th>
                                                        <th align="center" scope="col">Tagged Date/Time</th>
                                                        <th align="center" scope="col">Tagged By</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in ibcalltaggedmember_list" width="100%">
                                                        <td align="left" scope="col" title="S.No">
                                                            {{$index+1}}
                                                        </td>
                                                        <td align="left" scope="col" title="Member Name">
                                                            {{data.taggedmember_name}}
                                                        </td>
                                                        <td align="left" scope="col" title="Tagged Date/Time">
                                                            {{data.tagged_date}}
                                                        </td>
                                                        <td align="left" scope="col" title="Tagged By">
                                                            {{data.tagged_by}}
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12  ">
                            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Transfer Log </b></p>
                                <hr class="space" />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="table-responsive table-bordered">
                                                <div style="max-height:200px;overflow:auto;">
                                                    <table class="table">
                                                        <thead class="bg-gray text-white">
                                                            <tr width="100%">
                                                                <th align="center" scope="col">S.No</th>
                                                                <th align="center" scope="col">Transfer From</th>
                                                                <th align="center" scope="col">Transfer To</th>
                                                                <th align="center" scope="col">Allotted By/On</th>
                                                                <th align="center" scope="col">Transfer Date</th>
                                                                <th align="center" scope="col">Transfer Remarks</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in telecallingtransfer_list" width="100%">
                                                                <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                                                <td align="left" scope="col" title="Transfer From">{{data.assignedby_name}}</td>
                                                                <td align="left" scope="col" title="Transfer To'">{{data.transferby_name}}</td>
                                                                <td align="left" scope="col" title="Allotted By/On'">{{data.created_by}}/{{data.created_date}}</td>
                                                                <td align="left" scope="col" title="Transfer To'">{{data.transfered_date}}</td>
                                                                <td align="left" scope="col" title="Transfer To'" style="width:40%">{{data.transfer_remarks}}</td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-lg-8 col-sm-8 col-md-8">
                            <div class="dd-handle ng-scope">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Address Details </b></p>
                                <hr class="space" />
                                <div ui-scroll-viewport="" style="height:320px; overflow-y: auto;overflow-x: hidden;" class="row">
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="dd-handle ng-scope" ng-repeat="data in ibcalladdress_list">
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-lg-4 col-sm-4 col-md-4" style="color:#3f51b5;font-size:100%;font-weight :bold;">
                                                                <label class="label" style="font-size:12px;background-color:#09385f;">Address Type: {{data.addresstype_name}} </label>
                                                            </div>
                                                            <div class="col-lg-4 col-sm-4 col-md-4" style="color:#3f51b5;font-size:100%;font-weight :bold;">
                                                                Primary Status:  {{data.primary_status}}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-lg-5 col-sm-5 col-md-5">
                                                                <label style="font-weight:bold">Address Line 1</label>
                                                                <br />
                                                                {{data.addressline1}}
                                                            </div>
                                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                                <label style="font-weight:bold">Address Line 2</label>
                                                                <br />
                                                                {{data.addressline2}}
                                                            </div>
                                                            <div class="col-lg-3 col-sm-3 col-md-3">
                                                                <label style="font-weight:bold">Landmark </label>
                                                                <br />
                                                                {{data.landmark}}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-lg-2 col-sm-2 col-md-2">
                                                                <label style="font-weight:bold">Postal Code </label>
                                                                <br />
                                                                {{data.postal_code}}
                                                            </div>
                                                            <div class="col-lg-2 col-sm-2 col-md-2">
                                                                <label style="font-weight:bold">City </label>
                                                                <br />
                                                                {{data.city}}
                                                            </div>
                                                            <div class="col-lg-2 col-sm-2 col-md-2">
                                                                <label style="font-weight:bold">State </label>
                                                                <br />
                                                                {{data.state}}
                                                            </div>
                                                            <div class="col-lg-2 col-sm-2 col-md-2">
                                                                <label style="font-weight:bold">Taluka </label>
                                                                <br />
                                                                {{data.taluka}}
                                                            </div>
                                                            <div class="col-lg-2 col-sm-2 col-md-2">
                                                                <label style="font-weight:bold">District </label>
                                                                <br />
                                                                {{data.district}}
                                                            </div>
                                                            <div class="col-lg-2 col-sm-2 col-md-2">
                                                                <label style="font-weight:bold">Country </label>
                                                                <br />
                                                                {{data.country}}
                                                            </div>

                                                        </div>
                                                    </div>
                                                    <br />
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-lg-2 col-sm-2 col-md-2">
                                                                <label style="font-weight:bold">Latitude </label>
                                                                <br />
                                                                {{data.latitude}}
                                                            </div>
                                                            <div class="col-lg-2 col-sm-2 col-md-2">
                                                                <label style="font-weight:bold">Longitude </label>
                                                                <br />
                                                                {{data.longitude}}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-lg-4 col-sm-4 col-md-4">
                            <div class="dd-handle ng-scope">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;Mobile Number / Email Address Details </b></p>
                                <hr class="space" />
                                <div ui-scroll-viewport="" style="height:320px; overflow-y: auto;overflow-x: hidden;" class="row">
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="dd-handle ng-scope">
                                                    <p class="text-primary" style="font-size:14px;"><b>&nbsp; Mobile Number(s) </b></p>
                                                    <hr class="space" />
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-lg-6 col-md-6 col-sm-6">
                                                                <label class="label" style="font-size:11px;background-color:#09385f;">Primary Number :  </label>
                                                            </div>
                                                            <div class="col-lg-6 col-md-6 col-sm-6">
                                                                <p>{{txtprimary_mobileno}}</p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="row">
                                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                                        <label><u>Other Numbers </u></label>
                                                                    </div>
                                                                    <div class="col-lg-5 col-md-5 col-sm-5">
                                                                        <label><u>WhatsApp Number </u></label>
                                                                    </div>
                                                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                                                        <label><u>SMS To </u></label>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-lg-12 col-md-12 col-sm-12" ng-repeat="data in ibcallmobileno_list">
                                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                                        <p> {{data.mobile_no}} </p>
                                                                    </div>
                                                                    <div class="col-lg-5 col-md-5 col-sm-5">
                                                                        <p> {{data.whatsapp_status}} </p>
                                                                    </div>
                                                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                                                        <p> {{data.sms_to}} </p>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="dd-handle ng-scope">
                                                    <p class="text-primary" style="font-size:14px;"><b>&nbsp; Mail ID(s) </b></p>
                                                    <hr class="space" />
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-lg-6 col-md-6 col-sm-6">
                                                                <label class="label" style="font-size:11px;background-color:#09385f;">Primary Email Address :</label>
                                                            </div>
                                                            <div class="col-lg-6 col-md-6 col-sm-6">
                                                                <p style="word-wrap: break-word;">{{txtprimary_email}}</p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="row">
                                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                                        <label><u>Other Email Address </u> </label>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                                <div ng-repeat="data in ibcallemail_list">
                                                                    <p style="word-wrap: break-word;"> {{data.email_address}} </p>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="dd-handle ng-scope">
                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Follow Up Details </b></p>
                        <hr class="space" />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="table-responsive table-bordered">
                                        <div style="max-height:200px;overflow:auto;">
                                            <table class="table">
                                                <thead class="bg-gray text-white">
                                                    <tr width="100%">
                                                        <th scope="col">S.No</th>
                                                        <th scope="col">Follow Up Date</th>
                                                        <th scope="col">Follow Up Time</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in ibcallfollowup_list" width="100%">
                                                        <td title="S.No">{{$index+1}}</td>
                                                        <td title="Follow Up Date">{{data.followup_date}}</td>
                                                        <td title="Follow Up Time">{{data.followup_time}}</td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <br>
                    <div class="row text-center">
                        <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                    </div>
                    <br>
                </div>
            </div>
</div>