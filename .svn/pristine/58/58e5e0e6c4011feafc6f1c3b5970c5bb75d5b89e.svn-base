<h3>
    HR Dashboard
</h3>
<style>
    marquee {
        font-size: 15px;
        font-style: italic;
        height: 100px;
        padding: 2px 30px;
    }
</style>

<div ng-controller="hrmDashboardcontroller as dp">
    <div class="row ng-scope">
        <div class="col-lg-9">
            <div class="row">
                <div class="col-lg-4 col-md-4 col-sm-4">
                    <div class="panel widget">
                        <div class="panel-body">
                            <a href="#/app/myTeam">
                                <div class="text-center">
                                    <h2 class="text-purple mt0"><i class="fa fa-user"></i></h2>
                                    <p class="text-purple">My Profile</p>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-4 col-md-4">
                    <div class="panel widget">
                        <div class="panel-body">
                            <a href="#/app/myLeave">
                                <div class="text-center">
                                    <h2 class="text-pink mt0"><i class="fa fa-reply"></i></h2>
                                    <p class="text-pink">My Leave</p>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-4 col-md-4">
                    <div class="panel widget">
                        <div class="panel-body">
                            <a href="#/app/approveLeave">
                                <div class="text-center">
                                    <h2 class="text-info mt0"><i class="fa fa-thumbs-o-up"></i></h2>
                                    <p class="text-info">Approve Leave</p>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-4 col-sm-4 col-md-4">
                    <div class="panel widget">
                        <div class="panel-body">
                            <a href="#/app/companyPolicies">
                                <div class="text-center">
                                    <h2 class="text-dark mt0"><i class="fa fa-building-o"></i></h2>
                                    <p class="text-dark">Company Policies</p>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-4 col-md-4">
                    <div class="panel widget">
                        <div class="panel-body">
                            <a href="#/app/holidayCalender">
                                <div class="text-center">
                                    <h2 class="text-danger mt0"><i class="fa fa-calendar"></i></h2>
                                    <p class="text-danger">Office Calender</p>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-4 col-md-4">
                    <div class="panel widget">
                        <div class="panel-body">
                            <a href="#/app/monthlyAttendance">
                                <div class="text-center">
                                    <h2 class="text-danger mt0"><i class="fa fa-calendar"></i></h2>
                                    <p class="text-danger">Monthly Attendance Report</p>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="panel panel-info" style="height:230px;">
                <div class="panel-heading">What's Up!</div>
                <div class="panel-body">
                    <marquee behavior="scroll" direction="up" vspace="40px" onmouseover="this.stop();" onmouseout="this.start();">
                        <img border="0" ng-src="{{company_logo}}"
                             style="height:140%;width:100%">
                        Welcome to {{company_name}} !
                    </marquee>
                </div>
            </div>
        </div>
    </div>

    <div class="row">

        <div class="col-lg-6">
            <div class="card">
                <div class="card-body">
                    <h4>Monthly Attendence - <span class="text-primary">{{monthlyattendence.monthyear}}</span> </h4>

                    <div>
                        <canvas doughnutchart="" options="dp.pieOptions" data="dp.pieData" height="80" responsive="true"></canvas>
                    </div>
                    <div class="clearfix">
                        <div class="pull-left">
                            <p> Total No.Of Days : {{monthlyattendence.totalDays}}</p>
                            <p> No.Of Days Present : {{monthlyattendence.countPresent}} </p>
                            <p> No.Of Days Absent : {{monthlyattendence.countAbsent}}</p>
                        </div>
                        <div class="pull-right">
                            <p> No.Of Days Leave :  {{monthlyattendence.countLeave}}</p>
                            <p> No.Of Days Holiday :  {{monthlyattendence.countholiday}}</p>
                            <p> No.Of Days WeekOff :  {{monthlyattendence.countWeekOff}}</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-6">
            <div class="card">
                <div class="card-body">
                    <h4>
                        Monthwise Attendence
                    </h4>
                    <table style="position:absolute;top:14px;right:16px;;font-size:smaller;color:#545454">
                        <tbody>
                            <tr>
                                <td class="legendColorBox">
                                    <div style="border:1px solid #ccc;padding:1px">
                                        <div style="width:4px;height:0;border:5px solid #009688;overflow:hidden"></div>
                                    </div>
                                </td>
                                <td class="legendLabel">Present</td>
                            </tr>
                            <tr>
                                <td class="legendColorBox">
                                    <div style="border:1px solid #ccc;padding:1px">
                                        <div style="width:4px;height:0;border:5px solid #f44336;overflow:hidden"></div>
                                    </div>
                                </td>
                                <td class="legendLabel">Absent</td>
                            </tr>
                        </tbody>
                    </table>
                    <div>
                        <canvas barchart="" options="dp.barOptions" data="dp.barData" height="140" responsive="true"></canvas>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <hr />
    <div class="row" ng-switch="iattendence_privilege">
        <div class="col-lg-4" ng-switch-when="Y">
            <div class="card">
                <div class="card-heading bg-info shadow-z1">
                    <div class="pull-right">
                        <h3 class="m0 text-md">
                            <now format="h:mm:ss" class="h2 mt0"></now>
                            <now format="a" class="text-sm"></now>
                        </h3>
                    </div>
                    <div class="card-title">I - Attendence</div>
                    <small>Punch in</small>
                </div>
                <div class="card-body">
                    <div ng-switch="punchstatus">
                        <div ng-switch-default>
                            <div class="clearfix">
                                <div class="pull-left">
                                    <p>Punch in</p>
                                </div>
                                <div class="pull-right">
                                    <h4 class="m0">
                                        <now format="d" class="mt0"></now>
                                        <now format="MMMM" class="mt0"></now>
                                        <now format="y" class="mt0"></now>
                                    </h4>
                                </div>
                            </div>
                            <div class="text-center pv-lg">
                                <em class="fa fa-clock-o fa-4x"></em>
                            </div>
                        </div>

                        <div class="clearfix" ng-hide="punchin" ng-show="close" ng-switch-when="Y">
                            <div class="clearfix">
                                <div class="pull-left">
                                    <p>Punch Out</p>
                                </div>
                                <div class="pull-right">
                                    <h4 class="m0">
                                        <now format="d" class="mt0"></now>
                                        <now format="MMMM" class="mt0"></now>
                                        <now format="y" class="mt0"></now>
                                    </h4>
                                </div>
                            </div>
                            <br />
                            <div class="pull-left">
                                <p>
                                    Your attendance has been marked at :
                                </p>
                                <label style="font-size:16px;" class="text-success">{{login_time_audit}}</label>
                            </div>
                        </div>
                    </div>

                    <div class="clearfix">
                        <div>

                            <div class="pull-right" ng-switch="punchstatus">
                                <a class="btn btn-info" ng-hide="punchin" ng-click="dp.open()" ng-show="close" ng-switch-default>
                                    <em class="fa fa-hand-o-right"></em>
                                    <span>Punch In</span>
                                </a>
                                <a class="btn btn-info" ng-hide="punchin" ng-click="dp.open()" ng-show="close" ng-switch-when="Y">
                                    <em class="fa fa-hand-o-right"></em>
                                    <span>Punch Out</span>
                                </a>
                            </div>
                            <script type="text/ng-template" id="/myModalContent.html">
                                <div class="modal-header">
                                    <h4 id="myMapModalLabel" class="modal-title center">I-Attendence</h4>
                                </div>
                                <div class="modal-body">
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="col-lg-4">

                                            </div>
                                            <div class="col-lg-4">
                                                <h3 class="m0 text-md">
                                                    <now format="h:mm:ss" class="h2 mt0"></now>
                                                    <now format="a" class="text-sm"></now>
                                                </h3>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-controller="geo" class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div id="map" class="gmap"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <div class="clearfix">
                                        <div class="pull-right" ng-switch="punchstatus">
                                            <a class="btn btn-success" ng-click="punchinlocation()" ng-switch-default>
                                                <em class="fa fa-check"></em>
                                                <span>Punch In</span>
                                            </a>
                                            <a class="btn btn-success" ng-click="punch_outlocation()" ng-switch-when="Y">
                                                <em class="fa fa-check"></em>
                                                <span>Punch Out</span>
                                            </a>
                                            <a ng-click="ok()" class="btn btn-danger">
                                                <em class="fa fa-close"></em><span>Cancel</span>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </script>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-4" ng-switch-default>
            <div class="card" style="color:dimgray">
                <div class="card-heading bg-info shadow-z1">
                    <div class="pull-right">
                        <h3 class="m0 text-md">
                            <now format="h:mm:ss" class="h2 mt0"></now>
                            <now format="a" class="text-sm"></now>
                        </h3>
                    </div>
                    <div class="card-title">I - Attendence</div>
                    <small>Punch in</small>
                </div>
                <div class="card-body">
                    <div>
                        <div class="clearfix">
                            <div class="pull-left">
                                <p>Punch in</p>
                            </div>
                            <div class="pull-right">
                                <h4 class="m0">
                                    <now format="d" class="mt0"></now>
                                    <now format="MMMM" class="mt0"></now>
                                    <now format="y" class="mt0"></now>
                                </h4>
                            </div>
                        </div>
                        <div class="text-center pv-lg">
                            <em class="fa fa-clock-o fa-4x"></em>
                        </div>
                    </div>
                    <br/>
                    <div class="clearfix" style="display:none;">
                        <div class="pull-right">
                            <a class="btn btn-success" disabled>
                                <em class="fa fa-check"></em>
                                <span>Submit</span>
                            </a>
                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-4">
            <div class="card">
                <div class="card-heading bg-primary shadow-z1">
                    <div class="pull-right">
                        <h3 class="m0 text-lg"><em class="fa fa-sign-in"></em></h3>
                    </div>
                    <div class="card-title">Apply Login Time</div>
                    <small>Login Time Requisition </small>
                </div>
                <div class="card-body">
                    <div ng-show="applylogininfodiv">
                        <div class="clearfix">
                            <div class="pull-left">
                                <p>Login</p>
                            </div>
                            <div class="pull-right">
                                <h4 class="m0">
                                    <now format="d" class="mt0"></now>
                                    <now format="MMMM" class="mt0"></now>
                                    <now format="y" class="mt0"></now>
                                </h4>
                            </div>
                        </div>
                        <div class="text-center pv-lg">
                            <em class="fa fa-clock-o fa-4x"></em>
                        </div>
                    </div>
                    <div ng-show="applylogindiv">
                        <form name="logintimeform" id="logintimeform1" novalidate="" class="form-validate mb-lg" autocomplete="off">
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <label>Attendance Date</label>
                                    <p class="input-group">
                                        <input type="text" datepicker-popup="{{dp.format}}" ng-model="attendencedate" is-open="dp.open1"
                                               datepicker-options="dp.dateOptions" ng-required="true"
                                               close-text="Close" class="form-control" />
                                        <span class="input-group-btn">
                                            <button type="button" ng-click="dp.calender1($event)" class="btn btn-default">
                                                <em class="fa fa-calendar"></em>
                                            </button>
                                        </span>
                                    </p>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <label for="input_13">Login Time</label>
                                    <timepicker ng-model="logintime" show-meridian="ismeridian">
                                    </timepicker>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                        <label for="input_13">Reason</label>
                                        <textarea ng-model="txtlogintimereason" rows="3" md-maxlength="150" class="ng-valid md-input ng-valid-md-maxlength
                                                   ng-touched ng-dirty ng-valid-parse"
                                                  id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="false" required></textarea>
                                    </md-input-container>
                                </div>
                            </div>
                            <div class="clearfix">
                                <div class="pull-right">
                                    <a class="btn btn-success" ng-click="loginApply()" ng-disabled="logintimeform.$invalid">
                                        <em class="fa fa-check"></em>
                                        <span>Submit</span>
                                    </a>
                                    <a class="btn btn-danger" ng-click="logincloseclick()">
                                        <em class="fa fa-close"></em>
                                        <span>Close</span>
                                    </a>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="clearfix">
                        <div class="pull-right">
                            <a class="btn btn-primary" ng-click="applylogintime()" ng-hide="applyloginbutton">
                                <em class="fa fa-hand-o-right"></em>
                                <span>Apply Login</span>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-4">
            <div class="card">
                <div class="card-heading bg-purple-800 shadow-z1">
                    <div class="pull-right">
                        <h3 class="m0 text-lg"><em class="fa fa-sign-out"></em></h3>
                    </div>
                    <div class="card-title">Apply Logout Time</div>
                    <small>Logout Time Requisition </small>
                </div>
                <div class="card-body">
                    <div ng-show="applylogoutinfodiv">
                        <div class="clearfix">
                            <div class="pull-left">
                                <p>Logout</p>
                            </div>
                            <div class="pull-right">
                                <h4 class="m0">
                                    <now format="d" class="mt0"></now>
                                    <now format="MMMM" class="mt0"></now>
                                    <now format="y" class="mt0"></now>
                                </h4>
                            </div>
                        </div>
                        <div class="text-center pv-lg">
                            <em class="fa fa-clock-o fa-4x"></em>
                        </div>
                    </div>

                    <div ng-show="applylogoutdiv">
                        <form name="logouttimeform" id="logoutform" novalidate="" class="form-validate mb-lg" autocomplete="off">
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <label>Attendance Date</label>
                                    <p class="input-group">
                                        <input type="text" datepicker-popup="{{dp.format}}" ng-model="logoutattendencedate" is-open="dp.open2"
                                               datepicker-options="dp.dateOptions" ng-required="true"
                                               close-text="Close" class="form-control" />
                                        <span class="input-group-btn">
                                            <button type="button" ng-click="dp.calender2($event)" class="btn btn-default">
                                                <em class="fa fa-calendar"></em>
                                            </button>
                                        </span>
                                    </p>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <label for="input_13">Logout Time</label>
                                    <timepicker ng-model="logouttime" show-meridian="ismeridian">
                                    </timepicker>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                        <label for="input_13">Reason</label>
                                        <textarea ng-model="txtlogouttimereason" rows="3" md-maxlength="150" class="ng-valid md-input ng-valid-md-maxlength
                                                   ng-touched ng-dirty ng-valid-parse"
                                                  id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="false" required></textarea>
                                    </md-input-container>
                                </div>
                            </div>
                            <div class="clearfix">
                                <div class="pull-right">
                                    <a class="btn btn-success" ng-click="logoutApply()" ng-disabled="logouttimeform.$invalid">
                                        <em class="fa fa-check"></em>
                                        <span>Submit</span>
                                    </a>
                                    <a class="btn btn-danger" ng-click="logoutcloseclick()">
                                        <em class="fa fa-close"></em>
                                        <span>Close</span>
                                    </a>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="clearfix">
                        <div class="pull-right">
                            <a class="btn btn-purple" ng-click="applylogouttime()" ng-hide="applylogoutbutton">
                                <em class="fa fa-hand-o-right"></em>
                                <span>Apply Logout</span>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

    <div class="row">
        <div class="col-lg-4">
            <div class="card">
                <div class="card-heading bg-blue-grey-400 shadow-z1">
                    <div class="pull-right">
                        <h3 class="m0 text-md"><em class="fa fa-calendar"></em></h3>
                    </div>
                    <div class="card-title">Apply OD</div>
                    <small>OD</small>
                </div>
                <div class="card-body">
                    <div ng-show="applyodinfodiv">
                        <div class="clearfix">
                            <div class="pull-left">
                                <p>OD</p>
                            </div>
                            <div class="pull-right">
                                <h4 class="m0">
                                    <now format="d" class="mt0"></now>
                                    <now format="MMMM" class="mt0"></now>
                                    <now format="y" class="mt0"></now>
                                </h4>
                            </div>
                        </div>
                        <div class="text-center pv-lg">
                            <em class="fa fa-clock-o fa-4x"></em>
                        </div>
                    </div>

                    <div ng-show="applyoddiv">
                        <form name="odform" id="odform1" novalidate="" class="form-validate mb-lg" autocomplete="off">
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <label>OD Date</label>
                                    <p class="input-group">
                                        <input type="text" datepicker-popup="{{dp.format}}" ng-model="oddate" is-open="dp.open3"
                                               datepicker-options="dp.dateOptions" ng-required="true"
                                               close-text="Close" class="form-control" />
                                        <span class="input-group-btn">
                                            <button type="button" ng-click="dp.calender3($event)" class="btn btn-default">
                                                <em class="fa fa-calendar"></em>
                                            </button>
                                        </span>
                                    </p>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <label for="input_13">From</label>
                                    <timepicker ng-model="od_from" show-meridian="ismeridian">
                                    </timepicker>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <label for="input_13">To</label>
                                    <timepicker id="od_tohour" ng-model="od_to" show-meridian="ismeridian">
                                    </timepicker>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group">
                                        <label>Onduty Period</label>
                                        <div class="col">
                                            <label class="mda-radio">
                                                <input type="radio" name="radio_halffull" ng-click="odhalf('show')" ng-model="radio_halffull"
                                                       value="Half">
                                                <em class="bg-indigo-500"></em>Half
                                            </label>

                                        </div>
                                        <div class="col">
                                            &nbsp; &nbsp;
                                        </div>
                                        <div class="col">
                                            <label class="mda-radio">
                                                <input type="radio" name="radio_halffull" ng-model="radio_halffull" ng-click="odhalf()" value="Full" ng-checked="true">
                                                <em class="bg-indigo-500"></em>  Full
                                            </label>

                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6" ng-show="session">
                                    <div class="mda-form-group">
                                        <label>Session</label>
                                        <div class="col">
                                            <label class="mda-radio">
                                                <input type="radio" name="radio_FNAN" ng-model="radio_FNAN" ng-checked="true"
                                                       value="FN">
                                                <em class="bg-indigo-500"></em>FN
                                            </label>
                                        </div>
                                        <div class="col">
                                            &nbsp; &nbsp;
                                        </div>
                                        <div class="col">
                                            <label class="mda-radio">
                                                <input type="radio" name="radio_FNAN" ng-model="radio_FNAN" value="AN">
                                                <em class="bg-indigo-500"></em>AN
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                        <label for="input_13">Reason</label>
                                        <textarea ng-model="txt_odreason" rows="3" md-maxlength="150" class="ng-valid md-input ng-valid-md-maxlength
                                                   ng-touched ng-dirty ng-valid-parse"
                                                  id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="false" required></textarea>
                                    </md-input-container>
                                </div>
                            </div>
                            <div class="clearfix">
                                <div class="pull-right">
                                    <a class="btn btn-success" ng-click="odApply()" ng-disabled="odform.$invalid">
                                        <em class="fa fa-check"></em>
                                        <span>Submit</span>
                                    </a>
                                    <a class="btn btn-danger" ng-click="odcloseclick()">
                                        <em class="fa fa-close"></em>
                                        <span>Close</span>
                                    </a>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="clearfix">
                        <div class="pull-right">
                            <a class="btn btn-default bg-blue-grey-400" ng-click="applyod()" ng-hide="applyodbutton">
                                <em class="fa fa-hand-o-right"></em>
