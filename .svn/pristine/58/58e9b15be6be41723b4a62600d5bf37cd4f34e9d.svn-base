<h3>
     Company Document
</h3>
<div ng-controller="MstCompanyDocumentController">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="col-lg-10 col-sm-10 col-md-10">
                <button type="button" title=" Add Company Document" class="btn btn-info btn-sm" ng-hide="lsoneapipage=='OneApi'"  ng-click="addCompanyDocument()">
                    <span class="btn-label">
                        <i class="fa fa-plus"></i>
                    </span> Add Company Document
                </button>
            </div>
                <div class="col-lg-2 col-sm-2 col-md-2 " style="text-align:right;padding-bottom:10px;">
                    <button type="button" class="btn btn-success" ng-click="excelreport()" ng-hide="lsoneapipage=='OneApi'"  title="Export Excel"><i class="fa fa-file-excel-o"></i></button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    <br /><label style="background-color:white;left:90%;"><a download="" href="" id="btnpdf" style="display:none;"></a> </label>
                </div>
            </div>
        </div>
    <br />
    <div class="panel">
        <br/>
        <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="table-responsive">
                    <table datatable="ng" class="table table-hover table-striped">
                        <thead>
                            <tr class="text-white" style="background-color:#09385f;">
                                <th style="display:none;"> S.No </th>
                                <!-- <th> Created Date </th> -->
                                <th> One API Code</th>
                                <th> Company Document Name </th>
                                <th class="" ng-hide="lsoneapipage=='OneApi'"> Check Point Count </th>
                                <th class="" ng-hide="lsoneapipage=='OneApi'"> Document Type </th>
                                <th class="" ng-hide="lsoneapipage=='OneApi'"> Document Severity </th>
                                <th class="" ng-hide="lsoneapipage=='OneApi'"> Covenant Status </th>
                                <th class="" ng-hide="lsoneapipage=='OneApi'"> Created By </th>
                                <th class="" ng-hide="lsoneapipage=='OneApi'"> Created Date </th>

                                <!--<th> Updated By </th>-->
                                <th> Status </th>
                                <th class="text-center" ng-hide="lsoneapipage=='OneApi'">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                                                           
                            <tr ng-repeat="data in companydocument_data">
                                <td data-title="'S.No'" style="display:none;" class="text-center">{{$index+1}}</td>
                                <!-- <td data-title="'Created Date'">{{data.created_date}}</td> -->
                                <td data-title="'One API Code'">{{data.api_code}}</td>
                                <td data-title="'Company Document Name'">{{data.companydocument_name}}</td>
                                <td data-title="'Count of check points'" style="text-align: center;" ng-hide="lsoneapipage=='OneApi'">{{data.check_list}}</td>
                                <td data-title="'Document Type'" ng-hide="lsoneapipage=='OneApi'">{{data.documenttype_name}}</td>
                                <td data-title="'Document Severity'" ng-hide="lsoneapipage=='OneApi'">{{data.documentseverity_name}}</td>
                                <!-- <td data-title="'LMS Code'">{{data.lms_code}}</td>
    <td data-title="'Bureau Code'">{{data.bureau_code}}</td> -->
                                <td data-title="'Covenant'" ng-switch="data.covenant_type" ng-hide="lsoneapipage=='OneApi'">
                                    <p ng-switch-when='Y'>Yes</p>
                                    <p ng-switch-when='N'>No</p>
                                </td>
                                <td data-title="'Created By'" class="" ng-hide="lsoneapipage=='OneApi'">{{data.created_by}}</td>
                                <td data-title="'Created Date'" class="" ng-hide="lsoneapipage=='OneApi'">{{data.created_date}}</td>

                                <!--<td data-title="'Updated Date'">
        <span ng-if="data.updated_date">{{data.updated_date}}</span>
        <span ng-if="!data.updated_date">{{data.created_date}}</span>
    </td>
    <td data-title="'Updated By'">
        <span ng-if="data.updated_by">{{data.updated_by}}</span>
        <span ng-if="!data.updated_by">{{data.created_by}}</span>
    </td>-->

                                <td data-title="'Status'" ng-switch="data.status">
                                    <label class="label label-success" ng-switch-when='Active' style="font-size:10px;">Active</label>
                                    <label class="label label-warning" ng-switch-when='Inactive' style="font-size:10px;">InActive</label>
                                </td>
                                <td class="nowrap" data-title="'Action'" ng-hide="lsoneapipage=='OneApi'" style="text-align: center;" ng-switch="data.status">
                                    <button title="View" ng-click="viewCompanyDocument(data.companydocument_gid)" class="btn btn-info">
                                        <em class="fa fa-eye"></em>
                                    </button>
                                    <button title="Edit" ng-click="editcompanydocument(data.companydocument_gid)" class="btn btn-green">
                                        <em class="fa fa-edit"></em>
                                    </button>
                                    <button title="Status" ng-switch-when='Active' ng-click="Status_update(data.companydocument_gid)" class="btn btn-success">
                                        <em class="fa fa-thumbs-up"></em>
                                    </button>
                                    <button title="Status" ng-switch-when='Inactive' ng-click="Status_update(data.companydocument_gid)" class="btn btn-warning">
                                        <em class="fa fa-thumbs-down"></em>
                                    </button>
                                    <!--<button title="Delete" ng-click="delete(data.companydocument_gid)" class="btn btn-danger">
            <em class="icon icon-trash "></em>
        </button>-->
                                </td>
                            </tr>

                        </tbody>
                        
                    </table>
                </div>
            </div>
        </div>
        </div>
    <br />
    </div>
  
   
    <script type="text/ng-template" id="/statusCompanyDocument.html">
        <div class="modal-header bg-primary ng-scope">

            <h4 id="myModalLabel" class="modal-title">
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-thumbs-up"></i>
                </span>Status 
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="statusCompanyDocument" ng-submit="form.statusCompanyDocument()" autocomplete="off" class="form-validate form-horizontal">
                <div class="pd-sm-10">
                    <div class="row ">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="row ">
                                    <div class="col-sm-4 col-sm-4 col-sm-4">                                   
                                        <label> Company Document Name </label>
                                    </div>  
                                    <div class="col-sm-8 col-sm-8 col-sm-8"> 
                                        {{companydocument_name}}
                                    </div>
                               
                            </div>
                            <br/>
                            <div class="row ">
                                <div class="col-sm-4 col-sm-4 col-sm-4">                                   
                                    <label>Status  </label>
                                </div>  
                                <div class="col-sm-8 col-sm-8 col-sm-8"> 
                                    <div class="radio c-radio">
                                        <label>
                                            <input type="radio" name="rbo_status" value="Y" ng-model="rbo_status" ng-click="status_yes()" />
                                            &nbsp;&nbsp;<span class="fa fa-circle"></span> Active
                                        </label>
                                        <label>
                                            <input type="radio" name="rbo_status" value="N" ng-model="rbo_status" ng-click="status_no()" />
                                            <span class="fa fa-circle"></span> Inactive
                                        </label>
                                    </div>
                                </div>
                           
                        </div>                      
                            <div class="row ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="mda-form-group float-label">
                                        <textarea ng-model="txtremarks" style="resize:vertical;" required rows="4" aria-multiline="true" tabindex="0" aria-invalid="false" class="mda-form-control"></textarea>
                                        <label>Remarks *</label>
                                    </div>  
                                </div>
                            </div>
                        </div>
                    </div>     
                  
                </div>
                <div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="statusCompanyDocument.$invalid" ng-click="update_status()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
                <br/>
                <div class="table-responsive" ng-show="companydocumentinactivelog_data!=null">
                    <div style="max-height:300px;overflow:auto;" >
                        <table class="table table-striped table-secondary">
                            <thead class="bg-gray text-white">
                                <tr width="100%">
                                    <th align="center" scope="col">S.No</th>
                                    <th align="center" scope="col">Status</th>
                                    <th align="center" scope="col">Updated By</th>
                                    <th align="center" scope="col">Updated Date</th>
                                    <th align="center" scope="col">Remarks</th>
                                    
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in companydocumentinactivelog_data" width="100%">
                                    <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                    <td align="left" scope="col" title="'Status'">{{data.status}} </td>
                                    <td align="left" scope="col" title="'Updated By'">{{data.updated_by}}</td>
                                    <td align="left" scope="col" title="'Updated Date'">{{data.updated_date}}</td>
                                    <td align="left" scope="col" title="'Remarks'">{{data.remarks}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </form>
        </div>
    </script>
</div>