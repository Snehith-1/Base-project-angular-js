<h3>
    Approval And Approval History Summary
</h3>
<div ng-controller="AtmTrnAuditApprovalController">
    

    <div id="panelDemo5" class="panel panel-default">
        <div collapse="panelDemo5" class="panel-wrapper collapse" style="height: 0px;">
            <div class="panel-body">
                <div class="panel">
                    <tabset justified="true">
                        <tab>
                            <tab-heading>
                                <em class="fa  fa-check-circle  fa-1x"></em> Approval &nbsp;
                            </tab-heading>
                            <div>
                                <div class="table-responsive">
                                    <table class="table">
                                        <thead class="bg-gray text-white">
                                            <tr class="text-white" style="background-color:#09385f;">
                                                <th>S.No</th>
                                                <th>Approval Person</th>
                                                <th>Approval Type</th>
                                                <th>Approval Status</th>
                                                <th>Approve/Rejected Checker Date</th>
                                                <th>Approve/Rejected Checker By</th>
                                                <th>Approval Remarks</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in approvalhistory_list">
                                                <td data-title="'S.No'"> {{$index+1}} </td>
                                                <td data-title="'CustomerName'">{{data.approval_name}}</td>
                                                <td data-title="'created_by'">{{data.approval_type}}</td>
                                                <td data-title="'CustomerState'">{{data.approval_status}}</td>
                                                <td data-title="'CustomerState'">{{data.created_date}}</td>
                                                <td data-title="'CustomerState'">{{data.created_by}}</td>
                                                <td data-title="'CustomerState'">{{data.approve_remark}}</td>
                                                <td class="nowrap" data-title="'Action'" style="text-align: center;">
                                                    <button title="View" ng-click="auditview(data.observationapproval_gid,data.auditcreation_gid)" class="btn btn-info">
                                                        <em class="fa fa-eye"></em>
                                                    </button>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </tab>

                        <tab>
                            <tab-heading>
                                <em class="fa fa-history  fa-1x"></em> Approval History
                            </tab-heading>
                            <div>
                                <div class="table-responsive">
                                    <table class="table">
                                        <thead class="bg-gray text-white">
                                            <tr class="text-white" style="background-color:#09385f;">
                                                <th>S.No</th>
                                                <th>Approval Person</th>
                                                <th>Approval Type</th>
                                                <th>Approval Status</th>
                                                <th>Approval/Rejected Remark</th>
                                                <th>Approval/Rejected Date</th>
                                                <th>Approval/Rejected By</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in auditapprovalhistory_list">
                                                <td data-title="'S.No'"> {{$index+1}} </td>
                                                <td data-title="'CustomerName'">{{data.approval_name}}</td>
                                                <td data-title="'created_by'">{{data.approval_type}}</td>
                                                <td data-title="'CustomerState'">{{data.approval_status}}</td>
                                                <td data-title="'CustomerState'">{{data.approve_remark}}</td>
                                                <td data-title="'CustomerState'">{{data.created_date}}</td>
                                                <td data-title="'CustomerState'">{{data.created_by}}</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </tab>

                    </tabset>
                </div>
            </div>
        </div>
        <div class="text-center">
            <button ng-click="back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
        </div>
        <br />
    </div>


</div>
