<h3>
    My Audit Task
</h3>
<div ng-controller="AtmTrnAuditeeCheckerSummaryController">

    <div class="row">
        <div class="col-lg-2 col-md-2 col-sm-2">
            <div class="list-group-item picColor" style="height:60px;">
                <div class="media-box-heading" style="padding-top:10px;">
                    <div class="pull-right label" style="background-color:#141478;padding-top:5px;">{{auditeemaker_count}}</div>
                    <div style="font-size:14px;color:#141478;text-align:center;" ng-click="auditee_maker()"><b>Auditee Maker </b></div>
                </div>
            </div>
        </div>

        <div class="col-lg-2 col-md-2 col-sm-2">
            <div class="list-group-item picColor" style="height:60px;">
                <div class="media-box-heading" style="padding-top:10px;">
                    <div class="pull-right label" style="background-color:#141478;padding-top:5px;">{{auditeechecker_count}}</div>
                    <div style="font-size:14px;color:#141478;text-align:center;" ng-click="auditee_checker()">
                        <b>
                            Auditee Checker
                        </b>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-2">
            <div class="list-group-item picColor" style="height:60px;">
                <div class="media-box-heading" style="padding-top:10px;">
                    <div class="pull-right label" style="background-color:#141478;padding-top:5px;">{{maker_count}}</div>
                    <div style="font-size:14px;color:#141478;text-align:center;" ng-click="maker()"><b>Auditor Maker </b></div>
                </div>
            </div>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-2">
            <div class="list-group-item picColor" style="height:60px;">

                <div class="media-box-heading" style="padding-top:10px;">
                    <div class="pull-right label" style="background-color:#141478;padding-top:5px;">{{checker_count}}</div>
                    <div style="font-size:14px;color:#141478;text-align:center;" ng-click="checker()"><b>Auditor Checker</b></div>
                </div>
            </div>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-2">
            <div class="list-group-item picColor" style="height:60px;">

                <div class="media-box-heading" style="padding-top:10px;">
                    <div class="pull-right label" style="background-color:#141478;padding-top:5px;">{{approver_count}}</div>
                    <div style="font-size:14px;color:#141478;text-align:center;" ng-click="Approver()"><b>Auditor Approver</b></div>
                </div>
            </div>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-2">
            <div class="list-group-item picColor" style="height:60px;">
                <div class="media-box-heading" style="padding-top:10px;">
                    <div class="pull-right label" style="background-color:#141478;padding-top:5px;">{{taggedsamplecounts}}</div>
                    <div style="font-size:14px;color:#141478;text-align:center;" ng-click="taggedsamples()"><b>Tagged Samples  </b></div>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="panel panel-default ">
            <br />
            <div class="table-responsive">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <table datatable="ng" class="table table-hover table-striped">
                        <thead>
                            <tr class="text-white" style="background-color:#09385f;">
                                <th>S.No</th>
                                <th>Unique ref no</th>
                                <th>Audit Title</th>
                                <th>Audit Priority </th>
                                <th>Maker</th>
                                <th>Checker</th>
                                <th>TAT</th>
                                <th>Creation Date</th>
                                <th>Creation By</th>
                                <th>Status</th>
                                <th>Approval Status</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="data in myaudittaskauditee_list">
                                <td data-title="'S.No'">{{$index+1}}</td>
                                <td data-title="'Unique ref no'">{{data.audit_uniqueno}}</td>
                                <td data-title="'Audit Name'">{{data.audit_name}}</td>
                                <td data-title="'Audit Name'">{{data.auditpriority_name}}</td>
                                <td data-title="'Maker'">{{data.audit_maker}}</td>
                                <td data-title="'Checker'">{{data.audit_checker}}</td>
                                <td data-title="'TAT'">{{data.due_date}}</td>
                                <td data-title="'Created Date'">{{data.created_date}}</td>
                                <td data-title="'Created Date'">{{data.created_by}}</td>
                                <td data-title="'Status'">{{data.status_update}}</td>
                                <td data-title="'Status'">{{data.approval_status}}</td>
                                <td data-title="'Action'" class="nowrap">
                                    <button title="view" ng-click="viewtask(data.auditcreation_gid)" class="btn bg-blue-500">
                                        <em class="fa fa-eye"></em>
                                    </button>
                                    <button title="Audit Raise Query" ng-disabled="data.status_update == 'Closed'" ng-click="auditraisequery(data.auditcreation_gid)" class="btn btn-primary active">
                                        <em class="fa fa-comment-o"></em>
                                    </button>
                                    <!--<button title="Update observations" ng-hide="data.auditapprover_gid == employee_gid" ng-disabled="data.status_update == 'Closed'" ng-click="updateobservations(data.auditcreation_gid)" class="btn btn-green">
                                        <em class="fa fa-clipboard"></em>
                                    </button>-->
                                    <button title="Status Update" ng-hide="data.auditapprover_gid == employee_gid" ng-disabled="data.status_update == 'Closed'" ng-click="statusupdate(data.auditcreation_gid)" class="btn btn-green">
                                        <em class="fa fa-retweet"></em>
                                    </button>
                                    <button title="Auditee Checker Approval" ng-show="(data.auditeechecker_gid == employee_gid) && (data.approval_status =='Maker Initiated' || data.approval_status =='Auditor Checker Approval')" ng-click="auditeecheckerapproval(data.auditcreation_gid)" class="btn btn-green">
                                        <em class="fa fa-thumbs-o-up"></em>
                                    </button>

                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <br />
            </div>
            <br />
        </div>
    </div>
</div>
    </div>
</div>
<script type="text/ng-template" id="/statusupdate.html">
    <div class="modal-header bg-primary ng-scope">

        <h4>
            &emsp;
            <span class="btn-label bg-primary">
                <i class="fa fa-plus "></i>
            </span> Status Update
        </h4>
    </div>
    <div class="modal-body ng-scope">
        <form novalidate="" name="statusupdate" ng-submit="form.status_update()" autocomplete="off" class="form-validate form-horizontal">
            <div class="pd-sm-10">
                <div class="row ">
                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                        <div class="row ">
                            <div class="col-sm-4 col-sm-4 col-sm-4">
                                <label> Audit Name </label>
                            </div>
                            <div class="col-sm-8 col-sm-8 col-sm-8">
                                {{txtaudit_name}}
                            </div>

                        </div>
                        <br />
                        <div class="row ">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <label>Status Update <span style="color:red">*</span></label>
                                <select id="statusupdate" name="statusupdate" chosen="" ng-model="status_update" required ng-change="onselectedchangeaudit(status_update)" class="chosen-select form-control">
                                    <option value="Closed">Closed</option>
                                    <option value="Open">Open</option>
                                    <option value="Hold">Hold</option>
                                </select>
                            </div>
                        </div>
                        <br />
                    </div>
                </div>
            </div>
            <br />
            <div class="text-center">
                <button type="button" class="btn btn-success btn-md" ng-disabled="status_update.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
            </div>
            <br />
        </form>
    </div>
</script>
<script type="text/ng-template" id="/getApprovalRequest.html">
    <md-toolbar>
        <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
            <h2><em class="fa fa-thumbs-o-up"></em>&nbsp;Get Approval</h2>
            <span flex></span>

        </div>
    </md-toolbar>
    <div class="modal-body ng-scope">
        <form novalidate="" name="teamformsubmit" ng-submit="form.submitForm1()" autocomplete="off" class="form-validate form-horizontal">
            <div class="pd-sm-10">
                <div class="row">
                    <div class="col-sm-12 col-lg-12 col-md-12">
                        <br />
                        <label class="col-form-label"><strong>Approval Type &nbsp;<span style="color:red;">*</span></strong></label>
                        <div class="col">
                            <div class="radio c-radio">
                                <label>
                                    <input type="radio" id="approvaltype" required=""
                                           name="approvaltype" value="Sequence"
                                           ng-model="approve_type" />
                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Sequence
                                </label>
                                &nbsp;
                                <label>
                                    <input type="radio" id="approvaltype" required=""
                                           name="approval_type" value="Parallel"
                                           ng-model="approve_type" />
                                    <span class="fa fa-circle"></span>Parallel
                                </label>
                            </div>
                        </div>

                    </div>

                </div>
                <br />
                <div class="row">
                    <div class="col-sm-12 col-lg-12 col-md-12">
                        <label>Approval Person <span style="color:red">*</span></label>
                        <select name="approval_authority" id="approval_authority" chosen="" ng-options="m as m.employee_name for m in auditorchecker_list track by m.auditmapping_gid"
                                ng-change="Changed(cboapproval_membername)" ng-model="cboapproval_membername" class="chosen-select form-control"></select>
                    </div>
                </div>
                <br />
                <!--Tagged Member View Code Starts-->
                <p class="lead bb text-purple text-bold" style="font-size:16px;">Approval Members List</p>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div id="statictable">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th scope="col">S.No</th>
                                            <th scope="col">Member Name</th>
                                            <th scope="col">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr class="text-center" ng-repeat="data in approvalmember">
                                            <td title="S.No">{{$index+1}}</td>
                                            <td title="Member Name" align="left">{{data.employee_name}}</td>
                                            <td data-title="Action">

                                                <a title="Delete" class="btn btn-sm btn-danger" ng-click="ApprovalMembercancel(data.tmpapprovalmember_gid)">
                                                    <span class="icon icon-trash "></span>
                                                </a>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <!--Tagged Member View Code End-->
                <div class="row">
                    <div class="col-sm-12 col-lg-12 col-md-12">
                        <label>Remarks  &nbsp;<span style="color:red">*</span></label>
                        <textarea ng-model="approve_remarks" placeholder="Enter Remarks" rows="3" maxlength="1000" class="form-control"
                                  required id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                    </div>
                </div>
                <br />
            </div>
            <br />
            <div>
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="teamformsubmit.$invalid" ng-click="getapprovalclick()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                </div>
            </div>
        </form>
    </div>
</script>
<script type="text/ng-template" id="/raisequery.html">
    <md-toolbar>
        <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
            <h2><em class="fa fa-bolt"></em>&nbsp;Raise Query</h2>
            <span flex></span>
        </div>
    </md-toolbar>
    <div class="modal-body ng-scope">
        <form name="raisequery" id="raisequery" novalidate="" class="form-validate mb-lg" autocomplete="off">
            <div class="pd-sm-10">
                <div class="row ">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <label>Select User <span style="color:red">*</span></label>
                        <select name="cboemployees" chosen="" tabindex="0" multiple="multiple"
                                ng-options="m.employee_name for m in employee_list" required=""
                                ng-model="cboemployee_name" class="chosen-select form-control"></select>
                    </div>
                </div>
                <br />
                <div class="row ">
                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                        <div class="mda-form-group float-label">
                            <textarea ng-model="txtdescription" style="resize:vertical;" rows="4" aria-multiline="true" tabindex="0" aria-invalid="false" class="mda-form-control"></textarea>
                            <label>Description *</label>
                        </div>
                    </div>
                </div>
                <br />

                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                </div>
            </div>
        </form>
    </div>
</script>
<script type="text/ng-template" id="/importexcel.html">
    <div class="modal-header bg-blue-600 ng-scope">
        <h4 id="myModalLabel" class="modal-title">
            <span class="btn-label bg-blue-600">
                <em class="fa fa-plus"></em>
                Import Excel
            </span>
        </h4>
    </div>
    <div class="modal-body ng-scope">
        <div class="pd-sm-10">
            <br />
            <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <a ng-click="downloadtemplate_importexcel()" style="font-size: 13px;">
                                    <b> Click here to download Template </b>
                                </a>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label> Upload Excel : </label>
                            </div>
                            <div class="col-md-4 col-lg-4 col-sm-4">
                                <input type="file" id="fileimport" ng-model="test_document"
                                       onchange="angular.element(this).scope().excelupload(this.files, angular.element(this).scope().documentupload, this.name)"
                                       data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                &nbsp;
                            </div>
                            <div class="col-md-2 col-lg-2 col-sm-2">
                                <button title="Upload Excel" ng-click="uploadexcel()" class="btn btn-info">
                                    <em class="icon-cloud-upload"></em>
                                </button>
                                <span>
                                    <button title="Cancel Upload" ng-click="uploadexcelcancel()" class="btn btn-danger">
                                        <i class="fa icon-close"></i>
                                    </button>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <br />
                <div class="class">
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>

