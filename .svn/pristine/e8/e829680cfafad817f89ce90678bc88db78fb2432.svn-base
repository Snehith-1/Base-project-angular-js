                                                            <th class="text-nowrap" ng-repeat="(key, val) in raisedqueryarray[0]" ng-hide="key == 'taguser_flag'">{{stripAddr(key)}}</th>
                                                            <th class="text-center">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="row in raisedqueryarray" ng-style="row.taguser_flag == 'Y'?{'background-color':'lightblue'}:{}">
                                                            <td>{{$index +1}}</td>
                                                            <td ng-repeat="column in row" ng-hide="column == 'Y' || column == 'N'">
                                                                {{ column }}
                                                            </td>
                                                            <td class="text-nowrap text-center" data-title="'Action'">
                                                                <button title="Sample Query" ng-click="samplequery($index)" class="btn btn-info active">
                                                                    <em class="fa fa-comment-o"></em>
                                                                </button> 
                                                                <button title="Tag Sample" ng-click="btntaguser($index,auditcreation_gid,'Y')" class="btn btn-green active">
                                                                    <em class="icon icon-tag"></em>
                                                                </button> 
                                                                <button title="Observation  Score for Sample" ng-hide="auditobservation_name =='Observation score overall audit'" ng-click="samplecheckerobservationscoreview(auditcreation_gid,row.checklistmaster_gid,row.sampleimport_gid,row.observationscoresample_gid)" class="btn bg-blue-500">
