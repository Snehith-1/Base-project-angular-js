    <script type="text/ng-template" id="/raisequery.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-comment-o"></em>&nbsp;Raise Query</h2>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>
        </md-toolbar>
        <div class="modal-body ng-scope">
            <form name="raisequery" id="raisequery" novalidate="" class="form-validate mb-lg" autocomplete="off">

                <div class="pd-sm-10">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <label> Sample Name : </label>&nbsp;<span> {{txtsample_name}} </span>

                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <label class="col-form-label"> Query Title </label> <span style="color:red"> *</span>
                            <br />
                            <input ng-model="txtquery_title" type="text" tabindex="0" required
                                   aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                        </div>
                    </div>

                    <div class="row" style="display:none;">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <label>Query To </label>
                            <br />
                            <select name="approvalperson" id="approvalperson" chosen="" ng-options="m.employee_name for m in cboemployee_list"
                                    ng-model="cboqueryto" class="chosen-select form-control"></select>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <label>Query Description <span style="color:red">*</span></label>
                            <br />
                            <!--<textarea ng-model="txtquery_desc" rows="3" class="form-control" style="resize:vertical;"
                                      id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true" required></textarea>-->
                            <textarea style="resize:vertical;" ng-model="txtquery_desc" required rows="4" aria-multiline="true" tabindex="0" aria-invalid="false" class="form-control"></textarea>

                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="raisequery.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
            </form>
        </div>
    </script>
    <script type="text/ng-template" id="/taguser.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-tag"></em>&nbsp;Tag User</h2>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>
        </md-toolbar>
        <div class="modal-body ng-scope">
            <form name="taguser" id="taguser" novalidate="" class="form-validate mb-lg" autocomplete="off">
                <div class="pd-sm-10">
                    <div class="row ">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <label> Sample Name:  </label> <span>{{txtsample_name}}</span>

                        </div>
                    </div>
                    <br />
                    <div class="row ">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <label>Select User <span style="color:red">*</span></label>
                            <select name="cboemployees" chosen="" tabindex="2" multiple="multiple"
                                    ng-options="m.employee_name for m in employee_list" required=""
                                    ng-model="cboemployee_name" class="chosen-select form-control"></select>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <label>Tag Remarks <span style="color:red">*</span></label>
                            <br />
                            <!--<textarea ng-model="txttaguser_desc" rows="3" class="form-control" style="resize:vertical;"
                                      id="input_13" required ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>-->
                            <textarea style="resize:vertical;" ng-model="txttaguser_desc" required rows="4" aria-multiline="true" tabindex="0" aria-invalid="false" class="form-control"></textarea>

                        </div>
                    </div>
                    <br />
                    <div class="class">
                        <div class="text-center">
                            <!--<a class="btn btn-success" ng-click="btnconfirm()" ng-disabled="taguser.$invalid">
                                <em class="fa fa-check"></em>
                                <span>Confirm</span>
                            </a>
                            <a class="btn btn-danger" ng-click="ok()">
                                <em class="icon icon-close"></em>
                                <span>Cancel</span>
                            </a>-->
                            <button type="button" class="btn btn-success btn-md" ng-disabled="taguser.$invalid" ng-click="btnconfirm()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Confirm</button>&nbsp;&nbsp;
                            <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Cancel</button>

                        </div>
                    </div>
                    <br />
                    <div class="row dd-handle" ng-if="SampleTagUserList.length > 0">
                        <div class="col-sm-12 col-lg-12 col-md-12">
                            <label class="text-primary">Tagged Details </label>
                            <hr />
                            <div class="table-responsive">
                                <table class="table table-striped table-secondary">
                                    <thead>
                                        <tr>
                                            <th align="center" scope="col">S.No</th>
                                            <th align="center" scope="col">Tagged Information</th>
                                            <th align="center" scope="col">Tagged By</th>
                                            <th align="center" scope="col">Tagged Date & Time</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="data in SampleTagUserList">
                                            <td align="left" scope="col" title="S.No">
                                                {{$index+1}}
                                            </td>
                                            <td align="left" scope="col" title="Reassigned information">
                                                <p><b>Tagged user -</b> {{data.assigned_to}} </p>
                                                <p><b>Tagged Remarks -</b> {{data.description}}</p>
                                            </td>
                                            <td align="left" scope="col" title="Reassigned By">
                                                {{data.assigned_by}}
                                            </td>
                                            <td align="left" scope="col" title="Reassigned Date/Time">
                                                {{data.assigned_date}}
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </script>
    <script type="text/ng-template" id="/Approvalinformation.html">
        <!--<div class="modal-header bg-primary">
                <h4 class="modal-title">
                    <span class="btn-label bg-primary">
                        <i class="fa fa-info-circle">&nbsp; Assigned Information</i>
                       </span>
