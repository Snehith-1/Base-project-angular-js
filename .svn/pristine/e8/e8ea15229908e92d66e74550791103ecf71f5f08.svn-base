                                            <th>Name</th>
                                            <th>Age</th>
                                            <th>Designation</th>
                                            <th>Mobile No</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="promoter in promoters_list">
                                            <td data-title="'Name''">{{promoter.promoter_name}}</td>
                                            <td data-title="'Age''">{{promoter.promoter_age}}</td>
                                            <td data-title="'Designation''">{{promoter.promoter_designation}}</td>
                                            <td data-title="'Mobile No'">{{promoter.promotermobile_no}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                                
                            </div>

