                                            <td data-title="'Age''">{{guarantor.guarantor_age}}</td>
                                            <td data-title="'Designation''">{{guarantor.networth}}</td>
                                            <td data-title="'Mobile No'">{{guarantor.basisofNW}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
