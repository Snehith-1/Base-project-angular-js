                                                            <table class="table">
                                                                <thead class="bg-gray text-white">
                                                                    <tr>
                                                                        <th>S.No</th>
                                                                        <th>IFSC Code</th>
                                                                        <!-- <th>Primary Status</th> -->
                                                                        <th>Created Date </th>
                                                                        <th>Created By</th>
