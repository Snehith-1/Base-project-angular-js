<h3>
    Lawyer Empanelment
</h3>
<div ng-controller="lawfirmSummarycontroller">
    <div>
        <button type="button" title="Add Law Firm" class="btn btn-default bg-grey-400  btn-sm" ng-click="popuplawfirm()">
            <span class="btn-label">
                <i class="fa fa-plus"></i>
            </span><b>Add Law Firm</b>
        </button>
    </div>
    <br />
    <div class="row">
        <div class="panel">
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            Showing 1 to {{totalDisplayed}} of {{total}} Entries
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4">

                            Show <input ng-model="pagecount" id="pagecount" type="number" />
                            <button class="btn" id="loadmore" ng-click="loadMore(pagecount)">Load More</button>

                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <div class="input-group">
                                <input type="text" ng-model="searchtxt" placeholder="Search Here .." class="form-control input-sm" />
                                <span class="input-group-btn">
                                    <button type="submit" class="btn btn-default btn-sm">
                                        <i class="fa fa-search"></i>
                                    </button>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <br>
                <div class="table-responsive">
                    <table ng-table="tableParams2" class="table hover table-striped ">
                        <thead>
                            <tr class="bg-primary text-white">
                                <th style="display:none;">S.NO</th>
                                <th>Created Date</th>
                                <th>Firm Ref No</th>
                                <th>Firm Name</th>
                                <!--<th>No of Years in Practice</th>-->
                                <th>Email Address</th>
                                <th>Created By</th>
                                <th>Status</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="data in lawfirm_data  | filter : searchtxt | limitTo:totalDisplayed">
                                <td data-title="'S.No'" style="display:none;">{{$index+1}}</td>
                                <td data-title="'Created Date'" style="text-align:center;">{{data.created_date}}</td>
                                <td data-title="'firmref.no'" style="text-align:center;">{{data.firm_refno}}</td>
                                <td data-title="'firm Name'">{{data.firm_name}}</td>
                                <!--<td data-title="'no of years'" style="text-align:center;">{{data.no_years}}</td>-->
                                <td data-title="'Email Address'" >{{data.email_address}}</td>
                                <td data-title="'Created By'" style="text-align:center;">{{data.created_by}}</td>
                                <td data-title="Status" ng-switch="data.lawfirmuser_status" style="text-align:center;">
                                    <label class="label label-success" ng-switch-when='Y' style="font-size:10px;">Active</label>
                                    <label class="label label-danger" ng-switch-when='Inactive' style="font-size:10px;">InActive</label>
                                    <label class="label label-warning" ng-switch-when='Block' style="font-size:10px;">Block</label>
                                    <label class="label label-pink" ng-switch-when='Existingcases' style="font-size:10px;">Continue Only Existing Cases</label>
                                    <label class="label label-purple" ng-switch-default style="font-size:10px;">Not Created</label>
                                </td>

                                <td data-title="'Action'" class="nowrap" ng-switch="data.lawfirmuser_status" style="text-align:center;">
                                    <button title="View Details" ng-click="Viewfirm(data.lawfirm_gid)" class="btn bg-light-blue-600">
                                        <em class="fa fa-eye"></em>
                                    </button>
                                    <button title="Edit" ng-click="edit(data.lawfirm_gid)" class="btn btn-green">
                                        <em class="fa fa-edit "></em>
                                    </button>
                                    <button title="Login Activate" ng-click="logincreation(data.lawfirm_gid)" class="btn btn-purple" ng-switch-default>
                                        <em class="icon-lock"></em>
                                    </button>
                                    <button title="Login InActivate" ng-click="viewlogin(data.lawfirm_gid)" class="btn btn-warning" ng-switch-when='Y'>
                                        <em class="icon-lock"></em>
                                    </button>
                                    <button title="Lawyer Login Details" ng-click="viewlogin(data.lawfirm_gid)" class="btn btn-success" ng-switch-when='Inactive'>
                                        <em class="icon-lock"></em>
                                    </button>
                                    <button title="Lawyer Login Details" ng-click="viewlogin(data.lawfirm_gid)" class="btn btn-success" ng-switch-when='Block'>
                                        <em class="icon-lock"></em>
                                    </button>
                                    <button title="Lawyer Login Details" ng-click="viewlogin(data.lawfirm_gid)" class="btn btn-success" ng-switch-when='Existingcases'>
                                        <em class="icon-lock"></em>
                                    </button>
                                    <!--<button title="Delete" ng-click="delete(data.lawfirm_gid)" class="btn btn-danger">
                                        <em class="icon icon-trash "></em>
                                    </button>-->
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <script type="text/ng-template" id="/lawfirmLoginContent.html">

        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="icon-lock"></em>&nbsp;Law Firm Login Creation</h2>
                <span flex></span>
                <md-button class="md-icon-button" ng-click="close()" title="Close">
                    <md-icon md-svg-src="app/img/icons/ic_close_24px.svg" aria-label="Close dialog"></md-icon>
                </md-button>
            </div>
        </md-toolbar>
        <div class="modal-body ng-scope">
            <div class="panel-body">
                <form method="post">
                    <div class="input-group form-group">
                        <span class="input-group-addon text-sm">Law Firm Name:</span>
                        <input type="text" ng-model="firm_name" class="form-control" disabled />
                    </div>
                    <div class="input-group form-group">
                        <span class="input-group-addon text-sm">Law Firm Login Code:</span>
                        <input type="text" ng-model="firm_refno" class="form-control" />
                    </div>
                    <div class="input-group form-group">
                        <span class="input-group-addon text-sm">Law Firm Login Password:</span>
                        <input type="text" ng-model="lawfirm_userpassword" class="form-control" />
                    </div>
                </form>
            </div>
        </div>
        <div class="text-center">
            <button ng-click="sendaccouncreate()" class="btn btn-success">
                <i class="fa fa-send (alias)">
                </i>&nbsp;Submit
            </button>
            <button ng-click="ok()" class="btn btn-danger">
                <i class="fa fa-close (alias)">
                </i>&nbsp;Cancel
            </button>
        </div>
        <br />
    </script>
    <script type="text/ng-template" id="/ViewlawfirmLoginContent.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="icon-lock"></em>&nbsp;Law Firm Login Details</h2>
                <span flex></span>
                <md-button class="md-icon-button" ng-click="close()" title="Close">
                    <md-icon md-svg-src="app/img/icons/ic_close_24px.svg" aria-label="Close dialog"></md-icon>
                </md-button>
            </div>
        </md-toolbar>
        <div class="modal-body ng-scope">
            <div class="panel-body">
                <form method="post">
                    <div class="input-group form-group">
                        <span class="input-group-addon text-sm">Law Firm Name:</span>
                        <input type="text" ng-model="firm_name" class="form-control" disabled />
                    </div>
                    <div class="input-group form-group">
                        <span class="input-group-addon text-sm">Law Firm Login Code:</span>
                        <input type="text" ng-model="firm_refno" class="form-control" disabled />
                    </div>
                    <div class="input-group form-group" ng-hide="resetpassword">
                        <span class="input-group-addon text-sm">Law Firm Login Password:</span>
                        <input type="text" ng-model="lawfirmuser_password" class="form-control" disabled />



                    </div>
                    <div class="input-group form-group" ng-show="resetpassword">
                        <span class="input-group-addon text-sm">Law Firm Login Password:</span>
                        <input type="text" placeholder="Enter New Password" ng-model="lawfirmuser_password" class="form-control" />



                    </div>
                    <div ng-show="loginuserstatus=='Y'||loginuserstatus=='Existingcases'"><a class="text-sm pull-right" ng-hide="resetpassword" ng-click="resetpwd()">Reset Password</a></div>

                    <div class="input-group form-group">
                        <label>Law Firm Login Status :&emsp;</label><label>
                            <label class="radio-inline c-radio">
                                <input id="inlineradio1" type="radio" name="i-radio" value="Y" ng-change="mandatorycheck()" checked="checked" ng-model="loginuserstatus">
                                <span class="fa fa-circle"></span>Active
                            </label>
                            <label class="radio-inline c-radio">
                                <input id="inlineradio2" type="radio" name="i-radio" value="Inactive" ng-change="mandatorycheck()" ng-model="loginuserstatus">
                                <span class="fa fa-circle"></span>Inactive
                            </label>
                            <label class="radio-inline c-radio">
                                <input id="inlineradio3" type="radio" name="i-radio" value="Block" ng-change="mandatorycheck()" checked="checked" ng-model="loginuserstatus">
                                <span class="fa fa-circle"></span>Block
                            </label>
                            <label class="radio-inline c-radio">
                                <input id="inlineradio4" type="radio" name="i-radio" value="Existingcases" ng-change="mandatorycheck()" ng-model="loginuserstatus">
                                <span class="fa fa-circle"></span>Continue Only Existing Cases
                            </label>
                        </label>
                    </div>
                    <div ng-show="loginuserstatus=='Block'">
                        <div class="row" ng-if="lawyerstatus_remarks==''">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">

                                <label><strong>Remarks</strong> </label>
                                <textarea rows="2" aria-multiline="true" tabindex="0" aria-invalid="false" ng-model="$parent.txtblockremarks" class="form-control"></textarea>

                            </div>
                        </div>
                        <div class="row" ng-if="lawyerstatus_remarks!=''">
                            <div class="col-sm-12 col-sm-12 col-sm-12 " ng-click="remarks()">

                                <label><strong>Remarks :</strong> </label>&nbsp;&nbsp;{{block_remarks}}

                            </div>
                        </div>
                    </div>


                    <div class="row" ng-show="mandatoryfields">

                        <div class="col-sm-12 col-md-12 col-lg-12 text-center">

                            <label style="color:red;"> Enter Remarks</label>
                            <br />
                        </div>
                    </div>
                </form>

            </div>
        </div>
        <div class="text-center">
            <button ng-click="sendaccounstatus()" class="btn btn-success">
                <i class="fa fa-arrow-circle-right">
                </i>&nbsp;Submit
            </button>
            <button ng-click="ok()" class="btn btn-danger">
                <i class="fa fa-close (alias)">
                </i>&nbsp;Cancel
            </button>
        </div>
        <br />
    </script>
    </div>
