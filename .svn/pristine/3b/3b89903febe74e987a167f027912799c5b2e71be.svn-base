<h3>
    Auditor Mapping
</h3>
<div ng-controller="AtmMstAuditMappingAddController">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <button type="button" title="Add Auditor Mapping" class="btn btn-info btn-sm" ng-click="auditmappingadd()">
                <span class="btn-label">
                    <i class="fa fa-plus"></i>
                </span> Add Auditor Mapping
            </button>
        </div>
    </div>
    <br />
    <div class="panel">
        <br />
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="table-responsive">
                        <table datatable="ng" class="table table-hover table-striped">
                            <thead>
                                <tr class="text-white" style="background-color:#09385f;">
                                    <th style="display:none;"> S.No </th>                                    
                                    <th> Audit Mapping Code</th> 
                                    <th> Auditor Type</th>
                                    <th> Auditor Name </th>
                                    <th> LMS Code</th>
                                    <th> Bureau Code</th>
                                    <th> Created By </th>
                                    <th> Created Date </th>
                                    <th> Status </th>
                                    <th class="text-center">Action</th>
                                </tr>
                            </thead>
                            <tbody>

                                <tr ng-repeat="data in auditmapping_list ">
                                    <td data-title="'S.No'" style="display:none;" class="text-center">{{$index+1}}</td>                                    
                                    <td data-title="'Audit Mapping Code'">{{data.auditmapping_code}}</td> 
                                    <td data-title="'Audit Mapping Name'">{{data.auditmapping_name}}</td>    
                                    <td style="text-decoration:underline;text-decoration-color:#49afcd;">
                                        <label style="padding-right:25px; font-size: 15px;text-decoration:underline;text-decoration-color:#49afcd;"
                                               ng-click="showPopover(data.auditmapping_gid, data.auditmapping_name)">
                                            <em class="fa fa-question-circle" style="font-size:24px;align-content:center;"></em>
                                        </label>
                                    </td>    
                                    <td data-title="'LMS Code'">{{data.lms_code}}</td>
                                    <td data-title="'Bureau Code'">{{data.bureau_code}}</td>                               
                                    <td data-title="'Created By'">{{data.created_by}}</td>
                                    <td data-title="'Created Date'">{{data.created_date}} </td>
                                    <td data-title="'Status'" ng-switch="data.status">
                                        <label class="label label-success" ng-switch-when='Active' style="font-size:10px;">Active</label>
                                        <label class="label label-warning" ng-switch-when='Inactive' style="font-size:10px;">InActive</label>
                                    </td>
                                    <td class="nowrap" data-title="'Action'" style="text-align: center;" ng-switch="data.status">
                                        <button title="Edit" ng-click="editauditmapping(data.auditmapping_gid)" class="btn btn-green">
                                            <em class="fa fa-edit"></em>
                                        </button>
                                        <button title="Status" ng-switch-when='Active' ng-click="Status_update(data.auditmapping_gid)" class="btn btn-success">
                                            <em class="fa fa-thumbs-up"></em>
                                        </button>
                                        <button title="Status" ng-switch-when='Inactive' ng-click="Status_update(data.auditmapping_gid)" class="btn btn-warning">
                                            <em class="fa fa-thumbs-down"></em>
                                        </button>
                                        <button title="Delete" ng-click="deleteauditmapping(data.auditmapping_gid)" class="btn btn-danger">
                                            <em class="icon icon-trash "></em>
                                        </button>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <br />
    </div>
    <script type="text/ng-template" id="/addauditmapping.html">
        <div class="modal-header bg-primary ng-scope">
            <h4 id="myModalLabel" class="modal-title">
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-plus "></i>
                </span> Add Auditor Mapping
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="addauditmapping" ng-submit="form.addauditmapping()" autocomplete="off" class="form-validate form-horizontal">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="row">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtauditmapping_code" maxlength="20" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="" disabled>
                                    <label>Auditor Mapping Code<span style="color:red">*</span></label>                                    
                                </div>
                            </div>
                        </div>
                        <div class="row ">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <label>Auditor Mapping <span style="color:red">*</span></label>
                                <select id="auditmapping" name="auditmapping" chosen="" ng-model="audit_mapping" required  ng-change="onselectedchangeaudit(audit_mapping)" class="chosen-select form-control">
                                    <option value="Maker">Maker</option>
                                    <option value="Checker">Checker</option>                                 
                                    <option value="Approver">Approver</option>
                                    <option value="Manager">Manager</option>
                                </select>
                            </div>
                        </div>
                        <br />
                        <div class="row ">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <label>Employee Name <span style="color:red">*</span></label>
                                <select name="cboemployees" chosen="" tabindex="2" multiple="multiple" 
                                        ng-options="m.employee_name for m in employee_list" required=""
                                        ng-model="cboemployee" class="chosen-select form-control"></select>
                            </div>
                        </div>
                        <br />  
                        <div class="row ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtlms_code" maxlength="30" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>LMS Code </label>
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtbureau_code" maxlength="10" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Bureau Code </label>
                                </div>
                            </div>
                        </div>
                        <br />                   
                    </div>
                </div>
                <div class="row">
                </div>
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="addauditmapping.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                </div>
                <br />
            </form>
        </div>
    </script>
    <script type="text/ng-template" id="/editauditmapping.html">
        <div class="modal-header bg-primary ng-scope">
            <h4 id="myModalLabel" class="modal-title">
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-edit "></i>
                </span>Edit Auditor Mapping
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="editauditmapping" ng-submit="form.editauditmapping()" autocomplete="off" class="form-validate form-horizontal">
                <div class="row ">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="row ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div class="mda-form-group ">
                                    <input ng-model="txteditauditmapping_code" maxlength="20" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="" disabled>
                                    <label>Auditor Mapping Code</label>
                                </div>
                            </div>
                        </div>
                        <div class="row ">
                            <div class="col-lg-12 col-md-12 col-sm-12">  
                                <div class="mda-form-group ">
                                    <input ng-model="audit_mapping" maxlength="20" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="" disabled>
                                    <label>Auditor Mapping </label>
                                    <!--<select id="auditmappingedit" name="auditmappingedit" ng-model="audit_mapping"  required class="chosen-select form-control">
            <option value="Maker">Maker</option>
            <option value="Checker">Checker</option>
            <option value="Approver">Approver</option>
            <option value="Manager">Manager</option>
        </select>--></div>
                                </div>
                            </div>                      
                        <br />
                        <div class="row ">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <label>Employee Name <span style="color:red">*</span></label>                            
                                <select name="cboemployee" id="cboemployee" ng-options="m.employee_name for m in cboemployee_editlist" chosen="" multiple="multiple"
                                        ng-model="cboemployee_edit" class="chosen-select form-control" style="height:37px;" required size="1">                                   
                                </select>
                            </div>                      
                        </div>
                        <br />
                        <div class="row ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div class="mda-form-group">
                                    <input ng-model="txteditlms_code" maxlength="30" tabindex="0"  class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>LMS Code </label>
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div class="mda-form-group ">
                                    <input ng-model="txteditbureau_code" maxlength="10" tabindex="0"  aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Bureau Code </label>
                                </div>
                            </div>
                        </div>
                        <br />
                    </div>
                </div>
                <div>
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="editauditmapping.$invalid" ng-click="update()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
                <br />
            </form>
        </div>
    </script>
    <script type="text/ng-template" id="/showemployee.html">

        <div class="panel-body text-center">
            <div class="table-responsive">
                <table class="table hover table-striped row-border">
                    <thead>
                        <tr style="text-align: left;height:40px;">
                            <th style="color:white;background:#7986cb;width:2%;text-align:center;">{{auditmapping_name}}</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td style="width:2%; text-align:left"><b> Employee Name : </b> <br /></td>
                        </tr>
                        <tr style="text-align: left;height:20px;">
                            <td data-title="'Credit head'" style="width:2%; text-align:left"> {{employee_name}}</td>
                        </tr>                     
                    </tbody>
                </table>
            </div>
            <div>
                <div class="text-center">
                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close">&nbsp;&nbsp;</i>Close</button>
                </div>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="/statusauditmapping.html">
        <div class="modal-header bg-primary ng-scope">
            <h4>
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-thumbs-up"></i>
                </span>Status
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="statusauditmapping" ng-submit="form.statusauditmapping()" autocomplete="off" class="form-validate form-horizontal">
                <div class="row ">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="row ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label> Auditor Mapping</label>
                            </div>
                            <div class="col-lg-8 col-md-8 col-sm-8">
                                {{audit_mapping }}
                            </div>
                        </div>
                        <br />
                        <div class="row ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Status  </label>
                            </div>
                            <div class="col-lg-8 col-md-8 col-sm-8">
                                <div class="radio c-radio">
                                    <label>
                                        <input type="radio" name="rbo_status" value="Y" ng-model="rbo_status" ng-click="status_yes()" />
                                        &nbsp;&nbsp;<span class="fa fa-circle"></span> Active
                                    </label>
                                    <label>
                                        <input type="radio" name="rbo_status" value="N" ng-model="rbo_status" ng-click="status_no()" />
                                        <span class="fa fa-circle"></span> Inactive
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="row ">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="mda-form-group float-label">
                                    <textarea ng-model="txtremarks" style="resize:vertical;" required rows="4" aria-multiline="true" tabindex="0" aria-invalid="false" class="mda-form-control"></textarea>
                                    <label>Remarks<span style="color:red"> *</span></label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="statusauditmapping.$invalid" ng-click="update_status()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
                <br />
                <div class="table-responsive" ng-show="auditmappinginactivelog_list!=null">
                    <div style="max-height:300px;overflow:auto;">
                        <table class="table table-striped table-secondary">
                            <thead class="bg-gray text-white">
                                <tr width="100%">
                                    <th align="center" scope="col">S.No</th>
                                    <th align="center" scope="col">Status</th>
                                    <th align="center" scope="col">Updated By</th>
                                    <th align="center" scope="col">Updated Date</th>
                                    <th align="center" scope="col">Remarks</th>

                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in auditmappinginactivelog_list" width="100%">
                                    <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                    <td align="left" scope="col" title="'Status'">{{data.status}}</td>
                                    <td align="left" scope="col" title="'Updated By'">{{data.updated_by}}</td>
                                    <td align="left" scope="col" title="'Updated Date'">{{data.updated_date}}</td>
                                    <td align="left" scope="col" title="'Remarks'">{{data.remarks}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </form>
        </div>
    </script>
</div>   