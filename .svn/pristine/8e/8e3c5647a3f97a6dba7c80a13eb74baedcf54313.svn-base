<h3>
    Edit CAD Group Assignment
</h3>
<div ng-controller="MstCADGroupAssignmentEditController">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-edit"></i>
            </span>Edit CAD Group Assignment
        </div>
        <div class="panel-body">
            <form novalidate="" name="cadgroupassignment" ng-submit="form.cadgroupassignment()" autocomplete="off" class="form-validate mb-lg">
            <br>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-4 col-md-4 col-sm-4">
                        <label>Vertical</label><span style="color:red"> *</span>
                        <select class="chosen-select form-control" chosen="" id="vertical_name" ng-model="cbovertical_name" 
                        ng-options="m.vertical_gid as m.vertical_name for m in vertical_list" required>
                    </select>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4">
                        <label>	Program</label><span style="color:red"> *</span>
                        <select class="chosen-select form-control" chosen="" id="program_name" tabindex="0" ng-model="cboprogram_name" required
                        ng-options="m.program_gid as m.program for m in program_list" style="height:37px;" size="1">
                    </select>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4">
                        <label> CAD Group Name</label><span style="color:red"> *</span>
                        <select class="chosen-select form-control" chosen="" id="cadgroup_name" tabindex="0" ng-model="cbocadgroup_name" required
                        ng-options="m.cadgroup_gid as m.cadgroup_name for m in cadgroup_list" style="height:37px;" size="1" >
                        </select>
                    </div>
                </div>
            </div>
