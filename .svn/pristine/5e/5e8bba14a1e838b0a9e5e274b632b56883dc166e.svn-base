                                                                <a download="" href="" title="Download All" id="btnpdf2" ng-click="downloadall(lrfilepath,lrfilename)">
                                                                    <em class="icon-arrow-down"></em>&nbsp;
                                                                </a>
                                                            </th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in individualupload_list">
                                                            <td title="S.No">{{$index+1}}</td>
                                                            <td title="Document Title">
                                                                {{data.document_title}}
                                                            </td>
                                                            <td title="Document ID">{{data.sadocument_id}}</td>
                                                            <td title="Document Name">
                                                                {{data.sadocument_name}}
                                                            </td>
                                                            <td data-title="Action">
                                                                <button title="Download"
                                                                        class="btn btn-green btn-sm"
                                                                        ng-click="saindivdualdoc_downloads(data.safile_path,data.sadocument_name)">
                                                                    <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                    <em class="fa fa-download"></em>
                                                                </button>
                                                                <button title="View" class="btn btn-info btn-sm" ng-click="documentviewer(data.safile_path,data.sadocument_name)">
                                                                    <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                                </button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />
                        </div>
                    </div>
                </md-content>
                </div>
            </div>
        
           
        <div class="row dd-handle ng-scope ">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <label>Tag RM Employee  <span style="color:red;"> * </span></label>

                    <select name="RM" id="RM" chosen=""
                            ng-options="m.employee_gid as m.employee_name for m in employee_list"
                            ng-model="cboemployee" required class="chosen-select form-control">
                        <option value=""> Select RM </option>

                    </select>
                </div>

                <div class="col-lg-6 col-md-6 col-sm-6">

                    <label for="">Remarks</label>&nbsp;
                    <span style="color:red">*</span>
                    <textarea rows="2" class="form-control" required style="max-width: 100%; min-width: 100%;"
                              ng-model="txtremarks" maxlength="10000"></textarea>
                </div>


            </div>





        </div>

            <br />
            <br />
            <div class="row text-center">
                <button type="button" class="btn btn-success btn-md" ng-click="update_individual_RM()" ng-disabled="!cboemployee">
                    <i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Tag RM
                </button>&nbsp;&nbsp;

                <button type="button" class="btn btn-danger btn-md" ng-click="reject_individual_RM()">
                    <i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp; Reject
                </button>&nbsp;&nbsp;

                <!--Back-->

                <button ng-click="back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
            </div>
            <br>

        </div>
                    </div>
                
                        
                
            
        
        <br>

    </div>
    <script type="text/ng-template" id="/StaticMapAndPhotosView.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-image"></em>&nbsp;<label style="font-size:16px">Static Map and Photos</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>
        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            <p class="text-primary" style="font-size:14px;"><b>&nbsp; Static Map </b></p>
                            <img ng-src="{{staticmapImgUrl}}">
                        </div>
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            <p class="text-primary" style="font-size:14px;"><b>&nbsp; Photos </b></p>
                            <div ng-repeat="item in photoUrlList" ng-show="photoFound">
                                <img ng-src="{{item}}">
                                <br />
                                <br />
                            </div>
                            <label style="font-weight:bolder;color:red" ng-show="photoNotFound">Photos not found!</label>
                        </div>
                    </div>
                </div>
                <br />
                <div class="text-center">
                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                </div>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="/rejectrequest.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-ban"></em>&nbsp; Reject </h2>
                <span flex></span>
            </div>
        </md-toolbar>
        <div class="modal-body ng-scope">
            <form novalidate="" name="rejectedSubmit" ng-submit="form.rejectedSubmit()" autocomplete="off" class="form-validate form-horizontal">
                <div class="pd-sm-10">
                    <div class="row">
                        <div class="col-sm-12 col-lg-12 col-md-12">
                            <label>Remarks  &nbsp;<span style="color:red">*</span></label>
                            <textarea ng-model="txtreject_remarks" placeholder="Enter Remarks" rows="3" maxlength="1000" class="form-control"
                                      required id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                        </div>
                    </div>
                    <br />
                </div>
                <br />
                <div>
                    <div class="text-center">

                        <button type="button" class="btn btn-success btn-md" ng-disabled="rejectedSubmit.$invalid" ng-click="rejectSubmit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
            </form>
        </div>
    </script>

</div>
