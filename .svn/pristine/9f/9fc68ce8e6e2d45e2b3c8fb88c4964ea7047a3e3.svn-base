<h3>
    Sanction MIS Report - View
</h3>
<div ng-controller="MstSanctionMISReportViewController">
    <div>
        
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="panel">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-lg-9 col-md-9 col-sm-9" style="background-color:#191370;color:white;padding-top:10px;padding-bottom:10px;font-size:14px;font-weight :bold;">
                                    <h5>{{sanctionto_name}}</h5>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3" style="background-color:#3646b8;color:white;padding-top:10px;padding-bottom:10px;">
