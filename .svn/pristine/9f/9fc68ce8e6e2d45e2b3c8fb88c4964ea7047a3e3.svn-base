                                        <label>Approved By</label>
                                        <p>{{approved_by}}</p>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                        <label>Approved Date</label>
                                        <p>{{approved_date}}</p>
                                    </div>
                                </div>
                            </div>                            
                        </div>
                    </div>


                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div id="panelDemo18"> 
                                    <div class="dd-handle">
                                        <b  style="font-size:14px;" class="text-primary"> &nbsp;&nbsp;Approval Log </b>
                                        <hr />
                                        <div class="table-responsive" ng-if="sanctionletter_flag=='Y' || sanctiontocheckerlist.length > 0">
                                            <table class="table hover table-striped ">
                                                <thead>
                                                    <tr class="text-white bg-gray-light">
                                                        <th>S.No</th>
                                                        <th align="center">Submitted Date</th>
                                                        <th align="center">Status</th>
                                                        <th align="center">Submitted By</th>
                                                        <th align="center">Remarks</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in sanctiontocheckerlist">
                                                        <td data-title="'S.No'">{{$index+1}}</td>
                                                        <td data-title="'Submitted Date'" class="text-nowrap">{{data.created_date}}</td>
                                                        <td data-title="'Status'">{{data.sanction_status}}</td>
                                                        <td data-title="'Submitted By'">{{data.created_by}}</td>
                                                        <td data-title="'Submitted By'">{{data.remarks}}</td>
                                                    </tr>

                                                </tbody>
                                            </table>
                                        </div> 
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
