<h3>
    Activity Management
</h3>
<style>
    #bordersty {
        border: 1px solid #ebebeb;
        padding: 10px;
        border-radius: 25px;
    }
</style>
<style>
      .buttonstyle {
                border:none;
                background-color: rgb(255, 255, 255);
                color:#141478;

    }
</style>

<div ng-controller="osdTrnServiceRequestView">
    <div id="panelDemo8" class="panel panel-primary ">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-eye"></i>
            </span>View Service Request
        </div>
        <div class="panel-body">
            <div class="row" style="display:none;">
                <div class="col-lg-12 col-md-12 col-sm-12 ">
                    <div class="col-lg-6 col-md-6 col-sm-6 ">
                        <input name="servicerequest_gid" id="servicerequest_gid" style="font-size:12px"
                               placeholder="Enter Service Request gid" ng-model="servicerequest_gid" class="form-control">
                    </div>
                </div>
            </div>

            <div class="modal-body ng-scope">
                <form novalidate="" name="requestform" ng-submit="form.submitLoan()" autocomplete="off"
                      class="form-validate mb-lg">
                    <div ng-show="osd_View">
                        <!--Cancelled Details-->
                        <div ng-show="canceldtls">
                            <h4 class="text-pink">
                                <em class="fa fa-ban text-danger fa-lg"></em> Cancelled Details
                            </h4>
                            <div class="panel-body">

                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label>
                                                Cancelled By
                                            </label>
                                            <br>
                                            {{raised_by}}
                                        </div>

                                        <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label>Cancelled Date</label>
                                            <br>
                                            {{cancel_date}}
                                        </div>

                                    </div>
                                </div>


                            </div>
                        </div>

                        <div ng-show="Rejectdtls">
                            <h4 class="text-pink">
                                <em class="fa fa-ban text-danger fa-lg"></em> Rejected Details
                            </h4>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label>
                                                Rejected By
                                            </label>
                                            <br>
                                            {{rejected_by}}
                                        </div>

                                        <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label>Rejected Date</label>
                                            <br>
                                            {{rejected_date}}
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                        <div class="col-lg-8 col-md-8 col-sm-8">
                                            <label><strong>Rejected Remarks</strong> </label>
                                            <br>
                                            {{rejected_remarks}}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!--Reopen Details Starts-->
                        <div ng-show="reopendetails">
                            <h4 class="text-pink">
                                <em class="fa fa-folder-open text-danger fa-lg"></em> Reopen Details
                            </h4>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>Request Ref.No</label>
                                        <br>
                                        {{request_refno}}
                                    </div>

                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>Reopened By</label>
                                        <br>
                                        {{raised_by}}
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>Reopened Date/Time</label>
                                        <br>
                                        {{reopened_date}}
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="col-lg-8 col-md-8 col-sm-8">
                                        <label>Reopen Remarks : </label>
                                        <br />
                                        {{reopen_reason}}
                                    </div>
                                </div>
                            </div>
                            <br />
                            <p class="lead bb text-purple text-bold" style="font-size:16px;">Reopen Document</p>
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="text-center">
                                        <div class="dd-handle ng-scope">
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-secondary">
                                                            <thead class="bg-gray text-white">
                                                                <tr width="100%">
                                                                    <th align="center" scope="col">S.No</th>
                                                                    <th align="center" scope="col">Document Name</th>
                                                                    <th align="center" scope="col">Reopened By</th>
                                                                    <th align="center" scope="col">Reopened Date</th>
                                                                    <th align="center" scope="col">
                                                                        Action&nbsp;&nbsp;&nbsp;
                                                                        <!--<a download="" href="" title="Download All" id="btnpdf1" ng-click="downloadall(lblfilepath,lblfilename)">
                                                    <em class="icon-arrow-down"></em>&nbsp;
                                                </a>-->

                                                                    </th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in reopenrequestdocumentdtl" width="100%">
                                                                    <td align="left" scope="col" title="S.No">
                                                                        {{$index+1}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Document Name">
                                                                        {{data.document_name}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Reopened By">
                                                                        {{data.created_by}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Reopened Date">
                                                                        {{data.created_date}}
                                                                    </td>
                                                                    <td align="left" scope="col" data-title="Action">
                                                                        <button title="Download Document"
                                                                                ng-click="downloads(data.document_path,data.document_name)"
                                                                                class="btn btn-info">
                                                                            <em class="fa fa-download"></em>
                                                                        </button>
                                                                        <button title="View" class="btn btn-info btn-sm"  
                                                                                ng-click="documentviewer(data.document_path,data.document_name)">
                                                                            <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                                        </button>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                            </div>
                            <br />
                            <p class="lead bb text-purple text-bold" style="font-size:16px;">Transfer History</p>

                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="text-center">
                                        <div class="dd-handle ng-scope">
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-secondary">
                                                            <thead class="bg-gray text-white">
                                                                <tr width="100%">
                                                                    <th align="center" scope="col">S.No</th>
                                                                    <th align="center" scope="col">Assigned Team</th>
                                                                    <th align="center" scope="col">Assigned Member</th>
                                                                    <th align="center" scope="col">Assigned Date/Time</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in transferlistdtlreopen" width="100%">
                                                                    <td align="left" scope="col" title="S.No">
                                                                        {{$index+1}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Assigned Team">
                                                                        {{data.assigned_team}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Assigned Member">
                                                                        {{data.assigned_to}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Assigned Date/Time">
                                                                        {{data.assigned_date}}
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                            </div>
                            <br />
                            <p class="lead bb text-purple text-bold" style="font-size:16px;">Allocation History</p>

                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="text-center">
                                        <div class="dd-handle ng-scope">
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-secondary">
                                                            <thead class="bg-gray text-white">
                                                                <tr width="100%">
                                                                    <th align="center" scope="col">S.No</th>
                                                                    <th align="center" scope="col">Assigned Manager</th>
                                                                    <th align="center" scope="col">Assigned Date/Time</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in allocatelistdtlreopen" width="100%">
                                                                    <td align="left" scope="col" title="S.No">
                                                                        {{$index+1}}
                                                                    </td>

                                                                    <td align="left" scope="col" title="Assigned Member">
                                                                        {{data.allocate_managername}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Assigned Date/Time">
                                                                        {{data.assigned_date}}
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                            </div>
                            <br />
                            <br />
                           
                            <h4 class="text-pink">
                                <em class="fa fa-mail-forward text-danger fa-lg"></em> Forward Details
                            </h4>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="text-center">
                                            <div class="dd-handle ng-scope">
                                                <div class="row">
                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                        <div class="table-responsive">
                                                            <table class="table table-striped table-secondary">
                                                                <thead class="bg-gray text-white">
                                                                    <tr width="100%">
                                                                        <th align="center" scope="col">S.No</th>
                                                                        <th align="center" scope="col">Forward To</th>
                                                                        <th align="center" scope="col">Forward Date</th>
                                                                        <th align="center" scope="col">Forward Remarks</th>

                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr ng-repeat="data in forwardreopendtl" width="100%">
                                                                        <td align="left" scope="col" title="S.No">
                                                                            {{$index+1}}
                                                                        </td>
                                                                        <td align="left" scope="col" title="Forward To">
                                                                            {{data.forward_to}}
                                                                        </td>
                                                                        <td align="left" scope="col" title="Forward Date">
                                                                            {{data.forward_date}}
                                                                        </td>
                                                                        <td align="left" scope="col" title="Forward Remarks">
                                                                            {{data.forward_remarks}}
                                                                        </td>

                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>

                                        </div>
                                    </div>
                                </div>

                            </div>
                            <br />
                            <p class="lead bb text-purple text-bold" style="font-size:16px;">&nbsp; Forward Document Details</p>

                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="text-center">
                                        <div class="dd-handle ng-scope">
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-secondary">
                                                            <thead class="bg-gray text-white">
                                                                <tr width="100%">
                                                                    <th align="center" scope="col">S.No</th>
                                                                    <th align="center" scope="col">Document Name</th>
                                                                    <th align="center" scope="col">Created By</th>
                                                                    <th align="center" scope="col">Created Date</th>
                                                                    <th align="center" scope="col">
                                                                        Action&nbsp;&nbsp;&nbsp;
                                                                        <a download="" href="" title="Download All" id="btnpdf1" ng-click="downloadall(lblfilepath,lblfilename)">
                                                                            <em class="icon-arrow-down"></em>&nbsp;
                                                                        </a>
                                                                    </th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in forwardreopendocumentdtl" width="100%">
                                                                    <td align="left" scope="col" title="S.No">
                                                                        {{$index+1}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Document Name">
                                                                        {{data.document_name}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Created By">
                                                                        {{data.created_by}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Created Date">
                                                                        {{data.created_date}}
                                                                    </td>
                                                                    <td align="left" scope="col" data-title="Action">
                                                                        <button title="Download Document"
                                                                                ng-click="downloads(data.document_path,data.document_name)"
                                                                                class="btn btn-info">
                                                                            <em class="fa fa-download"></em>
                                                                        </button>
                                                                        <button title="View" class="btn btn-info btn-sm"  
                                                                                ng-click="documentviewer(data.document_path,data.document_name)">
                                                                            <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                                        </button>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                            </div>
                            <br />

                            <!--Reopen Details End-->
                            <!--History-->
                            <p class="lead bb text-purple text-bold" style="font-size:16px;">History</p>
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="text-center">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-secondary">
                                                        <thead class="bg-primary text-white">
                                                            <tr width="100%">
                                                                <th align="center">S.No</th>
                                                                <th align="center" scope="col">Request Refno</th>
                                                                <th align="center" scope="col">Request Title</th>
                                                                <th align="center" scope="col">Raised On/By</th>
                                                                <th align="center" scope="col">Completed On/By</th>
                                                                <th align="center" scope="col">Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr width="100%">
                                                                <td align="left" scope="col" title="S.No">{{index+1}}</td>
                                                                <td align="left" scope="col" title="Request Refno">
                                                                    {{request_refno}}
                                                                </td>
                                                                <td align="left" scope="col" title="Request Title">
                                                                    {{request_title}}
                                                                </td>
                                                                <td align="left" scope="col" title="Raised By/On">
                                                                    {{raised_by}}/{{raised_date}}
                                                                </td>
                                                                <td align="left" scope="col" title="Completed By/On">
                                                                    {{completed_by}}/{{completed_date}}
                                                                </td>
                                                                <td align="left" scope="col" data-title="Action">
                                                                    <button title="History" class="btn bg-blue-grey-500 btn-sm"
                                                                            ng-click="tickethistory(servicerequest_gid)">
                                                                        <em class="fa fa-history text-white"></em>
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />

                            <p class="lead bb text-purple text-bold" style="font-size:16px;">Reopen History</p>
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="text-center">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-secondary">
                                                        <thead class="bg-primary text-white">
                                                            <tr width="100%">
                                                                <th align="center">S.No</th>
                                                                <th align="center" scope="col">Request Refno</th>
                                                                <th align="center" scope="col">Request Title</th>
                                                                <th align="center" scope="col">Reopened By</th>
                                                                <th align="center" scope="col">Reopen Date</th>
                                                                <th align="center" scope="col">Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in reopendtl" width="100%">
                                                                <td align="left" scope="col" title="S.No">{{index+1}}</td>
                                                                <td align="left" scope="col" title="Request Refno">
                                                                    {{data.request_refno}}
                                                                </td>
                                                                <td align="left" scope="col" title="Request Title">
                                                                    {{data.request_title}}
                                                                </td>
                                                                <td align="left" scope="col" title="Reopened By">
                                                                    {{data.reopened_by}}
                                                                </td>
                                                                <td align="left" scope="col" title="Reopen Date">
                                                                    {{data.reopened_date}}
                                                                </td>
                                                                <td align="left" scope="col" data-title="Action">
                                                                    <button title="History" class="btn bg-blue-grey-500 btn-sm"
                                                                            ng-click="ticketreopenhistory(data.requestreopen_gid, data.servicerequest_gid)">
                                                                        <em class="fa fa-history text-white"></em>
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />
                        </div>

                        <div ng-show="reqdtls" class="">
                            <h4 class="text-pink">
                                <em class="fa fa-user  text-danger fa-lg"></em> Request Details
                            </h4>
                            <div class="panel-body">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>Request Ref.No</label>
                                                <br>
                                                {{request_refno}}
                                            </div>

                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>Request Raised By</label>
                                                <br>
                                                {{raised_by}}
                                            </div>

                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>Base Location</label>
                                                <br>
                                                {{baselocation_name}}
                                            </div>
                                        </div>
                                    </div>

                                    <br />
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">

                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>Request Raised Date</label>
                                                <br>
                                                {{raised_date}}
                                            </div>
                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>Request Title</label>
                                                <br>
                                                {{request_title}}
                                            </div>
                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>Assigned Team</label>
                                                <br>
                                                {{assigned_team}}
                                            </div>

                                        </div>

                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>Assigned Member</label>
                                                <br>
                                                {{assigned_to}}
                                            </div>
                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>Request Status</label>
                                                <br>
                                                {{request_status}}
                                            </div>
                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>Request Description</label>
                                                <br>
                                                {{request_description}}
                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                </div>
                                <br />
                                <div class="col-lg-16 col-md-16 col-sm-16">
                                    <div id="panelDemo4" class="panel panel-default">
                                        <div ng-class="{'panel-heading-collapsed':panelDemo4}" class="panel-heading" style="background-color: #c9dcc1;">
                                            <b> Asset Details</b>
                                            <paneltool tool-collapse="tool-collapse" ng-init="panelDemo4=true"></paneltool>
                                        </div>
                                        <div collapse="panelDemo4" class="panel-wrapper">
                                            <div class="panel-body">
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="text-center">
                                                            <div class="dd-handle ng-scope">
                                                                <div class="row">
                                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                                        <div class="table-responsive">
                                                                            <table class="table table-striped table-secondary">
                                                                                <thead class="bg-gray text-white">
                                                                                    <tr width="100%">
                                                                                        <th align="center" scope="col">S.No</th>
                                                                                        <th align="center" scope="col">Serial NO</th>
                                                                                        <th align="center" scope="col">Asset ID</th>
                                                                                        <th align="center" scope="col">Asset Name</th>
                                                                                        <th align="center" scope="col">Invoice Ref NO</th>
                                                                                        <th align="center" scope="col">Invoice Date</th>
                                                                                        <th align="center" scope="col">Issued By</th>
                                                                                        <th align="center" scope="col">Issued Date</th>
                                                                                        <th align="center" scope="col">Status</th>
                                                                                        <th align="center" scope="col">Warranty Reference No</th>
                                                                                        <th align="center" scope="col">Warranty Start Date</th>
                                                                                        <th align="center" scope="col">Warranty End Date</th>
                                                                                        <th align="center" scope="col">Extended Warranty Start Date</th>
                                                                                        <th align="center" scope="col">Extended Warranty End Date</th>


                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody>
                                                                                    <tr ng-repeat="data in assetdetails" class="nowrap" width="100%">
                                                                                        <td align="left" scope="col" title="S.No">
                                                                                            {{$index+1}}
                                                                                        </td>
                                                                                        <td align="left" scope="col" title="Serial NO">
                                                                                            {{data.serial_no}}
                                                                                        </td>
                                                                                        <td align="left" scope="col" title="Asset ID">
                                                                                            {{data.asset_id}}
                                                                                        </td>
                                                                                        <td align="left" scope="col" title="Asset Name">
                                                                                            {{data.asset_name}}
                                                                                        </td>
                                                                                        <td align="left" scope="col" title="Invoice Ref NO">
                                                                                            {{data.invoice_refno}}
                                                                                        </td>
                                                                                        <td align="left" scope="col" title="Invoice Date">
                                                                                            {{data.invoice_date}}
                                                                                        </td>
                                                                                        <td align="left" scope="col" title="Issued By">
                                                                                            {{data.issued_by}}
