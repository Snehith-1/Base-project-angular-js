<h3>
    Lesson Details
</h3>
<div ng-controller="viewLessoncontroller">
    <div id="panelDemo8" class="panel panel-success ">
        <div class="panel-heading">LESSON DETAILS</div>

        <div class="panel-body">
            <div class="row" style="display:none;">
                <div class="col-lg-12 col-md-12 col-sm-12 ">
                    <div class="col-lg-6 col-md-6 col-sm-6 ">
                        <input name="customer_gid" id="customer_gid" style="font-size:12px" placeholder="Enter Customer gid"
                               ng-model="customer_gid" class="form-control" required>
                    </div>
                </div>
            </div>
            <div class="row" style="display:none;">
            </div>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-5  col-sm-5 col-md-5">
                        <strong class="mt0">Lesson Ref No</strong><br />
                        <p>{{UploadDocumentname.loanref_no}}</p>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2">
                    </div>
                    <div class="col-lg-5  col-sm-5 col-md-5">
                        <strong class="mt0">Lesson Title </strong><br />
                        <p>{{UploadDocumentname.customer_name}}</p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 ">
                    <div class="col-lg-5  col-sm-5 col-md-5">
                        <strong>Transcript</strong><br />
                        <p>{{businesshead_name}}</p>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2">
                    </div>
                    <div class="col-lg-5  col-sm-5 col-md-5">
                        <strong>Description</strong><br />
                        <p>{{UploadDocumentname.cluster_manager_name}}</p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-5  col-sm-5 col-md-5">
                        <strong class="mt0">Video Upload </strong><br />
                        <p>{{UploadDocumentname.vertical_code}}</p>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2">
                    </div>
                    <div class="col-lg-5  col-sm-5 col-md-5">
                        <strong class="mt0">Video Duration</strong>
                        <br />
                        <p>{{UploadDocumentname.entity_name}}</p>
                    </div>
                </div>
            </div>     
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 ">
                    <strong>Free Preview</strong>
                    <p>{{UploadDocumentname.rm_name}}</p>
                </div>
            </div>
        </div>
    </div>
    <div class="panel-body text-center">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <button ng-click="viewlessonback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
        </div>
    </div>
</div>




