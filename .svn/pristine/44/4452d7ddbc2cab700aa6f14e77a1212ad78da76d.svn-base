<h3>
    Document Conversation
</h3>
<div ng-controller="idasTrnDocCoversation as dp">
    <div id="panelDemo13" class="panel panel-default">
        <div class="panel-body table-responsive">
            <p class="lead bb text-primary text-bold text-center">
                {{documentrecord_id}}
                <label style="font-weight:bold">/</label>{{document_name}}
            </p>
            <label>Document Details</label><br />
            <div class="row">
                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <label>Document Date <span>*</span></label>
                        <p class="input-group">
                            <input type="text" datepicker-popup="{{dp.format}}" ng-model="document_date" is-open="dp.open1"
                                   datepicker-options="dp.dateOptions" ng-required="true" ng-change="update()"
                                   close-text="Close" class="form-control" />
                            <span class="input-group-btn">
                                <button type="button" ng-click="dp.calender1($event)" class="btn btn-default">
                                    <em class="fa fa-calendar"></em>
                                </button>
                            </span>
                        </p>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2">

                    </div>
                   
                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <label class="col-form-label control-label"><strong> Types Of Copy *</strong></label><br />
                        <span>Scan Copy</span>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2">

                    </div>
                    <!--<div class="col-lg-2 col-md-2 col-sm-2">
                        <button title="Update"  ng-click="update()" class="btn btn-purple">
                            <em class="icon icon-note"></em>
                        </button>
                    </div>-->
                </div>

            </div>
            <label>Document Conversation</label><br />
            <div id="panelDemo1" class="panel" ng-repeat="data in docconversationlist">
                <div ng-class="{'panel-heading-collapsed':panelDemo1}" class="panel-heading bg-grey-100">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="col-lg-8">
                               <lable class="text-purple"><strong>CAD Query #{{data.query_no}}:</strong></lable> 
                                
                            </div>
                            <div class="col-lg-4">
                                <span class="text-purple"> {{data.cad_name}}/{{data.cadquery_on}}</span>
                                &nbsp;
                                <button title="Download Documents" ng-if="data.uploaddocument_count != '0'" ng-click="PopupDownload(data.docconversation_gid)" class="btn btn-purple">
                                    <em class="icon icon-link"></em>
                                </button>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-lg-12">
                            <div class="col-lg-12">
                                <p ng-bind-html="data.cad_query"></p>
                                </div>
                        </div>
                    </div>
                    <paneltool tool-collapse="tool-collapse"></paneltool>
                </div>
                <div collapse="panelDemo1" class="panel-wrapper" >
                   
                        <div ng-switch="data.flag">
                            <div class="panel-heading" ng-switch-when="Y">
                                <div class="row" >
                                    <div class="col-lg-12">
                                        <div class="col-lg-8">
                                            <lable class="text-pink"><strong>RM Response #{{data.query_no}}:</strong></lable> 
                                           
                                        </div>
                                        <div class="col-lg-4 ">
                                         <span class="text-pink">
                                             {{data.relationshipmgr_name}}/{{data.relationshipmgrquery_on}}
                                            </span>  
                                        </div>

                                    </div>
                                   
                                </div>
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                           
                                            <p ng-bind-html="data.rm_response"></p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                            <div ng-switch-when="N">
                                <div ng-show="data.hide">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="col-lg-12">
                                                <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                                    <label for="input_13">Reply * </label>
                                                    <textarea ng-model="reply" required rows="2" md-maxlength="1000" class="ng-valid md-input ng-valid-md-maxlength ng-touched ng-dirty ng-valid-parse"
                                                              id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                                </md-input-container>
                                            </div>
                                        </div>
                                    </div>
                                    <br />

                                    <div class="ng-scope row">
                                        <div class="text-left col-lg-12">
                                            <div class="col-lg-12">
                                                <button ng-click="raiseResponse(data.docconversation_gid,$index,reply)" class="btn btn-purple"><i class="fa fa-send (alias)"></i>&nbsp;&nbsp;Send</button>
                                                <button ng-click="data.hide=false" class="btn btn-danger"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Cancel</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />

                                <div class="row">
                                    <div class="col-lg-12">

                                        <div class="col-lg-12" ng-hide="data.hide">
                                            <button title="Reply Conversation" class="btn-pink" ng-click="data.hide=true">
                                                <em class="fa fa-mail-reply (alias) w3-text-pink"></em>&nbsp;
                                            </button>
                                            <span class="text-pink">Reply</span>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>


                        </div>
                    
                        </div>
                
            </div>
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-lg-2 col-md-2 col-sm-2">
                        <button type="button" ng-click="FileShow()" class="btn btn-info btn-xs">
                            <span class="icon icon-cloud-upload"></span>&nbsp;&nbsp;Upload File
                        </button>
                    </div>
                </div>
            </div>
            <br />
            <div ng-show="DivFile">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-2 col-md-2 col-sm-2">
                            <label>Document Title</label>&nbsp;<span style="color:red">*</span>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3">
                            <input type="text" ng-model="txtdocument_title" rows="1" tabindex="0" min="0" aria-required="true"
                                   aria-invalid="true" class="form-control" maxlength="256" />
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-2 col-md-2 col-sm-2">
                            <label>File Upload</label>
                        </div>

                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <input type="file" id="addupload" ng-model="test_document" ng-disabled="!txtdocument_title"
                                   onchange="angular.element(this).scope().uploadallocation(this.files, angular.element(this).scope().documentupload, this.name)"
                                   data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select"
                                   accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />
                        </div>
                    </div>
                </div>
            </div>
           
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div ng-repeat="data in uploaddocument" class="panel bg-grey-200">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>{{data.document_title}}</label><br />
                                <span>{{data.document_name}}</span>
                                &nbsp;&nbsp;
                                <button title="Un-Tag" ng-click="UploadDocCancel(data.conversationdocument_gid)" class="btn btn-danger">
                                    <em class="fa fa-remove (alias)"></em>
                                </button>

                            </div>
                            </div>
                        </div>

                </div>
              
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <md-input-container flex="" class="md-default-theme" style="height: auto;">
                            <label for="input_13">Raise Query * </label>
                            <textarea ng-model="content" required rows="2" md-maxlength="1000" class="ng-valid md-input ng-valid-md-maxlength ng-touched ng-dirty ng-valid-parse"
                                      id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                        </md-input-container>
                        <br />
                        <button type="button" ng-click="raiseQuery()" class="btn btn-primary btn-sm mb">Send</button>
                    </div>
                      <!--<div class="dd-handle ng-scope" >
                        <div class="mb-editor">-->
                            <!--<script type="text/ng-template" id="ng-wig/views/ng-wig.html">
                                <div class="ng-wig">
