                    </div>-->
                  

                </div>

            </div>
            <label>Final Remarks</label>
            <div class="row">
                <div class="col-lg-12">
                    <div class="col-lg-12">
                        <md-input-container flex="" class="md-default-theme" style="height: auto;">
                            <label for="input_13">Final Remarks * </label>
                            <textarea ng-model="scanfinal_remarks" required rows="2" md-maxlength="1000" class="ng-valid md-input ng-valid-md-maxlength ng-touched ng-dirty ng-valid-parse"
                                      id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                        </md-input-container>
                        <br />
                        <button ng-click="updateFinalRemarks()" class="btn btn-success">&nbsp;&nbsp;Update</button>
                    </div>
                  
                </div>
            </div>
            </div>
        </div>
    <br />

    <div class="ng-scope">
        <div class="text-center">

            <button ng-click="docconback()" class="btn btn-danger"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
        </div>
    </div>
    
    <script type="text/ng-template" id="/mailconversation.html">


        <div class="modal-body ng-scope">
            <h4 id="myModalLabel" class="modal-title">
                <p class="lead bb text-primary text-bold text-center">
                   Download Documents
                </p>
            </h4>
           
            <div class="text-center">
                <div class="dd-handle ng-scope">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="table-responsive">
                                <table class="table table-striped table-secondary">
                                    <thead class="bg-gray text-white">
                                        <tr width="100%">
                                            <th scope="col">S.No</th>
                                            <th scope="col">Document Name</th>
                                            <th scope="col">Document Title</th>
                                            <th scope="col">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="data in viewDocumentList" width="100%">
                                            <td title="'S.No'">{{$index+1}}</td>
                                            <td title="'Document Name'">{{data.document_name}}</td>
                                            <td title="'Document Title'">{{data.document_title}}</td>
                                            <td data-title="'Action'">
                                                <span class="fa fa-download" ng-click="downloads(data.document_path,data.document_name)" />
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>

                </div>
                <br />
                <button ng-click="ok()" class="btn btn-danger">
                    <i class="fa fa-close (alias)">
                    </i>&nbsp;Close
                </button>
            </div>
        </div>
    </script>
    </div>
   
