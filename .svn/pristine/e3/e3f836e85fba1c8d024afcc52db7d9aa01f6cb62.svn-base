            <br />
            <div class="table-responsive">
                <table datatable="ng" class="table hover table-striped">
                    <thead>
                        <tr class="bg-primary-light text-white">
                            <th class="text-center" style="display:none">S.No</th>
                            <th>Request Ref.No</th>
                            <th>Business Unit Name</th>
                            <th style="width:160px;">Raised On</th>
                            <th>Raised By</th>
                            <th>Activity Name</th>
                            <th>Request Title</th>
                            <th>Assigned Team</th>
                            <th>Assigned Member</th>
                            <th>Request Status</th>
                            <th class="text-nowrap">Source</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="data in servicerequestsummary | limitTo:limit">
                            <td data-title="'S.No'" style="display:none">{{$index+1}}</td>
                            <td data-title="'Request Ref.No'">{{data.request_refno}}</td>
                            <td data-title="'Business Unit Name'">{{data.department_name}}</td>
                            <td data-title="'Raised On'">{{data.raised_date}}</td>
                            <td data-title="'Raised By'">{{data.raised_by}}</td>
                            <td data-title="'Activity Name'">{{data.activity_name}}</td>
                            <td data-title="'Request Title'">{{data.request_title}}</td>
                            <td data-title="'Assigned Team'">{{data.assigned_team}}</td>
                            <td data-title="'Assigned Member'">{{data.assigned_to}}</td>
                            <td data-title="'Request Status'" ng-switch="data.request_status1">
                                <label ng-switch-when="Allotted" class="label label-warning">Allotted</label>
                                <label ng-switch-when="Work-In-Progress" class="label label-info">Work-In-Progress</label>
                                <label ng-switch-when="Reopen-Work-In-Progress" class="label label-info">Reopen - Work-In-Progress</label>
                                <label ng-switch-when="Completed" class="label label-success">Completed</label>
                                <label ng-switch-when="Reopen-Completed" class="label label-success">Reopen - Completed</label>
                                <label ng-switch-when="Closed" class="label label-success">Closed</label>
                                <label ng-switch-when="Reopen-Closed" class="label label-success">Reopen - Closed</label>
                                <label ng-switch-when="Rejected" class="label label-danger">Rejected</label>
                                <label ng-switch-when="Cancelled" class="label label-danger">Cancelled</label>
                            </td>
                            <td data-title="'Source'" ng-switch="data.source">
                                <label style="font-size:10px" ng-switch-when="E-mail" class="label label-green">E-Mail</label>
                                <label style="font-size:10px" ng-switch-when="Kotak_Api" class="label label-warning">Kotak API</label>
                                <label style="font-size:10px" ng-switch-when="Kotak_API" class="label label-warning">Kotak API</label>
                                <label style="font-size:10px" ng-switch-when="BRS" class="label label-pink">BRS</label>
                                <label style="font-size:10px" ng-switch-when="Internal" class="label label-primary">Internal</label>
                            </td>
                            <td class="nowrap" data-title="'Action'">
                                <button title="View" ng-click="viewNew(data.servicerequest_gid, data.bankalert_flag, data.bankalert2allocated_gid, data.customer_gid)" class="btn btn-info">
                                    <em class="fa fa-eye "></em>
                                </button>
                                <!--<button title="PDF" ng-click="viewPDF(data.servicerequest_gid)" class="btn btn-info">
            <em class="fa fa-angle-right "></em>
        </button>-->

                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <br />
            <div class="text-right">
                <button class="btn btn-success btn-md" ng-click="incrementLimit()">More</button>

                <button class="btn btn-warning btn-md" ng-click="decrementLimit()">Less</button>
              
            </div>
            <br />
        </div>
    </div>
    <!-- END panel-->
</div>