<h3>
    Report
</h3>
<div ng-controller="osdRptAllTicketsController as dpick">

    <br />
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">          
                <div class="col-lg-4  col-sm-4 col-md-4" ng-show="single" style="display:none">
                    <label>Business Unit  Name </label>&nbsp;

                    <p>{{lbldepartmentgid}}</p>
                </div>
                <div class="col-lg-4  col-sm-4 col-md-4" ng-show="single">
                    <label>Business Unit  Name </label>&nbsp;
                    <p>{{lbldepartmentname}}</p>
                </div>
                <div class="col-lg-4  col-sm-4 col-md-4"  ng-show="multiple">
                    <label>Business Unit  Name </label>&nbsp;
                    <select name="department" id="department" ng-options="m.department_name for m in deptmasterlist" chosen="" ng-change="onselectdept(cbodepartment)"
                            ng-model="cbodepartment" class="chosen-select form-control" style="height:37px;" size="1">
                        <option value=""> ----- Select Business Unit  -----</option>
                    </select>

                </div>
           
            <div class="col-lg-4  col-sm-4 col-md-4">
                <strong class="mt0">Activity</strong><br />
                <select id="select_value_label_93" name="activity_name"   dropzone="" ng-model="activitymaster_gid" required=""
                        class="chosen-select form-control"  size="1">
                    <option value="">--Select Activity--</option>
                    <option data-ng-repeat="data in activity_list" value="{{data.activitymaster_gid}}">
                        {{data.activity_name}}
                    </option>
                </select>


            </div>
            <div class="col-lg-4  col-sm-4 col-md-4">
                <strong class="mt0">Assigned Team</strong>
                <select name="activity_name"
                        tabindex="1" 
                        ng-model="supportteam_gid" class="chosen-select form-control">
                    <option value="">--Select Team--</option>
                    <option data-ng-repeat="data in supportdtl" value="{{data.supportteam_gid}}">
                        {{data.team_name}}
                    </option>
                </select>

            </div>
           
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">

            <!--<div class="col-lg-4  col-sm-4 col-md-4">
        <strong class="mt0">Request Ref.No</strong><br />
        <input ng-model="ref_no" type="text" placeholder="Enter Request Ref.No"
               maxlength="150" class="form-control ng-pristine ng-valid ng-touched" required>


    </div>
    <div class="col-lg-4  col-sm-4 col-md-4">
        <strong class="mt0">Raised By</strong>
        <select id="zonal_name" name="vertical" dropzone="" ng-model="raisedmember_gid" required=""
                class="form-control ng-valid ng-touched ng-valid-parse"
                tabindex="0" aria-label="State" aria-expanded="false" size="1">
            <option value="">--Select Raised Person--</option>
            <option data-ng-repeat="data in employee_list" value="{{data.employee_gid}}">
                {{data.employee_name}}
            </option>
        </select>

    </div>-->
            <div class="col-lg-4  col-sm-4 col-md-4">
                <strong class="mt0">Assigned Member</strong><br />
                <select id="zonal_name"    name="vertical" dropzone="" ng-model="assignedmember_gid" required=""
                        class="chosen-select form-control"
                        tabindex="0" aria-label="State" aria-expanded="false" size="1">
                    <option value="">---Select Assigned Member---</option>
                    <option data-ng-repeat="data in employee_list" value="{{data.employee_gid}}">
                        {{data.employee_name}}
                    </option>
                </select>
            </div>

            <div class="col-lg-2  col-sm-2 col-md-2">
                <strong class="mt0">Ticket Status</strong>
                <select name="activity_name"
                        ng-model="ticket_status" class="chosen-select form-control">
                    <option value="">--Select Ticket Status--</option>
                    <option value="Allotted">Allotted</option>
                    <option value="Work-In-Progress">Work-In-Progress</option>
                    <option value="Completed">Completed</option>
                    <option value="Closed">Closed</option>
                    <option value="Cancelled">Cancelled</option>
                    <option value="Rejected">Rejected</option>

                </select>

            </div>

            <div class="col-lg-2  col-sm-2 col-md-2">
                <strong class="mt0">Raised On</strong><br />
                <p class="input-group">

                    <input type="text" id="dob" name="date" ng-model="raised_date" datepicker-popup="{{dpick.format}}"
                           style="height:30px;" is-open="dpick.opened" min-date="dpick.minDate" datepicker-options="dpick.dateOptions"
                           close-text="Close" class="form-control" />
                    <span class="input-group-btn">
                        <button type="button" ng-click="dpick.open($event)" style="height:30px;" class="btn btn-default">
                            <em class="fa fa-calendar"></em>
                        </button>
                    </span>
                </p>
            </div>
            <div class="col-lg-2  col-sm-2 col-md-2">
                <strong class="mt0">Source</strong>
                <select name="source"
                        ng-model="ticket_source" class="chosen-select form-control">
                    <option value="">--Select Source--</option>
                    <option value="Internal">Internal</option>
                    <option value="E-Mail">E-Mail</option>
                    <option value="BRS">BRS</option>
                    <option value="Kotak_Api">Kotak_Api</option>                  

                </select>

            </div>

            <div class="col-lg-2  col-sm-2 col-md-2">
                <br />
                <button type="button" class="btn btn-green btn-md" style="background-color:#23b1e6;" ng-disabled="userform.$invalid" ng-click="search()" title="Search"><i class="fa fa-search"></i></button>&nbsp;&nbsp;
                <button type="button" class="btn btn-green btn-md" style="background-color:#0d87b5;" ng-disabled="userform.$invalid" ng-click="all()" title="Refresh"><i class="fa fa-refresh"></i></button>&nbsp;&nbsp;
                <button type="button" class="btn btn-green btn-md" style="background-color:#a2510b;" ng-disabled="userform.$invalid" ng-click="export()" title="Export Excel"><i class="fa fa-file"><a download="" href="" id="btnpdf" style="display:none;"></a></i></button>&nbsp;&nbsp;
            </div>

        </div>
    </div>
   
    <br />
    <!-- START panel-->
    <div class="panel">

        <div class="panel-body">

