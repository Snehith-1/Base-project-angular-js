                                                                <label ng-show="data.querystatus!=='Open'">
                                                                    <label class="label label-default" style="color:black;">Closed</label>
                                                                </label>
                                                            </td>
                                                        </tr>

                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
