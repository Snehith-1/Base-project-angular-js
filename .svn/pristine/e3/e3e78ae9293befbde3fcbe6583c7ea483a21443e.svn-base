<h3>
    Course Management
</h3>
<div ng-controller="editCoursecontroller">
    <div id="panelDemo8" class="panel panel-success ">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-plus"></i>
            </span>EDIT COURSE
        </div>

        <div class="panel-body">
            <div class="row" style="display:none;">
                <div class="col-lg-12 col-md-12 col-sm-12 ">
                    <div class="col-lg-6 col-md-6 col-sm-6 ">
                        <input name="course_gid" id="course_gid" style="font-size:12px" placeholder="Enter Customer gid"
                               ng-model="course_gid" class="form-control" required>
                    </div>
                </div>
            </div>

            <div class="modal-body ng-scope">
                <form novalidate="" name="userform" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">


                    <div class="row" style="display:none;">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="col-lg-8 col-md-8 col-sm-8 ">
                                <label style="font-size:5px;" ng-model="course_gid" class="col-form-label w3-text-indigo"><strong>{{LoanDetails.customer_gid}}</strong></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2">
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-5 col-md-5 col-sm-5">

                                <div class="mda-form-group float-label">
                                    <input ng-model="txtcustomername" required="" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Course Ref No *</label>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtcontactperson" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Course Title *</label>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <label class="col-form-label control-label"><strong>Category *</strong></label>
                                    <select id="customername" name="customer" ng-change="onselectedchangecustomer(customer)" dropzone="" ng-model="customer" required="" class=" mda-form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched" size="1">
                                        <option value="">---Select Category---</option>
                                        <option data-ng-repeat="data in customer_list" value="{{data.course_gid}}">
                                            {{data.categoryname}}
                                        </option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <label class="col-form-label control-label"><strong>Author *</strong></label>
                                    <select id="customername" name="customer" ng-change="onselectedchangecustomer(customer)" dropzone="" ng-model="customer" required="" class=" mda-form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched" size="1">
                                        <option value="">---Select Author---</option>
                                        <option data-ng-repeat="data in customer_list" value="{{data.customer_gid}}">
                                            {{data.customername}}
                                        </option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group">
                                    <label>Is Course AR Enabled *</label>
                                    <div class="col">
                                        <label class="mda-radio">
                                            <input type="radio" name="radio_ar" ng-model="radio_selfothers" ng-change="onchangeself(radio_selfothers)"
                                                   ng-click="isShowHide('hide')" value="Self">
                                            <em class="bg-teal-500"></em> Yes
                                        </label>
                                    </div>
                                    <div class="col">
                                        &nbsp; &nbsp;
                                    </div>
                                    <div class="col">
                                        <label class="mda-radio">
                                            <input type="radio" name="radio_ar" ng-model="radio_selfothers" ng-click="isShowHide('show')" ng-change="onchangeself(radio_selfothers)" value="Others">
                                            <em class="bg-teal-500"></em> No
                                        </label>
                                    </div>
                                </div>

                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group">
                                    <label>Do you Provide Worksheet *</label>
                                    <div class="col">
                                        <label class="mda-radio">
                                            <input type="radio" name="radio_worsheet" ng-model="radio_selfothers" ng-change="onchangeself(radio_selfothers)"
                                                   ng-click="isShowHide('hide')" value="Yes">
                                            <em class="bg-teal-500"></em> Yes
                                        </label>
                                    </div>
                                    <div class="col">
                                        &nbsp; &nbsp;
                                    </div>
                                    <div class="col">
                                        <label class="mda-radio">
                                            <input type="radio" name="radio_worsheet" ng-model="radio_selfothers" ng-click="isShowHide('show')" ng-change="onchangeself(radio_selfothers)" value="No">
                                            <em class="bg-teal-500"></em> No
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <label class="col-form-label control-label"><strong>Course Level*</strong></label>
                                    <select id="customername" name="customer" ng-change="onselectedchangecustomer(customer)" dropzone="" ng-model="customer" required="" class=" mda-form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched" size="1">
                                        <option value="">---Select Level---</option>
                                        <option data-ng-repeat="data in customer_list" value="{{data.customer_gid}}">
                                            {{data.customername}}
                                        </option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtcontactperson" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Permanent Link</label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                        <label for="input_13">Short Description *</label>
                                        <textarea ng-model="txt_remarks" rows="3" md-maxlength="200" class=" md-input ng-valid-md-maxlength ng-touched ng-dirty ng-valid-parse"
                                                  name="txt_remarks" id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                    </md-input-container>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <div class="mda-form-group float-label">
                                    <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                        <label for="input_13">Detail Description *</label>
                                        <textarea ng-model="txt_remarks" rows="3" md-maxlength="1000" class=" md-input ng-valid-md-maxlength ng-touched ng-dirty ng-valid-parse"
                                                  name="txt_remarks" id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                    </md-input-container>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-11 col-md-11 col-sm-11">
                            <div class="mda-form-group float-label">
                                <label class="col-form-label control-label"><strong>Course Slung*</strong></label>
                                <select id="customername" name="customer" ng-change="onselectedchangecustomer(customer)" dropzone="" ng-model="customer" required="" class=" mda-form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched" size="1">
                                    <option value="">---Select Tags---</option>
                                    <option data-ng-repeat="data in customer_list" value="{{data.customer_gid}}">
                                        {{data.customername}}
                                    </option>
                                </select>
                            </div>
                        </div>
                        <div class="col-lg-2 col-md-2 col-sm-2">
                        </div>
                    </div>
                </form>
            </div>
            <div>
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="userform.$invalid" ng-click="customerSubmit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="coursebackedit()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                </div>
            </div>

        </div>
    </div>


</div>



