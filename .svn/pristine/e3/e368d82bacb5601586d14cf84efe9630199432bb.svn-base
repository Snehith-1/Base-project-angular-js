<h3>
    Edit CIBIL Data
</h3>
<div ng-controller="MstCibilEditController as dp">
    <div id="panelDemo8" class="panel panel-primary">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-plus"></i>
            </span><strong>Edit CIBIL Data</strong>
        </div>

        <div class="modal-body ng-scope">
            <form novalidate="" name="cibilform" ng-submit="form.submitCibil()" autocomplete="off" class="form-validate mb-lg">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 ">

                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <p class="text-center text-primary text-bold" style="font-size: 20px;">
                                {{txtname}}
                            </p>

                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-lg-9 col-sm-9 col-md-9">
                            <div style="border: 1px solid #EEEEEE;">
                                <br />
                                <br />
                                <div class="row">

                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label control-label"><strong>Submission Type</strong> </label>
                                            <input type="text" ng-model="txtsubmission_type" class="form-control ng-pristine ng-valid ng-touched" required>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label>Current Balance<span style="color:red">*</span></label>
                                            <input id="txtInput1" ng-model="txtcurrent_balance" type="text" ng-change="amountschange1()"
                                                   class="form-control ng-pristine ng-valid ng-touched" style="text-align:right;">
                                            <span style="font-style:italic;">Number in words:</span>
                                            <span id="sanctionamount_words1" style="color:#003c96fc;" class="text-bold"></span>
                                        </div>

                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label>Amount Overdue <span style="color:red">*</span></label>
                                            <input id="txtInput" ng-model="txtoverdue_amount" type="text" ng-change="amountschange()"
                                                   class="form-control ng-pristine ng-valid ng-touched" style="text-align:right;">
                                            <span style="font-style:italic;">Number in words:</span>
                                            <span id="sanctionamount_words" style="color:#003c96fc;" class="text-bold"></span>

                                        </div>
                                    </div>
                                </div>
                                <br />
                              
                            </div>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <div style="border: 1px solid #C6C4C4;background-color: #F1F1F2;">
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-sm-6 col-md-6 col-lg-6">
                                            <label><strong>Account Number:</strong></label>
                                        </div>
                                        <div class="col-sm-6 col-md-6 col-lg-6">
                                            <p> {{txtaccount_no}}</p>
                                        </div>

                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-sm-6 col-md-6 col-lg-6">
                                            <label><strong>Indicator:</strong></label>
                                        </div>
                                        <div class="col-sm-6 col-md-6 col-lg-6">
                                            <p> {{txtindicator}}</p>
                                        </div>

                                    </div>

                                </div>
                                <br/>
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-sm-6 col-md-6 col-lg-6">
                                            <label><strong>Submitted On:</strong></label>
                                        </div>
                                        <div class="col-sm-6 col-md-6 col-lg-6">
                                            <p> {{txtsubmitted_on}}</p>
                                        </div>

                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>
                </div>
               
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-lg-9 col-sm-9 col-md-9">
                            <div style="border: 1px solid #EEEEEE;">
                                <br />
                                <div class="row">
                                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label control-label"><strong>OD Bucket 01(1-30)</strong> </label>
                                            <input id="txtInput2" ng-model="txtodbucket_01"  type="text" ng-change="amountschange2()"
                                             class="form-control ng-pristine ng-valid ng-touched" style="text-align:right;">
                                            <span style="font-style:italic;">Number in words:</span>
                                            <span id="sanctionamount_words2" style="color:#003c96fc;" class="text-bold"></span>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label control-label"><strong>OD Bucket 02(31-60)</strong> </label>
                                            <input id="txtInput3" ng-model="txtodbucket_02"  type="text" ng-change="amountschange3()"
                                                   class="form-control ng-pristine ng-valid ng-touched" style="text-align:right;">
                                            <span style="font-style:italic;">Number in words:</span>
                                            <span id="sanctionamount_words3" style="color:#003c96fc;" class="text-bold"></span>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label control-label"><strong>OD Bucket 03(61-90)</strong> </label>
                                            <input id="txtInput4" ng-model="txtodbucket_03"  type="text" ng-change="amountschange4()"
                                                   class="form-control ng-pristine ng-valid ng-touched" style="text-align:right;">
                                            <span style="font-style:italic;">Number in words:</span>
                                            <span id="sanctionamount_words4" style="color:#003c96fc;" class="text-bold"></span>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label control-label"><strong>OD Bucket 04(91-180)</strong> </label>
                                            <input id="txtInput5" ng-model="txtodbucket_04"  type="text" ng-change="amountschange5()"
                                                   class="form-control ng-pristine ng-valid ng-touched" style="text-align:right;">
                                            <span style="font-style:italic;">Number in words:</span>
                                            <span id="sanctionamount_words5" style="color:#003c96fc;" class="text-bold"></span>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label control-label"><strong>OD Bucket 05(Above 180)</strong> </label>
                                            <input id="txtInput6" ng-model="txtodbucket_05"  type="text" ng-change="amountschange6()"
                                                   class="form-control ng-pristine ng-valid ng-touched" style="text-align:right;">
                                            <span style="font-style:italic;">Number in words:</span>
                                            <span id="sanctionamount_words6" style="color:#003c96fc;" class="text-bold"></span>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label control-label"><strong>OD Days</strong> </label>
                                            <input type="text" ng-model="txtod_days" class="form-control ng-pristine ng-valid ng-touched" maxlength="10">
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-lg-9 col-sm-9 col-md-9">
                            <div style="border: 1px solid #EEEEEE;">
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label control-label"><strong>Account Status</strong> </label>
                                            <input type="text" ng-model="txtaccount_status" class="form-control ng-pristine ng-valid ng-touched">
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label control-label"><strong>Closed On</strong> </label>
                                            <md-input-container flex="" class="md-default-theme" style="height: auto;">

                                                <input type="date" id="fromDate" name="fromdate" ng-model="txtclosed_on" required>
                                            </md-input-container>

                                       
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label control-label"><strong>CIBIL</strong> </label>
                                            <input type="text" ng-model="txtcibil_status" class="form-control ng-pristine ng-valid ng-touched">
                                        </div>
                                    </div>
                                    </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label control-label"><strong>Highmark</strong> </label>
                                            <input type="text" ng-model="txthighmark_status" class="form-control ng-pristine ng-valid ng-touched">
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label control-label">
                                                <strong>Experian</strong>
                                            </label>
                                            <input type="text" ng-model="txtexperian_status" class="form-control ng-pristine ng-valid ng-touched">
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label control-label"><strong>Equifax</strong> </label>
                                            <input type="text" ng-model="txtequifax_status" class="form-control ng-pristine ng-valid ng-touched">
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                            </div>
                        </div>
                    </div>
            </form>
        </div>
        <div>
            <div class="text-center">
                <button type="button" class="btn btn-success btn-md" ng-disabled="cibilform.$invalid" ng-click="cibilUpdate()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                <button ng-click="cibilback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>

            </div>
            <br />
        </div>
        <br />
    </div>
</div>

