<style>
    .space {
        margin: 10px 0px;
    }
</style>
<h3>
    Customer Creation LMS
</h3>
<div ng-controller="MstCADCustomerCreationLMSController">
    <div class="panel panel-primary">
        <!--<div class="panel-heading">
        <span class="btn-label">
            <i class="fa fa-spinner"></i>
        </span>Initiate LMS
    </div>-->
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div style="color :#000000 ;font-size:14px;font-weight :bold;">
                    <div class="col-lg-9 col-md-9 col-sm-9" style="background-color:#191370;color:white;padding-top:10px;padding-bottom:10px;">
                        <!--<h5>Proposal Product View</h5>-->
                        <span class="btn-label">
                            <i class="fa fa-spinner"></i>
                        </span> Customer Creation In LMS
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3" style="background-color:#3646b8;color:white;padding-top:10px;padding-bottom:10px;">
                        <span class="btn-label">{{txtapplication_no}}</span>
                    </div>
                </div>
            </div>
        </div>
        <div style="margin-bottom:-10px;">
            <br />
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="dd-handle ng-scope" style="background-color: #FAF8F8;">
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-sm-4 col-lg-4 col-md-4" ng-hide="customername==null">
                                        <label style="font-weight:bold"> Customer First Name </label>
                                        <p>{{customername}}</p>
                                    </div>
                                    <div class="col-sm-4 col-lg-4 col-md-4" ng-show="customername==''||customername==null ">
                                        <label style="font-weight:bold"> Customer First Name </label>
                                        <p>{{Firstname}}</p>
                                    </div>
                                    <div class="col-sm-4 col-lg-4 col-md-4">
                                        <label style="font-weight:bold"> Customer Middle Name </label>
                                        <p>{{middlename}}</p>
                                    </div>
                                    <div class="col-sm-4 col-lg-4 col-md-4">
                                        <label style="font-weight:bold"> Customer Last Name </label>
                                        <p>{{lastname}}</p>
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-sm-4 col-lg-4 col-md-4">
                                        <label style="font-weight:bold">Customer Address 1 </label>
                                        <p>{{address1}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">Customer Address 2</label>
                                        <p>{{address2}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">Customer Address 3</label>
                                        <!--<p>{{}}</p>-->
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-sm-4 col-lg-4 col-md-4">
                                        <label style="font-weight:bold">Customer Country Code </label>
                                        <p>{{country}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">Customer State Code</label>
                                        <p>{{state}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">Customer City Code</label>
                                        <p>{{city}}</p>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-sm-4 col-lg-4 col-md-4">
                                        <label style="font-weight:bold">Customer Phone Number 1 </label>
                                        <p>{{mobilenumber}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">Customer Phone Number 2</label>
                                        <!--<p>{{}}</p>-->
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">Customer Email</label>
                                        <p>{{emailaddress}}</p>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-sm-4 col-lg-4 col-md-4">
                                        <label style="font-weight:bold">Customer Pincode </label>
                                        <p>{{pincode}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">User ID</label>
                                        <p>{{userid}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4" ng-show="bankaccnoinstitution_list!=null">
                                        <label> Bank Account Number </label>
                                        <select class="chosen-select form-control" ng-options="m.application_gid as m.bankaccount_number for m in bankaccnoinstitution_list" chosen="" id="initiate_bankacc" tabindex="0" ng-model="customer_bankacc"
                                                style="height:37px;" size="1" ng-change="customerbankaccins_change(customer_bankacc)">
                                            <option value=""></option>
                                        </select>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4" ng-show="bankaccnocontact_list!=null">
                                        <label> Bank Account Number </label>
                                        <select class="chosen-select form-control" ng-options="m.application_gid as m.bankaccount_number for m in bankaccnocontact_list" chosen="" id="initiate_bankacc" tabindex="0" ng-model="customer_bankacc"
                                                style="height:37px;" size="1" ng-change="customerbankacccont_change(customer_bankacc)">
                                            <option value=""></option>
                                        </select>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4" ng-show="bankaccnoinstitution_list==null && bankaccnocontact_list==null">
                                        <label> Bank Account Number </label>
                                        <select class="chosen-select form-control" ng-options="m.application_gid as m.bankaccount_number for m in bankaccnocontact_list" chosen="" id="initiate_bankacc" tabindex="0" ng-model="customer_bankacc"
                                                ng-disabled style="height:37px;" size="1" ng-change="customerbankacccont_change(customer_bankacc)">
                                            <option value=""></option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">

                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">Account Holder Name</label>
                                        <p>{{accountholder_name}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">Bank Name</label>
                                        <p>{{bank_name}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">Branch Name</label>
                                        <p>{{branch_name}}</p>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-sm-4 col-lg-4 col-md-4">
                                        <label style="font-weight:bold">IFSC Code </label>
                                        <p>{{ifsc_code}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">RO Name</label>
                                        <p>{{userid}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">KYC Document Type</label>
                                        <p>{{idproof_name}}</p>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-sm-4 col-lg-4 col-md-4">
                                        <label style="font-weight:bold">Document Number </label>
                                        <p>{{idproof_no}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">Customer Type</label>
                                        <!--<p>{{}}</p>-->
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">Sector</label>
                                        <p>{{sector}}</p>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-4 col-md-4 col-sm-4" ng-hide="gst">
                                        <label> Customer GST Number </label>
                                        <select class="chosen-select form-control" chosen="" id="initiate_gst" tabindex="0" ng-model="customer_gst"
                                                ng-options="m.institution_gid as m.Gst for m in gst_list" style="height:37px;" size="1" ng-change="customergst_change(customer_gst)">
                                            <option value=""></option>
                                        </select>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4" ng-show="gst">
                                        <label> Customer GST Number </label>
                                        <select class="chosen-select form-control" chosen="" id="initiate_gst" tabindex="0" ng-model="customer_gst"
                                                required ng-options="m.institution_gid as m.Gst for m in gst_list" style="height:37px;" size="1" ng-change="customergst_change()" ng-disabled>
                                            <option value=""></option>
                                        </select>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">RO Email ID</label>
                                        <p>{{RMmailID}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label style="font-weight:bold">RM Name</label>
                                        <p>{{RMName}}</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br />                        
                        <br />
                        <div class="row" ng-show="reject_flag=='Y' || lms_statuslog=='Rejected'">
                            <div class="col-md-12 col-lg-12 col-sm-12">
                                <div class="panel">
                                    <div class="panel-heading">
                                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;URN Rejected Summary </b></p>
                                        <hr />
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-secondary">
                                                        <thead class="bg-gray text-white">
                                                            <tr class="text-white" style="background-color:#09385f;">
                                                                <th style="display:none;" class="text-center"> S.No</th>
                                                                <th>Reference ID</th>
                                                                <th>Customer Name  </th>
                                                                <th> Rejected By </th>
                                                                <th> Rejected Date </th>
                                                                <th>Rejected Remarks</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in rejected_list">
                                                                <td data-title="'S.No'" style="display:none;" class="text-center"> {{$index+1}} </td>
                                                                <td data-title="'Reference ID'">{{data.cuclms_gid}}</td>
                                                                <td data-title="'Customer Name'"> {{data.customer_name}} </td>
                                                                <td data-title="'Rejected By'"> {{data.initiated_by}} </td>
                                                                <td data-title="'Rejected Date'"> {{data.initiated_date}} </td>
                                                                <td data-title="Rejected Remarks">{{data.rejected_remarks}}</td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                        <br />
                        <div class="dd-handle ng-scope">                           
                            <div class="row">
                                <br />
                                <div class="text-center">
                                    <button type="button" class="btn btn-success btn-md" ng-click="Initiate()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Initiate Request to LMS</button>
                                    <button type="button" ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                                </div>
                            </div>
