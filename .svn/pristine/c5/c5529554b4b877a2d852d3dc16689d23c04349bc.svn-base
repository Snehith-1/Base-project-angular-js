<h3>
    Edit Security Information
</h3>
<div ng-controller="idasMstsecurityEdit">
    <div id="panelDemo8" class="panel panel-primary ">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-edit"></i>
            </span>Edit Security Information
        </div>

        <div class="panel-body">
            <div class="row">
                <form name="security" id="security" novalidate="" class="form-validate mb-lg" autocomplete="off">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="row">
                          
                         
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-6 col-sm-6 col-md-6">

                                        <label>Security Type <span style="color:red"> *</span></label>
                                        <select id="security_type" name="security_type" dropzone="" ng-model="cbosecurity_type" class="chosen-select form-control" required=""
                                                ng-change="show_additional()" size="1">
                                            <option value="">-----Select Security Type----</option>
                                            <option data-ng-repeat="data in security_data" value="{{data.securitytype_gid}}">{{data.security_type}} </option>
                                        </select>
                                    </div>
                                    <div class="col-lg-6 col-sm-6 col-md-6">
                                        <label>Account Status <span style="color:red"> *</span></label>
                                        <select name="account_status" id="account_status" dropzone="" ng-model="cboaccount_status" required="" class="chosen-select form-control" size="1">
                                            <option value="">-----Select Account Status----</option>
                                            <option value="Live">Live</option>
                                            <option value="Closed">Closed</option>

                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div ng-hide="borrower">
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <br />
                                        <div class="col-lg-4 col-sm-4 col-md-4">

                                            <label>Cheque No </label>
                                            <input ng-model="txtborrowercheque_no" class="form-control">

                                        </div>
                                        <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label>Name of the Bank</label>
                                            <input ng-model="txtborrowertbank_name" name="txtlimit" class="form-control">

                                        </div>
                                        <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label>A/c. No of the cheque </label>
                                            <input ng-model="txtborroweraccount_no" name="txtlimit" class="form-control">

                                        </div>
                                    </div>
                                </div>
                                
                                   
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <br />
                                            <div class="col-lg-6 col-sm-6 col-md-6">
                                                <label>Are any deviation from the sanction terms  </label>
                                                <input ng-model="txtborrowerdeviation" class="form-control">

                                            </div>
                                            <div class="col-lg-6 col-sm-6 col-md-6">

                                                <label>Other Remarks if any </label>
                                                <input ng-model="txtborrowerother_remarks" class="form-control" />

                                            </div>
                                        </div>
                                        </div>
                                </div>
                            
                            <div class="col-lg-12 col-sm-12 col-md-12" ng-hide="guarantor">
                                <div class="row">
                                    <br />
                                    <div class="col-lg-4 col-sm-4 col-md-4">

                                        <label>Cheque No </label>
                                        <input ng-model="txtguarantor_cheque" class="form-control ">

                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                      
                                            <label>Name of the Bank ></label>
                                            <input ng-model="txtguarantor_bankname" name="txtlimit" class="form-control">

                                        
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                       
                                            <label>A/c. No of the cheque </label>
                                            <input ng-model="txtguarantor_acno" name="txtlimit" class="form-control">

                                    </div>
                                </div>

                                <div class="row">
                                 <br/>
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <label>Are any deviation from the sanction terms  </label>
                                            <input ng-model="txtguarantor_deviation" class="form-control">
                                        </div>

                                </div>
                                <div class="row">
                                    <br />
                                    <div class="col-lg-6 col-sm-6 col-md-6">
                                            <label>Name of the Personal Guarantor</label>
                                            <input ng-model="txtpersonalguarantor_name" class="form-control">

                                    </div>
                                    <div class="col-lg-6 col-sm-6 col-md-6">
                                            <label>PAN Number </label>
                                            <input ng-model="txtguarantor_panno" class="form-control">

                                       
                                    </div>
                                   
                                </div>
                                <br />
                            </div>

                            <div class="col-lg-12 col-sm-12 col-md-12" ng-hide="corporate_guarantee">
                                <div class="row">
                                    <br/>
                                    <div class="col-lg-6 col-sm-6 col-md-6">

                                        <label>Guarantor Name</label>
                                        <input ng-model="txtcorporate_guarantee" class="form-control ">
                                        </div>
                                    </div>
                                <br />
                                </div>

                         
                            <div class="col-lg-12 col-sm-12 col-md-12" ng-hide="personal_guarantee">
                                <div class="row">
                                    <br />
                                    <div class="col-lg-6 col-sm-6 col-md-6">

                                        <label>Guarantor Name</label>
                                        <input ng-model="txtpersonal_guarantee" class="form-control ">

                                    </div>
                                    <br/>
                                    </div>

                            </div>
                            <div class="col-lg-12 col-sm-12 col-md-12" ng-hide="fixed_deposits">
                                <div class="row">
                                    <br/>
                                    <div class="col-lg-6 col-sm-6 col-md-6">

                                        <label>Bank Name </label>
                                        <input ng-model="txtfd_bank_name" class="form-control ">

                                    </div>
                                    <div class="col-lg-6 col-sm-6 col-md-6">
                                        <label>FD Number <span style="color:red">*</span></label>
                                        <input ng-model="txtfd_no" name="txtlimit" class="form-control">

                                
                                    </div>
                                    
                                </div>

                                <div class="row">
                                       <br/>
                                        <div class="col-lg-6 col-sm-6 col-md-6">
                                            <label>Expiry Date </label>


                                            <md-input-container flex="" class="md-default-theme" style="height: auto;">

                                                <input type="date" id="fromDate" name="fromdate" ng-model="txtfd_expiry_date" required>
                                            </md-input-container>

                                        </div>
                                        <div class="col-lg-6 col-sm-6 col-md-6">
                                            <label>Auto Renewal </label>
                                            <div class="radio c-radio">
                                                <label>
                                                    <input type="radio" id="rdbauto_renewal" name="yes" value="Yes" ng-model="rdbauto_renewal" />
                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                </label>
                                                <label>
                                                    <input type="radio" id="rdbauto_renewal" name="no" value="No" ng-model="rdbauto_renewal" />
                                                    <span class="fa fa-circle"></span>No
                                                </label>
                                            </div>
                                        </div>
                                   
                                </div>


                            </div>
                            <div class="col-lg-12 col-sm-12 col-md-12" ng-hide="bank_guarantee">
                                <div class="row">
                                    <br/>
                                    <div class="col-lg-6 col-sm-6 col-md-6">

                                        <label>Bank Name </label>
                                        <input ng-model="txtbankguarantee_bankname" class="form-control ">

                                    </div>
                                    <div class="col-lg-6 col-sm-6 col-md-6">
                                           <label>Expiry Date </label>

                                            <md-input-container flex="" class="md-default-theme" style="height: auto;">

                                                <input type="date" id="fromDate" name="fromdate" ng-model="txtbankguarantee_expirydate" required>
                                            </md-input-container>
                                        
                                    </div>

                                </div>



                            </div>
                            <div class="col-lg-12 col-sm-12 col-md-12" ng-hide="insurance">
                                <div class="row">
                                    <br/>
                                    <div class="col-lg-4 col-sm-4 col-md-4">

                                        <label>Insurance Company Name</label>
                                        <input ng-model="txtinsurancecompany_name" class="form-control ">

                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                          <label>Policy No</label>
                                            <input ng-model="txtpolicy_no" name="txtlimit" class="form-control">

                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label>Policy Expiry Date </label>
                                            <md-input-container flex="" class="md-default-theme" style="height: auto;">

                                                <input type="date" id="fromDate" name="fromdate" ng-model="txtpolicy_expiry_date" required>
                                            </md-input-container>

                                    </div>
                                </div>

                            </div>
                            <div class="row">
                                <br />
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-12 col-sm-12 col-md-12">

                                        <label for="input_13">Security Description <span style="color:red"> *</span> </label>
                                        <textarea ng-model="txtsecurity_description" required rows="3" maxlength="1000" class="form-control"
                                                  id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>

                                    </div>
                                </div>
                                
                            </div>
                        <br />

                        </div>
                 
                        <div class="class">
             
                            <div class="text-center">
                                <a class="btn btn-success" ng-click="security_submit(lsacreate_gid)" ng-disabled="security.$invalid">
                                    <em class="fa fa-check"></em>
                                    <span>Update</span>
                                </a>
                                <a class="btn btn-info" ng-click="securityback()">
                                    <em class="fa fa-arrow-circle-left"></em>
                                    <span>Back</span>
                                </a>
                            </div>
                        </div>
                  
                </form>
            </div>
        </div>
    </div>
</div>
