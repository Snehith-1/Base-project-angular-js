<h3>
    Reopen History - 360
</h3>
<div ng-controller="osdTrnMyActivityReopenHistory">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <em class="fa fa-history"></em>&nbsp; Reopen History - View
            <button type="button" title="Window Close" class="pull-right btn btn-sm btn-default" ng-click="close()">
                <em class="fa fa-close text-danger fa-1x">&nbsp;Close</em>
            </button>
        </div>
        <div class="panel-body">
            <div class="col-lg-12 col-sm-12 col-md-12">
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <div class="row">
                        <!-- Closed details-->
                        <div ng-show="closeddtls">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <br/>
                            <h4 class="text-pink">
                                <em class="fa fa-times-circle text-danger fa-lg"></em> Closed Details
                            </h4>
                            <div class="panel-body">
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>
                                                    Closed By
                                                </label>
                                                <br>
                                                {{closed_by}}
                                            </div>

                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>Closed Date</label>
                                                <br>
                                                {{closed_date}}
                                            </div>

                                        </div>
                                    </div>
                                    
                                </div>
                                </div>
                                </div>
                        <!--Completed Details-->
                        <div ng-show="reopencompleteddtls"> 
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <br/>
                        <h4 class="text-pink">
                            <em class="fa fa-folder-open text-danger fa-lg"></em> Completed Details
                        </h4>
                        <br />
                        <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    <label>
                                        Completed By
                                    </label>
                                    <br>
                                    {{completed_by}}
                                </div>

                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    <label>Completed Date</label>
                                    <br>
                                    {{completed_date}}
                                </div>

                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div class="col-lg-8 col-md-8 col-sm-8">
                                    <label><strong>Completed Remarks</strong> </label>
                                    <br>
                                    {{completed_remarks}}
                                </div>
                            </div>
                        </div>
                        <br />
                        <p class="lead bb text-purple text-bold" style="font-size:16px;">Completed Document</p>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="text-center">
                                    <div class="dd-handle ng-scope">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-secondary">
                                                        <thead class="bg-gray text-white">
                                                            <tr width="100%">
                                                                <th align="center" scope="col">S.No</th>
                                                                <th align="center" scope="col">Document Name</th>
                                                                <th align="center" scope="col">Created By</th>
                                                                <th align="center" scope="col">Created Date</th>
                                                                <th align="center" scope="col">
                                                                    Action&nbsp;&nbsp;&nbsp;
                                                                    <a download="" href="" title="Download All" id="btnpdf1" ng-click="downloadall(lblfilepath,lblfilename)">
                                                                        <em class="icon-arrow-down"></em>&nbsp;
                                                                    </a>
                                                                </th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in completereopendocumentdtl" width="100%">
                                                                <td align="left" scope="col" title="S.No">
                                                                    {{$index+1}}
                                                                </td>
                                                                <td align="left" scope="col" title="Document Name">
                                                                    {{data.document_name}}
                                                                </td>
                                                                <td align="left" scope="col" title="Created By">
                                                                    {{data.created_by}}
                                                                </td>
                                                                <td align="left" scope="col" title="Created Date">
                                                                    {{data.created_date}}
                                                                </td>

                                                                <td align="left" scope="col" data-title="Action">
                                                                    <button title="Download Document"
                                                                            ng-click="downloads(data.document_path,data.document_name)"
                                                                            class="btn btn-green">
                                                                        <em class="fa fa-download"></em>
                                                                    </button>
                                                                    <button title="View" class="btn btn-info"  ng-click="documentviewer(data.document_path,data.document_name)">
                                                                        <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em> 
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>
                        </div>
                    </div>
                </div>
                    <br />
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <br />
                        <h4 class="text-pink">
                            <em class="fa fa-user  text-danger fa-lg"></em> Reopen Details
                        </h4>
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="container-fluid">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <!--<div class="col-lg-4 col-sm-4 col-md-4">
                                <label>Request Ref.No</label>
                                <br>
                                {{request_refno}}
                            </div>-->

                                                <div class="col-lg-4 col-sm-4 col-md-4">
                                                    <label>Reopened By</label>
                                                    <br>
                                                    {{raised_by}}
                                                </div>
                                                <div class="col-lg-4 col-sm-4 col-md-4">
                                                    <label>Reopened Date</label>
                                                    <br>
                                                    {{raised_date}}
                                                </div>
                                            </div>
                                        </div>
                                        <br />
                                    </div>
                                </div>
                            </div>
                        </div>

                        <h4 class="text-pink">
                            <em class="fa fa-mail-forward text-danger fa-lg"></em> Forward Details
                        </h4>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="text-center">
                                        <div class="dd-handle ng-scope">
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-secondary">
                                                            <thead class="bg-gray text-white">
                                                                <tr width="100%">
                                                                    <th align="center" scope="col">S.No</th>
                                                                    <th align="center" scope="col">Forward To</th>
                                                                    <th align="center" scope="col">Forward Date</th>
                                                                    <th align="center" scope="col">Forward Remarks</th>

                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in forwardreopendtl" width="100%">
                                                                    <td align="left" scope="col" title="S.No">
                                                                        {{$index+1}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Forward To">
                                                                        {{data.forward_to}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Forward Date">
                                                                        {{data.forward_date}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Forward Remarks">
                                                                        {{data.forward_remarks}}
                                                                    </td>

                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                            </div>

                        </div>
                        <br />
                        <p class="lead bb text-purple text-bold" style="font-size:16px;">&nbsp; Forward Document Details</p>

                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="text-center">
                                    <div class="dd-handle ng-scope">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-secondary">
                                                        <thead class="bg-gray text-white">
                                                            <tr width="100%">
                                                                <th align="center" scope="col">S.No</th>
                                                                <th align="center" scope="col">Document Name</th>
                                                                <th align="center" scope="col">Created By</th>
                                                                <th align="center" scope="col">Created Date</th>
                                                                <th align="center" scope="col">
                                                                    Action&nbsp;&nbsp;&nbsp;
                                                                    <a download="" href="" title="Download All" id="btnpdf1" ng-click="downloadall(lblfilepath,lblfilename)">
                                                                        <em class="icon-arrow-down"></em>&nbsp;
                                                                    </a>
                                                                </th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in forwarddocumentdtl" width="100%">
                                                                <td align="left" scope="col" title="S.No">
                                                                    {{$index+1}}
                                                                </td>
                                                                <td align="left" scope="col" title="Document Name">
                                                                    {{data.document_name}}
                                                                </td>
                                                                <td align="left" scope="col" title="Created By">
                                                                    {{data.created_by}}
                                                                </td>
                                                                <td align="left" scope="col" title="Created Date">
                                                                    {{data.created_date}}
                                                                </td>
                                                                <td align="left" scope="col" data-title="Action">
                                                                    <button title="Download Document"
                                                                            ng-click="downloads(data.document_path,data.document_name)"
                                                                            class="btn btn-green">
                                                                        <em class="fa fa-download"></em>
                                                                    </button>
                                                                    <button title="View" class="btn btn-info" ng-click="documentviewer(data.document_path,data.document_name)">
                                                                        <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>
                        <br />
                        <p class="lead bb text-purple text-bold" style="font-size:16px;"> &nbsp;&nbsp;&nbsp; Tagged Member List</p>
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="dd-handle ng-scope">
                                    <div class="row text-center">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="table-responsive">
                                                <table class="table table-striped table-secondary">
                                                    <thead class="bg-gray text-white">
                                                        <tr width="100%">
                                                            <th align="center" scope="col">S.No</th>
                                                            <th align="center" scope="col">Member Name</th>
                                                            <th align="center" scope="col">Tagged Date/Time</th>
                                                            <th align="center" scope="col">Tagged By</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in tagmemberdtl" width="100%">
                                                            <td align="left" scope="col" title="S.No">
                                                                {{$index+1}}
                                                            </td>
                                                            <td align="left" scope="col" title="Member Name">
                                                                {{data.employee_name}}
                                                            </td>
                                                            <td align="left" scope="col" title="Tagged Date/Time">
                                                                {{data.created_date}}
                                                            </td>
                                                            <td align="left" scope="col" title="Tagged By">
                                                                {{data.created_by}}
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br />
                        <!--Approval Details-->
                        <p class="lead bb text-purple text-bold" style="font-size:16px;">&nbsp; Approval Details</p>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="text-center">
                                    <div class="dd-handle ng-scope">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-secondary">
                                                        <thead class="bg-gray text-white">
                                                            <tr width="100%">
                                                                <th align="center" scope="col">S.No</th>
                                                                <th align="center" scope="col">Approval Person</th>
                                                                <th align="center" scope="col">Approval Status</th>
                                                                <th align="center" scope="col">Approval Status Date</th>
                                                                <th align="center" scope="col">Approval Initiated By</th>
                                                                <th align="center" scope="col">Approval Remarks</th>
                                                                <th align="center" scope="col">Request Remarks</th>
                                                                <th align="center" scope="col">Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in approvaldetails" width="100%">
                                                                <td align="left" scope="col" title="S.No">
                                                                    {{$index+1}}
                                                                </td>
                                                                <td align="left" scope="col" title="Approval Person">
                                                                    {{data.approved_by}}
                                                                </td>
                                                                <td align="left" scope="col" title="Approval Status">
                                                                    {{data.approval_status}}
                                                                </td>
                                                                <td align="left" scope="col" title="Approved / Rejected Date">
                                                                    {{data.approval_date}}
                                                                </td>
                                                                <td align="left" scope="col" title="Approval Initiated By">
                                                                    {{data.approvalinitiated_by}}
                                                                </td>
                                                                <td align="left" scope="col" title="Remarks">
                                                                    {{data.approval_remarks}}
                                                                </td>
                                                                <td title="Request Remarks" align="left" scope="col">
                                                                    <button title="View" ng-click="request_remarks(data.requestapproval_gid)" class="btn btn-info">
                                                                        <em class="fa fa-eye"></em>
                                                                    </button>
                                                                </td>
                                                                <td data-title="'Action'" class="nowrap">
                                                                    <label ng-switch="data.approval_status">
                                                                        <button title="Cancel Approval" ng-switch-when="Pending" ng-click="CancelApprovalPerson(data.approval_token)" class="btn btn-danger">
                                                                            <em class="fa fa-times-circle text-white fa-lg"></em>
                                                                        </button>
                                                                    </label>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>

                        <br />
                        <!--Transfer Details-->
                        <p class="lead bb text-purple text-bold" style="font-size:16px;">Transfer History</p>

                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="text-center">
                                    <div class="dd-handle ng-scope">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-secondary">
                                                        <thead class="bg-gray text-white">
                                                            <tr width="100%">
                                                                <th align="center" scope="col">S.No</th>
                                                                <th align="center" scope="col">Assigned Team</th>
                                                                <th align="center" scope="col">Assigned Member</th>
                                                                <th align="center" scope="col">Assigned Date/Time</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in transferlistdtlreopen" width="100%">
                                                                <td align="left" scope="col" title="S.No">
                                                                    {{$index+1}}
                                                                </td>
                                                                <td align="left" scope="col" title="Assigned Team">
                                                                    {{data.assigned_team}}
                                                                </td>
                                                                <td align="left" scope="col" title="Assigned Member">
                                                                    {{data.assigned_to}}
                                                                </td>
                                                                <td align="left" scope="col" title="Assigned Date/Time">
                                                                    {{data.assigned_date}}
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>
                        <br />
                        <p class="lead bb text-purple text-bold" style="font-size:16px;">Allocation History</p>

                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="text-center">
                                    <div class="dd-handle ng-scope">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-secondary">
                                                        <thead class="bg-gray text-white">
                                                            <tr width="100%">
                                                                <th align="center" scope="col">S.No</th>
                                                                <th align="center" scope="col">Assigned Manager</th>
                                                                <th align="center" scope="col">Assigned Date/Time</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in allocatelistdtlreopen" width="100%">
                                                                <td align="left" scope="col" title="S.No">
                                                                    {{$index+1}}
                                                                </td>

                                                                <td align="left" scope="col" title="Assigned Member">
                                                                    {{data.allocate_managername}}
                                                                </td>
                                                                <td align="left" scope="col" title="Assigned Date/Time">
                                                                    {{data.assigned_date}}
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>
                        <br />
                    </div>
                    </div>
                </div>
                <br />

            </div>
        </div>
      
    </div>
    <!--Cancel ApprovalMember Popup-->
    <script type="text/ng-template" id="/cancelmembermodal.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-times-circle text-white fa-lg"></em>&nbsp;Cancel Approval Member</h2>
                <span flex></span>

            </div>
        </md-toolbar>
        <div class="modal-body ng-scope">
            <form novalidate="" name="CancelSubmit" ng-submit="form.CancelSubmit()" autocomplete="off" class="form-validate form-horizontal">
                <div class="pd-sm-10">
                    <div class="row">
                        <div class="col-sm-12 col-lg-12 col-md-12">
                            <label>Remarks  &nbsp;<span style="color:red">*</span></label>
                            <textarea ng-model="txtremarks" placeholder="Enter Remarks" rows="3" maxlength="1000" class="form-control"
                                      required id="input_13" ng-trim="true" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                        </div>
                    </div>
                    <br />
                </div>
                <br />
                <div>
                    <div class="text-center">

                        <button type="button" class="btn btn-success btn-md" ng-disabled="CancelSubmit.$invalid" ng-click="CancelMemberSubmit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
            </form>
        </div>
    </script>

    <script type="text/ng-template" id="/RequestRemarks.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-file-text-o"></em>&nbsp;<label style="font-size:16px">Request Remarks</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">

                <form novalidate="" name="userform1" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="dd-handle ng-scope">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <label> Request Remarks</label>
                                        <br />
                                        <p>{{txtrequestapproval_remarks}}</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>
</div>