<h3>
    Document Verification - Physical Document
</h3>
<div ng-controller="IdasTrnPhyDocVerify">
    <div id="panelDemo13" class="panel panel-default">
        <div class="panel-body table-responsive">
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12 ">
                    <p class="lead bb text-primary text-bold text-center">
                        {{customerName}}
                        <label style="font-weight:bold">/</label>{{customercode}}
                    </p>
                    <p class="lead bb text-purple text-bold" style="font-size:16px;">Customer Details</p>

                    <div class="col-lg-12">
                        <label class="col-form-label">Address</label>
                        <br />
                        <p style="font-size:14px;">
                            {{contactperson}}<br />{{addressline1}}<br />
                            {{addressline2}}
                            <br />{{mobileno}}
                        </p>

                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label"> URN No :</label>
                        <br />
                        <p style="font-size:14px;">{{Customerurn}}</p>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 ">

                    <div class="col-lg-4">
                        <label class="col-form-label">Cluster Manager</label>
                        <br />
                        <p ng-model="ClusterMgr" style="font-size:14px;">{{clusterManager}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Relationship Manager</label>
                        <br />
                        <p style="font-size:14px;">{{relationshipmgmt}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Credit Manager</label>
                        <br />
                        <p style="font-size:14px;">{{creditManager}}</p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12">

                    <div class="col-lg-4">
                        <label class="col-form-label">Zonal Head</label>
                        <br />
                        <p ng-model="zonalHeadName" style="font-size:14px;">{{zonalHeadName}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Business Head</label>
                        <br />
                        <p style="font-size:14px;">{{businessHeadName}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">LSA Status</label>
                        <br />
                        <p style="font-size:14px;">{{lsa_status}}</p>
                    </div>
                </div>
            </div>
            <p class="lead bb text-purple text-bold" style="font-size:16px;">Sanction Details</p>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 ">

                    <div class="col-lg-4">
                        <label class="col-form-label">Sanction Ref No.</label>
                        <br />
                        <p ng-model="sanctionref_no" style="font-size:14px;">{{sanctionrefno}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Sancion Date</label>
                        <br />
                        <p style="font-size:14px;">{{SanctionDate}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Sanction Amount</label>
                        <br />
                        <p ng-model="sanction_amount" style="font-size:14px;">{{SanctionAmount}}</p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 ">

                    <div class="col-lg-4">
                        <label class="col-form-label">Facility </label>
                        <br />
                        <p ng-model="facility_type" style="font-size:14px;">{{FacilityType}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Vertical</label>
                        <br />
                        <p style="font-size:14px;">{{verticalCode}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Collateral Details</label>
                        <br />
                        <p ng-model="collateral_security" style="font-size:14px;">{{collateral_security}}</p>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <p class="lead bb text-purple text-bold" style="font-size:16px;">Upload Common Document</p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-lg-2 col-md-2 col-sm-2">
                        <button type="button" ng-click="FileShow()" class="btn btn-info btn-xs">
                            <span class="icon icon-cloud-upload"></span>&nbsp;&nbsp;Upload File
                        </button>
                    </div>
                </div>
            </div>
            <br />
            <div ng-show="DivFile">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-2 col-md-2 col-sm-2">
                            <label>Document Title</label>&nbsp;<span style="color:red">*</span>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3">
                            <input type="text" ng-model="txtdocument_title" rows="1" tabindex="0" min="0" aria-required="true"
                                   aria-invalid="true" class="form-control" maxlength="256" />
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-2 col-md-2 col-sm-2">
                            <label>File Upload</label>
                        </div>

                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <input type="file" id="commonupload" ng-model="common_document" ng-disabled="!txtdocument_title"
                                   onchange="angular.element(this).scope().commondocumentupload(this.files, angular.element(this).scope().commondocumentupload, this.name)"
                                   data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select"
                                   accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />
                        </div>
                    </div>
                </div>
                <br />
            </div>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="text-center">
                            <div class="dd-handle ng-scope">
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="table-responsive">
                                            <table class="table table-striped table-secondary">
                                                <thead class="bg-gray text-white">
                                                    <tr width="100%">
                                                        <th align="center" scope="col">S.No</th>
                                                        <th align="center" scope="col">Document Title</th>
                                                        <th align="center" scope="col">Document Name</th>
                                                        <th align="center" scope="col">Upload By/On</th>
                                                        <th align="center" scope="col">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in commondocument" width="100%">
                                                        <td scope="col" title="'S.No'">{{$index+1}}</td>
                                                        <td align="left" scope="col" title="'Document Title'">{{data.document_title}}</td>
                                                        <td align="left" scope="col" title="'Document Name'">{{data.document_name}}</td>
                                                        <td align="left" scope="col" title="'Upload By/On'">{{data.created_by}}/{{data.created_date}}</td>
                                                        <td align="left" scope="col" data-title="'Action'">
                                                            <button title="Download Document" ng-click="downloadsdocument(data.document_path,data.document_name)" class="btn btn-info">
                                                                <em class="fa fa-download"></em><a download="" href="" id="btnpdf" style="display:none;"></a>
                                                            </button>
                                                            <button title="Delete Document" ng-click="deletedocument(data.uploaddocument_gid)" class="btn btn-danger">
                                                                <em class="fa fa-close"></em>
                                                            </button>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>

                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <p class="lead bb text-purple text-bold" style="font-size:16px;">Mail Management</p>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-md-6 col-lg-6 col-sm-6">


                        <button title="Send Mail" ng-click="mailconversation()" class="btn btn-info">
                            <em class="fa fa-send (alias)"></em>
                        </button>
                        &nbsp;&nbsp;

                        <button title="Export Excel" ng-click="export()" class="btn btn-success ">
                            <em class="fa fa-file-excel-o"></em><a download="" href="" id="btnpdf" style="display:none;"></a>
                        </button>
                        &nbsp;&nbsp;


                        <button title="Import" ng-click="openpanel()" class="btn btn-pink">
                            <em class="icon icon-cloud-upload"></em>
                        </button>
                    </div>



                </div>
            </div>
            <br />
            <div class="row" ng-hide="show">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-md-1 col-lg-1 col-sm-1">
                        <span>File Upload</span>

                    </div>
                    <div class="col-md-2 col-lg-2 col-sm-2">
                        <input type="file" id="addupload" ng-model="test_document"
                               onchange="angular.element(this).scope().uploadallocation(this.files, angular.element(this).scope().documentupload, this.name)"
                               data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select"
                               accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />
                        &nbsp;
                    </div>
                    <div class="col-md-2 col-lg-2 col-sm-2">
                        <button ng-click="cancel()" class="btn btn-danger">
                            <i class="fa fa-close"></i>&nbsp;&nbsp;Cancel
                        </button>

                    </div>

                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-md-12 col-lg-12 col-sm-12">

                        <div class="table-responsive">
                            <table class="row-border hover" id="statictable">

                                <thead>
                                    <tr class="bg-success text-white">
                                        <th width="3%">S.No</th>
                                        <th width="10%">Sent On</th>
                                        <th width="10%">Sent By</th>
                                        <th width="10%">Subject</th>
                                        <th width="10%">Sent To</th>
                                        <th width="10%">Sent From</th>
                                        <th width="10%">Attachment</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="data in senddata">
                                        <td class="text-center">
                                            {{$index+1}}
                                        </td>
                                        <td>
                                            {{data.created_date}}
                                        </td>
                                        <td>
                                            {{data.created_by}}
                                        </td>

                                        <td>
                                            {{data.subject}}
                                        </td>
                                        <td>
                                            {{data.to_mail}}
                                        </td>
                                        <td>
                                            {{data.from_mail}}
                                        </td>
                                        <td>
                                            <a ng-click="exportattach(data.document_path,data.document_name)">IDAS Document Check List.xls</a>
                                        </td>

                                    </tr>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <br />

            <br />
            <p class="lead bb text-purple text-bold" style="font-size:16px;">Physical Document Details</p>
            <br />
            <div class="row">
                <div class="col-md-12 col-sm-12 col-lg-12">
                    <div class="col-lg-4 col-md-4 col-sm-4">
                        <button ng-click="DocumentVerify()" class="btn btn-success">
                            <i class="icon icon-check"></i>&nbsp;&nbsp;Document Verified
                        </button>
                    </div>
                    <div class="pull-right ">
                        <a title="Color Indicates" style="background-color:#F1BCB1;" class="ng-binding ng-scope label">
                            <em class="fa"></em>
                        </a>&nbsp;<lable> - Color Indicates for Document in Vetting Stage</lable>
                    </div>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <!--<input type="text" ng-model="search" class="mb form-control ng-valid ng-dirty ng-valid-parse ng-touched"
                               placeholder="Enter text to search..." />-->
                        <div>
                            <table datatable="ng" class="row-border hover">

                                <thead>
                                    <tr class="bg-primary-light text-white">
                                        <th width="3%">
                                            S.No
                                        </th>
                                        <th width="8%">
                                            Check All &nbsp;
                                            <input type="checkbox" id="c2" ng-model="all" ng-change="checkallDocument(all);" >
                                        </th>
                                        <th width="6%">Document Record ID</th>
                                        <th width="15%">Document Code/Name</th>
                                        <th width="10%">Scan Document Status</th>
                                        <th width="15%">Verified Date/By</th>
                                        <th width="8%">Query to RM</th>
                                        <th width="10%">Status</th>
                                        <th width="5%">Action</th>
                                    </tr>
                                <tbody>
                                    <tr ng-repeat="data in documentation_list"
                                        ng-style="data.maker_status=='Maker Pending'?{'background-color':'#F1BCB1'}:{}&&data.checker_status=='Checker Pending'?{'background-color':'#F1BCB1'}:{}">
                                        <td width="3%">
                                            {{$index+1}}

                                        </td>
                                        <td width="8%" align="center">
                                            <input type="checkbox" id="c1"
                                                   value={{data.checked}} ng-model="data.checked" ng-if="data.phydoc_status!='Verified'">
                                        </td>
                                        <td data-title="Document Record ID" width="6%">{{data.documentrecord_id}}</td>
                                        <td data-title="Document Code/Name" width="15%">{{data.document_code}}/{{data.document_name}}</td>
                                        <td data-title="Status" width="10%">
                                            <div ng-switch="data.maker_status">
                                                <label ng-switch-when="Maker Confirmed" class="label label-success">Maker Confirmed</label>
                                                <label ng-switch-when="Maker Pending" class="label label-warning">Maker Pending</label>
                                            </div>
                                            <div ng-switch="data.checker_status">
                                                <label ng-switch-when="Checker Confirmed" class="label label-success">Checker Confirmed</label>
                                                <label ng-switch-when="Checker Pending" class="label label-warning">Checker Pending</label>
                                            </div>

                                        </td>
                                        <td data-title="Verified Date/By" width="15%">
                                            {{data.phydocverifier_name}}/{{data.Phydocverified_on}}
                                        </td>
                                        <td width="8%">
                                            <label ng-if="data.phyconversation_count!='0'">
                                                <em class="fa fa-check success"></em>
                                            </label>
                                            <label ng-if="data.phyconversation_count=='0'">
                                                -
                                            </label>


                                        </td>

                                        <td data-title="Status" width="10%">
                                            <div ng-switch="data.phydoc_status">
                                                <label ng-switch-when="Pending" class="label label-warning">Pending</label>
                                                <label ng-switch-when="Verified" class="label label-success">Verified</label>
                                            </div>
                                        </td>
                                        <td data-title="Action" width="5%">
                                            <div ng-switch="data.phydoc_status">

                                                <button title="Document Conversation" ng-switch-when="Pending" ng-disabled="data.maker_status=='Maker Pending' && data.checker_status=='Checker Pending'" ng-click="docConMkr(data.sanctiondocument_gid,data.phyconversation_count,data.phydoc_status)" class="btn btn-pink">
                                                    <em class="icon icon-directions"></em>
                                                </button>
                                                <button title="Document Conversation" ng-switch-when="Verified" ng-click="docConMkr(data.sanctiondocument_gid,data.phyconversation_count,data.phydoc_status)" class="btn btn-success">
                                                    <em class="icon icon-directions"></em>
                                                </button>
                                                <span ng-if="data.phyconversation_count != '0'" class="badge bg-gray-light ">{{data.phyconversation_count}}</span>
                                            </div>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>
            </div>

        </div>
        <br />
        <div>

            <div class="ng-scope" id="down">
                <div class="text-center">
                    <button ng-click="BatchConfirm()" ng-if="batch_status=='Pending'" ng-disabled="phydocunverified_count!=''" class="btn btn-success"><i class="fa fa-thumbs-o-up"></i>&nbsp;&nbsp;Batch Confirm</button>
                    <button ng-click="phydocback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                </div>
            </div>
        </div>
        <br />
        <script type="text/ng-template" id="/mailconversation.html">


            <div class="modal-body ng-scope">
                <h4 id="myModalLabel" class="modal-title">
                    <p class="lead bb text-primary text-bold text-center">
                        Document Mail - Conversation
                    </p>
                </h4>
                <div class="row">
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <div class="col-lg-6">
                            <label>From :</label> no-reply@samunnati.com
                        </div>

                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <div class="col-lg-6">
                            <label>To :</label>{{rmmail_id}}
                        </div>
                    </div>
                </div>


                <div ng-show="mailbox.mail.cc" class="input-group form-group">
                    <span class="input-group-addon text-sm">CC:</span>
                    <input type="text" class="form-control" required="" ng-model="cc_mail" />
                </div>
                <div ng-show="mailbox.mail.bcc" class="input-group form-group">
                    <span class="input-group-addon text-sm">BCC:</span>
                    <input type="text" class="form-control" required="" ng-model="bcc_mail" />
                </div>
                <form novalidate="" name="userform5" ng-submit="form.submitForm1()" autocomplete="off">
                    <div class="input-group form-group">

                        <span class="input-group-addon text-sm">Subject:</span>
                        <input type="text" class="form-control" required="" ng-model="subject_mail">

                    </div>
                </form>
                <div class="btn-group btn-group-sm mb pull-left">
                    <strong>
                        Attachment : <a ng-click="export()">IDAS Document Check List.xls</a>
                    </strong>
                </div>
                <div class="btn-group btn-group-sm mb pull-right">
                    <button type="button" ng-class="{'active':mailbox.mail.cc}" ng-click="mailbox.mail.cc = !mailbox.mail.cc" href="#" class="btn btn-default btn-sm">CC</button>
                    <button type="button" ng-class="{'active':mailbox.mail.bcc}" ng-click="mailbox.mail.bcc = !mailbox.mail.bcc" href="#" class="btn btn-default btn-sm">BCC</button>
                </div>
                <div class="imgcrop-area" style="height:250px;">
                    <p ng-bind-html="mailcontent"></p>
                </div>
                <br />
                <div class="text-center">
                    <button ng-click="sendMail()" ng-disabled="userform5.$invalid" class="btn btn-success">
                        <i class="fa fa-send (alias)">
                        </i>&nbsp;Send
                    </button>
                    <button ng-click="ok()" class="btn btn-danger">
                        <i class="fa fa-close (alias)">
                        </i>&nbsp;Cancel
                    </button>
                </div>
            </div>
        </script>
        <script type="text/ng-template" id="/buyerlist.html">


            <div class="modal-body ng-scope">
                <h4 id="myModalLabel" class="modal-title">
                    <p class="lead bb text-primary text-bold text-center">
                        Status of BAL
                    </p>
                </h4>

                <div class="text-center">
                    <div class="dd-handle ng-scope">
                        <p><strong>Make sure that  BAL has received for the following</strong></p>
                        <div class="row">

                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="table-responsive">
                                    <table class="table" style="padding: 0px 0px">
                                        <thead class="bg-indigo-400">
                                            <tr class="bg-primary-light text-white">
                                                <th class="text-nowrap">S No</th>
                                                <th class="text-nowrap">Buyer Name</th>
                                                <th class="text-nowrap">Exposure</th>
                                                <th class="text-nowrap">Document Details</th>


                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="list in buyer_list">
                                                <td data-title="S No" class="text-nowrap">{{$index+1}}</td>
                                                <td data-title="Buyer Name" class="text-nowrap">{{list.buyer_name}}</td>
                                                <td data-title="Buyer Exposure" class="text-nowrap">{{list.buyer_exposure}}</td>
                                                <td data-title="Document Details" class="text-nowrap">
                                                    <p ng-hide="list.document_name=='---'">
                                                        {{list.document_name}} / {{list.uploaded_by}} on {{list.uploaded_date}}
                                                        <button title="Download" class="btn btn-default btn-sm"
                                                                ng-click="downloadsBAL(list.document_path,list.document_name)">
                                                            <em class="fa fa-download"></em>
                                                        </button>
                                                    </p>
                                                    <p ng-show="list.document_name=='---'">
                                                        {{list.document_name}}
                                                    </p>

                                                </td>


                                            </tr>

                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

                    </div>
                    <br />
                    <button ng-click="batch_ok()" class="btn btn-success">
                        <i class="icon icon-like">
                        </i>&nbsp;Confirm
                    </button>
                    <button ng-click="batch_close()" class="btn btn-danger">
                        <i class="fa fa-close (alias)">
                        </i>&nbsp;Close
                    </button>
                </div>
            </div>
        </script>

        <script type="text/ng-template" id="/DocumentVerificationModalContent.html">
            <md-toolbar>
                <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                    <h2><em class="fa fa-plus"></em>&nbsp;Document Verification</h2>
                    <span flex></span>

                </div>
            </md-toolbar>

            <div class="modal-body ng-scope">
                <form name="documentVerificationform" id="documentVerificationform" novalidate="" class="form-validate mb-lg" autocomplete="off">
                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <label>Document Date <span style="color:red;">*</span></label>
                            <p class="input-group">
                                <input type="date" id="phydocument_date" name="phydocument_date" ng-model="phydocument_date"
                                       style="width:570px;height:35px" class="form-control" ng-change="futuredatecheck(phydocument_date)" required>
                            </p>
                        </div>
                        <br /> <br /> <br /><br /><br />
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <label class="col-form-label control-label"><strong> Types Of Document <span style="color:red;">*</span></strong></label><br />
                            <div class="radio c-radio">
                                <label>
                                    <input type="radio" required name="types_of_copy" value="Original" ng-model="types_of_copy" />
                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Original
                                </label>
                                <label>
                                    <input type="radio" required name="types_of_copy" value="Copy" ng-model="types_of_copy" />
                                    <span class="fa fa-circle"></span>Copy
                                </label>
                                <label>
                                    <input type="radio" required name="types_of_copy" value="CTC" ng-model="types_of_copy" />
                                    <span class="fa fa-circle"></span>CTC
                                </label>

                            </div>


                        </div>
                        <br /> <br /> <br /><br /><br />
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <label>Remarks<span style="color:red;">*</span></label>
                            <textarea ng-model="remarks" placeholder="Enter the Description" rows="4" class="form-control" style="resize:vertical;"
                                      id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true" required></textarea>
                        </div>
                    </div>
                    <br /> <br />
                    <div clas="row">
                        <div class="text-center">
                            <a class="btn btn-success" ng-click="documentverificationSubmit()" ng-disabled="documentVerificationform.$invalid">
                                <em class="fa fa-check"></em>
                                <span>Document Verified</span>
                            </a>
                            <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                        </div>
                    </div>
                </form>
            </div>
        </script>

    </div>
</div>
