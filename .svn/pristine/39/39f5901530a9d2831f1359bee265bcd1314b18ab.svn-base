<h3>
    &nbsp;&nbsp;Partial Matched View
</h3>

<style>
    #bordersty {
        border: 1px solid #ebebeb;
        padding: 10px;
        border-radius: 25px;
    }
</style>
<style>
    .buttonstyle {
        border: none;
        background-color: rgb(255, 255, 255);
        color: #141478;
    }
</style>
<div ng-controller="brsTrnPartialMatchedViewController">
    <div class="panel">
        <div class="row">
            <div class="col-lg-12 col-sm-12 col-md-12">
                <div class="dd-handle ng-scope">
                    <p class="text-primary" style="font-size:16px;">
                        <b>
                            &nbsp;&nbsp;Partial Matched View
                        </b>
                        <a>
                            <button type="button" title="Back to Summary" ng-click="Back()" class="pull-right btn btn-sm btn-primary ">
                                <em class="fa fa-arrow-circle-left fa-1x"></em>
                            </button>
                        </a>
                    </p>


                </div>
            </div>
            <br />           

            <div style="margin-bottom:-10px;">
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div id="bordersty" style="background-color: #FAF8F8;">
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Reference  Number
                                            </label>
                                            <br>
                                            <span>
                                                {{banktransc_gid}}
                                            </span>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Bank Name
                                            </label>
                                            <br>
                                            <span>
                                                {{lblbank_name}}
                                            </span>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <!-- <label class="text-primary">
                                                Customer Reference No.
                                            </label>
                                            <br>
                                            <span>
                                                {{lblcustomer_refno}}
                                            </span> -->
                                            <label class="text-primary">
                                                Cheque No.
                                            </label>
                                            <br>
                                            <span>
                                                {{lblchq_no}}
                                            </span>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Branch Name
                                            </label>
                                            <br>
                                            <span>
                                                {{lblbranch_name}}
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <br>
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Credit/Debit
                                            </label>
                                            <br>
                                            <span>
                                                {{lblcr_dr}}<br>
                                            </span>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Transaction Value
                                            </label>
                                            <br>
                                            <span>
                                                {{lbltransc_value}}
                                            </span>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <!-- <label class="text-primary">
                                                Transaction Balance
                                            </label>
                                            <br>
                                            <span>
                                                {{lbltransc_balance}}
                                            </span> -->
                                            <label class="text-primary">
                                                Transaction Particulars
                                            </label>
                                            <br>
                                            <span>
                                                {{lbltransact_particulars}}
                                            </span>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Remarks
                                            </label>
                                            <br>
                                            <span>
                                                {{lblremarks}}
                                            </span>
                                        </div>

                                    </div>

                                    <br />
                                </div>
                                <br />
                            </div>
                            <br />

                        </div>
                    </div>
                </div>
                <br>
            </div>
            <br />

            <br />

        </div>
        <br />
        <br />

        <div class="row">
            <div class="col-lg-12 col-sm-12 col-md-12">
                <div class="dd-handle ng-scope">
                    <p class="text-primary" style="font-size:16px;">
                        <b>
                            &nbsp;&nbsp; LMS Repayment Details
                        </b>

                    </p>
                </div>

            </div>
        </div>
        <br />
        <div class="row">
            <div class="col-lg-12 col-sm-12 col-md-12">
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <div class="table-responsive">
                        <table class="table table-striped table-secondary">
                            <thead class="bg-gray text-white">
                                <tr width="100%">
                                    <th class="text-center" style="display:none"> S.No </th>
                                    <th> Repayment ID </th>
                                    <th> Account Number </th>
                                    <th> Transaction Date </th>
                                    <th> Repayment Date </th>
                                    <th> Account Code</th>                                    
                                    <th> Repayment Amount</th>
                                    <th> Remarks</th>
                                    <th>Knockoff-Status</th>
                                    <th>Knockoff-Date</th>
                                    <th>Transaction ID</th>
                                    <th>Created By</th>

                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in repayment_lmshistory">
                                    <td data-title="'S.No'" style="text-align: center; width: 10%; display: none;"> {{$index+1}} </td>
                                    <td data-title="'Repayment ID'">{{data.bankrepaytransc_gid }}</td>
                                    <td data-title="'Account Number'">{{data.account_number }}</td>
                                    <td data-title="'Repayment Date'">{{data.transaction_date }}</td>
                                    <td data-title="'Transaction Particulars'">{{data.repayment_date }}</td>
                                    <td data-title="'Account Code'">{{data.account_code}}</td>                                   
                                    <td data-title="'Repayment Amount'">{{data.repayment_amount}}</td>
                                    <td data-title="'Remarks'">{{data.remarks}}</td>
                                    <td data-title="'Knockoff-Status'" ng-switch="data.knockoff_status">                                      
                                        <label style="font-size:10px" ng-switch-when="PartiallyMatched" class="label label-warning">Partial Matched</label>
                                    </td>
                                    <td data-title="'Knockoff-Date'">{{data.knockoff_date}}</td>
                                    <td data-title="Transaction ID">{{data.banktransc_gid}}</td>
                                    <td data-title="'Created By'">{{data.created_by}}</td>

                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <br />        
        <div class="row">
            <div class="text-center">
                <br />
                <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
            </div>
        </div>
        <br />
        <br />
        <!--<div class="text-center">

            <button type="button" title="Back to Summary" ng-click="Back()" class=" btn btn-sm btn-primary ">
                <em class="fa fa-arrow-circle-left fa-1x"></em>
            </button>
        </div>-->
    </div>
</div>

<br />
<br />
