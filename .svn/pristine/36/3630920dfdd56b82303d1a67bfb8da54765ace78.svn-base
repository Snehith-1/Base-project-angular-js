<style>
    .nav.nav-pills .active > a {
        background-color: rgb(19,14,89);
        background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);
        color: #ffffff;
    }
</style>
<h3>
    Customer Application 
</h3>
<div ng-controller="AgrMstApplcreationSocialTradeEditController as dp">
    <div class="panel">
        <div class="panel-heading" style="background-color:rgb(19,14,89);background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);color:#ffffff">
            <h5> Edit Economic Capital </h5>
        </div>
        <div class="row">

                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="panel-body">
                            <div class="row">
                                <form name="SocialTradeCapitalupdate" ng-submit="form.submitForm1()" novalidate="" autocomplete="off" class="form-validate form-horizontal">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <br />
                                                    <div class="col-sm-6 col-md-6 col-lg-6">
                                                        <label><strong>Social Capital</strong></label>
                                                        <div text-angular="" ng-model="SocialCapital"
                                                             placeholder="Type Social Capital Description..." maxlength="5000" required name="wysiswyg-editor" class="btn-group-justified">
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6 col-md-6 col-lg-6">
                                                        <label><strong>Trade Capital</strong></label>
                                                        <div text-angular="" ng-model="TradeCapital"
                                                             placeholder="Type Trade Trade Description..." maxlength="5000" required name="wysiswyg-editor1" class="btn-group-justified">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <br /><br />
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <br />
                                                <div class="text-center">
                                                    <button type="button" class="btn btn-success btn-md" ng-disabled="SocialTradeCapitalupdate.$invalid" ng-click="update_SocialTradeCapital()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                                                    <button ng-click="SocialTradeapital_Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                            <br />
                        </div>
                    </div>
                </div>

</div>
    </div>
</div>