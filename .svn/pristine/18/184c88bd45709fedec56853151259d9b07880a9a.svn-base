<h3>
    Audit Report
</h3>
<div ng-controller="AtmTrnAuditReportSummaryController">

    <button type="button" class="btn btn-success pull-right" ng-click="exporauditreport()" title="Export Excel"><i
        class="fa fa-file-excel-o"><a download="" href="" id="btnpdf" style="display:none;"></a></i></button>
    <br /><br />
        <div class="panel">
        <br />       
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="table-responsive">
                        <table datatable="ng" class="table table-hover table-striped">
                            <thead>
                                <tr class="text-white" style="background-color:#09385f;">
                                    <th> S.No </th>
                                    <th> AuditRef No </th>
                                    <th> Entity </th>
                                    <th> Audit Name </th>
                                    <th> Audit Type </th>
                                    <th> Audit Department </th>
                                    <th> Priority </th>
                                    <th> Checkpoint Group </th>
                                    <th> Approval Status </th>
                                    <th> Audit Created By</th>
                                    <th> Created Date</th>
                                    <th class="text-center">Action</th>
                                </tr>
                            </thead>
                            <tbody>

                                <tr ng-repeat="data in audit_list ">
                                    <td class="text-center"> {{$index+1}} </td>
                                    <td data-title="'Created Date'">{{data.auditcreation_gid}} </td>
                                    <td data-title="'Created Date'">{{data.entity_name}} </td>
                                    <td data-title="'Projects'">{{data.audit_name}}</td>
                                    <td data-title="'Projects'">{{data.audittype_name}}</td>
                                    <td data-title="'LMS Code'">{{data.auditdepartment_name}}</td>
                                    <td data-title="'Bureau Code'">{{data.auditpriority_name}}</td>
                                    <td data-title="'Bureau Code'">{{data.checkpointgroup_name}}</td>
                                    <td data-title="'Created By'">{{data.approval_status}}</td>
                                    <td data-title="'Created By'">{{data.created_by}}</td>
                                    <td data-title="'Created By'">{{data.created_date}}</td>
                                    <td class="nowrap" data-title="'Action'" style="text-align: center;">
                                        <button title="view" ng-click="view(data.auditcreation_gid);" class="btn bg-blue-500">
                                            <em class="fa fa-eye"></em>
                                        </button>
                                        <button title="Audit Report" ng-click="sampleobservation_report(data.auditcreation_gid);" class="btn btn-success">
                                            <em class="fa fa-file-excel-o"></em>
                                        </button>
                                        <button title="Audit Sample Report" ng-click="sample_report(data.auditcreation_gid);" class="btn btn-pink">
                                            <a download="" href="" id="btnpdf" style="display:none;"></a><em class="fa fa-file-text-o"></em>
                                        </button>
                                        <!--<button title="Audit Observation Report" ng-click="observationreport(data.auditcreation_gid);" class="btn btn-primary">
        <a download="" href="" id="btnpdf" style="display:none;"></a><em class="fa fa-file"></em>
    </button>
    <button title="Audit Query Report" ng-click="query_report(data.auditcreation_gid);" class="btn btn-pink">
        <a download="" href="" id="btnpdf" style="display:none;"></a><em class="fa fa-file-o"></em>
    </button>
    <button title="Audit Sample Report" ng-click="sample_report(data.auditcreation_gid);" class="btn btn-success">
        <a download="" href="" id="btnpdf" style="display:none;"></a><em class="fa fa-file-text-o"></em>
    </button>
    <button title="Audit Report" ng-click="sampleobservation_report(data.auditcreation_gid);" class="btn btn-success pull-right">
        <a download="" href="" id="btnpdf" style="display:none;"></a><em class="fa fa-file-excel-o"></em>
    </button>-->
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <br />
                    </div>
                    </div>
                </div>
            </div>
        </div>
        <br />
    </div>