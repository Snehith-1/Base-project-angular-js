<style>
    .space {
        margin: 10px 0px;
    }
</style>
<h3>
    Tagged Member
</h3>
<div ng-controller="MstAssignedInboundCallViewController as dp">
    <div class="panel">
        <div class="panel-heading bg-primary">
            <h5><i class="fa fa-eye"></i> &nbsp; Tagged Member View </h5>
        </div>
        <div class="modal-body ng-scope">
            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Assigned Details </b></p>
                <hr class="space" />
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Ticket Reference ID</label><br>
                            {{txtticket_refid}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Enquiry Status</label><br>
                            {{txtcallclosure_status}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Assigned Employee</label><br>
                            {{txtassignemployee_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Assigned Date</label><br>
                            {{txtassign_date}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Entity</label><br>
                            {{txtentity_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Source of Contact</label><br>
                            {{txtsourceofcontact_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Call Received Number</label><br>
                            {{txtcallreceivednumber_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Customer Type</label><br>
                            {{txtcustomer_type}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Call Received Date</label><br>
                            {{txtcallreceived_date}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Enquiry Name</label><br>
                            {{txtcaller_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Internal Reference</label><br>
                            {{txtinternalreference_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Office Landline Number</label><br>
                            {{txtoffice_landlineno}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Company</label><br>
                            {{txtcallerassociate_company}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Enquiry Type</label><br>
                            {{txtcalltype_name}}
                        </div>
                                              
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Allocated TAT (days)</label><br>
                            {{txttat_days}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Allocated TAT Date</label><br>
                            {{txttat_date}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>TAT in Hrs</label><br>
                            {{txttat_hours}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Function</label><br>
                            {{txtfunction_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Function Remarks</label><br>
                            {{txtfunction_remarks}}
                        </div>
                    </div>
                </div>
                <br>
            </div>

                    <div class="dd-handle ng-scope">
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-md-12 col-lg-12 col-sm-12">
                                    <label>Requirement/Enquiry Title</label><br>
                                    {{txtrequirement}}
                                </div>
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-md-12 col-lg-12 col-sm-12">
                                    <label>Enquiry Detailed Remarks</label><br>
                                    {{txtenquiry_description}}
                                </div>
                            </div>
                            <br>
                        </div>
                    </div>

                    <div class="dd-handle ng-scope">
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <label>Remarks by Inbound Team</label><br>
                                    {{txtassignclosure_remarks}}
                                </div>
                            </div>
                            <br>
                            <br>
                        </div>
                    </div>
                    <div class="dd-handle ng-scope" ng-show="followupshow">
                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Follow Up Details </b></p>
                        <hr class="space" />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="table-responsive table-bordered">
                                        <div style="max-height:200px;overflow:auto;">
                                            <table class="table">
                                                <thead class="bg-gray text-white">
                                                    <tr width="100%">
                                                        <th scope="col">S.No</th>
                                                        <th scope="col">Follow Up Date</th>
                                                        <th scope="col">Follow Up Time</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in ibcallfollowup_list" width="100%">
                                                        <td title="S.No">{{$index+1}}</td>
                                                        <td title="Follow Up Date">{{data.followup_date}}</td>
                                                        <td title="Follow Up Time">{{data.followup_time}}</td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="dd-handle ng-scope" ng-show="followupshow">
                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;Extend Follow-Up Details </b></p>
                        <hr class="space" />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="table-responsive table-bordered">
                                        <div style="max-height:200px;overflow:auto;">
                                            <table class="table">
                                                <thead class="bg-gray text-white">
                                                    <tr width="100%">
                                                        <!--<th scope="col">S.No</th>-->
                                                        <th scope="col">Follow-Up Date</th>
                                                        <th scope="col">Follow-Up Time</th>
                                                        <th scope="col">Follow-Up By</th>
                                                        <th scope="col">Follow-Up Remarks</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in inboundcallfollowup_list" width="100%">
                                                        <!--<td title="S.No">{{$index+1}}</td>-->
                                                        <td title="Follow Up Date">{{data.extendfollowup_date}}</td>
                                                        <td title="Follow Up Time">{{data.extendfollowup_time}}</td>
                                                        <td title="Follow Up Date">{{data.extendfollowup_by}}</td>
                                                        <td title="Follow Up Time">{{data.extendfollowup_remarks}}</td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--<div class="dd-handle ng-scope" style="background-color: #f9f8f8;" ng-show="txtcallclosure_status == 'Follow Up'">
                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Follow Up Details </b></p>
                <hr class="space" />
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>FollowUp Date </label><br>
                            {{txtfollowup_date}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>FollowUp Time</label><br>
                            {{txtfollowup_time}}
                        </div>
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            <label>FollowUP Remarks</label><br>
                            {{txtfollowup_remarks}}
                        </div>

                    </div>
                </div>
                <br>
            </div>-->
