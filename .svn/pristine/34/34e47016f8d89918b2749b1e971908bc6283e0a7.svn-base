                    <div class="dd-handle ng-scope" ng-show="completedshow">
                        <div>
                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Upload Call Recording </b></p>
                            <hr />
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div ui-scroll-viewport="" style="max-height:200px;overflow-y: auto;" class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="table-responsive table-bordered">
                                                <table class="table">
                                                    <thead class="bg-gray text-white">
                                                        <tr>
                                                            <th>Document Title</th>
                                                            <th>Upload Document Name</th>
                                                            <th>Document Upload By</th>
                                                            <th>Document Upload Date</th>
                                                            <th align="center" scope="col">
                                                                Action&nbsp;&nbsp;&nbsp;
                                                                <a download="" href="" title="Download All" id="btnpdf9" ng-click="downloadallcall(lufilepath,lufilename)">
                                                                    <em class="icon-arrow-down"></em>&nbsp;
                                                                </a>
                                                            </th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in Uploadcallrecordingdocument_list">
                                                            <td>{{data.document_title}}</td>
                                                            <td>{{data.document_name}}</td>
                                                            <td>{{data.created_by}}</td>
                                                            <td>{{data.created_date}}</td>
                                                            <td class="nowrap" data-title="'Action'">

                                                                <!--<button title="Cancel" class="btn btn-danger btn-sm"
                                                                ng-click="rec_cancel(data.ibcallrecordingocupload_gid)">
                                                            <em class="icon icon-trash"></em>
                                                        </button>-->
                                                                <button title="Download" class="btn btn-green btn-sm"
                                                                        ng-click="rec_downloads(data.document_path,data.document_name)"><a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                    <em class="fa fa-download"></em>
                                                                </button>
                                                                <button title="View" class="btn btn-info btn-sm"
                                                                ng-click="documentviewer(data.document_path,data.document_name)"><a download="" href="" style="display:none;"></a>
                                                                <em class="fa fa-file"></em>
                                                               </button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                    <br>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="dd-handle ng-scope" ng-show="completedshow">
                        <div>
                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Upload Call Proof Document </b></p>
                            <hr />
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div ui-scroll-viewport="" style="max-height:200px;overflow-y: auto;" class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="table-responsive table-bordered">
                                                <table class="table">
                                                    <thead class="bg-gray text-white">
                                                        <tr>
                                                            <th>Document Title</th>
                                                            <th>Upload Document Name</th>
                                                            <th>Document Upload By</th>
                                                            <th>Document Upload Date</th>
                                                            <th align="center" scope="col">
                                                                Action&nbsp;&nbsp;&nbsp;
                                                                <a download="" href="" title="Download All" id="btnpdf8" ng-click="downloadallproof(lpfilepath,lpfilename)">
                                                                    <em class="icon-arrow-down"></em>&nbsp;
                                                                </a>
                                                            </th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in Uploadcallproofdocument_list">
                                                            <td>{{data.document_title}}</td>
                                                            <td>{{data.document_name}}</td>
                                                            <td>{{data.created_by}}</td>
                                                            <td>{{data.created_date}}</td>
                                                            <td class="nowrap" data-title="'Action'">

                                                                <!--<button title="Cancel" class="btn btn-danger btn-sm"
                                                                ng-click="recproof_cancel(data.ibcallproofdocupload_gid)">
                                                            <em class="icon icon-trash"></em>
                                                        </button>-->
                                                                <button title="Download" class="btn btn-green btn-sm"
                                                                        ng-click="recproof_downloads(data.document_path,data.document_name)"><a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                    <em class="fa fa-download"></em>
                                                                </button>
                                                                <button title="View" class="btn btn-info btn-sm"
                                                                ng-click="documentviewerproof(data.document_path,data.document_name)"><a download="" href="" style="display:none;"></a>
                                                                <em class="fa fa-file"></em>
                                                               </button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                    <br>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="dd-handle ng-scope" style="background-color: #f9f8f8;" ng-show="completedshow">
                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Completed Details </b></p>
                        <hr class="space" />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-md-3 col-lg-3 col-sm-3">
                                    <label>Completed By</label><br>
                                    {{txtcompleted_by}}
                                </div>
                                <div class="col-md-3 col-lg-3 col-sm-3">
                                    <label>Completed Date</label><br>
                                    {{txtcompleted_date}}
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <label>Completed Remarks</label><br>
                                    {{txtcompleted_remarks}}
                                </div>

                            </div>
                        </div>
                        <br>
                    </div>

                    <div class="dd-handle ng-scope" style="background-color: #f9f8f8;" ng-show="closedshow">
                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Closed Details </b></p>
                        <hr class="space" />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-md-3 col-lg-3 col-sm-3">
                                    <label>Closed By</label><br>
                                    {{txtclosed_by}}
                                </div>
                                <div class="col-md-3 col-lg-3 col-sm-3">
                                    <label>Closed Date</label><br>
                                    {{txtclosed_date}}
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <label>Closed Remarks</label><br>
                                    {{txtclosed_remarks}}
                                </div>

                            </div>
                        </div>
                        <br>
                    </div>
                    <div class="dd-handle ng-scope" style="background-color: #f9f8f8;" ng-show="RejectedShow">
                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Rejected Details </b></p>
                        <hr class="space" />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-md-3 col-lg-3 col-sm-3">
                                    <label>Rejected By</label><br>
                                    {{txtrejected_by}}
                                </div>
                                <div class="col-md-3 col-lg-3 col-sm-3">
                                    <label>Rejected Date</label><br>
                                    {{txtrejected_date}}
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <label>Rejected Remarks</label><br>
                                    {{txtrejected_remarks}}
                                </div>

                            </div>
                        </div>
                        <br>
                    </div>

                </div>
                <br>

                <div class="row">
                    <div class="text-center">
                        <button class="text-center btn btn-info" ng-click="Back()"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back&nbsp;&nbsp;</button>
                    </div>
                </div>
                <br><br>
            </div>

</div>