        <script type="text/ng-template" id="/initiatetransfer.html">
            <div class="modal-header bg-green-200 ng-scope">

                <h4 id="myModalLabel" class="modal-title">
                    &emsp;
                    <span class="btn-label bg-green-200">
                        <i class="fa fa-share-square-o"></i>
                    </span>Initiate Transfer
                </h4>
            </div>
            <div class="modal-body ng-scope">
                <form novalidate="" name="userformsubmit" ng-submit="form.submitForm1()" autocomplete="off" class="form-validate form-horizontal">
                    <div class="pd-sm-10">
                        <div class="row">
                            <div class="col-sm-12 col-md-12 col-lg-12">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <label>Assigned To </label> &nbsp;<span style="color:red">*</span>
                                    <select name="cboassign_to" id="cboassign_to" chosen="" ng-options="m.employee_gid as m.employee_name for m in employee_list"
                                            ng-model="cboassign_to" class="chosen-select form-control" required></select>
                                </div>
                            </div>
                        </div>

                    </div>
                    <br />

                    <div>
                        <div class="text-center">
                            <button type="button" class="btn btn-success btn-md" ng-disabled="userformsubmit.$invalid" ng-click="transfer_submit(bankalert2allocated_gid)"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                            <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                        </div>
                    </div>
                </form>
            </div>
        </script>

        <script type="text/ng-template" id="/rmtransfer.html">
