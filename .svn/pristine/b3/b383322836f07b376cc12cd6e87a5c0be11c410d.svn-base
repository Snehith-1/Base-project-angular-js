<h3>
    Release Management
</h3>
<div ng-controller="ReadytoRelease as dp">
    <div id="panel-department" class="panel_test alert1">
        <div id="panelDemo8" class="panel panel-primary">
            <div class="panel-heading">
                <em class="fa fa-cloud-upload"></em>&emsp; Create Release Plan
            </div>
            <div class="panel-body">
                <div class="table-responsive">
                    <div class="panel-body">
                        <form name="Releaseform" novalidate="" class="form-validate mb-lg" autocomplete="off">
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-3 col-sm-6 col-md-6">
                                        <label>Target Release Date *</label>
                                        <br />
                                        <p class="input-group">
                                            <input type="text" datepicker-popup="{{dp.format}}" ng-model="release_date" is-open="dp.open1"
                                                   datepicker-options="dp.dateOptions" ng-required="true"
                                                   close-text="Close" class="form-control" />
                                            <span class="input-group-btn">
                                                <button type="button" ng-click="dp.calender1($event)" class="btn btn-default">
                                                    <em class="fa fa-calendar"></em>
                                                </button>
                                            </span>
                                        </p>
                                        <!--<input type="date" id="release_date" name="release_date" ng-model="release_date" required>-->
                                    </div>
                                    <div class="col-lg-4 col-sm-6 col-md-6">
                                        <label>Release Status *</label>
                                        <select name="releaseStatus" ng-model="releaseStatus" class="form-control" size="1" required>
                                            <option value="">----- Select Status ----</option>
                                            <option value="Release – Plan">Release - Plan</option>
                                            <option value="Release – CR">Release - CR</option>
                                            <option value="Release – Scheduled">Release - Scheduled</option>
                                        </select>
                                    </div>
                                    <div class="col-lg-1 col-sm-1 col-md-1">

                                    </div>
                                    <div class="col-lg-4 col-sm-6 col-md-6">
                                        <label>Done by *</label>
                                        <input type="text" ng-model="DoneBy" name="DoneBy" class="form-control" maxlength="256" required />
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <label>Release Remarks *</label>
                                        <textarea class="form-control" rows="4" name="ReleaseRemarks" ng-model="ReleaseRemarks" required></textarea>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="card-header">
                                            <strong class="w3-text-pink">
                                                <em class="fa fa-star fa-lg"></em>&emsp;CHANGE DETAILS
                                            </strong>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="tab-content">
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-6 col-sm-6 col-md-6">
                                                            <label style="font-weight:bold">Change Description</label>
                                                            <textarea class="form-control" rows="4" name="changedescription" ng-model="change_description" required></textarea>
                                                        </div>
                                                        <div class="col-lg-6 col-sm-6 col-md-6">
                                                            <label style="font-weight:bold">Impacted Module</label>
                                                            <textarea class="form-control" rows="4" name="impactedmodule" ng-model="impacted_module" required></textarea>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-6 col-sm-6 col-md-6">
                                                            <label style="font-weight:bold">Impacted System</label>
                                                            <textarea class="form-control" rows="4" name="impactedsystem" ng-model="impacted_system" required></textarea>
                                                        </div>
                                                        <div class="col-lg-6 col-sm-6 col-md-6">
                                                            <label style="font-weight:bold">Reasons for Change (also, specify whether'necessary' or 'desirable'</label>
                                                            <textarea class="form-control" rows="4" name="reasonsforchange" ng-model="reasonsfor_change" required></textarea>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-6 col-sm-6 col-md-6">
                                                            <label style="font-weight:bold">Alternative ways (if any) of achieving the same results	</label>
                                                            <textarea class="form-control" rows="4" name="alternativeway" ng-model="alternative_way" required></textarea>
                                                        </div>
                                                        <div class="col-lg-6 col-sm-6 col-md-6">
                                                            <label style="font-weight:bold">Resources (additional /reduced effort required to implement the change)	</label>
                                                            <textarea class="form-control" rows="4" name="resources" ng-model="resources" required></textarea>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="card-header">
                                            <strong class="w3-text-indigo">
                                                <em class="fa fa-file fa-lg"></em>&emsp; UAT SIGN OFF DOCUMENT
                                            </strong>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="tab-content">
                                                <br />
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-2 col-md-2 col-sm-2">
                                                            <label class="col-form-label"><strong>File Upload</strong></label>
                                                        </div>

                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <input type="file" id="addupload" ng-model="test_document" onchange="angular.element(this).scope().upload(this.files, angular.element(this).scope().documentupload, this.name)"
                                                                   data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                        <div class="table-responsive">
                                                            <table class="table table-striped table-secondary">
                                                                <thead class="bg-blue text-white">
                                                                    <tr width="100%">
                                                                        <th scope="col">S.No</th>
                                                                        <th scope="col">Document Name</th>
                                                                        <th scope="col">Action</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr ng-repeat="data in filename_list" width="100%">
                                                                        <td title="'S.No'">{{$index+1}}</td>
                                                                        <td title="'Document Name'">{{data.filename}}</td>
                                                                        <td data-title="'Action'">
                                                                            <a class="btn btn-xs btn-danger" ng-click="document_cancelclick(data.id,data)"><span class="fa fa-close"></span>Cancel</a>
                                                                        </td>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="card-header">
                                            <strong class="w3-text-red">
                                                <em class="fa fa-tag fa-lg"></em>&emsp;  TAGGED ISSUE
                                            </strong>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="tab-content">
                                                <div class="table-responsive">
                                                    <table ng-table="IssueTable" id="tabletest" class="table table-striped table-secondary">
                                                        <thead class="bg-blue text-white">
                                                            <tr width="100%">
                                                                <th><input type="checkbox" id="c2" ng-model="all" ng-change="checkall(all);"> </th>
                                                                <th scope="col">S.No</th>
                                                                <th scope="col">Issue Ref no</th>
                                                                <th scope="col">Issue Date</th>
                                                                <th scope="col">Issue Title</th>
                                                                <th scope="col">Issue Type</th>
                                                                <th scope="col">Priority</th>
                                                                <th scope="col">Issue Status</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in tabledata" width="100%">

                                                                <td>
                                                                    <input type="checkbox" id="c1"
                                                                           value={{data.checked}} ng-model="data.checked">
                                                                </td>
                                                                <td title="'S.No'">{{$index+1}}</td>
                                                                <td title="'Issue Ref no">{{data.issue_refno}}</td>
                                                                <td title="'Issue Date'">{{data.issue_date}}</td>
                                                                <td data-title="'Issue Title'">{{data.issue_title}}</td>
                                                                <td data-title="'Issue Type'">{{data.issue_type}}</td>
                                                                <td data-title="'Priority'">{{data.priority}}</td>
                                                                <td data-title="'Issue Status'">{{data.issue_status}}</td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="panel-footer w3-center">
                                <a class="btn btn-success" ng-click="MakeRelease()" ng-disabled="Releaseform.$invalid">
                                    <i class="fa fa-check" style="font-size:15px;">
                                        &nbsp;Submit
                                    </i>
                                </a>
                                <a class="btn btn-info" ng-click="back()">
                                    <i class="fa fa-arrow-left" style="font-size:15px;">
                                        &nbsp;Back
                                    </i>
                                </a>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="popupreleasedetails" class="w3-modal">

        <div class="w3-animate-zoom panel-info w3-modal-content w3-round-large" style="width:400px;">
            <div class="text-center w3-light-green text-uppercase">
