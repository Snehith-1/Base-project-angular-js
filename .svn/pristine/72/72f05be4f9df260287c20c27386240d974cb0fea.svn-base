<style>
    .space {
        margin: 10px 0px;
    }
</style>
<h3>
    Group Details
</h3>
<div ng-controller="MstCADGroupDtlAddController as dp">
    <div class="panel">
        <div class="panel-heading" style="background-color:rgb(19,14,89);background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);color:#ffffff">
            <h5> Add Group Details </h5>
        </div>
        <div class="panel-body">
                <div class="row">
                    <form name="userform" ng-submit="form.submitLoan()" novalidate="" autocomplete="off" class="form-validate form-horizontal">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                                <div class="row">
                                    <div class="col-md-12 col-lg-12 col-sm-12">
                                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Group Information </b></p>
                                        <hr />
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Group Name <span style="color:red"> *</span></label>
                                            <br />
                                            <input type="text" ng-model="txtgroup_name" tabindex="0" class="form-control" required ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" maxlength="50" />
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Date of Formation </label>
                                            <br />
                                            <p class="input-group">
                                                <input type="text" id="dateofformation" name="date" ng-model="txtdate_of_formation" datepicker-popup="{{dp.format}}"
                                                       style="height:36px;" is-open="dp.open1" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                                       close-text="Close" class="form-control" placeholder="DD-MM-YYYY" ng-change="futuredatecheck(txtdate_of_formation)" />
                                                <span class="input-group-btn">
                                                    <button type="button" ng-click="dp.calender1($event)" style="height:36px;" class="btn btn-default">
                                                        <em class="fa fa-calendar"></em>
                                                    </button>
                                                </span>
                                            </p>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Group Type <span style="color:red"> *</span></label>
                                            <br />
                                            <select name="GroupType" id="GroupType"
                                                    ng-model="cboGroupType" class="chosen-select form-control" required>
                                                <option value=""></option>
                                                <option value="SHG"> SHG </option>
                                                <option value="JLG"> JLG </option>
                                            </select>
                                        </div>

                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-12 col-lg-12 col-sm-12">
                                        <br />                                       
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Having URN? <span style="color:red"> *</span></label>
                                            <div class="radio c-radio">
                                                <label>
                                                    <input type="radio" id="URN" name="URN" value="Yes" ng-model="rdbgroupurn_status" ng-change="onselectedurn_yes()" required />
                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                </label>
                                                <label>
                                                    <input type="radio" id="URN" name="URN" value="No" ng-model="rdbgroupurn_status" ng-change="onselectedurn_yes()" required />
                                                    <span class="fa fa-circle"></span>No
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4" ng-show="URN_yes">
                                            <label class="col-form-label"> URN <span style="color:red"> *</span></label>
                                            <br />
                                            <input type="text" ng-model="txtgroup_urn" tabindex="0" class="form-control" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" maxlength="15" />
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Member Count of the Group <span style="color:red"> *</span></label>
                                            <br />
                                            <input type="text" ng-model="txtgroupmember_count" tabindex="0" class="form-control" required ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" maxlength="5" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12 col-lg-12 col-sm-12">
                                        <br />
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Count of males </label>
                                            <br />
                                            <input type="text" ng-model="txtmale_count" tabindex="0" class="form-control" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" maxlength="10" />
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Count of females </label>
                                            <br />
                                            <input type="text" ng-model="txtfemale_count" tabindex="0" class="form-control" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" maxlength="10" />
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Internal Rating </label>
                                            <br />
                                            <select name="internalrating_name" ng-options="m as m.internalrating_name for m in internalrating_list track by m.internalrating_gid"
                                                    ng-model="cbointernalrating" class="chosen-select form-control" chosen="" style="height:37px;"
                                                    size="1">
                                                <option value=""></option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="dd-handle ng-scope">
                                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Group Document(s) </b></p>
                                        <hr />
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label class="col-form-label"> Document Title <span style="color:red"> *</span></label>
                                                    <br />
                                                    <select name="groupdocument_name" ng-options="m as m.groupdocument_name for m in groupdoc_list track by m.groupdocument_gid"
                                                            ng-model="cbogroupdocumentname" class="chosen-select form-control" chosen="" style="height:37px;"
                                                            size="1">
                                                        <option value=""></option>
                                                    </select>
                                                    <!--<input type="text" ng-model="txtgroupdocument_title" tabindex="0" class="form-control" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" maxlength="50" />-->

                                                </div>
                                                <div class="col-lg-6 col-sm-6 col-md-6">
                                                    <div class="float-label">
                                                        <strong>Upload Document</strong>
                                                        <br /><br />
                                                        <input type="file" id="fileGroupDocument" tabindex="5" multiple onchange="angular.element(this).scope().groupdocument_upload(this.files, angular.element(this).scope().documentupload, this.name)"
                                                               ng-model="txtgroup_document" data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <br />
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <br />
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-secondary">
                                                            <thead class="bg-gray text-white">
                                                                <tr width="100%">
                                                                    <th scope="col">S.No</th>
                                                                    <th scope="col">Document Title</th>
                                                                    <th scope="col">Document Name</th>
                                                                    <th align="center" scope="col">
                                                                        Action&nbsp;&nbsp;&nbsp;
                                                                        <a download="" href="" title="Download All" id="btnpdf3" ng-click="downloadall()">
                                                                            <em class="icon-arrow-down"></em>&nbsp;
                                                                        </a>
                                                                    </th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in groupdocument_list">
                                                                    <td title="S.No">{{$index+1}}</td>
                                                                    <td title="Document Title">{{data.document_title}}</td>
                                                                    <td>{{data.document_name}}</td>
                                                                    <td data-title="Action">
                                                                        <button title="Download" class="btn btn-green btn-sm"
                                                                                ng-click="downloads(data.document_path,data.document_name)">
                                                                            <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                            <em class="fa fa-download"></em>
                                                                        </button>
                                                                        <button title="View" class="btn btn-info btn-sm" ng-click="documentviewer(data.document_path,data.document_name)">
                                                                            <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                                        </button>
                                                                        <a title="Delete" class="btn btn-sm btn-danger" ng-click="groupdocument_delete(data.group2document_gid)">
                                                                            <span class="fa fa-trash-o"></span>
                                                                        </a>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <br />
                                    </div>
                                </div>
                            </div>
                            <div class="dd-handle ng-scope">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Address Details </b></p>
                                <hr />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-sm-4 col-md-4 col-lg-4">

                                            <strong class="ng-binding w3-text-indigo">
                                                <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="groupaddress_add()"><span class="fa fa-plus" />  Add Address</a>
                                            </strong>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="table-responsive table-bordered">
                                                <div style="max-height:200px;overflow:auto;">
                                                    <table class="table">
                                                        <thead class="bg-gray text-white">
                                                            <tr width="100%">
                                                                <th scope="col">S.No</th>
                                                                <th scope="col">Address Type</th>
                                                                <th scope="col">Address</th>
                                                                <th> Primary Status</th>
                                                                <th> Static Map / Photos</th>
                                                                <th scope="col">Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in mstaddress_list">
                                                                <td title="S.No">{{$index+1}}</td>
                                                                <td title="AddressType">{{data.address_type}}</td>
                                                                <td title="Address">
                                                                    {{data.addressline1}},{{data.addressline2}},<br />{{data.taluka}},{{data.district}},
                                                                    <br />{{data.state}},{{data.country}},{{data.postal_code}}
                                                                    <br /> <b>Latitude:</b> {{data.latitude}}
                                                                    <br /> <b>Longitude:</b> {{data.longitude}}
                                                                </td>
                                                                <td title="PrimaryStatus">{{data.primary_status}}</td>
                                                                <td title="Static Map and Photos">
                                                                    <button title="View" ng-click="StaticMapAndPhotos_View(data.latitude, data.longitude, data.addressline1, data.addressline2, data.postal_code)" class="btn btn-xs btn-info">
                                                                        <em class="fa fa-eye"></em>
                                                                    </button>
                                                                </td>
                                                                <td data-title="Action">
                                                                    <a class="btn btn-xs btn-danger" ng-click="groupaddress_delete(data.group2address_gid)"><span class="fa fa-trash-o"></span></a>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />


                            </div>
                            <div class="dd-handle ng-scope">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;&nbsp;&nbsp; Equipment Holding Details </b></p>
                                <hr />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-sm-4 col-md-4 col-lg-4">
                                            <strong class="ng-binding w3-text-indigo">
                                                <button class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" title="Add Equipment Holding"
                                                        ng-click="addequipment_holding()">
                                                    <span class="fa fa-plus" />  Add Equipment Holding
                                                </button>
                                            </strong>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="table-responsive table-bordered">
                                                <table class="table">
                                                    <thead class="bg-gray text-white">
                                                        <tr width="100%">
                                                            <th scope="col">S.No</th>
                                                            <th scope="col">Equiqment Name</th>
                                                            <th scope="col">Available for Rent / Hire</th>
                                                            <th> Insurance Status</th>
                                                            <th scope="col">Quantity / Description / Insurance Details </th>
                                                            <th scope="col">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in mstequipmentholding_list" width="100%">
                                                            <td title="S.No"> {{$index+1}} </td>
                                                            <td title="Equiqment name">{{data.equipment_name}}</td>
                                                            <td title="Available for Rent / Hire">{{data.availablerenthire}}</td>
                                                            <td title="Insurance status">{{data.insurance_status}}</td>
                                                            <td title="Quantity / Description / Insurance Details">
                                                                <button title="View" ng-click="equipment_View(data.group2equipment_gid)" class="btn btn-xs btn-info">
                                                                    <em class="fa fa-eye"></em>
                                                                </button>
                                                            </td>
                                                            <td data-title="Action">
                                                                <button class="btn btn-xs btn-danger" ng-click="deleteequipment_holding(data.group2equipment_gid)">
                                                                    <span class="fa fa-trash-o"></span>
                                                                </button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;&nbsp;&nbsp; Livestock Holdings Details </b></p>
                                <hr />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-sm-4 col-md-4 col-lg-4">
                                            <strong class="ng-binding w3-text-indigo">
                                                <button class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" title="Add Livestock Holdings"
                                                        ng-click="addlivestock_holding()">
                                                    <span class="fa fa-plus" />  Add Livestock Holdings
                                                </button>
                                            </strong>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="table-responsive table-bordered">
                                                <table class="table">
                                                    <thead class="bg-gray text-white">
                                                        <tr width="100%">
                                                            <th scope="col">S.No</th>
                                                            <th scope="col">Livestock Name</th>
                                                            <th scope="col">Count</th>
                                                            <th> Insurance Status</th>
                                                            <th scope="col">Breed / Insurance Details </th>
                                                            <th scope="col">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in mstlivestockholding_list" width="100%">
                                                            <td title="S.No"> {{$index+1}} </td>
                                                            <td title="Livestock name">{{data.livestock_name}}</td>
                                                            <td title="Count">{{data.count}}</td>
                                                            <td title="Insurance status">{{data.insurance_status}}</td>
                                                            <td title="Breed / Insurance Details">
                                                                <button title="View" ng-click="livestock_View(data.group2livestock_gid)" class="btn btn-xs btn-info">
                                                                    <em class="fa fa-eye"></em>
                                                                </button>
                                                            </td>
                                                            <td data-title="Action">
                                                                <button class="btn btn-xs btn-danger" ng-click="deletelivestock_holding(data.group2livestock_gid)">
                                                                    <span class="fa fa-trash-o"></span>
                                                                </button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                                <div class="row">
                                    <div class="col-md-12 col-lg-12 col-sm-12">
                                        <br />
                                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Bank Details </b></p>
                                        <hr />
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> IFSC Code </label> <span style="color:red">*</span>
                                            <br />
                                            <input type="text" ng-model="txtifsc_code" ng-pattern="/^[A-Z]{4}0[A-Z0-9]{6}$/" maxlength="15" tabindex="0" class="form-control" ng-change="IFSCValidation()" />
                                            <label class="fa fa-check" style="color:green" ng-show="ifscvalidation"></label>&nbsp;<label style="font-weight:bolder;color:green" ng-show="ifscvalidation">Verified</label>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Bank Account Number </label> <span style="color:red">*</span>
                                            <br />
                                            <input type="text" ng-model="txtbank_accountno" ng-pattern="/^\d{9,15}$/" maxlength="15" tabindex="0" class="form-control" ng-change="BankAccValidation()" />
                                            <label class="fa fa-check" style="color:green" ng-show="bankaccvalidation"></label>&nbsp;<label style="font-weight:bolder;color:green" ng-show="bankaccvalidation">Verified</label>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Account Holder Name </label> <span style="color:red">*</span>
                                            <br />
                                            <input type="text" ng-model="txtaccountholder_name" tabindex="0" class="form-control" disabled ng-show="bankaccvalidation" />
                                            <input type="text" ng-model="txtaccountholder_name" tabindex="0" class="form-control" ng-hide="bankaccvalidation" />
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-md-12 col-lg-12 col-sm-12">

                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label">Bank Name </label> <span style="color:red">*</span>
                                            <br />
                                            <input type="text" ng-model="txtbank_name" tabindex="0" class="form-control" disabled ng-show="ifscvalidation" />
                                            <input type="text" ng-model="txtbank_name" tabindex="0" class="form-control" ng-hide="ifscvalidation" />
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Branch Name </label> <span style="color:red">*</span>
                                            <br />
                                            <input type="text" ng-model="txtbank_branch" tabindex="0" class="form-control" disabled ng-show="ifscvalidation" />
                                            <input type="text" ng-model="txtbank_branch" tabindex="0" class="form-control" ng-hide="ifscvalidation" />
                                        </div>
                                        <div class="col-lg-2 col-md-2 col-sm-2">
                                            <br />
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <a class="btn btn-sm btn-deafult" style="background-color:#09385f;color:white;" ng-click="bank_add()"><span class="fa fa-plus" /> Add </a>
                                                </div>
                                            </div>
                                            <div class="row" ng-show="mandatoryfields">
                                                <div class="col-sm-12 col-md-12 col-lg-12 text-left">
                                                    <label style="color:red;"> Enter All Mandatory Values</label>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                        <br />
                                    </div>
                                </div>
                                <br /><br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="table-responsive table-bordered">
                                                <table class="table">
                                                    <thead class="bg-gray text-white">
                                                        <tr width="100%">
                                                            <th scope="col">S.No</th>
                                                            <th>IFSC Code</th>
                                                            <th>Bank Account Number</th>
                                                            <th>Account Holder Name</th>
                                                            <th>Bank Name</th>
                                                            <th>Bank Branch</th>
                                                            <th scope="col">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in mstbank_list" width="100%">
                                                            <td title="S.No">{{$index+1}}</td>
                                                            <td title="IFSC Code">{{data.ifsc_code}}</td>
                                                            <td title="Bank Account Number">{{data.bank_accountno}}</td>
                                                            <td title="Account Holder Name">{{data.accountholder_name}}</td>
                                                            <td title="Bank Name">{{data.bank_name}}</td>
                                                            <td title="Bank Branch">{{data.bank_branch}}</td>
                                                            <td data-title="Action">
                                                                <a class="btn btn-xs btn-danger" ng-click="bank_delete(data.group2bank_gid)"><span class="fa fa-trash-o"></span></a>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br /><br />
                            </div>

                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <br />
                                    <div class="text-center">
                                        <button type="button" class="btn btn-success" ng-disabled="userform.$invalid"
                                                ng-click="group_submit()">
                                            <i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit
                                        </button>
                                        <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </form>

                </div>
            </div>
        <br />
    </div>
    <script type="text/ng-template" id="/groupaddresstype.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-plus"></em>&nbsp;<label style="font-size:16px">Add Address</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">

                <form novalidate="" name="userform1" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-9 col-md-9 col-sm-9">
                                <label><strong>Address Type </strong> <span style="color:red">*</span></label>
