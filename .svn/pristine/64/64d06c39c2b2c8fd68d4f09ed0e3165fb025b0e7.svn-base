<h3>
    Buyer / Supplier Approval
    <a href="#/app/AgrMstCustomerApprovalSummary">
        <button type="button" title="Back to Customer Approval" class="pull-right btn btn-sm btn-default">
            <em class="fa fa-arrow-circle-left text-info fa-1x"></em>
        </button>
    </a>
</h3>
<div ng-controller="AgrMstOnboardingApprovalCompletedController">
    <div class="card">
        <md-content class="md-padding">
            <md-tabs md-dynamic-height="" md-border-bottom="">
                <md-tab md-on-select="GetBuyeronboardingApprovedlist()">
                    <md-tab-label>
                        Buyer Onboarding - Approved
                        &emsp;<i class='pull-right label label-primary'>{{approvalcount.BuyerApproved}}</i>
                    </md-tab-label>
                    <md-tab-body>
                        <md-content class="md-padding">

                            <div class="">
                                <div style="display: flex;">
                                    <div class="col-lg-11 col-md-11 col-sm-11">
                                        <h4 class="text-pink" style="display:table-cell; flex: 10;">
                                            Buyer Onboarding - Approved
                                        </h4>
                                    </div>
                                    <div class="col-lg-1 col-md-1 col-sm-1" style="display:table-cell;padding:0px 10px; position: relative; float:right;">
                                        <button type="button" class="btn btn-success" ng-click="BuyerOnboardExport()" title="Export Excel"><i class="fa fa-file-excel-o"></i></button>
                                        <br /><label style="background-color:white;left:90%;align-items:center;"><a download="" href="" id="btnpdf" style="display:none;"></a> Export Excel </label>
                                    </div>
                                </div>
                                <div class="panel-body">
                                    <div class="table-responsive">
                                        <table datatable="ng" class="table table-hover table-striped">
                                            <thead class="nowrap">
                                                <tr class="text-white" style="background-color:#09385f;">
                                                    <th> S.No </th>
                                                    <th> Buyer ID </th>
                                                    <th> Virtual Account Number</th>
                                                    <th> Customer/Buyer Name </th>
                                                    <!--<th> Vertical </th>-->
                                                    <th style="width:10%;"> Applicant Type </th>
                                                    <th> Created By </th>
                                                    <!-- <th> Submitted Date </th> -->
                                                    <th> Approved By </th>
                                                    <th> Approved Date </th>
                                                    <th> Tag Status </th>
                                                    <th> Application Initiated</th>
                                                    <th> ERP ID</th>
                                                    <th class="text-center" style="width:14%">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in buyerapplicationlist">
                                                    <td data-title="'S.No'" class="text-center">{{$index+1}}</td>
                                                    <td data-title="'Buyer ID'">  {{data.application_no}} </td>
                                                    <td data-title="'Virtual Account Number'">{{data.virtualaccount_number}}</td>
                                                    <td data-title="'Applicant Name">  {{data.customer_name}} </td>
                                                    <!--<td data-title="'Vertical">  {{data.vertical_name}} </td>-->
                                                    <td data-title="'Applicant Type" style="width:10%;">  {{data.applicant_type}} </td>
                                                    <td data-title="'Submitted By'">	 {{data.created_by}} </td>
                                                    <!-- <td data-title="'Submitted Date'">	 {{data.submittedto_approval}} </td> -->
                                                    <td data-title="'Approved By'">	 {{data.onboard_approvedby}} </td>
                                                    <td data-title="'Approved Date'">
                                                        {{data.onboard_approveddate}}
                                                    </td>
                                                    <td data-title="'Tag status'">
                                                        {{data.lgltag_status}}
                                                    </td>
                                                    <td data-title="'Application Initiated'">
                                                        <p ng-if="data.application_initiateddate!=''">{{data.application_initiateddate}}</p>
                                                        <label ng-if="data.application_initiateddate==''" class="label label-info" style="font-size:11px;">Not Initiated</label>
                                                    </td>
                                                    <td data-title="'ERP ID'">	 {{data.erp_id}} </td>
                                                    <td class="nowrap" data-title="'Action'" style="text-align: center;width:14%;" ng-switch="data.application_status">
                                                        <button title="View" ng-click="applcreation_view(data.application_gid)" class="btn btn-info">
                                                            <em class="fa fa-eye"></em>
                                                        </button>
                                                        <button title="RM Transfer" ng-click="transferRM(data.application_gid,data.application_no,data.customer_name)" class="btn btn-success">
                                                            <em class="fa fa-exchange"></em>
                                                        </button>
                                                        <button title="Edit" ng-click="applcreation_edit(data.application_gid)" class="btn btn-green">
                                                            <em class="icon-pencil"></em>
                                                        </button>
                                                        <button title="Limit Management" ng-click="onboardappdetailinfo(data.application_gid)" class="btn bg-pink-400">
                                                            <em class="fa fa-retweet text-white"></em>
                                                        </button>
                                                        <button title="Tag" ng-click="lgl_tag(data.application_gid,data.application_no,data.customer_name)" class="btn btn-purple">
                                                            <em class="fa fa-tag"></em>
                                                        </button>
                                                        <button title="Post To ERP" ng-click="posttoerp_buyer(data.application_gid)" class="btn btn-pink" ng-disabled="data.erp_id != ''">
                                                            <em class="fa fa-arrow-circle-right"></em>
                                                        </button>
                                                        <button title="Create Virtual Account" ng-click="create_virtualaccount(data.application_gid,data.application_no,data.virtualaccount_number)" class="btn btn-default" ng-disabled="data.vacreation_status == 'Y'">
                                                            <em class="fa fa-bank"></em>
                                                        </button>

                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                </div>
                            </div>
                        </md-content>
                    </md-tab-body>

                </md-tab>


