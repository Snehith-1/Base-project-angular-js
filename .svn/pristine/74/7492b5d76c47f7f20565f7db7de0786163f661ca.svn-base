
<h3>
    Law Firm Management
</h3>
<div ng-controller="addLawfirmcontroller">
    <div class="panel  panel-primary">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-plus"></i>
                Add Lawfirm
            </span>
        </div>
        <div class="panel-body bg-grey-50">
            <form novalidate="" name="userform" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                <div class="modal-body ng-scope">
                   
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <label>Firm Name <span style="color:red">*</span> </label>
                                <input ng-model="txtfirm_name" required="" maxlength="75" tabindex="0" aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                               
                            </div>
                            <div class="col-lg-1 col-md-1 col-sm-1">

                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <label>Email Address <span style="color:red">*</span></label>
                                <input type="email" ng-model="txtemailaddress" maxlength="40" required tabindex="0" aria-required="true" aria-invalid="true"
                                       class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                               
                            </div>

                        </div>
                    </div>
                    <br/>
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <label>No of Years in business/practice <span style="color:red">*</span></label>
                                <input ng-model="txtexperience" tabindex="0" required="" maxlength="15" aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                
                            </div>
                            <div class="col-lg-1 col-md-1 col-sm-1">

                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                
                                    <label for="input_13">Web Site  </label>
                                    <textarea ng-model="txtwebsite" rows="1" class="form-control "
                                              id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                             
                            </div>

                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                   <label for="input_13">Contact Details <span style="color:red">*</span> </label>
                                    <textarea ng-model="txtcontact_no" rows="1" class="form-control "
                                              id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                              
                            </div>
                            <div class="col-lg-1 col-md-1 col-sm-">

                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                 <label for="input_13">Address </label>
                                    <textarea ng-model="txtaddress1" rows="2" class="form-control "
                                              id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                              
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <label for="input_13">Remarks  </label>
                                <textarea ng-model="txtremarks" rows="5" cols="50" class="form-control"
                                          id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                            </div>
                            </div>
                    </div>
                    <br />
                    <br />
                        <div class="row">
                               
                                        <div class="col-sm-4 col-md-4 col-lg-4">
                                            <br />
                                            <strong class="ng-binding w3-text-indigo">
                                                <a class="btn btn-sm btn-info" ng-click="addlawfirm2member()"><span class="fa fa-plus" />  Add Member</a>
                                            </strong>
                                            &nbsp;&nbsp;
                                            <strong class="ng-binding w3-text-indigo">
                                                <a class="btn btn-sm btn-green" ng-click="taglawyer()"><span class="icon icon-tag" /> &nbsp; Tag Lawyer</a>
                                            </strong>
                                        </div>
                                    </div>
                          

                            <div class="dd-handle ng-scope">

                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="table-responsive">
                                                <table class="table table-striped table-secondary">
                                                    <thead class="bg-gray text-white">
                                                        <tr width="100%">
                                                            <th scope="col">S No</th>
                                                            <th scope="col">Member Name</th>
                                                            <th scope="col">Designation</th>

                                                            <th scope="col">Enrolment No</th>
                                                            <th scope="col">Mobile No</th>
                                                            <th scope="col">Email Address</th>

                                                            <th scope="col">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in member_list" width="100%">
                                                            <td data-title="'S.No'">{{$index+1}}</td>
                                                            <td title="Member Name">{{data.lawyer_name}}</td>
                                                            <td title="Designation">{{data.designation}}</td>
                                                            <td title="Enrolment No">{{data.enrolment_no}}</td>
                                                            <td title="Mobile No">{{data.mobile_no}}</td>
                                                            <td title="Email Address">{{data.email_address}}</td>

                                                            <td data-title="Action" style="text-align:center;">
                                                                <a class="btn btn-xs btn-danger" ng-click="memberdelete(data.lawfirmmember_gid)"><span class="fa fa-close"></span></a>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                    
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <strong class="ng-binding w3-text-indigo">
                                    <em class="fa fa-file text-primary fa-lg"></em>&emsp;
                                    Upload Bank Empaneled Certificate
                                </strong>
                            </div>
                        </div>
                    </div>
                    <div class="dd-handle ng-scope">

                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-5 col-md-5 col-sm-5">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="document_type" tabindex="0" maxlength="45" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                        <label>Document Title </label>
                                    </div>
                                </div>
                                <div class="col-lg-1 col-md-1 col-sm-1">
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                        <strong>Upload Documents</strong>
                                        <input type="file" filestyle="" id="addupload" ng-model="test_document" onchange="angular.element(this).scope().upload(this.files, angular.element(this).scope().documentupload, this.name)"
                                               data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                   
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="table-responsive">
                                        <table class="table table-striped table-secondary">
                                            <thead class="bg-gray text-white">
                                                <tr width="100%">
                                                    <th scope="col">S.No</th>
                                                    <th scope="col">Document Title</th>
                                                    <th scope="col">Document Name</th>
                                                    <th scope="col">Uploaded Details</th>
                                                    <th scope="col">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in filename_list" width="100%">
                                                    <td title="S.No">{{$index+1}}</td>
                                                    <td title="Document Title">{{data.document_type}}</td>
                                                    <td title="Document Name">{{data.filename}}</td>
                                                    <td title="Uploaded Details">{{data.uploaded_by}} - {{data.updated_date}}</td>
                                                    <td data-title="Action">
                                                        <a class="btn btn-xs btn-danger" ng-click="document_cancelclick(data.document_gid)"><span class="fa fa-close"></span>
                                                        </a>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </form>
            <div>
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="userform.$invalid" ng-click="lawfirmSubmit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="lawfirmback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                </div>
            </div>

        </div>
    </div>
    <script type="text/ng-template" id="/addmember.html">
        <div class="modal-header bg-green-200 ng-scope">
            <h4 id="myModalLabel" class="modal-title">
                <span class="btn-label bg-green-200">
                    <em class="fa fa-plus"></em>
                    Add Member
                </span>
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <div class="pd-sm-10">

                <div class="row">
                    <form name="contactdetailsform" id="contactdetailsform" novalidate="" class="form-validate mb-lg" autocomplete="off">
                        <br />
                        <div class="row">
                            <div class="col-sm-12 col-md-12 col-lg-12">
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <div class="mda-form-group">
                                        <input ng-model="txtmember_name" class="mda-form-control ng-pristine ng-valid ng-touched" required>
                                        <label>Member Name <span style="color:red">*</span></label>
                                    </div>
                                    </div>
                                    <div class="col-sm-6 col-md-6 col-lg-6">
                                    <div class="mda-form-group">
                                        
                                        <input ng-model="txtenrolment_no" required=""  type="text"  tabindex="0"
                                               aria-required="true" ng-change="enrolmentno_validation()" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                        <label>Enrolment No <span style="color:red">*</span></label>
                                        <div ng-show="validationMsg">
                                            <label style="color:red; font-size:90%;"> Enrolment No Already Exists</label>
                                        </div>
                                    </div>
                                    <!-- <div class="col-sm-6 col-md-6 col-lg-6">
                                        
                                        <input ng-model="txtenrolment_no" required="" maxlength="68" tabindex="0" aria-required="true" aria-invalid="true"
                                               ng-change="enrolmentno_validation()" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                               
                                            <div ng-show="validationMsg">
                                                <label style="color:red;"> Enrolment No Already Exists</label>
                                            </div>
                                            <label>Enrolment Number <span style="color:red">*</span> </label>
                                    </div>
                                   -->
                                </div>
                            </div>
                                
                            </div>
                        </div>
                      
                        <div class="row">
                            <div class="col-sm-12 col-md-12 col-lg-12">
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <div class="mda-form-group">
                                        

                                        <input ng-model="txtmobileno" required="" minlength="10" type="number" maxlength="12" tabindex="0"
                                               aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                        <label>Mobile No <span style="color:red">*</span></label>
                                    </div>
                                    </div>
                                    <div class="col-sm-6 col-md-6 col-lg-6">
                                        <div class="mda-form-group">
                                          
                                            <input ng-model="txtdesignation" required="" type="text" tabindex="0"
                                                   aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                            <label>Designation<span style="color:red">*</span></label>
                                        </div>
                                        </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12 col-md-12 col-lg-12">
                               
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <div class="mda-form-group">

                                        <input ng-model="txtemailaddress" required="" type="email" tabindex="0" aria-required="true" aria-invalid="true"
                                               class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                        <label>Email Address <span style="color:red">*</span></label>
                                    </div>
                                </div>
                            </div>
                        </div>

                      
                      
                        <div class="class">
                            <br />
                            <div class="text-center">
                                <a class="btn btn-success" ng-click="submitmember()" ng-disabled="contactdetailsform.$invalid">
                                    <em class="fa fa-check"></em>
                                    <span>Submit</span>
                                </a>
                                <a class="btn btn-danger" ng-click="ok()">
                                    <em class="fa fa-close"></em>
                                    <span>Cancel</span>
                                </a>
                            </div>
                        </div>

                    </form>
                </div>

            </div>

        </div>
    </script>

    <script type="text/ng-template" id="/taglawyer.html">
        <div class="modal-header bg-green-200 ng-scope">
            <h4 id="myModalLabel" class="modal-title">
                <span class="btn-label bg-green-200">
                    <em class="fa fa-plus"></em>
                   Tag Lawyer
                </span>
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <div class="pd-sm-10">

                <div class="row">
                    <form name="contactdetailsform" id="contactdetailsform" novalidate="" class="form-validate mb-lg" autocomplete="off">
                        <br />
                        <div class="row">
                            <div class="col-sm-12 col-md-12 col-lg-12">
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                       <label>Select Lawyer <span style="color:red">*</span> </label>

                                        <select name="lawfirm2lawyer"   ng-options="c.lawyer_name for c in lawfirm2lawyer_list"
                                                ng-model="cbolawfirm2lawyer"
                                                class="chosen-select form-control" required>
                                            <option value="">----Select Lawyer----</option>
                                        </select>

                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <label>Designation<span style="color:red">*</span></label>
                                      <input ng-model="txtdesignation" required="" type="text" tabindex="0"
                                               aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                       
                                    </div>
                                </div>
                            </div>
                        <br/>

                        <div class="class">
                            <br />
                            <div class="text-center">
                                <a class="btn btn-success" ng-click="submittag()" ng-disabled="contactdetailsform.$invalid">
                                    <em class="fa fa-check"></em>
                                    <span>Submit</span>
                                </a>
                                <a class="btn btn-danger" ng-click="ok()">
                                    <em class="fa fa-close"></em>
                                    <span>Cancel</span>
                                </a>
                            </div>
                        </div>

                    </form>
                </div>

            </div>

        </div>
    </script>
</div>