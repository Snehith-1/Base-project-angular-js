    <div ng-controller="samformController as Ctrl">
        <div class="panel-body">
            <form name="createsbaform" ng-submit="form.createsbaform()" novalidate="" autocomplete="off" class="form-validate form-horizontal"  >

                    <div class="dd-handle ng-scope">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-3 col-md-3 col-sm-3">
                                    <!--<div class="p-sm-5 card-body">-->
                                    <p style="font-size:22px;" class="gilroy-bold"><b>SBA Registration Portal</b> </p>
                                    <!--<p style="font-size:22px;"><b>&nbsp;&nbsp; SBA Registration Portal </b></p>-->
                                    <p style="font-size:12px;" class="gilroy-bold"> Fields marked in (<span style="color:red;"> * </span>) are mandatory</p>

                                    <!--</div>-->
                                </div>


                            </div>
                        </div>
                        <br />
                        <div class="mt-3 row">
                            <div class="col-lg-12 col-md-12 col-sm-12 ">
                                <!--<div class="col-lg-2 col-md-2 col-sm-2">
                                    <label style="padding-top: 13px;"> SBA Type <span style="color:red;"> * </span></label>
                                    <select name="source_name"
                                            ng-model="sbatype_name" ng-change="sbaType_change(sbatype_name)" class="chosen-select form-control">
                                        <option value="">Select SBA Type</option>
                                        <option value="Individual">Individual</option>
                                        <option value="Entity">Company</option>
                                    </select>
                                </div>-->
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label>SBA Type <span style="color:red">*</span></label><br>
                                    <select name="satype_name" id="satype_name" required chosen="" class="chosen-select form-control"
                                            
                                        ng-options="m.satype_name for m in applicationadd_salist"
                                            ng-model="cbosatype_name" ng-change="sbaType_change(cbosatype_name)">
                                        <option value=""> -----Select SBA Type-----</option>
                                    </select>
                                </div>


                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label style="padding-top: 13px;">PAN number  <span style="color:red;"> * </span></label>
                                    <!-- <input name="firstname" ng-model="txtpannumber" type="text" id="nameid" class="form-control"
                        maxlength="68" required="">-->
                                    <input type="text" ng-model="txtpannumber" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched"
                                           maxlength="10" ng-change="PANValidation()"  aria-invalid="true" required>
                                    <label class="fa fa-check" style="color:green"
                                           ng-show="panvalidationind"></label>&nbsp;<label  style="font-weight:bolder;color:green"
                                                                                           ng-show="panvalidationind">Verified</label>
                                </div>

                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label style="padding-top: 13px;">Aadhaar Number  <span style="color:red;"> * </span></label>
                                    <input name="firstname" ng-model="txtaadhar" required type="text" id="nameid" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched"
                                           maxlength="12" ng-pattern="/^[0-9]*$/"  aria-invalid="true" >
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 ">
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label style="padding-top: 13px;" > SBA First Name <span style="color:red;"> * </span></label>
                                    <input name="entity" ng-model="txtfirstname" type="text" id="entityid" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched "
                                            maxlength="68" required aria-invalid="true" aria-required="true">
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label style="padding-top: 13px;"> SBA Middle Name</label>
                                    <input name="entity" ng-model="txtmiddlename" type="text" id="entityid" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched"
                                            maxlength="68" aria-invalid="true" aria-required="true">
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label style="padding-top: 13px;"> SBA Last Name <span style="color:red;"> * </span></label>
                                    <input name="entity" ng-model="txtlastname" type="text" id="entityid" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched"
                                            maxlength="68" required aria-invalid="true" aria-required="true">
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label class="col-form-label" style="padding-top: 13px;">Date of Birth <span style="color:red"> *</span></label>
                                    <br />
                                    <p class="input-group">
                                        <input type="text" id="dob_date" name="dob_date" required ng-model="cbodob_date" datepicker-popup="{{Ctrl.format}}"
                                               style="height:36px;" is-open="Ctrl.open1" min-date="Ctrl.minDate" datepicker-options="Ctrl.dateOptions"
                                               close-text="Close" class="form-control"  placeholder="DD-MM-YYYY" />
                                        <span class="input-group-btn">
                                            <button type="button" ng-click="Ctrl.calender1($event)" style="height:36px;" class="btn btn-default">
                                                <em class="fa fa-calendar"></em>
                                            </button>
                                        </span>
                                    </p>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label style="padding-top: 13px;">Educational Qualification  <span style="color:red;"> * </span></label>
                                    <!--<select name="educationalqualification" chosen="" ng-options="m.qualification for m in reportingtoList"
                                ng-model="txteducationalqualification" class="chosen-select form-control" required="">
                            <option value=""> Select Educational Qualification </option>
                        </select>-->
                                    <select name="EducationalQualification" id="EducationalQualification" required chosen="" ng-options="m.educationalqualification_gid as m.educationalqualification_name for m in educationalqualification_list"
                                            ng-model="cboEducationalQualification" class="chosen-select form-control" >
                                        <option value=""> </option>
                                    </select>

                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label style="padding-top: 13px;">Father's Name </label>
                                    <input name="firstname" ng-model="txtfathersname" type="text" id="nameid" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched"
                                           maxlength="68" aria-invalid="true" >
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">

                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label>Present Occupation <span style="color:red;"> * </span></label>
                                    <input name="presentoccupation" ng-model="txtpresentoccupation" required type="text" id="nameid" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched"

                                           maxlength="68"  aria-invalid="true" aria-required="true">
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label>Work experience (years)  <span style="color:red;"> * </span></label>

                                    <input name="workexperience" ng-model="txtworkexperience" required type="text" id="nameid" ng-pattern="/^[0-9]*$/" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched"
                                           maxlength="68"  aria-invalid="true">
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label>Exp in Agri business Industry<span style="color:red;"> * </span></label>
                                    <input name="expinagribusiness" ng-model="txtexpinagribusiness" ng-pattern="/^[0-9]*$/" type="text" id="nameid" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched"
                                           maxlength="68" aria-invalid="true" aria-required="true">
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label>Mobile Number <span style="color:red;"> * </span></label>
                                    <input type="text" name="mobilenumber" maxlength="10" ng-pattern="/^[0-9]*$/"
                                           ng-model="txtmobilenumber"  aria-invalid="true" required class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched"> 
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label>Alternative Mobile Number </label>
                                    <input type="text" name="mobilenumber" maxlength="10" ng-pattern="/^[0-9]*$/"
                                           ng-model="txtaltmobilenumber" class="form-control">
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label>Email Address<span style="color:red;"> * </span></label>
                                    <input type="email" name="userpersonalemail" maxlength="64" ng-change="email_check()" ng-pattern="/^[a-zA-Z0-9](\.?[a-zA-Z0-9_-]){0,}@[a-z0-9-]+\.([a-z]{1,6}\.)?[a-z]{2,6}$/"
                                            aria-invalid="true" aria-required="true" ng-model="txtpersonalemail" required class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                    <div ng-messages="sbaform.userpersonalemail.$dirty && sbaform.userpersonalemail.$error">

                                        <p class="text-danger" ng-message="pattern">This field must be a valid email address</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label>Address Line 1 <span style="color:red;"> * </span></label>
                                    <input name="address" ng-model="txtaddressline1" required class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched "
                                            aria-invalid="true" type="text" id="peraddressid1" 
                                           maxlength="225">
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">

                                    <label>Address Line 2 <span style="color:red;"> * </span></label>
                                    <input name="address" ng-model="txtaddressline2"  required class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched"
                                             aria-invalid="true" type="text" id="peraddressid2" 
                                           maxlength="225">

                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label>Country</label>
                                    <select name="country" chosen="" ng-options="m.country_name for m in countryList"
                                            ng-model="cbocountry" class="chosen-select form-control">
                                        <option value="">India</option>
                                    </select>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">

                                    <label>Pin Code <span style="color:red;"> * </span> </label>
                                    <input name="postalcode" ng-pattern="/^\d+$/" ng-model="txtpostalcode" required type="text" id="postalid1" ng-change="onchangepostal_code()" class="form-control"
                                           maxlength="6">
                                    <div ng-messages="sbaform.temppostalcode.$dirty && sbaform.temppostalcode.$error">

                                        <p class="text-danger" ng-message="pattern">This field required only numbers</p>
                                    </div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label>City</label>
                                    <input name="city" ng-model="txtcity" type="text" id="cityid1" disabled class="form-control"
                                           maxlength="68">
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label>State</label>
