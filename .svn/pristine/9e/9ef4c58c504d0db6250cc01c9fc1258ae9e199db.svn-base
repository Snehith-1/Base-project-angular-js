<h3>
    CAD Report - Covenent
</h3>
<style>
    .tabactivecolorstyle {
        background-color: #84cbdb;
    }
</style>
<div ng-controller="MstRptCadCovenantController">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="col-lg-12 col-md-12 col-sm-12">

                <tabset justified="true" class="panel panel-default panel-body">
                    <tab ng-click="scanneddocumentssummary()">
                        <tab-heading>
                            Softcopy Vetting Documents <label style="font-size:11px;" class="label label-info pull-right">{{MakerPendingCount}}</label>
                        </tab-heading>
                        <div class="row">
                            <br />
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-2 col-md-6 col-sm-12">
                                            <button type="button" id="scanmaker" class="btn btn-labeled" ng-click="scanmaker_summary()" style="width:210px;height:50px;border:1px solid rgb(179, 177, 177);">
                                                <div class="pull-right label" style="background-color:#09385f;padding-top:5px;padding-bottom:5px;">  <b>{{overallCountinfo.MakerPendingCount}} </b></div>
                                                <b style="font-size: 14px;"> Maker </b>
                                            </button>
                                            <br />
                                        </div>
                                        <div class="col-lg-2 col-md-6 col-sm-12">
                                            <button type="button" id="scanchecker" class="btn btn-labeled" ng-click="scanchecker_summary()" style="width:210px;height:50px;border:1px solid rgb(179, 177, 177);">
                                                <div class="pull-right label" style="background-color:#09385f;padding-top:5px;padding-bottom:5px;">  <b>{{overallCountinfo.CheckerApprovalPendingCount}} </b></div>
                                                <b style="font-size: 14px;"> Checker </b>
                                            </button>
                                            <br />
                                        </div>
                                        <div class="col-lg-2 col-md-6 col-sm-12">
                                            <button type="button" id="scanapprover" class="btn btn-labeled" ng-click="scanapprover_summary()" style="width:210px;height:50px;border:1px solid rgb(179, 177, 177);">
                                                <div class="pull-right label" style="background-color:#09385f;padding-top:5px;padding-bottom:5px;">  <b>{{overallCountinfo.ApproverPendingCount}} </b></div>
                                                <b style="font-size: 14px;"> Approval </b>
                                            </button>
                                            <br />
                                        </div>
                                        <div class="col-lg-1 col-sm-1 col-md-1 pull-right" style="text-align:right;padding-bottom:10px;">
                                            <button type="button" id="{{lsstatus}}" class="btn btn-success" ng-click="exportsanctionreport(lsstatus)" title="Export Excel"><i class="fa fa-file-excel-o"></i></button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                            <br /><label style="background-color:white;left:90%;"><a download="" href="" id="btnpdf" style="display:none;"></a> Export Excel </label>

                                        </div>
                                        <br />
                                    </div>
                                </div>
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="panel-body">
                                        <div class="table-responsive">
                                            <table datatable="ng" class="table table-hover table-striped">
                                                <thead>
                                                    <tr class="text-white nowrap" style="background-color:#09385f;">
                                                        <th> S.No </th>
                                                        <th> Application Number</th>
                                                        <th> Customer Name </th>
                                                        <th> CC Approved Date </th>
                                                        <!--<th> Sanction Ref.No </th>-->
                                                        <th> CAD Group </th>
                                                        <th> CAD Accepted Date </th>
                                                        <th> CAD Accepted By </th>
                                                        <th ng-show="lsmaker"> Maker Name </th>
                                                        <th ng-show="lschecker"> checker Name </th>
                                                        <th ng-show="lsapproval"> Approval Name </th>
                                                        <!--<th> Maker Name </th>-->
                                                        <th class="text-center">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in scannedmakerpendinglist">
                                                        <td data-title="'S.No'"> {{$index+1}} </td>
                                                        <td data-title="'Application Number'"> {{data.application_no}} </td>
                                                        <td data-title="'Customer Name'"> {{data.customer_name}} </td>
                                                        <td data-title="'AC Approved Date'"> {{data.ccapproved_date}} </td>
                                                        <!--<td data-title="'Sanction Ref.No'"> </td>-->
                                                        <td data-title="'CAD Group'"> {{data.cadgroup_name}} </td>
                                                        <td data-title="'CAD Accepted Date'"> {{data.cadaccepted_date}} </td>
                                                        <td data-title="'CAD Accepted By'"> {{data.cadaccepted_by}} </td>
                                                        <!--<td data-title="'Maker Name'"> {{data.cadaccepted_by}} </td>-->
                                                        <td ng-show="lsmaker" data-title="'Maker Name'"> {{data.maker_name}} </td>
                                                        <td ng-show="lschecker" data-title="'Maker Name'"> {{data.checker_name}} </td>
                                                        <td ng-show="lsapproval" data-title="'Maker Name'"> {{data.approver_name}} </td>
                                                        <td align="center" class="nowrap">
                                                            <button title="View" ng-click="scannedview(data.application_gid)" class="btn btn-info">
                                                                <em class="fa fa-eye"></em>
                                                            </button>
                                                            <button title="Process" ng-click="scanned_process(data.application_gid, data.created_by)" style="background-color:#871490;" class="btn btn-pink">
                                                                <em class="fa fa-spinner"></em>
                                                            </button>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </tab>
                    <tab ng-click="physicaldocumentsSummary()">
                        <tab-heading>
                            Physical Documents <label style="font-size:11px;" class="label label-info pull-right">{{MakerPendingCount}}</label>
                        </tab-heading>
                        <div class="row">
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-2 col-md-12 col-sm-12">
                                        <button type="button" id="phymaker" class="btn btn-labeled" ng-click="phymaker_summary()" style="width:210px;height:50px;border:1px solid rgb(179, 177, 177);">
                                            <div class="pull-right label" style="background-color:#09385f;padding-top:5px;padding-bottom:5px;"> <b> {{overallCountinfo.MakerPendingCount}} </b></div>
                                            <b style="font-size: 14px;"> Maker </b>
                                        </button>
                                        <br />
                                    </div>
                                    <div class="col-lg-2 col-md-12 col-sm-12">
                                        <button type="button" id="phychecker" class="btn btn-labeled" ng-click="phychecker_summary()" style="width:210px;height:50px;border:1px solid rgb(179, 177, 177);">
                                            <div class="pull-right label" style="background-color:#09385f;padding-top:5px;padding-bottom:5px;"> <b> {{overallCountinfo.CheckerApprovalPendingCount}}  </b></div>
                                            <b style="font-size: 14px;"> Checker </b>
                                        </button>
                                        <br />
                                    </div>
                                    <div class="col-lg-2 col-md-12 col-sm-12">
                                        <button type="button" id="phyapprover" class="btn btn-labeled" ng-click="phyapprover_summary()" style="width:210px;height:50px;border:1px solid rgb(179, 177, 177);">
                                            <div class="pull-right label" style="background-color:#09385f;padding-top:5px;padding-bottom:5px;"> <b> {{overallCountinfo.ApproverPendingCount}} </b></div>
                                            <b style="font-size: 14px;"> Approval </b>
                                        </button>
                                        <br />
                                    </div>
                                    <div class="col-lg-1 col-sm-1 col-md-1 pull-right" style="text-align:right;padding-bottom:10px;">
                                        <button type="button" id="{{lsstatus}}" class="btn btn-success" ng-click="exportphysicalreport(lsstatus)" title="Export Excel"><i class="fa fa-file-excel-o"></i></button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                        <br /><label style="background-color:white;left:90%;"><a download="" href="" id="btnpdf" style="display:none;"></a> Export Excel </label>

                                    </div>
                                    <br />
                                </div>
                            </div>
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="panel-body">
                                        <div class="table-responsive">
                                            <table datatable="ng" class="table table-hover table-striped">
                                                <thead>
                                                    <tr class="text-white nowrap" style="background-color:#09385f;">
                                                        <th> S.No </th>
                                                        <th> Application Number</th>
                                                        <th> Customer Name </th>
                                                        <th> CC Approved Date </th>
                                                        <th> Approval Status </th>
                                                        <!--<th> Sanction Ref.No </th>-->
                                                        <th> CAD Group </th>
                                                        <th> CAD Accepted Date </th>
                                                        <th> CAD Accepted By </th>
                                                        <th ng-show="lpmaker"> Maker Name </th>
                                                        <th ng-show="lpchecker"> checker Name </th>
                                                        <th ng-show="lpapproval"> Approval Name </th>
                                                        <th class="text-center">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in physicalmakerpendinglist">
                                                        <td data-title="'S.No'"> {{$index+1}} </td>
                                                        <td data-title="'Application Number'"> {{data.application_no}} </td>
                                                        <td data-title="'Customer Name'"> {{data.customer_name}} </td>
                                                        <td data-title="'AC Approved Date'"> {{data.ccapproved_date}} </td>
                                                        <td data-title="'Approval Status'"> {{data.approval_status}} </td>
                                                        <!--<td data-title="'Sanction Ref.No'"> </td>-->
                                                        <td data-title="'CAD Group'"> {{data.cadgroup_name}} </td>
                                                        <td data-title="'CAD Accepted Date'"> {{data.cadaccepted_date}} </td>
                                                        <td data-title="'CAD Accepted By'"> {{data.cadaccepted_by}} </td>
                                                        <td ng-show="lpmaker" data-title="'Maker Name'"> {{data.maker_name}} </td>
                                                        <td ng-show="lpchecker" data-title="'Maker Name'"> {{data.checker_name}} </td>
                                                        <td ng-show="lpapproval" data-title="'Maker Name'"> {{data.approver_name}} </td>
                                                        <td align="center" class="nowrap">
                                                            <button title="View" ng-click="physicalview(data.application_gid)" class="btn btn-info">
                                                                <em class="fa fa-eye"></em>
                                                            </button>
                                                            <button title="Process" ng-click="physical_process(data.application_gid, data.created_by)" style="background-color:#871490;" class="btn btn-pink">
                                                                <em class="fa fa-spinner"></em>
                                                            </button>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </tab>
                </tabset>

            </div>
        </div>
    </div>

</div>