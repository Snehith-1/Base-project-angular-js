<h3>
    User Registration
</h3>
<div ng-controller="SysMstApiUserRegistrationController">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <button type="button" title="Register User" class="btn btn-info btn-sm" ng-click="adduser()">
                <span class="btn-label">
                    <i class="fa fa-plus"></i>
                </span>Register User
            </button>
        </div>
    </div>
    <br />
    <div class="panel">
        <br />
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="table-responsive">
                        <table datatable="ng" class="table table-hover table-striped">
                            <thead>
                                <tr class="text-white" style="background-color:#09385f;">
                                    <th style="display:none;">S.No</th>
                                    <th>Created Date</th>
                                    <th>User Code</th>
                                    <th>Partner Name</th>
                                    <th>Email ID</th>
                                    <th style="display:none;">Partner Owner</th>
                                    <th>Partner SPOC</th>
                                    <th>Created By</th>
                                    <th>User Status</th>
                                    <th>Web Access Status</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in user_list">
                                    <td data-title="'S.No'" style="display:none;" class="text-center"> {{$index+1}} </td>
                                    <td data-title="'Created Date'" class=""> {{data.created_date}}</td>
                                    <td data-title="'User Code'" class=""> {{data.externaluser_code}} </td>
                                    <td data-title="'System Name'">{{data.externalsystem_name}}</td>
                                    <td data-title="'Email ID'">{{data.email_id}}</td>
                                    <td data-title="'System Owner'" style="display:none;"> {{data.externalsystem_ownername}} </td>
                                    <td style="text-decoration:underline;text-decoration-color:#49afcd;" align="center" scope="col">
                                        <label style="padding-right:25px; font-size: 15px;text-decoration:underline;text-decoration-color:#49afcd;"
                                               ng-click="showPopover(data.user2oneapi_gid,data.externalsystem_name,data.externalsystem_ownername)">
                                            <em class="fa fa-question-circle" style="font-size:24px;align-content:center;"></em>
                                        </label>
                                    </td>
                                    <td data-title="'Created By'" class=""> {{data.created_by}} </td>
                                    <td data-title="'Status'" ng-switch="data.externaluser_status" align="center">
                                        <label class="label label-success" align="center" ng-switch-when='Active' style="font-size:10px;">Active</label>
                                        <label class="label label-warning" align="center" ng-switch-when='Inactive' style="font-size:10px;">InActive</label>
                                    </td>
                                    <td data-title="'Web Access Status'" ng-switch="data.web_active" align="center">
                                        <label class="label label-success" align="center" scope="col" ng-switch-when='Active' style="font-size:10px;">Active</label>
                                        <label class="label label-warning" align="center" scope="col" ng-switch-when='Inactive' style="font-size:10px;">InActive</label>
                                    </td>
                                    <td class="nowrap" data-title="'Action'" style="text-align: center;">
                                        <span ng-switch="data.externaluser_status">
                                            <button title="Edit" ng-click="edituser(data.user2oneapi_gid)" ng-disabled="data.externaluser_status=='Inactive'||data.web_active=='Inactive'" class="btn btn-green">
                                                <em class="fa fa-edit"></em>
                                            </button>
                                            <button title="Status" ng-switch-when='Active' ng-disabled="data.externaluser_status=='Inactive'||data.web_active=='Inactive'" ng-click="Status_update(data.user2oneapi_gid)" class="btn btn-success">
                                                <em class="fa fa-thumbs-up"></em>
                                            </button>
                                            <button title="Status" ng-switch-when='Inactive' ng-click="Status_update(data.user2oneapi_gid)" class="btn btn-warning">
                                                <em class="fa fa-thumbs-down"></em>
                                            </button>
                                            <button type="button" class="btn btn-pink" ng-disabled="data.externaluser_status=='Inactive'||data.web_active=='Inactive'" ng-click="user_resetpwd(data.user2oneapi_gid)" title="Update Password">
                                                <em class="fa fa-lock"></em>
                                            </button>
                                        </span>
                                        <span ng-switch="data.web_active">
                                            <button type="button" class="btn btn-green" ng-disabled="data.web_active=='Inactive'||data.externaluser_status=='Inactive'" ng-switch-when='Active' ng-click="WebAccess_Deactivate(data.user2oneapi_gid)" title="Web-Access">
                                                <em class="fa fa-snowflake-o"></em>
                                            </button>
                                            <button type="button" class="btn btn-warning" ng-switch-when='Inactive' ng-click="WebAccess_Activate(data.user2oneapi_gid)" title="Web-Access">
                                                <em class="fa fa-snowflake-o"></em>
                                            </button>
                                        </span>

                                    </td>
                                </tr>

                            </tbody>

                        </table>
                    </div>
                </div>
            </div>
        </div>
        <br />
    </div>
    <script type="text/ng-template" id="/adduser.html">
        <div class="modal-header bg-primary ng-scope">

            <h4 class="modal-title">
                <span class="btn-label bg-primary">
                    <i class="fa fa-plus "></i>
                </span> Register User
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="Adduser" ng-submit="form.Adduser()" autocomplete="off" class="form-validate form-horizontal">
                <div class="pd-sm-10">
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="row ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="mda-form-group float-label">
                                        <label class="col-form-label"> User Code <span style="color:red"> </span></label>
                                        <br />
                                        <input type="text" ng-model="txtexternal_usercode" tabindex="0" class="form-control" placeholder="Auto Generate" disabled />

                                    </div>
                                </div>
                            </div>

                            <div class="row ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="mda-form-group float-label">
                                        <label class="col-form-label"> Partner Name <span style="color:red"> *</span></label>
                                        <br />
                                        <input ng-model="txtexternalsystem_name" required tabindex="1" maxlength="30" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required" style="">

                                    </div>
                                </div>
                            </div>

                            <div class="row ">
                                <div class="col-sm-12 col-lg-12 col-md-12">
                                    <label>Partner SPOC <span style="color:red">*</span></label>
                                    <select name="txtexternalsystem_ownername" chosen="" multiple="multiple" tabindex="2"
                                            ng-options="m.employee_name for m in employee_list" required=""
                                            ng-model="txtexternalsystem_ownername" class="chosen-select form-control"></select>
                                </div>
                            </div>

                            <div class="row ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="mda-form-group float-label">
                                        <label class="col-form-label"> Email ID <span style="color:red"> *</span></label>
                                        <br />
                                        <input ng-model="txtemail_id" ng-pattern="/^[^\s@]+@[^\s@]+\.[^\s@]{2,}$/" required tabindex="3" maxlength="30" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required" style="">

                                    </div>
                                </div>
                            </div>

                            <div class="row ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="mda-form-group float-label">
                                        <label class="col-form-label"> Password <span style="color:red"> </span></label>
                                        <br />
                                        <input ng-model="txtpassword" tabindex="4" disabled maxlength="30" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required" style="">

                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="Adduser.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
                <br />
            </form>
        </div>
    </script>
    <script type="text/ng-template" id="/showpopupModal.html">

        <div class="panel-body text-center">
            <div class="table-responsive">
                <table class="table hover table-striped row-border">

                    <thead>
                        <tr style="text-align: left;height:40px;">
                            <th style="color:white;background:#7986cb;width:2%;text-align:center;">{{externalsystem_name}}</th>
                        </tr>
                    </thead>
                    <tbody>

                        <tr>

                            <td style="width:2%; text-align:left"><b>System Owner Name: </b> <br /></td>

                        </tr>
                        <tr>


                            <td data-title="'Member Name'" style="width:2%; text-align:left"> {{externalsystem_ownername}}</td>
                        </tr>

                    </tbody>

                </table>
            </div>
            <div>
                <div class="text-center">

                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close">&nbsp;&nbsp;</i>Close</button>
                </div>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="/statususer.html">
        <div class="modal-header bg-primary ng-scope">
            <h4>
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-thumbs-up"></i>
                </span>Status
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="statususer" ng-submit="form.statususer()" autocomplete="off" class="form-validate form-horizontal">
                <div class="row ">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="row ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label> System Name</label>
                            </div>
                            <div class="col-lg-8 col-md-8 col-sm-8">
                                {{externalsystem_name }}
                            </div>
                        </div>
                        <br />
                        <div class="row ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Status  </label>
                            </div>
                            <div class="col-lg-8 col-md-8 col-sm-8">
                                <div class="radio c-radio">
                                    <label>
                                        <input type="radio" name="rbo_status" value="Y" ng-model="rbo_status" ng-click="status_yes()" />
                                        &nbsp;&nbsp;<span class="fa fa-circle"></span> Active
                                    </label>
                                    <label>
                                        <input type="radio" name="rbo_status" value="N" ng-model="rbo_status" ng-click="status_no()" />
                                        <span class="fa fa-circle"></span> Inactive
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="row ">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="mda-form-group float-label">
                                    <textarea ng-model="txtremarks" style="resize:vertical;" required rows="4" aria-multiline="true" tabindex="0" aria-invalid="false" class="mda-form-control"></textarea>
                                    <label>Remarks<span style="color:red"> *</span></label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="statususer.$invalid" ng-click="update_status()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
                <br />
                <div class="table-responsive" ng-show="userinactivelog_list!=null">
                    <div style="max-height:300px;overflow:auto;">
                        <table class="table table-striped table-secondary">
                            <thead class="bg-gray text-white">
                                <tr width="100%">
                                    <th align="center" scope="col">S.No</th>
                                    <th align="center" scope="col">Status</th>
                                    <th align="center" scope="col">Updated By</th>
                                    <th align="center" scope="col">Updated Date</th>
                                    <th align="center" scope="col">Remarks</th>

                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in userinactivelog_list" width="100%">
                                    <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                    <td align="left" scope="col" title="'Status'">{{data.status}}</td>
                                    <td align="left" scope="col" title="'Updated By'">{{data.updated_by}}</td>
                                    <td align="left" scope="col" title="'Updated Date'">{{data.updated_date}}</td>
                                    <td align="left" scope="col" title="'Remarks'">{{data.remarks}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </form>
        </div>
    </script>
    <script type="text/ng-template" id="/edituser.html">
        <div class="modal-header bg-primary ng-scope">

            <h4>
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-edit "></i>
                </span>Edit User Details
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="edituser" ng-submit="form.edituser()" autocomplete="off" class="form-validate form-horizontal">

                <div class="row ">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="row ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div class="mda-form-group float-label">
                                    <label class="col-form-label"> User Code <span style="color:red"> </span></label>
                                    <br />
                                    <input type="text" ng-model="txtexternal_usercode" tabindex="0" class="form-control" placeholder="Auto Generate" disabled />

                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row ">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="mda-form-group">
                                    <input ng-model="txtexternalsystem_name" required maxlength="255" tabindex="1" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                    <label>System Name <span style="color:red">*</span></label>
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row ">
                            <div class="col-sm-12 col-lg-12 col-md-12">
                                <label>System Owner <span style="color:red">*</span></label>
                                <select name="systemasssigned_edit" chosen="" multiple="multiple" tabindex="2"
                                        ng-options="m.employee_name for m in cboemployee_editlist" required=""
                                        ng-model="cboemployee_edit" class="chosen-select form-control"></select>
                            </div>
                        </div>
                        <br />
                        <div class="row ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div class="mda-form-group float-label">
                                    <label class="col-form-label"> Email ID <span style="color:red"> *</span></label>
                                    <br />
                                    <input ng-model="txtemail_id" ng-pattern="/^[^\s@]+@[^\s@]+\.[^\s@]{2,}$/" required tabindex="3" maxlength="30" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required" style="">

                                </div>
                            </div>
                        </div>
                        <div class="row ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div class="mda-form-group float-label">
                                    <label class="col-form-label"> Password <span style="color:red"> </span></label>
                                    <br />
                                    <input ng-model="txtpassword" tabindex="4" disabled maxlength="30" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required" style="">

                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="edituser.$invalid" ng-click="update()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
                <br />
            </form>
        </div>
    </script>
    <style>
        .modal-content {
            margin-top: 120px;
        }
    </style>
    <script type="text/ng-template" id="/webaccessstatus.html">
        <div class="modal-content" style="margin-top:0px">
            <!--<div class="modal-header bg-primary ng-scope">

                <h4 id="myModalLabel" class="modal-title">
                    &emsp;
                    <span class="btn-label bg-primary">
                        <i class="fa fa-snowflake-o"></i>
                    </span>Web Access Status
                </h4>
            </div>-->
            <md-toolbar>
                <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                    <em style="font-size:16px"></em>&nbsp;<label style="font-size:16px"><em class="icon icon-check"></em>&emsp; Web Access Status </label>
                    <span flex></span>
                    <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
                </div>
            </md-toolbar>
            <div class="modal-body ng-scope">
                <form novalidate="" name="statusloantype" ng-submit="form.webaccessstatus()" autocomplete="off" class="form-validate form-horizontal">
                    <div class="pd-sm-10">
                        <div class="row ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div class="row ">
                                    <div class="col-sm-4 col-sm-4 col-sm-4">
                                        <label> User API Code </label>
                                    </div>
                                    <div class="col-sm-8 col-sm-8 col-sm-8">
                                        {{txtexternal_usercode}}
                                    </div>

                                </div>
                                <br />
                                <div class="row ">
                                    <div class="col-sm-4 col-sm-4 col-sm-4">
                                        <label> Web Status  </label>
                                    </div>
                                    <div class="col-sm-8 col-sm-8 col-sm-8">
                                        <div class="radio c-radio">
                                            <label>
                                                <input type="radio" name="rbo_status" value="Y" ng-model="rbo_status" ng-click="status_yes()" />
                                                &nbsp;&nbsp;<span class="fa fa-circle"></span> Activate
                                            </label>
                                            <label>
                                                <input type="radio" name="rbo_status" value="N" ng-model="rbo_status" ng-click="status_no()" />
                                                <span class="fa fa-circle"></span> De-Activate
                                            </label>
                                        </div>
                                    </div>

                                </div>
                                <div class="row ">
                                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                        <div class="mda-form-group float-label">
                                            <textarea ng-model="txtremarks" style="resize:vertical;" required rows="4" aria-multiline="true" tabindex="0" aria-invalid="false" class="mda-form-control"></textarea>
                                            <label>Remarks *</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div>
                        <br />
                        <div class="text-center">
                            <button type="button" class="btn btn-success btn-md" ng-disabled="statusloantype.$invalid" ng-click="update_status()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                            <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                        </div>
                    </div>
                    <br />
                    <div class="table-responsive" ng-show="log_list!=null">
                        <div style="max-height:300px;overflow:auto;">
                            <table class="table table-striped table-secondary">
                                <thead class="bg-gray text-white">
                                    <tr width="100%">
                                        <th align="center" scope="col">S.No</th>
                                        <th align="center" scope="col">Status</th>
                                        <th align="center" scope="col">Updated By</th>
                                        <th align="center" scope="col">Updated Date</th>
                                        <th align="center" scope="col">Remarks</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <tr width="100%" ng-repeat="data in log_list">
                                        <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                        <td data-title="'Status'" ng-switch="data.status">
                                            <label class="label label-success" ng-switch-when='Y' style="font-size:10px;">Active</label>
                                            <label class="label label-warning" ng-switch-when='N' style="font-size:10px;">InActive</label>
                                        </td>
                                        <td align="left" scope="col" title="'Updated By'">{{data.updated_by}}</td>
                                        <td align="left" scope="col" title="'Updated Date'">{{data.updated_date}}</td>
                                        <td align="left" scope="col" title="'Remarks'">{{data.remarks}}</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </script>
    <!--reset password popup-->
    <script type="text/ng-template" id="/empresetpassowrd.html">
        <div class="modal-header bg-primary ng-scope">

            <h4 id="myModalLabel" class="modal-title">
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-lock "></i>
                </span>RESET PASSWORD
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="userform" ng-submit="form.submitForm1()" autocomplete="off" class="form-validate form-horizontal">
                <div class="pd-sm-10">

                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-sm-1"></div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <strong class="mt0">User Code</strong>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <input class="form-control" type="text" ng-model="txtexternal_usercode" maxlength="20" name="productgroupcode" tabindex="0" readonly />
                            </div>

                        </div>
                    </div>
                    <br />

                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-sm-1"></div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <strong class="mt0">System Name</strong>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <input class="form-control" type="text" ng-model="txtexternalsystem_name" maxlength="30" name="productgroupname" tabindex="1" readonly />
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-sm-1"></div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <strong class="mt0">Password</strong><span class="text-danger">*</span>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <input type="password" name="Password" required="" minlength="8" maxlength="20" placeholder="Enter Password"
                                       ng-pattern="/(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[\W])/" ng-model="Password" class="form-control" />
                                <span ng-show="userform.Password.$dirty &amp;&amp; userform.Password.$error.required" class="text-danger">This field is required</span>
                                <span ng-show="userform.Password.$dirty &amp;&amp; userform.Password.$error.pattern" class="text-danger">Password should contain: "Atleast one upper case letter, one lower case letter, one special character, one number and minimum eight characters."</span>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
