<h3>
    Visitor Management
</h3>
<div ng-controller="MstVisitorViewController as dp">
    <div class="panel">
        <div class="panel-heading" style="background-color:rgb(19,14,89);background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);color:#ffffff">
            <em class="fa fa-eye"></em>&nbsp; Visitor View
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="dd-handle ng-scope">
                                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Basic Details </span> </b></p>
                                            <hr />
                                            <div>
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                                            <label>Visitor ID </label>
                                                            <p> {{lblvisitor_id}} </p>
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                                            <label> Branch Name </label>
                                                            <p> {{lblbranch_name}} </p>
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                                            <label> Visit Date </label>
                                                            <p> {{lblvisit_date}} </p>
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                                            <label> Visit Type </label>
                                                            <p> {{lblvisit_type}} </p>
                                                        </div>                                                        
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                                            <label> Visiting Official Name </label>
                                                            <p>{{lblvisitingofficial_name}}</p>
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                                            <label> In Time </label>
                                                            <p>{{lblin_time}}</p>
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                                            <label> Tentative Out Times </label>
                                                            <p>{{lbltentative_outtime}}</p>
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                                            <label>Actual Out Time </label>
                                                            <p>{{lblactualout_time}}</p>
                                                        </div>                                                        
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <label> Purpose of Visit  </label>
                                                            <p>{{lblpurposeof_visit}}</p>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-md-12">
                                        <div class="dd-handle ng-scope">
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <p class="text-primary" style="font-size:14px;"><b>&nbsp;&nbsp;  Visitor Details </b></p>
                                                    <hr />
                                                    <div class="table-responsive">
                                                        <table ng-table="tableParams2" class="table table-striped table-bordered table-success">
                                                            <thead style="background-color:#dde6e9;">
                                                                <tr>
                                                                    <th style="width:5%;text-align:center;">S.No</th>
                                                                    <th> Visitor Name </th>
                                                                    <th> Visitor ID Proof </th>
                                                                    <th> Visitor Details </th>
                                                                    <th> Visitor Email Address </th>
                                                                    <th> Visitor Mobile Number  </th>
                                                                    <th> Visitor Tag Details  </th>
                                                                    <th> Upload Documents </th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in visitornameview_list" width="100%">
                                                                    <td title="S.No" style="text-align:center;">{{$index+1}}</td>
                                                                    <td title="Visitor Name">
                                                                        <b>Visitor Name : </b> {{data.visitor_name}} <br />
                                                                        <b>Visitor Company Name  : </b> {{data.visitorcompany_name}}  <br />
                                                                    </td>
                                                                    <td title="Visitor ID Proof">
                                                                        <b>Visitor ID Proof : </b> {{data.visitor_idproof}} <br />
                                                                        <b>Visitor ID Proof Number  : </b> {{data.visitor_idproofno}}  <br />
                                                                    </td>
                                                                    <td title="Visitor Details">
                                                                        <b>Temperature : </b> {{data.temperature}} <br />
                                                                        <b>SPO2  : </b> {{data.spo2}}  <br />
                                                                        <b>Vaccination Status   : </b> {{data.vaccination_status}}  <br />
                                                                    </td>
                                                                    <td title="Visitor Email Address">{{data.visitor_email}}</td>
                                                                    <td title="Visitor Mobile Number">{{data.visitor_mobileno}}</td>
                                                                    <td title="Visitor Tag Details">
                                                                        <b>Tag ID : </b> {{data.tag_id}} <br />
                                                                        <b>Tag Validity Date - From  : </b> {{data.tag_validity_from}}  <br />
                                                                        <b>Tag Validity Date - To   : </b> {{data.tag_validity_to}}  <br />
                                                                    </td>
                                                                    <td title="Upload Documents" class="text-center">
                                                                        <a class="btn btn-primary" title="Documents" 
                                                                           ng-click="upload_doc(data.visitorname_gid)"><span class="fa fa-file-text-o"></span></a>
                                                                    </td>
                                                                </tr>

                                                            </tbody>
                                                        </table>
                                                    </div>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="text-center">
                                        <br />
                                        <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                                    </div>
                                    <br /><br />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script type="text/ng-template" id="/UploadDocuments.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-file-text-o"></em>&nbsp;<label style="font-size:16px">Upload Documents</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="uploaddocument" ng-submit="form.uploaddoc()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="table-responsive">
                                <table class="table table-striped table-secondary">
                                    <thead class="bg-gray text-white">
                                        <tr width="100%">
                                            <th scope="col">S.No</th>
                                            <th scope="col">Document Name</th>
                                            <th scope="col">File Name</th>
                                            <th scope="col">Uploaded By / On </th>                                        
                                            <th align="center" scope="col">
                                                Action&nbsp;&nbsp;&nbsp;
                                                <a download="" href="" title="Download All" id="btnpdf3" ng-click="downloadall()">
                                                    <em class="icon-arrow-down"></em>&nbsp;
                                                </a>
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="data in UploadDocument_List">
                                            <td title="S.No">{{$index+1}}</td>
                                            <td title="Document Name">{{data.visitphoto_name}}</td>
                                            <td title="File Name">{{data.file_name}}</td>
                                            <td title="Uploaded By / On">{{data.uploaded_by}} / {{data.updated_date}} </td>
                                            <td data-title="Action">
                                               
                                                <button title="Download" class="btn btn-green btn-sm"
                                                        ng-click="download_doc(data.visitphoto_path,data.file_name)">
                                                    <em class="fa fa-download"></em>
                                                    <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                </button>
                                                <button title="View" class="btn btn-info btn-sm" ng-click="documentviewer(data.visitphoto_path,data.file_name)">
                                                    <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                </button>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>
</div>