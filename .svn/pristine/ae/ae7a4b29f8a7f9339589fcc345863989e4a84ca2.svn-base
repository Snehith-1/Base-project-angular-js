                        <div class="col-md-12 col-lg-12 col-sm-12">

                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <label class="col-form-label"> Bank Account number </label>
                                <br />
                                <input type="text" ng-model="txtbank_accountno" ng-pattern="/^([A-Z0-9]{9,18})$/" tabindex="0" class="form-control" />
                                <label class="fa fa-check" style="color:green" ng-show="bankaccvalidation"></label>&nbsp;<label style="font-weight:bolder;color:green" ng-show="bankaccvalidation">Verified</label>
                                <label ng-if="bankaccvalidation == false" style="font-weight:bolder;color:red">Not Verified</label>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <label class="col-form-label"> Confirm Bank Account number </label>
                                <br />
                                <input type="text" name="confirmbankaccountnumber" ng-model="txtconfirmbankaccount_number" ng-pattern="/^([A-Z0-9]{9,18})$/" tabindex="0" ui-validate="'$value==txtbank_accountno'" ui-validate-watch="'txtconfirmbankaccount_number'" type="password" id="bank_no" class="form-control" ng-change="BankAccValidation()" />
                                <span ng-show="userform2.confirmbankaccountnumber.$dirty &amp;&amp; userform2.confirmbankaccountnumber.$error.validator" class="text-danger">Bank Account number does not match</span>
                            </div>
                            <br />
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <div class="col-md-12 col-lg-12 col-sm-12">
                                <label class="col-form-label"> Account Holder Name </label>
                                <br />
                                <input type="text" ng-model="txtbank_accountname" tabindex="0" class="form-control" disabled ng-show="bankaccvalidation" />
                                <input type="text" ng-model="txtbank_accountname" tabindex="0" class="form-control" required ng-hide="bankaccvalidation" />
                                <br />
                            </div>
                            </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <label class="col-form-label"> Bank Account Level </label>
                                <br />
                                <select id="bankaccount_level" name="bankaccountlevel" ng-options="m.bankaccountlevel_gid as m.bankaccountlevel_name for m in bankaccountlevel_list"
                                        ng-model="cbobankaccountlevel" class="chosen-select form-control" chosen="" style="height:37px;"
                                        size="1">
                                    <option value="" disabled hidden> -----Select Bank Account Level-----</option>
                                </select>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <label class="col-form-label"> Bank Account Type </label>
                                <br />
                                <select id="bankaccount_type" name="bankaccounttype" ng-options="m.bankaccounttype_gid as m.bankaccounttype_name for m in bankaccounttype_list"
                                        ng-model="cbobankaccounttype" class="chosen-select form-control" chosen="" style="height:37px;"
                                        size="1">
                                    <option value="" disabled hidden> -----Select Bank Account Type-----</option>
                                </select>
                            </div>
                        </div>
                    </div>

                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success" ng-disabled="userform1.$invalid" ng-click="update_bank()"><i class="fa fa-check"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>

                    </div>
                </form>
            </div>
        </div>
    </script>

    <script type="text/ng-template" id="/editaddressdetails.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-edit"></em>&nbsp;<label style="font-size:16px">Edit Address</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">

                <form novalidate="" name="edituserform1" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-9 col-md-9 col-sm-9">
                                <label><strong>Address Type </strong> <span style="color:red"> *</span></label>
                                <select id="address_type" name="address_type" required="" ng-options="m.address_gid as m.address_type for m in addresstype_list" style="height:37px;"
                                        ng-model="cboaddresstype" class="form-control ng-valid ng-touched ng-valid-parse"
                                        size="1">
                                    <option value="" disabled hidden> ----- Select Address Type -----</option>
                                </select>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <label>Primary Status </label> <span style="color:red">*</span>
                                <div class="radio c-radio">
                                    <label>
                                        <input type="radio" id="primary" name="primaryaddress" required value="Yes" ng-model="rdbprimaryaddress" />
                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                    </label>
                                    <label>
                                        <input type="radio" id="primary" name="primaryaddress" required value="No" ng-model="rdbprimaryaddress" />
                                        <span class="fa fa-circle"></span>No
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-sm-12 col-sm-12 col-sm-12">
                                <label><strong>Address Line 1 </strong> <span style="color:red">*</span></label>
                                <input type="text" ng-model="txtaddressline1" tabindex="0" class="form-control" maxlength="300" required />
                                <label style="font-weight:bolder;color:red" ng-show="geocodingFailed">Invalid Address!</label>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-sm-12 col-sm-12 col-sm-12">
                                <label><strong>Address Line 2 </strong></label>
                                <input type="text" ng-model="txtaddressline2" tabindex="0" class="form-control" maxlength="300" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Land Mark </strong> </label>
                                    <input type="text" ng-model="txtlandmark" tabindex="0" class="form-control" maxlength="100" />

                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Postal Code </strong> <span style="color:red">*</span></label>
                                    <input type="text" ng-model="txtpostal_code" tabindex="0" class="form-control" required
                                           ng-pattern="/^[1-9]{1}[0-9]{5}$/" ng-change="onchangepostal_code(); getGeoCoding();" maxlength="6" />

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label>City <span style="color:red"> *</span></label>
                                    <input type="text" ng-model="txtcity" required ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" tabindex="0" class="form-control" disabled />
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label>Taluka<span style="color:red"> *</span></label>
                                    <input type="text" ng-model="txttaluka" tabindex="0" class="form-control" required ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" disabled />

                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label><strong>District</strong> <span style="color:red"> *</span></label>
                                    <input type="text" ng-model="txtdistrict" tabindex="0" class="form-control" required ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" disabled />
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label>State </label> <span style="color:red">*</span>
                                    <br />
                                    <input type="text" ng-model="txtstate" tabindex="0" class="form-control"
                                           required ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" disabled />
                                </div>

                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label>Country</label> <span style="color:red">*</span>
                                    <input type="text" ng-model="txtcountry" tabindex="0" class="form-control" required ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" />

                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Latitude </strong></label>
                                    <input type="text" ng-model="txtlatitude" tabindex="0" class="form-control" disabled />

                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Longitude </strong></label>
                                    <input type="text" ng-model="txtlongitude" tabindex="0" class="form-control" disabled />
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success" ng-disabled="edituserform1.$invalid" ng-click="addressUpdate()"><i class="fa fa-check"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>

                    </div>
                </form>
            </div>
        </div>
    </script>

    <script type="text/ng-template" id="/editgstdetails.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-edit"></em>&nbsp;<label style="font-size:16px">Edit GST Details</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">

                <form novalidate="" name="userform1" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="float-label">
                                    <strong>GST Number</strong><span style="color:red">*</span>
                                    <br /><br />
                                    <input ng-model="txteditgst_number" type="text" tabindex="0" required ng-pattern="/^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[1-9A-Z]{1}Z[0-9A-Z]{1}$/" maxlength="15"
                                           ng-change="onchangeeditgst_number()" aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                </div>
                                <br />
                            </div>

                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <label class="col-form-label"> GST State </label>
                                <br />
                                <input type="text" ng-model="txteditgst_state" tabindex="0" class="form-control" disabled required />
                                <br />
                            </div>

                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <br />
                                <label>GST Registered <span style="color:red">*</span></label>
                                <div class="radio c-radio">
                                    <label>
                                        <input type="radio" id="gstregistered" name="gstregistered" value="Yes" ng-model="rdbgstregistered" required />
                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                    </label>
                                    <label>
                                        <input type="radio" id="gstregistered" name="gstregistered" value="No" ng-model="rdbgstregistered" required />
                                        <span class="fa fa-circle"></span>No
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success" ng-disabled="userform1.$invalid" ng-click="update_gst()"><i class="fa fa-check"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>

                    </div>
                </form>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="/StaticMapAndPhotosView.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-image"></em>&nbsp;<label style="font-size:16px">Static Map and Photos</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>
        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            <p class="text-primary" style="font-size:14px;"><b>&nbsp; Static Map </b></p>
                            <img ng-src="{{staticmapImgUrl}}">
                        </div>
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            <p class="text-primary" style="font-size:14px;"><b>&nbsp; Photos </b></p>
                            <div ng-repeat="item in photoUrlList" ng-show="photoFound">
                                <img ng-src="{{item}}">
                                <br />
                                <br />
                            </div>
                            <label style="font-weight:bolder;color:red" ng-show="photoNotFound">Photos not found!</label>
                        </div>
                    </div>
                </div>
                <br />
                <div class="text-center">
                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                </div>
            </div>
        </div>
    </script>
</div>