<h3>
    Edit Buyer
    <a href="#/app/MstBuyerSummary">
        <button type="button" title="Back to Home" class="pull-right btn btn-sm btn-primary ">
            <em class="fa fa-arrow-circle-left fa-1x"></em>
        </button>
    </a>
</h3>
<div ng-controller="MstBuyerEditController as dpick">
    <div class="panel panel-primary">
        <div class="panel-heading">
            Edit buyer
        </div>
        <div class="panel-body">
            <br />
            <form novalidate="" name="userform2" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">

                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <br />
                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Basic Details </b></p>
                            <hr />
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <label class="col-form-label"> Buyer Code </label> <span style="color:red">*</span>
                                <br />
                                <input type="text" ng-model="txtbuyer_code" tabindex="0" class="form-control" placeholder="Auto Generate" disabled />
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <label class="col-form-label"> Buyer Name </label> <span style="color:red">*</span>
                                <br />
                                <input type="text" ng-model="txtbuyer_name" required tabindex="0" class="form-control" />
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <label class="col-form-label"> COI Date </label> <span style="color:red">*</span>
                                <br />
                                <p class="input-group">
                                    <input type="text" id="dob" name="date" ng-model="txtcoi_date" required datepicker-popup="{{dpick.format}}"
                                           style="height:37px;" is-open="dpick.opened" min-date="dpick.minDate" datepicker-options="dpick.dateOptions"
                                           close-text="Close" class="form-control" />
                                    <span class="input-group-btn">
                                        <button type="button" ng-click="dpick.open($event)" style="height:37px;" class="btn btn-default">
                                            <em class="fa fa-calendar"></em>
                                        </button>
                                    </span>
                                </p>
                            </div>
                            <br />
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <br />
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <label class="col-form-label"> Business Start Date </label> <span style="color:red">*</span>
                                <br />
                                <p class="input-group">
                                    <input type="text" id="businessstart_date" name="businessstart_date" ng-model="txtbusinessstart_date" required datepicker-popup="{{dpick.format}}"
                                           style="height:37px;" is-open="dpick.opened1" min-date="dpick.minDate" datepicker-options="dpick.dateOptions"
                                           close-text="Close" class="form-control" ng-change="onchangebusinessstartdate()" />
                                    <span class="input-group-btn">
                                        <button type="button" ng-click="dpick.open1($event)" style="height:37px;" class="btn btn-default">
                                            <em class="fa fa-calendar"></em>
                                        </button>
                                    </span>
                                </p>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label class="col-form-label"> Year in Business </label> <span style="color:red">*</span>
                                <br />
                                <input type="text" ng-model="txtyear_business" tabindex="0" class="form-control" disabled />
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label class="col-form-label"> Months in Business </label> <span style="color:red">*</span>
                                <br />
                                <input type="text" ng-model="txtmonth_business" tabindex="0" class="form-control" disabled />
                            </div>
                            <div class="col-lg-5 col-md-5 col-sm-5">
                                <label class="col-form-label"> Constitution </label> <span style="color:red">*</span>
                                <br />
                                <select id="constitution_name" name="Constitution" required="" ng-options="m.constitution_gid as m.constitution_name for m in constitution_list" chosen=""
                                        ng-model="cboconstitution" class="chosen-select form-control" style="height:37px;"
                                        size="1">
                                    <option value="" disabled hidden> -----Select Constitution-----</option>
                                </select>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <label class="col-form-label"> CIN / Reg No </label>&nbsp;<span style="font-weight:bolder;color:red" ng-show="userform2.cinno.$invalid">is not valid</span>
                                <br />
                                <input type="text" name="cinno" ng-model="txtcin_no" tabindex="0" class="form-control" ng-pattern="/^(L|U)[0-9]{5}[A-Z]{2}[0-9]{4}[A-Z]{3}[0-9]{6}$/" maxlength="21"/>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <label class="col-form-label"> PAN </label> <span style="color:red">*</span>&nbsp;<span style="font-weight:bolder;color:red" ng-show="userform2.panno.$invalid">is not valid</span>
                                <br />
                                <input type="text" name="panno" ng-model="txtpan_no" ng-pattern="/^[A-Z]{5}[0-9]{4}[A-Z]{1}$/" maxlength="10" required tabindex="0" class="form-control" ng-change="getPANbasedGST()" />
                                <label class="fa fa-check" style="color:green" ng-show="panvalidation"></label>&nbsp;<label style="font-weight:bolder;color:green" ng-show="panvalidation">Verified</label>
                            </div>

                            <br />
                        </div>
                    </div>
                    <br />
                    <div class="row ">
                        <div class="col-md-12 col-lg-12 col-sm-12">

                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label"> Contact Person First Name </label> <span style="color:red">*</span>
                                <br />
                                <input type="text" ng-model="txtcontactperson_fn" required tabindex="0" class="form-control" />
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label"> Contact Person Middle Name </label>
                                <br />
                                <input type="text" ng-model="txtcontactperson_mn" tabindex="0" class="form-control" />
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label"> Contact Person Last Name </label>
                                <br />
                                <input type="text" ng-model="txtcontactperson_ln" tabindex="0" class="form-control" />
                            </div>
                            <br />
                        </div>
                    </div>
                    <br />

                </div>
                <br />
                <div class="row">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <div class="panel">
                            <div class="panel-heading">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Buyer GST Details  </b></p>
                                <hr />
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">

                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="col-form-label"> GST Number </label>
                                            <br />
                                            <input type="text" ng-model="txtgst_no" tabindex="0" class="form-control" maxlength="15" ng-pattern="/^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[1-9A-Z]{1}Z[0-9A-Z]{1}$/" ng-change="onchangegst_number()" />
                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label class="col-form-label"> GST State </label>
                                            <br />
                                            <input type="text" ng-model="txtgst_state" tabindex="0" class="form-control" disabled />
                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label>GST Registered </label> 
                                            <div class="radio c-radio">
                                                <label>
                                                    <input type="radio" id="gstregisterstatus_yes" name="gstregister_status" value="Yes" ng-model="$parent.rdbgstregister_status" ng-change="ngchange()" />
                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                </label>
                                                <label>
                                                    <input type="radio" id="gstregisterstatus_no" name="gstregister_status" value="No" ng-model="$parent.rdbgstregister_status" ng-change="ngchange()" />
                                                    <span class="fa fa-circle"></span>No
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3 align-top">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="gst_add()"><span class="fa fa-plus" /> Add </a>
                                                </div>
                                            </div>
                                            <div class="row" ng-show="mandatoryfieldsgst">
                                                <div class="col-sm-12 col-md-12 col-lg-12 text-left">
                                                    <label style="color:red;"> Enter All Mandatory Values</label>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                            <div class="table-responsive table-bordered">
                                                <div style="max-height:200px;overflow:auto;">
                                                    <table class="table">
                                                        <thead class="bg-gray text-white">
                                                            <tr>
                                                                <th>SNo</th>
                                                                <th>GST No</th>
                                                                <th>GST State</th>                                           
                                                                <th>GST Registered</th>
                                                                <th>Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="list in buyergst_list">
                                                                <td>{{$index+1}}</td>
                                                                <td>{{list.gst_no}}</td>
                                                                <td>{{list.gststate_name}}</td>  
                                                                <td>{{list.gstregister_status}}</td>
                                                                <td class="text-nowrap">
                                                                    <button title="Edit" ng-click="gst_edit(list.buyer2gst_gid)"
                                                                            class="btn btn-green btn-xs">
                                                                        <em class="fa fa-edit"></em>
                                                                    </button>
                                                                    <button title="Delete" ng-click="gst_delete(list.buyer2gst_gid)"
                                                                            class="btn btn-danger btn-xs ">
                                                                        <em class="fa fa-trash-o"></em>
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-md-6 col-lg-6 col-sm-6">
                        <div class="panel">
                            <div class="panel-heading">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Contact Person Mobile Number  </b></p>
                                <hr />
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-sm-5 col-md-5 col-lg-5">
                                            <label><strong>Mobile Number </strong></label> <span style="color:red">*</span>&nbsp;<span style="font-weight:bolder;color:red" ng-show="userform2.mobileno.$invalid">is not valid</span>
                                            <input name="mobileno" ng-model="txtmobile_no" type="text" tabindex="0" maxlength="10" ng-pattern="/^[0-9]*$/"
                                                   aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched">

                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label>Primary Status </label> <span style="color:red">*</span>
                                            <div class="radio c-radio">
                                                <label>
                                                    <input type="radio" id="primarymobileno_yes" name="primarymobile_no" value="Yes" ng-model="$parent.rdbprimarymobile_no" ng-change="ngchange()" />
                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                </label>
                                                <label>
                                                    <input type="radio" id="primarymobileno_no" name="primarymobile_no" value="No" ng-model="$parent.rdbprimarymobile_no" ng-change="ngchange()" />
                                                    <span class="fa fa-circle"></span>No
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label>Whatsapp Number </label> <span style="color:red">*</span>
                                            <div class="radio c-radio">
                                                <label>
                                                    <input type="radio" id="whatsappmobileno_yes" name="whatsappmobile_no" value="Yes" ng-model="$parent.rdbwhatsappmobile_no" ng-change="ngchange()" />
                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                </label>
                                                <label>
                                                    <input type="radio" id="whatsappmobileno_no" name="whatsappmobile_no" value="No" ng-model="$parent.rdbwhatsappmobile_no" ng-change="ngchange()" />
                                                    <span class="fa fa-circle"></span>No
                                                </label>
                                            </div>
                                        </div>

                                        <div class="col-lg-1 col-md-1 col-sm-1 align-top">
                                            <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="mobileno_add()"><span class="fa fa-plus" /></a>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                            <div class="table-responsive table-bordered">
                                                <div style="max-height:200px;overflow:auto;">
                                                    <table class="table">
                                                        <thead class="bg-gray text-white">
                                                            <tr>
                                                                <th>SNo</th>
                                                                <th>Mobile No</th>
                                                                <th>Primary Status</th>
                                                                <th>Whatsapp Number </th>
                                                                <th>Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="list in mobileno_list">
                                                                <td>{{$index+1}}</td>
                                                                <td>{{list.mobile_no}}</td>
                                                                <td>{{list.primary_mobileno}}</td>
                                                                <td>{{list.whatsapp_mobileno}}</td>
                                                                <td class="text-nowrap">
                                                                    <button title="Edit" ng-click="mobileno_edit(list.buyer2mobileno_gid)"
                                                                            class="btn btn-green btn-xs">
                                                                        <em class="fa fa-edit"></em>
                                                                    </button>
                                                                    <button title="Delete" ng-click="mobileno_delete(list.buyer2mobileno_gid)"
                                                                            class="btn btn-danger btn-xs ">
                                                                        <em class="fa fa-trash-o"></em>
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-6 col-sm-6">
                        <div class="panel">
                            <div class="panel-heading">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Contact Person Email Address </b></p>
                                <hr />
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-sm-6 col-md-6 col-lg-6">
                                            <label><strong>Email Address </strong></label>&nbsp;<span style="font-weight:bolder;color:red" ng-show="userform2.emailaddress.$invalid">is not valid</span> 
                                            <input type="text" name="emailaddress" ng-model="txtemail_address" ng-pattern="appemail_pattern" class="form-control">
                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label>Primary Status </label>
                                            <div class="radio c-radio">
                                                <label>
                                                    <input type="radio" id="rdbprimaryemailaddress_yes" name="primaryemail_address" value="Yes" ng-model="$parent.rdbprimaryemail_address" ng-change="ngchange()" />
                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                </label>
                                                <label>
                                                    <input type="radio" id="rdbprimaryemailaddress_no" name="primaryemail_address" value="No" ng-model="$parent.rdbprimaryemail_address" ng-change="ngchange()" />
                                                    <span class="fa fa-circle"></span>No
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-sm-2 col-md-2 col-lg-2"></div>
                                        <div class="col-lg-1 col-md-1 col-sm-1 align-top">
                                            <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="emailaddress_add()"><span class="fa fa-plus" /></a>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                            <div class="table-responsive table-bordered">
                                                <table class="table">
                                                    <thead class="bg-gray text-white">
                                                        <tr>
                                                            <th>SNo</th>
                                                            <th>Email Address</th>
                                                            <th>Primary Status</th>
                                                            <th>Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="list in email_list">
                                                            <td>{{$index+1}}</td>
                                                            <td>{{list.email_address}}</td>
                                                            <td>{{list.primary_emailaddress}}</td>
                                                            <td class="text-nowrap">
                                                                <button title="Edit" ng-click="emailaddress_edit(list.buyer2emailaddress_gid)"
                                                                        class="btn btn-green btn-xs">
                                                                    <em class="fa fa-edit"></em>
                                                                </button>
                                                                <a class="btn btn-xs  btn-danger" ng-click="emailaddress_delete(list.buyer2emailaddress_gid)"><span class="fa fa-trash-o"></span></a>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>

                        </div>
                    </div>
                </div>

                <div class="panel">
                    <div class="panel-heading">
                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Address Details </b></p>
                        <hr />
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-sm-4 col-md-4 col-lg-4">

                                    <strong class="ng-binding w3-text-indigo">
                                        <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="address_add()"><span class="fa fa-plus" />  Add Address</a>
                                    </strong>
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="table-responsive table-bordered">
                                        <div style="max-height:200px;overflow:auto;">
                                            <table class="table">
                                                <thead class="bg-gray text-white">
                                                    <tr width="100%">
                                                        <th scope="col">S.No</th>
                                                        <th scope="col">Address Type</th>
                                                        <th scope="col">Address</th> 
                                                        <th> Primary Status</th>
                                                        <th> Static Map / Photos</th>
                                                        <th scope="col">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in address_list" width="100%">
                                                        <td title="S.No">{{$index+1}}</td>
                                                        <td title="AddressType">{{data.addresstype_name}}</td>
                                                        <td title="Address">
                                                            {{data.addressline1}},{{data.addressline2}},<br />{{data.taluka}},{{data.district}},
                                                            <br />{{data.state_name}},{{data.country}},{{data.postal_code}}
                                                            <br /> <b>Latitude:</b> {{data.latitude}}
                                                            <br /> <b>Longitude:</b> {{data.longitude}}
                                                        </td> 
                                                        <td title="AddressType">{{data.primary_address}}</td>
                                                        <td title="Static Map and Photos">
                                                            <button title="View" ng-click="StaticMapAndPhotos_View(data.latitude, data.longitude, data.addressline1, data.addressline2, data.postal_code)" class="btn btn-xs btn-info">
                                                                <em class="fa fa-eye"></em>
                                                            </button>
                                                        </td>
                                                        <td data-title="Action">
                                                            <button title="Edit" ng-click="address_edit(data.buyer2address_gid)"
                                                                    class="btn btn-green btn-xs">
                                                                <em class="fa fa-edit"></em>
                                                            </button>
                                                            <a class="btn btn-xs btn-danger" ng-click="address_delete(data.buyer2address_gid)"><span class="fa fa-trash-o"></span></a>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br />
                    </div>
                </div>

                <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <br />
                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Bank Details </b></p>
                            <hr />
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label"> IFSC Code </label>
                                <br />
                                <input type="text" ng-model="txtifsc_code" ng-pattern="/^[A-Z]{4}0[A-Z0-9]{6}$/" maxlength="11" tabindex="0" class="form-control" ng-change="IFSCValidation()" />
                                <label class="fa fa-check" style="color:green" ng-show="ifscvalidation"></label>&nbsp;<label style="font-weight:bolder;color:green" ng-show="ifscvalidation">Verified</label>
                            </div>
                            <div class="col-lg-4 col-md-6 col-sm-6">
                                <label class="col-form-label">Bank Name </label>
                                <br />
                                <input type="text" ng-model="txtbank_name" tabindex="0" class="form-control" disabled ng-show="ifscvalidation" />
                                <input type="text" ng-model="txtbank_name" tabindex="0" class="form-control" required ng-hide="ifscvalidation" />
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label"> Branch Name </label>
                                <br />
                                <input type="text" ng-model="txtbranch_name" tabindex="0" class="form-control"  ng-show="ifscvalidation" />
                                <input type="text" ng-model="txtbranch_name" tabindex="0" class="form-control" required ng-hide="ifscvalidation" />
                            </div>
                            <br />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <br />

                            <div class="col-lg-8 col-md-8 col-sm-8">
                                <label class="col-form-label">Branch Address </label>
                                <br />
                                <textarea style="resize:none" ng-model="txtbank_address" maxlength="5000" rows="1" class="form-control"
                                           ng-show="ifscvalidation" id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                <textarea style="resize:none" ng-model="txtbank_address" maxlength="5000" rows="1" class="form-control"
