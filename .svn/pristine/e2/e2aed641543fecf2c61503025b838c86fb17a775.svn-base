<h3>
    Add Program
</h3>
<div ng-controller="MstProgramAddController as dpick">
    <div class="panel panel-primary">
        <div class="panel-heading">
            Add Program
        </div>
        <div class="panel-body">
            <br />
            <form novalidate="" name="userform2" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">

                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <br />
                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Basic Details </b></p>
                            <hr />
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label">
                                    Program Ref No
                                </label> <span style="color:red">*</span>
                                <br />
                                <input type="text" ng-model="txtprogram_refno" tabindex="0" class="form-control" placeholder="Auto Generate" disabled />
                            </div>
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label> Entity <span style="color:red">*</span></label>
                                <select name="entity_name" id="entity_name" chosen="" ng-options="m.entity_name for m in entity_list"
                                        ng-model="cboentity" ng-change="entity_change(cboentity)" class="chosen-select form-control" required ">
                                    <option value=""></option>
                                </select>
                            </div>

                            <div class="col-sm-4 col-sm-4 col-sm-4">
                                <label class="col-form-label"> Vertical <span style="color:red"> *</span></label>
                                <br />
                                <select name="Vertical" id="Vertical" chosen="" multiple ng-options="m.vertical_name for m in vertical_list"
                                        ng-model="cbovertical" class="chosen-select form-control" required></select>
                            </div>
                            <br />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <br />
                            <div class="col-sm-8 col-md-8 col-lg-8">
                                <label><strong>Short Program Name </strong><span style="color:red">*</span></label>
                                <input type="text" ng-model="txtprogram_name" ng-maxlength="256" class="form-control" required>
                            </div>

                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label>Approved By  <span style="color:red">*</span></label>
                                <select name="CAD Group Managers" chosen="" multiple="multiple" tabindex="2"
                                        ng-options="m.employee_name for m in cboapprovedadd_list" required=""
                                        ng-model="cboapproved" class="chosen-select form-control"></select>
                                <br />
                            </div>
                            <br />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <br />
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label"> Approved Date </label> <span style="color:red">*</span>

                                <p class="input-group">
                                    <input type="text" id="approved_date" name="approved_date" ng-model="txtapproved_date" required datepicker-popup="{{dpick.format}}"
                                           style="height:37px;" is-open="dpick.opened1" min-date="dpick.minDate" datepicker-options="dpick.dateOptions"
                                           close-text="Close" class="form-control" />
                                    <span class="input-group-btn">
                                        <button type="button" ng-click="dpick.open1($event)" style="height:37px;" class="btn btn-default">
                                            <em class="fa fa-calendar"></em>
                                        </button>
                                    </span>
                                </p>
                            </div>
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label><strong>Overall Program Limit (in Rs)</strong> <span style="color:red"> *</span></label>
                                <input id="txtInput1" type="text" ng-model="txtprogram_limit" ng-change="txtprogramlimit_change()"
                                       class="form-control" style="text-align:right;" required="" maxlength="15" ng-pattern="/^([0-9\s])+(.[0-9\s]+)*$/" />
                                <span style="font-style:italic;">Number in words:</span>
                                <span id="documentlimitamount_words" style="color:#003c96fc;" class="text-bold"></span>

                            </div>
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label><strong>Maximum Limit per Transaction (in Rs)</strong> <span style="color:red"> *</span></label>
                                <input id="txtInput2" type="text" ng-model="txtmax_limit" ng-change="txtmaxlimit_change()"
                                       class="form-control" style="text-align:right;" required="" ng-maxlength="15" ng-pattern="/^([0-9\s])+(.[0-9\s]+)*$/" />
                                <span style="font-style:italic;">Number in words:</span>
                                <span id="maxlimitamount_words" style="color:#003c96fc;" class="text-bold"></span>
                                <div ng-hide="amount_validation">
                                    <label style="color:red;">Amount Exceeded From Overall Program Limit </label>
                                </div>
                            </div>
                            <br />
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label><strong>LMS Code </strong></label>
                                <input type="text" ng-model="txtlms_code" ng-maxlength="300" class="form-control">
                            </div>
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label><strong>Bureau Code </strong></label>
                                <input type="text" ng-model="txtbureau_code" ng-maxlength="300" class="form-control">
                            </div>
                            <div class="col-sm-4 col-md-4 col-lg-4">

                            </div>
                            <br />
                        </div>

                    </div>
                    <br />
                    <div class="row ">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <div class="col-lg-12 col-md-12 col-sm-12 menu">
                                <label><strong>Description </strong> </label>
                                <textarea rows="4" type="text" ng-model="txtdescription" ng-maxlength="10000" tabindex="0" class="form-control ng-pristine ng-valid ng-touched" aria-multiline="true" aria-disabled="false" aria-invalid="false" style=""></textarea>
                            </div>
                            <br />
                        </div>
                    </div>
                    <br />
                </div>
                <br />
                <div class="row" id="SamfinProduct" ng-show="Product_Samfin">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <div class="panel">
                            <div class="panel-heading">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Product Details  </b></p>
                                <hr />
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">

                                        <div class="col-sm-4 col-md-4 col-lg-4 align-top">
                                            <label> Product <span style="color:red">*</span></label>
                                            <select name="loanproduct_name" id="loanproduct_name" chosen="" ng-options="m.loanproduct_name for m in loanproduct_data"
                                                    ng-model="cboloan_product" class="chosen-select form-control" ng-change="loanproductname_change(cboloan_product)">
                                                <option value=""></option>
                                            </select>
                                        </div>
                                        <div class="col-sm-4 col-md-4 col-lg-4 align-top">
                                            <label> Sub Product <span style="color:red">*</span></label>
                                            <select name="loansubproduct_name" id="loansubproduct_name" chosen="" ng-options="m.loansubproduct_name for m in loansubproduct_data"
                                                    ng-model="cboloan_subproduct" class="chosen-select form-control">
                                                <option value=""></option>
                                            </select>
                                        </div>

                                        <div class="col-lg-3 col-md-3 col-sm-3" style="padding-top:25px;">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="product_add()"><span class="fa fa-plus" /> Add </a>
                                                </div>
                                            </div>
                                            <div class="row" ng-show="mandatoryfields">
                                                <div class="col-sm-12 col-md-12 col-lg-12 text-left">
                                                    <label style="color:red;"> Enter All Mandatory Values</label>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                            <div class="table-responsive table-bordered">
                                                <div style="max-height:200px;overflow:auto;">
                                                    <table class="table">
                                                        <thead class="bg-gray text-white">
                                                            <tr>
                                                                <th>SNo</th>
                                                                <th>Product Name</th>
                                                                <th>SubProduct Name</th>
                                                                <th>Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="list in loanproduct_list">
                                                                <td>{{$index+1}}</td>
                                                                <td>{{list.loanproduct_name}}</td>
                                                                <td>{{list.loansubproduct_name}}</td>
                                                                <td class="text-nowrap">
                                                                    <button title="Delete" ng-click="product_delete(list.program2loanproduct_gid)"
                                                                            class="btn btn-danger btn-xs ">
                                                                        <em class="fa fa-trash-o"></em>
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>
                </div>
                <br />
                <div class="row" id="SamagroProduct" ng-show="Product_Samagro">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <div class="panel">
                            <div class="panel-heading">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Product Details  </b></p>
                                <hr />
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">

                                        <div class="col-sm-4 col-md-4 col-lg-4 align-top">
                                            <label> Product <span style="color:red">*</span></label>
                                            <select name="loanproduct_name" id="loanproduct_name" chosen="" ng-options="m.loanproduct_name for m in agrloanproduct_data"
                                                    ng-model="cboloan_product" class="chosen-select form-control" ng-change="agrloanproductname_change(cboloan_product)">
                                                <option value=""></option>
                                            </select>
                                        </div>
                                        <div class="col-sm-4 col-md-4 col-lg-4 align-top">
                                            <label> Loan Program <span style="color:red">*</span></label>
                                            <select name="loansubproduct_name" id="loansubproduct_name" chosen="" ng-options="m.loansubproduct_name for m in agrloansubproduct_data"
                                                    ng-model="cboloan_subproduct" class="chosen-select form-control">
                                                <option value=""></option>
                                            </select>
                                        </div>

                                        <div class="col-lg-3 col-md-3 col-sm-3" style="padding-top:25px;">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="product_add()"><span class="fa fa-plus" /> Add </a>
                                                </div>
                                            </div>
                                            <div class="row" ng-show="mandatoryfields">
                                                <div class="col-sm-12 col-md-12 col-lg-12 text-left">
                                                    <label style="color:red;"> Enter All Mandatory Values</label>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                            <div class="table-responsive table-bordered">
                                                <div style="max-height:200px;overflow:auto;">
                                                    <table class="table">
                                                        <thead class="bg-gray text-white">
                                                            <tr>
                                                                <th>SNo</th>
                                                                <th>Product Name</th>
                                                                <th>Loan Program</th>
                                                                <th>Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="list in loanproduct_list">
                                                                <td>{{$index+1}}</td>
                                                                <td>{{list.loanproduct_name}}</td>
                                                                <td>{{list.loansubproduct_name}}</td>
                                                                <td class="text-nowrap">
                                                                    <button title="Delete" ng-click="product_delete(list.program2loanproduct_gid)"
                                                                            class="btn btn-danger btn-xs ">
                                                                        <em class="fa fa-trash-o"></em>
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>
                </div>
                <br />

                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="dd-handle ng-scope">

                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Upload Documents </b></p>
                            <hr />
                            <div class="col-lg-6 col-sm-6 col-md-6">
                                <div class="mda-form-group float-label">
                                    <strong>Upload Documents</strong>
                                    <input type="file" id="file" tabindex="5" multiple onchange="angular.element(this).scope().ProgramDocumentUpload(this.files, angular.element(this).scope().documentupload, this.name)"
                                           ng-model="txtbureau_document" data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="table-responsive">
                                            <table class="table table-striped table-secondary">
                                                <thead class="bg-gray text-white">
                                                    <tr width="100%">
                                                        <th scope="col">S.No</th>
                                                        <th scope="col">Document Name</th>
                                                        <th align="center" scope="col">
                                                            Action&nbsp;&nbsp;&nbsp;
                                                            <a download="" href="" title="Download All" id="btnpdf1" ng-click="downloadall()">
                                                                <em class="icon-arrow-down"></em>&nbsp;
                                                            </a>
                                                        </th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in upload_list">
                                                        <td title="S.No">{{$index+1}}</td>
                                                        <td title="Document Name">{{data.document_name}}</td>
                                                        <td data-title="Action">
                                                            <button title="Download" class="btn btn-green btn-sm"
                                                                    ng-click="downloads(data.document_path,data.document_name)">
                                                                <em class="fa fa-download"></em>
                                                                <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                            </button>
                                                            <button title="View" class="btn btn-info btn-sm" ng-click="documentviewer(data.document_path,data.document_name)">
                                                                <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                            </button>
                                                            <a title="Delete" class="btn btn-sm btn-danger" ng-click="uploaddocumentcancel(data.tmp_documentGid)">
                                                                <span class="icon icon-trash "></span>
                                                            </a>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />
                        </div>
                    </div>
                </div>
                <br />
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="userform2.$invalid" ng-click="program_submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                </div>
            </form>
            <br />
        </div>
    </div>
</div>