<h3>
   Disbursement Request
</h3>
<div ng-controller="MstRMDisbursementRequestController">
    <button type="button" class="btn btn-info button-sm" ng-click="initiate_disbursement()" title="Initiate Disbursement">
        <span class="btn-label">
            <i class="fa fa-plus"></i>
        </span>
        Initiate Disbursement
    </button>
    <br><br>
    <div class="panel">
        <div class="row">
            <br>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="table-responsive">
                        <table datatable="ng" class="table table-hover table-striped">
                            <thead>
                                <tr class="text-white nowrap" style="background-color:#09385f;">
                                    <th> S.No </th>
                                    <th> Disbursement Request ID </th>
                                    <th> Application Number</th>
                                    <th> Customer Name </th>
                                    <!--<th> Sanction Ref No </th>-->
                                    <th> LSA Ref No</th>
                                    <th> Product Type </th>
                                    <th> Disbursement To </th>
                                    <th> Created Date</th>
                                    <!--<th> Created Date </th>-->
                                    <th> Status </th>
                                    <th class="text-center">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in disbursementrequest_list">
                                    <td data-title="'S.No'"> {{$index+1}} </td>
                                    <td data-title="'Disbursement Request ID'"> {{data.disbursementrequest_code}} </td>
                                    <td data-title="'Application Ref No'"> {{data.application_no}} </td>
                                    <td data-title="'Customer Name'"> {{data.customer_name}} </td>
                                    <!--<td data-title="'Sanction Ref No'"> {{data.sanction_refno}} </td>-->
                                    <td data-title="'LSA Ref No'"> {{data.lsareference_number}} </td>
                                    <td data-title="'Product Type'"> {{data.product_type}} </td>
                                    <td data-title="'Disbursement To'"> {{data.disbursement_to}} </td>
                                    <td data-title="'Created By'">  {{data.created_date}} </td>
                                    <!--<td data-title="'Created Date'"> {{data.created_date}} </td>-->
                                    <td data-title="'Status'"> {{data.disbursementassign_status}} </td>
                                    <td align="center" class="nowrap">
                                        <button title="Edit" ng-show="data.disbursementassign_status != 'Approved'" ng-click="edit(data.application_gid,data.application2sanction_gid,data.application2loan_gid,data.generatelsa_gid,data.rmdisbursementrequest_gid,data.lsgeneratelsa_gid,data.disbursement_to)" class="btn btn-green">
                                            <em class="icon-pencil"></em>
                                        </button>
                                        <button title="Edit" ng-show="data.disbursementassign_status == 'Approved'" disabled class="btn btn-green">
                                            <em class="icon-pencil"></em>
                                        </button>
                                        <button title="View" class="btn btn-info" ng-click="view(data.application_gid,data.application2sanction_gid,data.application2loan_gid,data.generatelsa_gid,data.rmdisbursementrequest_gid,data.lsgeneratelsa_gid)">
                                            <em class="fa fa-eye "></em>
                                        </button>
                                        <button title="Disbursement View" ng-click="completed_process(data.application_gid, data.application2sanction_gid, data.application2loan_gid,data.customer_urn,data.rmdisbursementrequest_gid,data.lsareference_gid)" style="background-color:#871490;" class="btn btn-pink">
                                            <em class="fa fa-spinner"></em>
                                        </button>
                                    </td>
                                </tr>                                
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <br />
        <div class="row">
            <div class="text-center">
                <button type="button" ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
            </div>
            <br>
        </div>
    </div>
</div>