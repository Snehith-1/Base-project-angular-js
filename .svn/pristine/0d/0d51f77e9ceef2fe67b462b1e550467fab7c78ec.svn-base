                </div>
            </div>
    <br />
    <div id="panelDemo8" class="panel panel-primary ">
        <div class="col-lg-12 col-sm-12 col-md-12 panel">
            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">

                    <h4 id="myModalLabel" class="modal-title">
                        <span class="btn-label bg-blue-600">
                            Sample Import Excel
                        </span>
                    </h4>
                </div>
                <br />
                <div class="modal-body ng-scope">
                    <div class="pd-sm-10">
                        <br />
                        <div class="row">
                            <div class="col-sm-12 col-md-12 col-lg-12">
                                <div class="row">
                                    <div class="col-md-12 col-lg-12 col-sm-12">
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <a ng-click="downloadtemplate_importexcel()" style="font-size: 13px;">
                                                <b> Click here to download Template </b>
                                            </a>
                                        </div>
                                        <div class="col-lg-2 col-md-2 col-sm-2">
                                            <label> Upload Excel : </label>
                                        </div>
                                        <div class="col-md-4 col-lg-4 col-sm-4">
                                            <input type="file" id="fileimport" ng-model="test_document"
                                                   onchange="angular.element(this).scope().excelupload(this.files, angular.element(this).scope().documentupload, this.name)"
                                                   data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                            &nbsp;
                                        </div>
                                        <div class="col-md-2 col-lg-2 col-sm-2">
                                            <button title="Upload Excel" ng-click="uploadexcel()" class="btn btn-info">
                                                <em class="icon-cloud-upload"></em>
                                            </button>
                                            <span>
                                                <button title="Cancel Upload" ng-click="uploadexcelcancel()" class="btn btn-danger">
                                                    <i class="fa icon-close"></i>
                                                </button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="class">
                                <br />
                               
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


            <div id="panelDemo5" class="panel panel-default">
                <div collapse="panelDemo5" class="panel-wrapper collapse" style="height: 0px;">
                    <div class="panel-body">
                        <div class="panel">
                            <tabset justified="true">
                                <tab>
                                    <tab-heading>
                                        <em class="fa  fa-check-circle  fa-1x"></em> Sample View &nbsp;
                                    </tab-heading>
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <br />
                                                <div class="table-responsive">
                                                    <table datatable="ng" class="table table-hover table-striped">
                                                        <thead>
                                                            <tr class="text-white" style="background-color:#09385f;">
                                                                <th> S.No </th>
                                                                <th> Created By </th>
                                                                <th> Sample Unique ID</th>
                                                                <th> Sample Name </th>
                                                                <th> Tagged Employee </th>
                                                                <th> Status </th>
                                                                <th> Created Date </th>
                                                                <th class="text-center">Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in sample_list ">
                                                                <td data-title="'S.No'" class="text-center"> {{$index+1}} </td>
                                                                <td data-title="'Created By'">{{data.created_by}}</td>
                                                                <td data-title="'Sample Unique ID'">{{data.audit_uniqueno}}</td>
                                                                <td data-title="'Sample Name'">{{data.sample_name}}</td>
                                                                <td style="text-decoration:underline;text-decoration-color:#49afcd;">
                                                                    <label style="padding-right:25px; font-size: 15px;text-decoration:underline;text-decoration-color:#49afcd;"
                                                                           ng-click="showPopover(data.sampleimport_gid, data.sample_name)">
                                                                        <em class="fa fa-info-circle" style="font-size:24px;align-content:center;"></em>
                                                                    </label>
                                                                </td>
                                                                <td data-title="'Status'">{{data.samplequery_status}} </td>
                                                                <td data-title="'Created Date'">{{data.created_date}} </td>
                                                                <td class="nowrap" data-title="'Action'" style="text-align: center;" ng-switch="data.status">
                                                                    <button title="View" ng-click="view(data.auditcreation_gid, data.sampleimport_gid)" class="btn btn-info">
                                                                        <em class="fa fa-eye"></em>
                                                                    </button>
                                                                    <button title="Sample Query" ng-click="samplequery(data.auditcreation_gid,data.sampleimport_gid)" class="btn btn-primary active">
                                                                        <em class="fa fa-comment-o"></em>
                                                                    </button>
                                                                    <button title="Tag User" ng-disabled="data.samplequery_status =='Closed'" ng-click="btntaguser(data.auditcreation_gid, data.sampleimport_gid)" style="color:white;" class="btn bg-deep-purple-400">
                                                                        <em class="fa fa-tag"></em>
                                                                    </button>
                                                                    
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="text-center">
                                        <button ng-click="back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                                    </div>
                                </tab>

                                <tab>
                                    <tab-heading>
                                        <em class="fa fa-history  fa-1x"></em> Update Observations
                                    </tab-heading>
                                    <div class="dd-handle ng-scope">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <table class="table">
                                                                <thead class="bg-gray text-white">
                                                                    <tr>
                                                                        <th rowspan="25" style="vertical-align : middle;text-align:center; width: 5% ">S.No.</th>
                                                                        <!--<th> <input type="checkbox" id="c2" ng-model="all" ng-change="selectall(all) padding-top: 2px;"> Select</th>-->
                                                                        <th rowspan="25" style="vertical-align : middle;text-align:center; width: 5% "><input type="checkbox" id="c2" ng-model="all" ng-change="selectall(all) padding-top: 2px;"> Select</th>
                                                                        <th rowspan="25" style="vertical-align : middle;text-align:center; width: 10% ">Audit Department</th>
                                                                        <th rowspan="25" style="vertical-align : middle;text-align:center; width: 20% ">Audit Type</th>
                                                                        <th rowspan="25" style="vertical-align : middle;text-align:center; width: 10% ">Checkpoint group</th>
                                                                        <th rowspan="25" style="vertical-align : middle;text-align:center; width: 5% ">Audit Name</th>
                                                                        <th rowspan="25" style="vertical-align : middle;text-align:center; width: 10% ">Checkpoint Group</th>
                                                                        <th rowspan="25" style="vertical-align : middle;text-align:center; width: 20% ">Checkpoint Description</th>
                                                                        <th rowspan="25" style="vertical-align : middle;text-align:center; width: 10% ">Risk Category</th>
                                                                        <th rowspan="25" style="vertical-align : middle;text-align:center; width: 10% ">Positive Conformity</th>
                                                                        <th rowspan="25" style="vertical-align : middle;text-align:center; width: 15% ">Noteto Auditor</th>
                                                                        <th colspan="4" style="horizontal-align : middle;text-align:center; width: 15% ">Standard Score</th>
                                                                        <th colspan="4" style="horizontal-align : middle;text-align:center; width: 15% ">Captured Score</th>
                                                                    <tr>
                                                                        <th scope="col" style="vertical-align : middle;text-align:center; width: 3%">Yes</th>
                                                                        <th scope="col" style="vertical-align : middle;text-align:center; width: 3%">No</th>
                                                                        <th scope="col" style="vertical-align : middle;text-align:center; width: 3%">Partial</th>
                                                                        <th scope="col" style="vertical-align : middle;text-align:center; width: 3%">NA</th>
                                                                        <th scope="col" style="vertical-align : middle;text-align:center; width: 3%">Yes</th>
                                                                        <th scope="col" style="vertical-align : middle;text-align:center; width: 3%">No</th>
                                                                        <th scope="col" style="vertical-align : middle;text-align:center; width: 3%">Partial</th>
                                                                        <th scope="col" style="vertical-align : middle;text-align:center; width: 3%">NA</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>

                                                                    <tr ng-repeat="data in makercheckpointobservation_list">
                                                                        <td data-title="'S.No'"> {{$index+1}} </td>
                                                                        <td>
                                                                            <input type="checkbox" id="c1"
                                                                                   value={{data.checked}} ng-model="data.checked">
                                                                        </td>
                                                                        <td data-title="'CustomerName'">{{data.auditdepartment_name}}</td>
                                                                        <td data-title="'created_by'">{{data.audittype_name}}</td>
                                                                        <td data-title="'CustomerState'">{{data.checkpointgroup_name}}</td>
                                                                        <td data-title="'CustomerState'">{{data.audit_name}}</td>
                                                                        <td align="center">
                                                                            <em class="fa fa-info-circle" ng-click=checkpointintent(data.auditcreation_gid,data.checkpoint_intent) style="font-size:24px;align-content:center;"></em>
                                                                        </td>
                                                                        <td align="center">
                                                                            <em class="fa fa-info-circle" ng-click=description(data.auditcreation_gid,data.checkpoint_description) style="font-size:24px;align-content:center;"></em>
                                                                        </td>
                                                                        <td data-title="'created_by'">{{data.riskcategory_name}}</td>
                                                                        <td data-title="'CustomerState'">{{data.positiveconfirmity_name}}</td>
                                                                        <td align="center">
                                                                            <em class="fa fa-info-circle" ng-click=notetoauditor(data.auditcreation_gid,data.noteto_auditor) style="font-size:24px;align-content:center;"></em>
                                                                        </td>
                                                                        <td data-title="'CustomerName'">{{data.yes_score}}</td>
                                                                        <td data-title="'created_by'">{{data.no_score}}</td>
                                                                        <td data-title="'CustomerState'">{{data.partial_score}}</td>
                                                                        <td data-title="'created_by'">{{data.na_score}}</td>
                                                                        <td>
                                                                            <div class="col">
                                                                                <label class="mda-radio">
                                                                                    <input type="radio" name="radiodata{{data}}" id="yesscore" ng-model="data.yes_radio" ng-change="onselected(data.yes_score, data.auditcreation2checklist_gid,data.checklistmasteradd_gid)" value="1" class="form-control" required />
                                                                                    <em class="bg-teal-500"></em>
                                                                                </label>
                                                                            </div>
                                                                        </td>
                                                                        <td>
                                                                            <div class="col">

                                                                                <label class="mda-radio">
                                                                                    <input type="radio" name="radiodata{{data}}" id="noscore" ng-model="data.no_radio" ng-change="onselected(data.no_score,data.auditcreation2checklist_gid,data.checklistmasteradd_gid)" value="1" class="form-control" required />
                                                                                    <em class="bg-teal-500"></em>
                                                                                </label>
                                                                            </div>
                                                                        </td>
                                                                        <td>
                                                                            <div class="col">
                                                                                <label class="mda-radio">
                                                                                    <input type="radio" name="radiodata{{data}}" id="partialscore" ng-model="data.partialscore_radio" ng-change="onselected(data.partial_score, data.auditcreation2checklist_gid,data.checklistmasteradd_gid)" value="1" class="form-control" required />

                                                                                    <em class="bg-teal-500"></em>
                                                                                </label>
                                                                            </div>
                                                                        </td>
                                                                        <td>
                                                                            <div class="col">
                                                                                <label class="mda-radio">
                                                                                    <input type="radio" name="radiodata{{data}}" id="nascore" ng-model="data.nascore_radio" ng-change="onselected(data.na_score, data.auditcreation2checklist_gid,data.checklistmasteradd_gid)" value="1" class="form-control" required />
                                                                                    <em class="bg-teal-500"></em>
                                                                                </label>
                                                                            </div>
                                                                        </td>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-12 col-sm-12 col-sm-12">
                                            <div class="col-lg-10 col-md-10 col-sm-10">

                                            </div>
                                            <div class="col-lg-2 col-md-2 col-sm-2">
                                                <label><strong>Total Score </strong> <span style="color:red">*</span></label>
                                                <input type="text" readonly ng-model="txttotal_score" style="text-align:right;" tabindex="0" class="form-control" required placeholder="0" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="text-center">
                                        <button type="button" class="btn btn-success btn-md" ng-disabled="createobservation.$invalid" ng-click="submitobservation()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                                        <button type="button" class="btn btn-success btn-md" ng-show="makerapproval_flag =='N' || makerstatus_flag =='N'" ng-disabled="maker_initiated =='Maker Initiated'" ng-click="makerinitiateapproval()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Initiate Approval</button>&nbsp;&nbsp;
                                        <button ng-click="back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                                    </div>
                                    <br />
                                </tab>
                            </tabset>
                        </div>
                    </div>
                </div>
            </div>
        </div>


<script type="text/ng-template" id="/checkpointintent.html">
    <div class="modal-header bg-primary">
        <h4 class="modal-title">
            <span class="btn-label bg-primary">
                <i class="fa fa-edit "></i>
            </span> Check Point Intent
        </h4>
    </div>
    <div class="modal-body">
        <br>
        <div style="white-space: pre-wrap; width:100%; word-wrap: break-word;
            display: inline-block;">
            {{txtcheckpointintent}}
        </div>
        <div align="center">
            <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
        </div>
    </div>
</script>

<script type="text/ng-template" id="/checkpointdescription.html">
