<h3>
    Document Conversation
</h3>
<div ng-controller="idasTrnRmResponse">
    <div id="panelDemo13" class="panel panel-default">
        <div class="panel-body table-responsive">
            <p class="lead bb text-primary text-bold text-center">
                {{documentrecord_id}}
                <label style="font-weight:bold">/</label>{{document_name}}
            </p>
            <div class="row">
                <div class="col-md-12 col-sm-12 col-lg-12">
                    <div class="col-md-6 col-sm-6 col-lg-6">
                        <label>Document Details - Vetting</label>
                    </div>
                    <div class="col-md-6 col-sm-6 col-lg-6">
                        <label>Document Details - Complaint Certificate</label>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                 
                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <label>Document Date</label><br />
                        <span ng-bind="scandocument_date"></span>
                    </div>
                   

                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <label class="col-form-label control-label"><strong> Types Of Document </strong></label><br />
                        <span>{{types_of_copy}}</span>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <label>Document Date</label><br />
                        <span ng-bind="phydocument_date"></span>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <label class="col-form-label control-label"><strong> Types Of Document </strong></label><br />
                        <span>{{phydocument_type}}</span>
                    </div>

                </div>

            </div>
          
            <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="col-sm-6 col-md-6  col-lg-6">
                        <div class="panel panel-primary ">
                            <h4 class="panel-heading" align="left">
                                <label class="text-white">Scan Document Conversation</label><br />
                            </h4>

                        </div>
                        <div id="panelDemo1" class="panel" ng-repeat="data in scandocconversation">
                            <div ng-class="{'panel-heading-collapsed':panelDemo1}" class="panel-heading bg-grey-100">
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="pull-left">
                                            <lable class="text-purple"><strong>CAD Query #{{data.query_no}}:({{data.docconversationref_no}})</strong></lable>

                                        </div>
                                        <div class="pull-right">
                                            <span class="text-sm"> {{data.cad_name}}/{{data.cadquery_on}}</span>
                                            &nbsp;
                                            <button title="View Documents" ng-if="data.uploaddocument_count != '0'" ng-click="PopupDownload(data.docconversation_gid)" class="btn btn-purple">
                                                <em class="icon icon-link"></em>
                                            </button>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                            <p ng-bind-html="data.cad_query"></p>
                                        </div>
                                    </div>
                                </div>
                                <paneltool tool-collapse="tool-collapse"></paneltool>
                            </div>
                            <div collapse="panelDemo1" class="panel-wrapper">

                                <div ng-switch="data.flag">
                                    <div class="panel-heading" ng-switch-when="Y">
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class="pull-left">
                                                    <lable class="text-pink"><strong>RM Response #{{data.query_no}}:</strong></lable>

                                                </div>
                                                <div class="pull-right">
                                                    <span class="text-sm">
                                                        {{data.relationshipmgr_name}}/{{data.relationshipmgrquery_on}}
                                                    </span>
                                                </div>

                                            </div>

                                        </div>
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class="col-lg-12">

                                                    <p ng-bind-html="data.rm_response"></p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div ng-switch-when="N">
                                        <div ng-show="data.hide">

                                            <div class="row">
                                                <div class="col-md-12 col-lg-12 col-sm-12">
                                                    <div class="col-lg-2 col-md-2 col-sm-2">
                                                        <button type="button" ng-click="FileShow()" class="btn btn-info btn-xs">
                                                            <span class="icon icon-cloud-upload"></span>&nbsp;&nbsp;Upload File
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                            <div ng-show="DivFile">
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label>Document Title</label>&nbsp;<span style="color:red">*</span>
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <input type="text" ng-model="txtdocument_title" rows="1" tabindex="0" min="0" aria-required="true"
                                                                   aria-invalid="true" class="form-control" maxlength="256" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />

                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label>File Upload</label>
                                                        </div>

                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <input type="file" id="addupload" ng-model="test_document" ng-disabled="!txtdocument_title"
                                                                   onchange="angular.element(this).scope().uploadallocation(this.files, angular.element(this).scope().documentupload, this.name)"
                                                                   data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select"
                                                                   accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <br/>
                                            </div>
                                            
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <div ng-repeat="data in uploaddocument" class="panel bg-grey-200">
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label>{{data.document_title}}</label><br />
                                                            <span>{{data.document_name}}</span>
                                                            &nbsp;&nbsp;
                                                            <button title="Un-Tag" ng-click="UploadDocCancel(data.conversationdocument_gid)" class="btn btn-danger">
                                                                <em class="fa fa-remove (alias)"></em>
                                                            </button>

                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                            <form novalidate="" name="formReply" autocomplete="off" class="form-validate form-horizontal">
                                                <div class="row">
                                                    <div class="col-lg-12">
                                                        <div class="col-lg-12">
                                                            <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                                                <label for="input_13">Reply * </label>
                                                                <textarea ng-model="reply" required rows="2" md-maxlength="1000" class="ng-valid md-input ng-valid-md-maxlength ng-touched ng-dirty ng-valid-parse"
                                                                          id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                                            </md-input-container>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="ng-scope row">
                                                    <div class="text-left col-lg-12">
                                                        <div class="col-lg-12">
                                                            <button ng-click="raiseResponse(data.docconversation_gid,$index,reply)" ng-disabled="formReply.$invalid" class="btn btn-purple"><i class="fa fa-send (alias)"></i>&nbsp;&nbsp;Send</button>
                                                            <button ng-click="data.hide=false" class="btn btn-danger"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Cancel</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                        <br />

                                        <div class="row">
                                            <div class="col-lg-12">

                                                <div class="col-lg-12" ng-hide="data.hide">
                                                    <button title="Reply Conversation" class="btn-pink" ng-click="data.hide=true">
                                                        <em class="fa fa-mail-reply (alias) w3-text-pink"></em>&nbsp;
                                                    </button>
                                                    <span class="text-pink">Reply</span>
                                                </div>
                                            </div>
                                        </div>
                                        <br />
                                    </div>


                                </div>

                            </div>

                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-6">
                        <div class="panel panel-primary ">
                            <h4 class="panel-heading" align="left">
                                <label class="text-white">Physical Document Conversation</label><br />
                            </h4>
                       </div>
                        <div id="panelDemo1" class="panel" ng-repeat="data in phydocconversation">
                            <div ng-class="{'panel-heading-collapsed':panelDemo1}" class="panel-heading bg-grey-100">
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                            <div class="pull-left">
                                                <lable class="text-purple"><strong>CAD Query #{{data.query_no}}:({{data.docconversationref_no}})</strong></lable>

                                            </div>
                                            <div class="pull-right" ng-switch="data.forwarded_flag">
                                                <span class="text-sm"> {{data.cad_name}}/{{data.cadquery_on}}</span>
                                                &nbsp;
                                                <button title="View Documents" ng-if="data.uploaddocument_count != '0'" ng-click="PopupDownload(data.docconversation_gid)" class="btn btn-purple">
                                                    <em class="icon icon-link"></em>
                                                </button>
                                                <i class="fa fa-mail-forward (alias)" title="Fowarded By : {{data.forwarded_by_name}}&#013;Fowarded On:{{data.forwarded_on}}" ng-switch-when="Y"></i>

                                            </div>
                                        </div>

                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                            <p ng-bind-html="data.cad_query"></p>
                                        </div>
                                    </div>
                                </div>
                                <paneltool tool-collapse="tool-collapse"></paneltool>
                            </div>
                            <div collapse="panelDemo1" class="panel-wrapper">

                                <div ng-switch="data.flag">
                                    <div class="panel-heading" ng-switch-when="Y">
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class="col-lg-12">
                                                    <div class="pull-left">
                                                        <lable class="text-pink"><strong>RM Response #{{data.query_no}}:</strong></lable>

                                                    </div>
                                                    <div class="pull-right">
                                                        <span class="text-sm">
                                                            {{data.relationshipmgr_name}}/{{data.relationshipmgrquery_on}}
                                                        </span>
                                                    </div>
                                                </div>


                                            </div>

                                        </div>
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class="col-lg-12">

                                                    <p ng-bind-html="data.rm_response"></p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div ng-switch-when="N">
                                        <form novalidate="" name="formReply" autocomplete="off" class="form-validate form-horizontal">
                                            <div ng-show="data.hide">
                                                <div class="row">
                                                    <div class="col-lg-12">
                                                        <div class="col-lg-12">

                                                            <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                                                <label for="input_13">Reply * </label>
                                                                <textarea ng-model="reply" required rows="2" md-maxlength="1000" class="ng-valid md-input ng-valid-md-maxlength ng-touched ng-dirty ng-valid-parse"
                                                                          id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                                            </md-input-container>

                                                        </div>
                                                    </div>
                                                </div>
                                                <br />

                                                <div class="ng-scope row">
                                                    <div class="text-left col-lg-12">
                                                        <div class="col-lg-12">
                                                            <button ng-click="raiseResponse(data.docconversation_gid,reply)" ng-disabled="formReply.$invalid" class="btn btn-purple"><i class="fa fa-send (alias)"></i>&nbsp;&nbsp;Send</button>
                                                            <button ng-click="data.hide=false" class="btn btn-danger"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Cancel</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <br />

                                            <div class="row">
                                                <div class="col-lg-12">

                                                    <div class="col-lg-12" ng-hide="data.hide">
                                                        <button title="Reply Conversation" class="btn-pink" ng-click="data.hide=true">
                                                            <em class="fa fa-mail-reply (alias) w3-text-pink"></em>&nbsp;
                                                        </button>
                                                        <span class="text-pink">Reply</span>
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                        </form>
                                    </div>


                                </div>

                            </div>

                        </div>
                    </div>
                </div>
            </div>
          

           
            <div class="row">
                <div class="col-lg-12">
                    <div class="col-lg-6">
                        <label>Final Remarks - Vetting</label>
                        <p ng-bind="scanfinal_remarks"></p>

                    </div>
                    <div class="col-lg-6">
                        <label>
                            Final Remarks - Complaint Certificate
                        </label>
                        <p ng-bind="phyfinal_remarks">

                        </p>
                    </div>

                </div>
            </div>


            <br />
            <div class="ng-scope">
                <div class="text-center">

                    <button ng-click="rmresponseback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                </div>
            </div>
            <script type="text/ng-template" id="/mailconversation.html">


                <div class="modal-body ng-scope">
                    <h4 id="myModalLabel" class="modal-title">
                        <p class="lead bb text-primary text-bold text-center">
                            View Documents
                        </p>
                    </h4>

                    <div class="text-center">
                        <div class="dd-handle ng-scope">
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="table-responsive">
                                        <table class="table table-striped table-secondary">
                                            <thead class="bg-gray text-white">
                                                <tr width="100%">
                                                    <th scope="col">S.No</th>
                                                    <th scope="col">Document Name</th>
                                                    <th scope="col">Document Title</th>
                                                    <th scope="col">Upload By/On</th>
                                                    <th scope="col">Action</th>
                                                </tr>
                                            </thead>

                                            <tbody>
                                                <tr ng-repeat="data in viewDocumentList" width="100%">
                                                    <td title="'S.No'">{{$index+1}}</td>
                                                    <td align="left" title="'Document Name'">{{data.document_name}}</td>
                                                    <td align="left" title="'Document Title'">{{data.document_title}}</td>
                                                    <td title="'Upload By/On'">{{data.created_by}}/{{data.created_date}}</td>
                                                    <td data-title="'Action'">
                                                        <span class="fa fa-download" ng-click="downloads(data.document_path,data.document_name)" /><a download="" href="" id="btnpdf" style="display:none;"></a>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>

                                    </div>
                                </div>
                            </div>

                        </div>
                        <br />
                        <button ng-click="ok()" class="btn btn-danger">
                            <i class="fa fa-close (alias)">
                            </i>&nbsp;Close
                        </button>
                    </div>
                </div>
            </script>
        </div>
    </div>


</div>

