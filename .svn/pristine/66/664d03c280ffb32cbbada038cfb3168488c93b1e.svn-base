<h3>
    Vendor Consultant Register
</h3>
<div ng-controller="externalregisterViewEdit">
    <div class="panel widget">
        <div style="background-image: url('app/img/bg4.jpg')" class="panel-body text-center bg-center">
            <div class="row row-table">
                <div class="col-xs-12 text-white">
                    <div ng-switch="photo_path">
                        <img src="app/img/user-bg1.jfif" alt="Image" ng-switch-when="N"
                             class="img-thumbnail img-circle thumb96">
                        <img ng-src="{{photo_path}}" alt="Image" ng-switch-default
                             class="img-thumbnail img-circle thumb96">
                    </div>
                    <h3 class="m0">{{txtexternal_vendorName}}</h3>
                </div>
            </div>
        </div>
        <div class="panel-heading">
            <h4 class="text-primary">Vendor Details</h4>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <div class="col-lg-2 col-sm-2 col-md-2">
                        <label>Vendor Code</label>
                    </div>
                    <div class="col-lg-4 col-sm-4 col-md-4">
                        <p>{{txtexternal_vendorcode}}</p>
                    </div>
                    <div class="col-lg-2 col-sm-2 col-md-2">
                        <label>Vendor Name </label>
                    </div>
                    <div class="col-lg-4 col-sm-4 col-md-4">
                        <p>{{txtexternal_vendorName}}</p>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <div class="col-lg-2 col-sm-2 col-md-2">
                        <label>Contact Person </label>
                    </div>
                    <div class="col-lg-4 col-sm-4 col-md-4">
                        <p>{{txtcontact_person}}</p>
                    </div>
                    <div class="col-lg-2 col-sm-2 col-md-2">
                        <label>Email Address </label>
                    </div>
                    <div class="col-lg-4 col-sm-4 col-md-4">
                        <p>{{txtemail_ID}}</p>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <div class="col-lg-2 col-sm-2 col-md-2">
                        <label>Contact No. </label>
                    </div>
                    <div class="col-lg-4 col-sm-4 col-md-4">
                        <p>{{txtcontact_Number}}</p>
                    </div>
                    <div class="col-lg-2 col-sm-2 col-md-2">
                        <label>Address Line 1 </label>
                    </div>
                    <div class="col-lg-4 col-sm-4 col-md-4">
                        <p>{{txtaddress_line1}}</p>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <div class="col-lg-2 col-sm-2 col-md-2">
                        <label>Address Line 2 </label>
                    </div>
                    <div class="col-lg-4 col-sm-4 col-md-4">
                        <p>{{txtaddress_line2}}</p>
                    </div>
                    <div class="col-lg-2 col-sm-2 col-md-2">
                        <label>State</label>
                    </div>
                    <div class="col-lg-4 col-sm-4 col-md-4">
                        <p>{{state_name}}</p>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <div class="col-lg-2 col-sm-2 col-md-2">
                        <label>District</label>

                    </div>
                    <div class="col-lg-4 col-sm-4 col-md-4">
                        <p>{{district_name}}</p>
                    </div>
                    <div class="col-lg-2 col-sm-2 col-md-2">
                        <label>Country</label>

                    </div>
                    <div class="col-lg-4 col-sm-4 col-md-4">
                        <p>{{txtcountry}}</p>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <div class="col-lg-2 col-sm-2 col-md-2">
                        <label>Postal Code</label>
                    </div>
                    <div class="col-lg-8 col-sm-8 col-md-8">
                        <p>{{txtpostalCode}}</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="text-center">

                <a class="btn btn-info" ng-click="cancel()">
                    <em class="fa fa-arrow-circle-left"></em>
                    <span>Back</span>
                </a>
            </div>
        </div>
        <br />
    </div>
</div>