<a href="#/app/requestCompliancesummary?lstab='Pending'">
    <button type="button" title="Back" class="pull-right btn btn-sm btn-primary">
        <em class="fa fa-arrow-circle-left fa-1x"></em>
    </button>
</a>
<h3>
    View - Request Compliance
</h3>

<div ng-controller="requestComplianceviewcontroller">
    
        <div class="row animated fadeInLeft anim-delay-5">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="panel panel-primary ">
                    <div class="panel-heading">
                        <em class="fa fa-eye"></em>&nbsp; Request Compliance Details
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-lg-4  col-sm-4 col-md-4">
                                <strong class="mt0">Request  Ref No</strong><br />
                                <p>{{requestref_no}}</p>
                            </div>
                            <div class="col-lg-4  col-sm-4 col-md-4">
                                <strong class="mt0">Request Date</strong>
                                <br />
                                <p>{{request_date}}</p>
                            </div>
                            <div class="col-lg-4  col-sm-4 col-md-4">
                                <strong class="mt0">Request type</strong>
                                <br />
                                <p>{{request_type}}</p>
                            </div>

                        </div>
                    </div>
                    
                    <div class="row" >
                        <div class="col-lg-12 col-md-12 col-sm-12" >

                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <strong>Description</strong>
                              
                                    <p ng-bind-html="txtremarks"></p>
                               
                            </div>
                        </div>
                    </div>

                  
                    <div class="row" ng-show="request_status=='Rejected'">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <br />
                                <div class="dd-handle" style="background-color: #f9f8f8;">

                                    <div class="col-lg-12 col-sm-12 col-md-12 ">
                                        <b style="color:red;font-size:14px;">
                                            <em class="fa fa-thumbs-o-down fa-lg"></em>
                                            <label>{{request_status}} Details</label>
                                        </b>
                                        <hr />
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12 ">
                                            <div class="col-lg-12  col-sm-12 col-md-12">
                                                <strong class="mt0">Rejected Remarks</strong>
                                                <br />
                                                <p>{{rejected_remarks}}</p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12 ">
                                            <div class="col-lg-6  col-sm-6 col-md-6">
                                                <strong class="mt0">Rejected On</strong>
                                                <br />
                                                <p>{{updated_date}}</p>
                                            </div>
                                            <div class="col-lg-6  col-sm-6 col-md-6">
                                                <strong class="mt0">Rejected By</strong>
                                                <br />
                                                <p>{{updated_by}}</p>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            </div>
                    </div>

                    <div class="row" ng-show="request_status=='Completed'">
                        <div class="col-lg-12 col-md-12 col-sm-12">

                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <br />
                                <div class="dd-handle" style="background-color: #f9f8f8;">

                                    <div class="col-lg-12 col-sm-12 col-md-12 ">
                                        <b style="color:green;font-size:14px;">
                                            <em class="fa fa-thumbs-o-up fa-lg"></em>
                                            <label>{{request_status}} Details</label>
                                        </b>
                                        <hr />
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12 ">
                                            <div class="col-lg-12  col-sm-12 col-md-12">
                                                <strong class="mt0">Completed Remarks</strong>
                                                <br />
                                                <p>{{completed_remarks}}</p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12 ">
                                            <div class="col-lg-6  col-sm-6 col-md-6">
                                                <strong class="mt0">Completed On</strong>
                                                <br />
                                                <p>{{updated_date}}</p>
                                            </div>
                                            <div class="col-lg-6  col-sm-6 col-md-6">
                                                <strong class="mt0">Completed By</strong>
                                                <br />
                                                <p>{{updated_by}}</p>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                    <br />
                    <br />

                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-6 col-sm-6 col-md-6">
                                <b style="color:darkblue">
                                    <em class="fa fa-file text-primary fa-lg"></em>&emsp;
                                    Uploaded Documents
                                </b>
                            </div>
                            <div class="col-lg-6 col-sm-6 col-md-6">
                                <b style="color:darkblue">
                                    <em class="fa fa-file text-primary fa-lg"></em>&emsp;
                                     Corrected Documents By Legal Team
                                </b>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-6 col-sm-6 col-md-6">

                            <div class="col-lg-12 col-sm-12 col-md-12 table-responsive cell-border">
                                <table class="table">

                                    <tbody>

                                        <tr ng-repeat="data in list">

                                            <td>
                                                {{$index+1}}
                                            </td>
                                            <td>
                                                {{data.file_name}}
                                            </td>
                                            <td>
                                                <a class="btn" download="" href="" id="btnpdf" ng-click="downloads(data.file_path,data.file_name)"><span class="fa fa-download"></span></a>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>

                            </div>

                        </div>
                        <div class="col-lg-6 col-sm-6 col-md-6">

                            <div class="col-lg-12 col-sm-12 col-md-12 table-responsive cell-border" >
                                <table class="table">
                                    <tbody>
                                        <tr ng-repeat="data in file_list" width="100%">

                                            <td align="left" scope="col" title="'Document Name'">
                                                {{data.document_name}}
                                            </td>
                                            <td>
                                                <a class="btn" download="" href="" id="btnpdf" ng-click="downloadsdocument(data.document_path,data.document_name)"><span class="fa fa-download"></span></a>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>

                            </div>

                        </div>
                    </div>
                    <br />
                </div>
            </div>
        <br/>
        </div>
                 <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="panel widget">
                            <div class="panel-parimary panel-heading " style="background-color:gainsboro">
                                <b style="color:darkblue">
                                    <em class="fa fa-question-circle  fa-lg"> </em>&emsp;Query

                                </b>
                            </div>

                            <div ng-repeat="data in querieslog_list">
                                <div ng-switch="data.session_user">
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            &nbsp;<div style="padding-left:200px;" ng-switch-when="Y" class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="media bb p bg-green-50" style="width:initial;border-radius:15px;">
                                                    <small class="pull-right text-bold">{{data.created_date}}</small>
                                                    <div class="media-body">
                                                        <span class="media-heading">
                                                            <p class="m0">
                                                                <strong class="text-info">{{data.sender_name}}</strong>
                                                            </p>
                                                            <p class="m0 ">{{data.queries}}</p>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>

                                            <div ng-switch-when="N" style="padding-right:200px;" class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="media bb p bg-deep-orange-50" style="width:initial;border-radius:15px;">
                                                    <small class="pull-right text-bold">
                                                        {{data.created_date}}
                                                    </small>
                                                    <div class="media-body">
                                                        <span class="media-heading">
                                                            <p class="m0">
                                                                <strong class="text-info">{{data.sender_name}}</strong>
                                                            </p>
                                                            <p class="m0 ">{{data.queries}}</p>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>


                                    </div>

                                </div>
                            </div>
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <br />
                                <div class="row text-center">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <textarea id="txtqueries" name="txtqueries" rows="5" cols="60" ng-model="txtqueries"
                                                  placeholder="Enter Queries..." class="form-control" align="left"></textarea>
                                        <br />
                                        <button id="response_send" ng-disabled="!txtqueries" class="btn btn-green" ng-click="sendclick()">
                                            <i class="fa fa-send">&nbsp;Send</i>
                                        </button>
                                        <button id="response_cancel" ng-disabled="!txtqueries" class="btn btn-danger" ng-click="cancelclick()">
                                            <i class="fa fa-remove">&nbsp;Cancel</i>
                                        </button>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>
                </div>
            
        <div class="panel-body text-center">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <button ng-click="requestback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>&nbsp;&nbsp;
            </div>
        </div>
    
</div>

