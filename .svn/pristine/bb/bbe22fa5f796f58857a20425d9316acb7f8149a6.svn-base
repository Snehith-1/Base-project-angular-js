<style>
    .space {
        margin: 10px 0px;
    }
</style>
<div ng-controller="MstDisbCoApplicantDtlAddController as dp">
    <div class="panel page1">
        <div class="panel-heading" style="background-color:rgb(19,14,89);background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);color:#ffffff">
            <em class="fa fa-plus"></em>&nbsp; Add Individual Co-Applicant Details - {{lblindividual_name}}
        </div>
        <div class="panel-body">
            <div>
                <form name="IndividualSave" ng-submit="form.submitForm1()" novalidate="" autocomplete="off" class="form-validate form-horizontal">
                    <div class="dd-handle ng-scope">
                        <p class="text-primary" style="font-size: 14px;"><b>&nbsp;&nbsp; Co-Applicant Details </b></p>
                        <hr class="space">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label"> Aadhar Number </label> <span style="color:red">*</span>
                                    <br />
                                    <p class="input-group">
                                        <!--<input type="password" ng-model="txtaadhar_no" maxlength="15" tabindex="0" required class="form-control" pattern="^[0-9]{1}[0-9]{3}[0-9]{4}[0-9]{4}$" />-->
                                        <input type="{{inputType}}" ng-model="txtaadhar_no" maxlength="15" tabindex="0" required class="form-control" pattern="^[0-9]{1}[0-9]{3}[0-9]{4}[0-9]{4}$" />
                                        <span class="input-group-btn">
                                            <button type="button" style="height:35px;" ng-show="eye" ng-click="show()" class="btn btn-default "><em class="fa fa-eye"></em></button>
                                            <button type="button" style="height:35px;" ng-show="eyeslash" ng-click="noshow()" class="btn btn-default "><em class="fa fa-eye-slash"></em></button>
                                        </span>
                                    </p>
                                </div>
                                <div class="col-md-4 col-lg-4 col-sm-4">
                                    <label>PAN Status</label><span style="color:red;">&nbsp; *</span>
                                    <select name="PAN Status" id="PAN Status" required ng-model="cbopanstatus" class="form-control" style="height:32px;" size="1" ng-change="change_pan(cbopanstatus)">
                                        <option value="">---- Select PAN Status ----</option>
                                        <option value="Customer Submitting PAN">Customer Submitting PAN</option>
                                        <option value="Customer Submitting Form 60"> Customer Submitting Form 60 </option>
                                    </select>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4" ng-show="havepan">
                                    <label class="col-form-label"> PAN Value </label> <span style="color:red">*</span>
                                    <br />
                                    <input type="text" ng-model="txtpan_no" ng-pattern="/^[A-Z]{5}[0-9]{4}[A-Z]{1}$/" maxlength="10" tabindex="0" class="form-control" ng-change="PANValidation()" />
                                    <label class="fa fa-check" style="color:green" ng-show="panvalidation"></label>&nbsp;
                                    <label style="font-weight:bolder;color:green" ng-show="panvalidation">Verified</label>
                                    <label class="fa fa-warning text-danger" style="font-weight:bolder;" ng-show="AlreadyaddedIndividualpan"> This PAN number already Exists, created by <span style="color:black;"> {{lblcreated_by}} </span></label>
                                </div>
                            </div>
                        </div>
                        <br />

                        <div class="row" ng-show="havenotpan">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">

                                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Customer do not have the PAN and he/she submitted Form 60 and it fulfils the following, </b></p>
                                        <hr />
                                        <div ng-show="view_nopanreasons">
                                            <table class="table">
                                                <tbody>
                                                    <tr ng-repeat="data in panabsencereason_list">
                                                        <td width="10%">
                                                            &nbsp;
                                                            <input type="checkbox" id="c1"
                                                                   value={{data.checked}} ng-disabled="{{data.checked}}" ng-model="data.checked">
                                                        </td>
                                                        <td width="90%">{{data.panabsencereason}}</td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                            <br />
                                        </div>
                                        <div ng-hide="view_nopanreasons">
                                            <table class="table">
                                                <tbody>
                                                    <tr ng-repeat="data in panabsencereason_list">
                                                        <td width="10%">
                                                            &nbsp;
                                                            <input type="checkbox" id="c1"
                                                                   value={{data.checked}} ng-model="data.checked" disabled>
                                                        </td>
                                                        <td width="90%">{{data.panabsencereason}}</td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                            <br />
                                        </div>
                                        <br />

                                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Form-60 Document Upload </b></p>
                                        <hr />
                                        <div class="col-lg-6 col-sm-6 col-md-6">
                                            <div class="float-label">
                                                <strong>Upload Documents</strong>
                                                <input type="file" id="panform60file" tabindex="5" multiple onchange="angular.element(this).scope().IndividualPANForm60DocumentUpload(this.files, angular.element(this).scope().documentupload, this.name)"
                                                       ng-model="panform60file" data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                            </div>
                                        </div>
                                        <br />
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <br />
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-secondary">
                                                            <thead class="bg-gray text-white">
                                                                <tr width="100%">
                                                                    <th scope="col">S.No</th>
                                                                    <th scope="col">Document Name</th>
                                                                    <th align="center" scope="col">
                                                                        Action&nbsp;&nbsp;&nbsp;
                                                                        <a download="" href="" title="Download All" id="btnpdf288" ng-click="downloadallform60()">
                                                                            <em class="icon-arrow-down"></em>&nbsp;
                                                                        </a>
                                                                    </th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in contactpanform60_list">
                                                                    <td title="S.No">{{$index+1}}</td>
                                                                    <td title="Document Name">{{data.document_name}}</td>
                                                                    <td data-title="Action">
                                                                        <button title="Download" class="btn btn-green btn-sm"
                                                                                ng-click="form60_downloads(data.document_path,data.document_name)">
                                                                            <a download="" href="" id="btnpdf118" style="display:none;"></a>
                                                                            <em class="fa fa-download"></em>
                                                                        </button>
                                                                        <button title="View" class="btn btn-info btn-sm" ng-click="form60_documentviewer(data.document_path,data.document_name)">
                                                                            <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                                        </button>
                                                                        <a title="Delete" class="btn btn-sm btn-danger" ng-click="IndividualPANForm60DocumentDelete(data.coapplicantcontact2panform60_gid)">
                                                                            <span class="icon icon-trash "></span>
                                                                        </a>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <br />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label"> First Name </label> <span style="color:red">*</span>
                                    <br />
                                    <input type="text" ng-model="txtfirst_name" tabindex="0" class="form-control" disabled ng-show="panvalidation" />
                                    <input type="text" ng-model="txtfirst_name" tabindex="0" class="form-control" required ng-hide="panvalidation" />
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label"> Middle Name </label>
                                    <br />
                                    <input type="text" ng-model="txtmiddle_name" tabindex="0" class="form-control" disabled ng-show="panvalidation">
                                    <input type="text" ng-model="txtmiddle_name" tabindex="0" class="form-control" ng-hide="panvalidation">
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label"> Last Name </label> <span style="color:red">*</span>
                                    <br />
                                    <input type="text" ng-model="txtlast_name" tabindex="0" class="form-control" disabled ng-show="panvalidation" />
                                    <input type="text" ng-model="txtlast_name" tabindex="0" class="form-control" ng-hide="panvalidation" />
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-3 col-md-3 col-sm-3">
                                    <label class="col-form-label"> Date of Birth </label> <span style="color:red">*</span>
                                    <br />
                                    <input ng-model="txtindividual_dob" id="individual_dob" name="individual_dob" maxlength="10" ng-pattern="/^([0-9]{2}[-]{1}[0-9]{2}[-]{1}[0-9]{4})/"
                                           required tabindex="0" aria-required="true" aria-invalid="true" class="form-control" ng-keyup="onchangeddobindividual(txtindividual_dob)" ng-change="futuredatecheck(txtindividual_dob)" placeholder="DD-MM-YYYY">

                                </div>
                                <div class="col-lg-1 col-md-1 col-sm-1">
                                    <label class="col-form-label"> Age</label>
                                    <br />
                                    <input type="text" ng-model="txtage" tabindex="0" class="form-control" disabled ng-change="onchangeddobage_individual()" ng-pattern="/^[0-9]*$/">
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3">
                                    <label class="col-form-label"> Gender </label> <span style="color:red">*</span>
                                    <br />
                                    <select name="Gender" required="" ng-options="m.gender_name for m in gender_list"
                                            ng-model="cboGender" class="form-control" style="height:37px;" chosen="" size="1">
                                        <option value=""></option>
                                    </select>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3">
                                    <label class="col-form-label"> Designation </label> <span style="color:red">*</span>
                                    <br />
                                    <select name="Designation" ng-options="m.designation_type for m in designation_list" required=""
                                            ng-model="cboDesignation" class="form-control " style="height:37px;" chosen="" size="1">
                                        <option value=""></option>
                                    </select>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label> Politically Exposed person (PEP) </label> <span style="color:red">*</span>
                                    <div class="radio c-radio">
                                        <label>
                                            <input type="radio" id="pep_status" name="pep_status" value="Yes" required ng-model="rdbpep_status" ng-change="ngchange()" />
                                            &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                        </label>
                                        <label>
                                            <input type="radio" id="pep_status" name="pep_status" value="No" required ng-model="rdbpep_status" ng-change="ngchange()" />
                                            <span class="fa fa-circle"></span>No
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">

                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label"> PEP Verified On </label> <span style="color:red">*</span>
                                    <br />
                                    <p class="input-group">
                                        <input type="text" id="pepvefifydate" name="date" ng-model="txtpepverified_date" datepicker-popup="{{dp.format}}"
                                               style="height:36px;" is-open="dp.open2" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                               close-text="Close" class="form-control" placeholder="DD-MM-YYYY" required />
                                        <span class="input-group-btn">
                                            <button type="button" ng-click="dp.calender2($event)" style="height:36px;" class="btn btn-default">
                                                <em class="fa fa-calendar"></em>
                                            </button>
                                        </span>
                                    </p>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label"> Marital Status </label> <span style="color:red">*</span>
                                    <br />
                                    <select name="MaritalStatus" required="" ng-options="m.maritalstatus_name for m in maritalstatus_list"
                                            ng-model="cboMaritalStatus" class="form-control" style="height:37px;" chosen="" size="1">
                                        <option value=""></option>
                                    </select>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label"> Educational Qualification </label> <span style="color:red">*</span>
                                    <br />
                                    <select name="EducationalQualification" required="" ng-options="m.educationalqualification_name for m in educationalqualification_list"
                                            ng-model="cboEducationalQualification" class="form-control" style="height:37px;" chosen="" size="1">
                                        <option value=""></option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label"> Main Occupation </label> <span style="color:red">*</span>
                                    <br />
                                    <input type="text" ng-model="txtmain_occupation" tabindex="0" class="form-control" required />
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label"> Annual Income </label> <span style="color:red">*</span>
                                    <br />
                                    <input type="text" id="annual_income" ng-model="txtannual_income" tabindex="0" ng-change="txtannualincome_change()" required class="form-control" style="text-align:right;" />
                                    <span style="font-style:italic;">Number in words:</span>
                                    <span id="words_annualincome" style="color:#003c96fc;" class="text-bold"></span>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label"> Monthly Income </label>
                                    <br />
                                    <input type="text" id="monthly_income" ng-model="txtmonthly_income" tabindex="0" ng-change="txtmonthlyincome_change()" class="form-control" style="text-align:right;" />
                                    <span style="font-style:italic;">Number in words:</span>
                                    <span id="words_monthlyincome" style="color:#003c96fc;" class="text-bold"></span>
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-sm-12 col-sm-12 col-sm-12">
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label>Years in Current Residence </label><span style="color:red">*</span>
                                    <input type="text" ng-model="txtcurrentresidence_years" maxlength="3" tabindex="0" required class="form-control" />
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label>Distance from Branch/Regional Office (in Kms.)  </label> <span style="color:red">*</span>
                                    <input type="text" ng-model="txtbranch_distance" maxlength="5" required tabindex="0" class="form-control" />
                                </div>

                            </div>
                        </div>
                        <br />
                    </div>
                    <div class="dd-handle ng-scope">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <p class="text-primary" style="font-size: 14px;"><b>&nbsp;&nbsp; Family Details </b></p>
                                <hr />
                                <div class="col-lg-3 col-md-3 col-sm-3">
                                    <label class="col-form-label"> Father's First Name </label> <span style="color:red">*</span>
                                    <br />
                                    <input type="text" ng-model="txtfather_firstname" tabindex="0" class="form-control" required />
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label class="col-form-label"> Father's Middle Name </label>
                                    <br />
                                    <input type="text" ng-model="txtfather_middlename" tabindex="0" class="form-control" />
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label class="col-form-label"> Father's Last Name </label> <span style="color:red">*</span>
                                    <br />
                                    <input type="text" ng-model="txtfather_lastname" tabindex="0" class="form-control" required />
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label class="col-form-label"> Nominee  </label>
                                    <br />
                                    <div class="radio c-radio">
                                        <label>
                                            <input type="radio" id="fathernominee_status " name="fathernominee_status " value="Yes" ng-model="rdbfathernominee_status" ng-change="onselectedfathernominee_yes(rdbfathernominee_status)" />
                                            &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                        </label>
                                        <label>
                                            <input type="radio" id="fathernominee_status " name="fathernominee_status " value="No" ng-model="rdbfathernominee_status" ng-change="onselectedfathernominee_yes(rdbfathernominee_status)" />
                                            <span class="fa fa-circle"></span>No
                                        </label>
                                    </div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label class="col-form-label"> Date of Birth </label>
                                    <br />
                                    <input ng-model="txtfather_dob" id="father_dob" name="father_dob" maxlength="10" ng-pattern="/^([0-9]{2}[-]{1}[0-9]{2}[-]{1}[0-9]{4})/"
                                           tabindex="0" aria-required="true" aria-invalid="true" class="form-control" ng-keyup="onchangeddobfather(txtfather_dob)" ng-change="futuredatecheck(txtfather_dob)" placeholder="DD-MM-YYYY">

                                </div>
                                <div class="col-lg-1 col-md-1 col-sm-1">
                                    <label class="col-form-label"> Age </label>
                                    <br />
                                    <input type="text" ng-model="txtfather_age" tabindex="0" class="form-control" disabled ng-change="onchangeddobage_father()" ng-pattern="/^[0-9]*$/" />
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-3 col-md-3 col-sm-3">
                                    <label class="col-form-label"> Mother's First Name </label>
                                    <br />
                                    <input type="text" ng-model="txtmother_firstname" tabindex="0" class="form-control" />
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label class="col-form-label"> Mother's Middle Name </label>
                                    <br />
                                    <input type="text" ng-model="txtmother_middlename" tabindex="0" class="form-control" />
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label class="col-form-label"> Mother's Last Name </label>
                                    <br />
                                    <input type="text" ng-model="txtmother_lastname" tabindex="0" class="form-control" />
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label class="col-form-label"> Nominee  </label>
                                    <br />
                                    <div class="radio c-radio">
                                        <label>
                                            <input type="radio" id="mothernominee_status " name="mothernominee_status " value="Yes" ng-model="rdbmothernominee_status" ng-change="onselectedmothernominee_yes(rdbmothernominee_status)" />
                                            &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                        </label>
                                        <label>
                                            <input type="radio" id="mothernominee_status " name="mothernominee_status " value="No" ng-model="rdbmothernominee_status" ng-change="onselectedmothernominee_yes(rdbmothernominee_status)" />
                                            <span class="fa fa-circle"></span>No
                                        </label>
                                    </div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label class="col-form-label"> Date of Birth </label>
                                    <br />
                                    <input ng-model="txtmother_dob" id="mother_dob" name="mother_dob" maxlength="10" ng-pattern="/^([0-9]{2}[-]{1}[0-9]{2}[-]{1}[0-9]{4})/"
                                           tabindex="0" aria-required="true" aria-invalid="true" class="form-control" ng-keyup="onchangeddobmother(txtmother_dob)" ng-change="futuredatecheck(txtmother_dob)" placeholder="DD-MM-YYYY">

                                </div>
                                <div class="col-lg-1 col-md-1 col-sm-1">
                                    <label class="col-form-label"> Age </label>
                                    <br />
                                    <input type="text" ng-model="txtmother_age" tabindex="0" class="form-control" disabled ng-change="onchangeddobage_mother()" ng-pattern="/^[0-9]*$/" />
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-3 col-md-3 col-sm-3">
                                    <label class="col-form-label"> Spouse First Name </label>
                                    <br />
                                    <input type="text" ng-model="txtspouse_firstname" tabindex="0" class="form-control" />
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label class="col-form-label"> Spouse Middle Name </label>
                                    <br />
                                    <input type="text" ng-model="txtspouse_middlename" tabindex="0" class="form-control" />
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label class="col-form-label"> Spouse Last Name </label>
                                    <br />
                                    <input type="text" ng-model="txtspouse_lastname" tabindex="0" class="form-control" />
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label class="col-form-label"> Nominee  </label>
                                    <br />
                                    <div class="radio c-radio">
                                        <label>
                                            <input type="radio" id="spousenominee_status " name="spousenominee_status " value="Yes" ng-model="rdbspousenominee_status" ng-change="onselectedspousenominee_yes(rdbspousenominee_status)" />
                                            &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                        </label>
                                        <label>
                                            <input type="radio" id="spousenominee_status " name="spousenominee_status " value="No" ng-model="rdbspousenominee_status" ng-change="onselectedspousenominee_yes(rdbspousenominee_status)" />
                                            <span class="fa fa-circle"></span>No
                                        </label>
                                    </div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <label class="col-form-label"> Date of Birth </label>
                                    <br />
                                    <input ng-model="txtspouse_dob" id="spouse_dob" name="spouse_dob" maxlength="10" ng-pattern="/^([0-9]{2}[-]{1}[0-9]{2}[-]{1}[0-9]{4})/"
                                           tabindex="0" aria-required="true" aria-invalid="true" class="form-control" ng-keyup="onchangeddobspouse(txtspouse_dob)" ng-change="futuredatecheck(txtspouse_dob)" placeholder="DD-MM-YYYY">

                                </div>
                                <div class="col-lg-1 col-md-1 col-sm-1">
                                    <label class="col-form-label"> Age </label>
                                    <br />
                                    <input type="text" ng-model="txtspouse_age" tabindex="0" class="form-control" disabled ng-change="onchangeddobage_spouse()" ng-pattern="/^[0-9]*$/" />
                                </div>
                            </div>
                        </div>
