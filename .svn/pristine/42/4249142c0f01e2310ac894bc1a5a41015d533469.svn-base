<h3>
    Customer Creation  Request
</h3>
<style>
    .picColor:hover {
        background-color: transparent;
        box-shadow: none;
        color: #144A6E;
        border: 1px solid #A59F9D;
        transform: translateY(-3px);
        box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2);
        text-decoration: underline;
        text-decoration-color: #141478;
    }

    .tab {
        overflow: hidden;
        border: 1px solid #ccc;
        background-color: #f8f7f7;
        height: 48px;
    }

        /* Style the buttons inside the tab */
        .tab button {
            background-color: inherit;
            float: left;
            border: none;
            outline: none;
            cursor: pointer;
            padding: 14px 16px;
            transition: 0.3s;
            font-size: 14px;
            font-weight: bold;
        }

            /* Change background color of buttons on hover */
            .tab button:hover {
                background-color: rgb(226, 221, 221);
                text-decoration: underline;
                height: 56px;
            }

    /* Style the tab content */
    .tabcontent {
        display: none;
        padding: 6px 12px;
        border: 1px solid #fff;
        border-top: none;
    }

    .tab button.active {
        background-color: #d5591e;
        background-image: linear-gradient(316deg, rgb(236 195 100) 0%, rgb(171 225 166) 74%);
    }
</style>
<script>
    function openApplication(evt, ApplicationName) {
        var i, tabcontent, tablinks;
        tabcontent = document.getElementsByClassName("tabcontent");
        for (i = 0; i < tabcontent.length; i++) {
            tabcontent[i].style.display = "none";
        }
        tablinks = document.getElementsByClassName("tablinks");
        for (i = 0; i < tablinks.length; i++) {
            tablinks[i].className = tablinks[i].className.replace(" active", "");
        }
        document.getElementById(ApplicationName).style.display = "block";
        evt.currentTarget.className += " active";
    }

    // Get the element with id="defaultOpen" and click on it
    document.getElementById("defaultOpen").click();

</script>
<div ng-controller="MstCustomerUpdatedRequestSummaryController">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="col-lg-9 col-md-9 col-sm-9">
                <div class="tab">
                    <button class="col-lg-3 col-md-3 col-sm-3" ng-click="pending()" style="color:#141478;font-size:12px">
                        <em class="fa fa-user fa-x" style="color:#141478;font-size: 18px;"></em>
                        Pending
                        <br />
                        <!--<div class="label" style="padding-top:4px;font-size: 14px;padding-bottom:4px;color:#141478;">{{cadreview_count}}</div>-->
                    </button>
                    <button class="col-lg-3 col-md-3 col-sm-3"  onclick="openApplication(event, ' Customer URN Updation')" id="defaultOpen" ng-click="urn_update()" style="color:#00796b;font-size:12px">
                        <em class="fa fa-group fa-x" style="color:#00796b;font-size: 18px;"></em>
                        Updated URN
                        <br />
                        <!--<div class="label" style="font-size: 14px;padding-top:4px;padding-bottom:4px;color:#00796b;">{{accept_count}}</div>-->
                    </button>
                    <button class="col-lg-3 col-md-3 col-sm-3" ng-click="rejected()" style="color:#C10B68;font-size:12px">
                        <em class="fa icon-close fa-x" style="color:#C10B68;font-size: 18px;"></em>
                        Rejected
                        <br />
                        <!--<div class="label" style="padding-top:4px;padding-bottom:4px;color:#C10B68;font-size: 14px;">{{urngrouping_count}}</div>-->
                    </button>
                </div>
            </div>
            <div class="col-lg-3 col-sm-3 col-md-3">
                <div class="list-group-item picColor" style="height:50px;">
                    <div class="media-box">
                        <div class="media-box-body clearfix">
                            <div class="media-box-heading" style="padding-top:6px;">
                                <div class="pull-right label" style="background-color:#C10B68;padding-top:6px;padding-bottom:4px;font-size:8px;">{{total_count}}</div>
                                <div style="font-size:14px;color:#C10B68;"><b> Total Applications </b></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <br />
    <br />
    <div class="panel" id=" Customer URN Updation">
        <br />
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="table-responsive">
                        <table datatable="ng" class="table table-hover table-striped">
                            <thead>
                                <tr class="text-white" style="background-color:#09385f;">
                                    <th style="display:none;" class="text-center"> S.No</th>
                                    <th>Reference ID</th>
                                    <th>Application Number</th>
                                    <th>Customer Name  </th>
                                    <th> Initiated By </th>
                                    <th> Initiated Date </th>
                                    <th>Customer URN</th>
                                    <th class="text-center">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in updated_list">
                                    <td data-title="'S.No'" style="display:none;" class="text-center"> {{$index+1}} </td>
                                    <td data-title="'Reference ID'">{{data.cuclms_gid}}</td>
                                    <td data-title="'Application Number'"> {{data.application_no}} </td>
                                    <td data-title="'Customer Name'"> {{data.customer_name}} </td>
                                    <!--<td data-title="' LMS Staus'"> {{data.lms_status}} </td>-->
                                    <!--<td data-title="'LMS Staus' " ng-switch="data.lms_status">
        <label style="font-size:10px" ng-switch-when="Businessops" class="label label-success"> Businessops </label>
    </td>-->
                                    <td data-title="'Initiated By'"> {{data.initiated_by}} </td>
                                    <td data-title="'Initiated Date'"> {{data.initiated_date}} </td>
                                    <td data-title="'Customer URN'"> {{data.customer_urn}} </td>
                                    <td class="nowrap" data-title="'Action'" style="text-align: center;">
                                        <button title="View" ng-click="view(data.cuclms_gid,data.application_gid)" class="btn btn-info">
                                            <em class="fa fa-eye"></em>
                                        </button>
                                        <!--<button title="Assign Application" ng-click="assign_disbursement(data.application_gid)" class="btn btn-pink">
        <em class="icon-user-follow"></em>
                </button>-->
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

        <br />
    </div>
    <script type="text/ng-template" id="/assigndisbursement.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="icon-user-follow"></em>&nbsp;<label style="font-size:16px">Assign Disbursement</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="assignform" ng-submit="form.submitassign()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <label>Credit Ops Group <span style="color:red"> *</span> </label>
                                        <select name="creditopsgroup" id="creditopsgroup" ng-options="m.creditopsgroupmapping_gid as m.creditopsgroup_name for m in creditOpsGrouplist" ng-change="creditopsgroup_change(cbocreditopsgroup)"
                                                ng-model="cbocreditopsgroup" class="form-control " chosen="">
                                            <option value=""> </option>
                                        </select>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <label> Maker <span style="color:red"> *</span> </label>
                                        <select name="creditmanager" id="creditmanager" chosen="" ng-options="m as m.employee_name for m in Creditops_maker track by m.creditops2maker_gid"
                                                ng-model="cbocreditops_maker" class="form-control" required></select>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <label> Checker <span style="color:red"> *</span> </label>
                                        <select name="regionalcredit" id="regionalcredit" chosen="" ng-options="m as m.employee_name for m in Creditops_checker track by m.creditops2checker_gid"
                                                ng-model="cborcreditops_checker" class="form-control" required></select>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <!--<label> Approver <span style="color:red"> *</span> </label>
                                        <select name="nationalcredit" id="nationalcredit" chosen="" ng-options="m.employee_name for m in approver_list"
                                                ng-model="cbocreditops_approver" class="form-control" required></select>-->
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <label> Remarks </label>
                                        <textarea ng-model="txtremarks" rows="4" class="form-control" style="resize:vertical;"
                                                  id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                    </div>
                                </div>
                            </div>
                            <br />
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success" ng-disabled="assignform.$invalid" ng-click="assign_application()"><i class="fa fa-check"></i>&nbsp;&nbsp;Assign</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                        <br />
                    </div>
                </form>
            </div>
        </div>
    </script>
</div>