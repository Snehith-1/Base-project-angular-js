<h3>
    Cheque Management
</h3>
<div ng-controller="AgrMstChequeCheckerDtlsController">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="col-lg-2 col-md-2 col-sm-2">
                <button type="button" title="Add Cheque" class="btn btn-info btn-sm" ng-click="addMaker()">
                    <span class="btn-label">
                        <i class="fa fa-plus"></i>
                    </span> Add Cheque
                </button>
            </div>
            <div class="col-lg-10 col-md-10 col-sm-10">
                <div id="panelDemo5" class="panel panel-default">
                    <div ng-class="{'panel-heading-collapsed':panelDemo5}" class="panel-heading" style="background-color: #c9dcc1">
                        <b> Approval Details </b>
                        <paneltool tool-collapse="tool-collapse" ng-init="panelDemo5=true"></paneltool>
                    </div>
                    <div collapse="panelDemo5" class="panel-wrapper">
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="dd-handle ng-scope">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label> Maker Name </label>
                                                    <p>{{maker_name}}</p>
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label> Checker Name </label>
                                                    <p>{{checker_name}}</p>
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label> Approval Name </label>
                                                    <p>{{approver_name}}</p>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label> Maker Submitted Date </label>
                                                    <p>{{maker_approveddate}}</p>
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label> Checker Submitted Date </label>
                                                    <p>{{checker_approveddate}}</p>
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label> Approved Date </label>
                                                    <p>{{approver_approveddate}}</p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>

            </div>
        </div>
    </div>
    <div class="panel">
        <br />
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="table-responsive">
                        <table datatable="ng" class="table table-hover table-striped">
                            <thead>
                                <tr class="text-white" style="background-color:#09385f;">
                                    <th> S.No</th>
                                    <th> Name </th>
                                    <th> Check Type </th>
                                    <!--<th> Cheque Number </th>-->
                                    <th> Stackholder Type </th>
                                    <th> Created Date </th>
                                    <th> Created By </th>
                                    <th class="text-center">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in udcmanagement_list">
                                    <td data-title="'S.No'">{{$index+1}}</td>
                                    <td data-title="'Name'"> {{data.customer_name}} </td>
                                    <td data-title="'Check Type'"> {{data.cheque_type}}</td>
                                    <!--<td data-title="'Cheque Number'"> {{data.cheque_no}}</td>-->
                                    <td data-title="'Stackholder Type'"> {{data.stakeholder_name}}</td>
                                    <td data-title="'Created Date'"> {{data.created_date}} </td>
                                    <td data-title="'Created By'"> {{data.created_by}} </td>
                                    <td class="nowrap" data-title="'Action'" style="text-align: center;">
                                        <button title="View" class="btn btn-info btn-sm"
                                                ng-click="viewCheque(data.udcmanagement2cheque_gid,data.udcmanagement_gid)">
                                            <em class="fa fa-eye"></em>
                                        </button>
                                        <button title="Edit" ng-click="editUdc(data.udcmanagement_gid,data.udcmanagement2cheque_gid)" class="btn btn-green">
                                            <em class="fa fa-edit"></em>
                                        </button>
                                        <button title="Delete" ng-click="deleteUdc(data.udcmanagement_gid)" class="btn btn-danger">
                                            <em class="icon icon-trash "></em>
                                        </button>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <br />
        <div class="row">
            <div class="text-center">
                <button ng-click="proceedtoapproval()" class="btn btn-success"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Proceed To Approval</button>
                <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
            </div>
            <br />
        </div>
    </div>
</div>