<h3>
    Customer
    <a href="#/app/FndTrnCustomerApprovalController">
        <button type="button" title="Back" class="pull-right btn btn-sm btn-primary ">
            <em class="fa fa-arrow-circle-left fa-1x"></em>
        </button>
    </a>
</h3>
<div ng-controller="FndTrnApprovalEditController as dpick">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <em class="fa fa-plus"></em>&nbsp;  Edit Customer
        </div>
        <div class="panel-body">
            <form novalidate="" name="userform2" ng-submit="form.userform2()" autocomplete="off" class="form-validate mb-lg">
                <div>

                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <label class="col-form-label"> Customer Code </label> <span style="color:red">*</span>
                                <br />
                                <input type="text" ng-model="customer_code" tabindex="0" class="form-control" placeholder="Auto Generate" disabled />
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <label class="col-form-label"> Customer Name </label> <span style="color:red">*</span>
                                <br />
                                <input type="text" ng-model="customer_name" required tabindex="0" class="form-control" />
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <label class="col-form-label"> PAN </label> <span style="color:red">*</span>
                                <br />
                                <input type="text" ng-model="pan_no" required ng-pattern="/^[A-Z]{5}[0-9]{4}[A-Z]{1}$/" maxlength="10" tabindex="0" class="form-control" ng-change="getPANbasedGST()" />
                                <label class="fa fa-check" style="color:green" ng-show="panvalidation"></label>&nbsp;<label style="font-weight:bolder;color:green" ng-show="panvalidation">Verified</label>
                            </div>

                            <br />
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <br />
                        <div class="col-lg-3 col-md-3 col-sm-3">
                            <label class="col-form-label"> Date of Incorporation </label> <span style="color:red">*</span>
                            <br />
                            <p class="input-group">
                                <input type="text" id="businessstart_date" disabled name="businessstart_date" ng-model="businessstart_date" required datepicker-popup="{{dpick.format}}"
                                       style="height:37px;" is-open="dpick.opened1" min-date="dpick.minDate" datepicker-options="dpick.dateOptions"
                                       close-text="Close" class="form-control" ng-change="onchangebusinessstartdate(businessstart_date)" />
                                <span class="input-group-btn">
                                    <button type="button" ng-click="dpick.open1($event)" style="height:37px;" class="btn btn-default">
                                        <em class="fa fa-calendar"></em>
                                    </button>
                                </span>
                            </p>
                        </div>
                        <div class="col-lg-2 col-md-2 col-sm-2">
                            <label class="col-form-label"> Year in Business </label> <span style="color:red">*</span>
                            <br />
                            <input type="text" ng-model="year_business" tabindex="0" class="form-control" disabled />
                        </div>
                        <div class="col-lg-2 col-md-2 col-sm-2">
                            <label class="col-form-label"> Months in Business </label> <span style="color:red">*</span>
                            <br />
                            <input type="text" ng-model="month_business" tabindex="0" class="form-control" disabled />
                        </div>
                        <div class="col-lg-5 col-md-5 col-sm-5">
                            <label class="col-form-label"> CIN / Reg No </label>
                            <br />
                            <input type="text" ng-model="cin_no" tabindex="0" class="form-control" ng-pattern="/^(L|U)[0-9]{5}[A-Z]{2}[0-9]{4}[A-Z]{3}[0-9]{6}$/" maxlength="21" />
                        </div>

                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <!--<div class="col-lg-6 col-md-6 col-sm-6">
                            <label class="col-form-label"> Constitution </label> <span style="color:red">*</span>
                            <br />
                            <select chosen="" name="Constitution" required="" ng-options="m.constitution_name for m in constitution_list"
                                    ng-model="Constitution_name" class="chosen-select form-control" style="height:37px;"
                                    size="1">
                                <option value=""> -----Select Constitution-----</option>
                            </select>
                        </div>-->
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <label class="col-form-label"> Constitution </label> <span style="color:red">*</span>
                            <br />
                            <select id="constitution_name" name="Constitution" required="" ng-options="m.constitution_gid as m.constitution_name for m in constitution_list" chosen=""
                                    ng-model="cboconstitution" class="chosen-select form-control" style="height:37px;"
                                    size="1">
                                <option value=""> -----Select Constitution-----</option>
                            </select>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <label class="col-form-label"> Credit Assessment Agency </label>
                            <br />
                            <select id="assessmentagency_name" name="Credit Assessment Agency" ng-options="m.assessmentagency_gid as m.assessmentagency_name for m in assessmentagency_list" chosen=""
                                    ng-model="cboassessmentagency" class="chosen-select form-control" style="height:37px;"
                                    size="1">
                                <option value=""> -----Select Assessment Agency-----</option>
                            </select>
                        </div>
                        <!--<div class="col-lg-6 col-md-6 col-sm-6">
                            <label class="col-form-label"> Credit Assessment Agency </label> <span style="color:red">*</span>
                            <br />
                            <select chosen="" name="Credit Assessment Agency" required="" ng-options="m.assessmentagency_name for m in assessmentagency_list"
                                    ng-model="assessmentagency_name" class="chosen-select form-control" style="height:37px;"
                                    size="1">
                                <option value=""> -----Select Assessment Agency-----</option>
                            </select>
                        </div>-->

                        <br />
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <div class="col-lg-3 col-md-3 col-sm-3">
                            <label class="col-form-label">Assessment Rating </label> 
                            <br />
                            <select id="assessmentagencyrating_name" name="Assessment Rating" ng-options="m.assessmentagencyrating_gid as m.assessmentagencyrating_name for m in assessmentagencyrating_list" chosen=""
                                    ng-model="cboassessmentagencyrating" class="chosen-select form-control" style="height:37px;"
                                    size="1">
                                <option value=""> -----Select Assessment Rating-----</option>
                            </select>
                        </div>

                        <div class="col-lg-3 col-md-3 col-sm-3">
                            <label class="col-form-label"> Rating As on</label>
                            <br />
                            <p class="input-group">
                                <input type="text" id="rating_date" disabled name="ratingason_date" ng-model="rating_date" datepicker-popup="{{dpick.format}}"
                                       style="height:37px;" is-open="dpick.opened2" min-date="dpick.minDate" datepicker-options="dpick.dateOptions"
                                       close-text="Close" class="form-control" ng-change="onchangebusinessstartdate()" />
                                <span class="input-group-btn">
                                    <button type="button" ng-click="dpick.open2($event)" style="height:37px;" class="btn btn-default">
                                        <em class="fa fa-calendar"></em>
                                    </button>
                                </span>
                            </p>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3">
                            <label class="col-form-label">AML Category </label>
                            <br />
                            <select id="amlcategory_name" name="AML Category " ng-options="m.amlcategory_gid as m.amlcategory_name for m in amlcategory_list" chosen=""
                                    ng-model="cboamlcategory" class="chosen-select form-control" style="height:37px;"
                                    size="1">
                                <option value=""> -----Select AML Category-----</option>
                            </select>
                        </div>

                        <div class="col-lg-3 col-md-3 col-sm-3">
                            <label class="col-form-label">Business Category </label> <span style="color:red">*</span>
                            <br />
                            <select id="businesscategory_name" name="Business Category " required="" ng-options="m.businesscategory_gid as m.businesscategory_name for m in businesscategory_list" chosen=""
                                    ng-model="cbobusinesscategory" class="chosen-select form-control" style="height:37px;"
                                    size="1">
                                <option value=""> -----Select Business Category-----</option>
                            </select>
                        </div>

                        <br />
                    </div>
                </div>
                <br />

                <br />
                <div class="row ">
                    <div class="col-md-12 col-lg-12 col-sm-12">

                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <label class="col-form-label"> Contact Person First Name </label> <span style="color:red">*</span>
                            <br />
                            <input type="text" ng-model="contactperson_fn" required tabindex="0" class="form-control" />
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <label class="col-form-label"> Contact Person Middle Name </label>
                            <br />
                            <input type="text" ng-model="contactperson_mn" tabindex="0" class="form-control" />
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <label class="col-form-label"> Contact Person Last Name </label>
                            <br />
                            <input type="text" ng-model="contactperson_ln" tabindex="0" class="form-control" />
                        </div>
                        <br />
                    </div>
                </div>
                <br />
                <div class="col-lg-3 col-md-3 col-sm-3">
                    <label class="col-form-label">Designation </label> <span style="color:red">*</span>
                    <br />
                    <select id="designation_type" name="Designation " required="" ng-options="m.designation_gid as m.designation_type for m in designation_list" chosen=""
                            ng-model="cbodesignation" class="chosen-select form-control" style="height:37px;"
                            size="1">
                        <option value=""> -----Select Designation-----</option>
                    </select>
                </div>

                <div class="row ">
                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <label class="col-form-label">Bank Proof Type </label> <span style="color:red">*</span>
                        <br />
                        <select id="individualproof_name" name="Bank Proof Type " required="" ng-options="m.individualproof_gid as m.individualproof_name for m in individualproof_list" chosen=""
                                ng-model="cboindividualproof" class="chosen-select form-control" style="height:37px;"
                                size="1">
                            <option value=""> -----Select Bank Proof Type-----</option>
                        </select>
                    </div>

                    <div class="col-lg-2 col-sm-2 col-md-2">
                        <label class="col-form-label"><strong>MSME Registration</strong> <span style="color:red">*</span></label>
                        <div class="radio c-radio">
                            <label>
                                <input type="radio" id="msme_registration" name="msme_registration" value="Yes" ng-model="newDependency" required ng-change="onselectedDep_yes()" />
                                &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                            </label>
                            <label>
                                <input type="radio" id="msme_registration" name="msme_registration" value="No" ng-model="newDependency" required ng-change="onselectedDep_yes()" />
                                <span class="fa fa-circle"></span>No
                            </label>

                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-2 col-sm- col-md-2 form-group">
                            <div>
                                <label> MSME Registration Number <span style="color:red"></span></label>
                                <input type="text" ng-model="dependency_name" tabindex="0" class="form-control" />
                                <!--<textarea name="pagenameAdd" placeholder="Enter Dependency here..."
                                ng-model="msme_registration" class="form-control" rows="4"></textarea>-->
                            </div>
                        </div>
                    </div>

                    <br />


                </div>

                <div class="row">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <div class="panel">
                            <div class="panel-heading">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Customer GST Details  </b></p>
                                <hr />
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">

                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="col-form-label"> GST Number </label>
                                            <br />
                                            <input type="text" ng-model="txtgst_no" tabindex="0" class="form-control" maxlength="15" ng-pattern="/^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[1-9A-Z]{1}Z[0-9A-Z]{1}$/" ng-change="onchangegst_number()" />
                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label class="col-form-label"> GST State </label>
                                            <br />
                                            <input type="text" ng-model="txtgst_state" tabindex="0" class="form-control" disabled />
                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label>GST Registered </label>
                                            <div class="radio c-radio">
                                                <label>
                                                    <input type="radio" id="gstregister_status" name="gstregister_status" value="Yes" ng-model="rdbgstregister_status" ng-change="ngchange()" />
                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                </label>
                                                <label>
                                                    <input type="radio" id="gstregister_status" name="gstregister_status" value="No" ng-model="rdbgstregister_status" ng-change="ngchange()" />
                                                    <span class="fa fa-circle"></span>No
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3 align-top">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="gst_add()"><span class="fa fa-plus" /> Add </a>
                                                </div>
                                            </div>
                                            <div class="row" ng-show="mandatoryfields">
                                                <div class="col-sm-12 col-md-12 col-lg-12 text-left">
                                                    <label style="color:red;"> Enter All Mandatory Values</label>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                            <div class="table-responsive table-bordered">
                                                <div style="max-height:200px;overflow:auto;">
                                                    <table class="table">
                                                        <thead class="bg-gray text-white">
                                                            <tr>
                                                                <th>SNo</th>
                                                                <th>GST No</th>
                                                                <th>GST State</th>
                                                                <th>GST Registered</th>
                                                                <th>Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="list in customergst_list">
                                                                <td>{{$index+1}}</td>
                                                                <td>{{list.gst_no}}</td>
                                                                <td>{{list.gststate_name}}</td>
                                                                <td>{{list.gstregister_status}}</td>
                                                                <td class="text-nowrap">
                                                                    <button title="Edit" ng-click="gst_edit(list.customer2gst_gid)"
                                                                            class="btn btn-green btn-xs">
                                                                        <em class="fa fa-edit"></em>
                                                                    </button>

                                                                    <button title="Delete" ng-click="gst_delete(list.customer2gst_gid)"
                                                                            class="btn btn-danger btn-xs ">
                                                                        <em class="fa fa-trash-o"></em>
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-md-6 col-lg-6 col-sm-6">
                        <div class="panel">
                            <div class="panel-heading">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Contact Person Mobile Number  </b></p>
                                <hr />
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-sm-5 col-md-5 col-lg-5">
                                            <label><strong>Mobile Number </strong></label> <span style="color:red">*</span>
                                            <input ng-model="txtmobile_no" type="text" tabindex="0" maxlength="10" oninput="this.value = this.value.replace(/\D+/g, '')" 
                                                   aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched">

                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label>Primary Status </label> <span style="color:red">*</span>
                                            <div class="radio c-radio">
                                                <label>
                                                    <input type="radio" id="primarymobileno" name="primarymobile_no" value="Yes" ng-model="rdbprimarymobile_no" ng-change="ngchange()" />
                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                </label>
                                                <label>
                                                    <input type="radio" id="primarymobileno" name="primarymobile_no" value="No" ng-model="rdbprimarymobile_no" ng-change="ngchange()" />
                                                    <span class="fa fa-circle"></span>No
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label>Whatapp Number </label> <span style="color:red">*</span>
                                            <div class="radio c-radio">
                                                <label>
                                                    <input type="radio" id="whatsappmobileno" name="whatsappmobile_no" value="Yes" ng-model="rdbwhatsappmobile_no" ng-change="ngchange()" />
                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                </label>
                                                <label>
                                                    <input type="radio" id="whatsappmobileno" name="whatsappmobile_no" value="No" ng-model="rdbwhatsappmobile_no" ng-change="ngchange()" />
                                                    <span class="fa fa-circle"></span>No
                                                </label>
                                            </div>
                                        </div>

                                        <div class="col-lg-1 col-md-1 col-sm-1 align-top">
                                            <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="mobileno_add()"><span class="fa fa-plus" /></a>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                            <div class="table-responsive table-bordered">
                                                <div style="max-height:200px;overflow:auto;">
                                                    <table class="table">
                                                        <thead class="bg-gray text-white">
                                                            <tr>
                                                                <th>SNo</th>
                                                                <th>Mobile No</th>
                                                                <th>Primary Status</th>
                                                                <th>Whatapp Number </th>
                                                                <th>Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="list in mobileno_list">
                                                                <td>{{$index+1}}</td>
                                                                <td>{{list.mobile_no}}</td>
                                                                <td>{{list.primary_mobileno}}</td>
                                                                <td>{{list.whatsapp_mobileno}}</td>
                                                                <td class="text-nowrap">
                                                                    <button title="Edit" ng-click="mobileno_edit(list.customer2mobileno_gid)"
                                                                            class="btn btn-green btn-xs">
                                                                        <em class="fa fa-edit"></em>
                                                                    </button>

                                                                    <button title="Delete" ng-click="mobileno_delete(list.customer2mobileno_gid)"
                                                                            class="btn btn-danger btn-xs ">
                                                                        <em class="fa fa-trash-o"></em>
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-6 col-sm-6">
                        <div class="panel">
                            <div class="panel-heading">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Contact Person Email Address </b></p>
                                <hr />
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-sm-6 col-md-6 col-lg-6">
                                            <label><strong>Email Address </strong></label> <span style="color:red">*</span>
                                            <input type="text" ng-model="txtemail_address" ng-pattern="appemail_pattern" class="form-control">
                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label>Primary Status </label> <span style="color:red">*</span>
                                            <div class="radio c-radio">
                                                <label>
                                                    <input type="radio" id="emailaddress" name="primaryemail_address" value="Yes" ng-model="rdbprimaryemail_address" ng-change="ngchange()" />
                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                </label>
                                                <label>
                                                    <input type="radio" id="emailaddress" name="primaryemail_address" value="No" ng-model="rdbprimaryemail_address" ng-change="ngchange()" />
                                                    <span class="fa fa-circle"></span>No
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-sm-2 col-md-2 col-lg-2"></div>
                                        <div class="col-lg-1 col-md-1 col-sm-1 align-top">
                                            <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="emailaddress_add()"><span class="fa fa-plus" /></a>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="col-lg-12">
                                            <div class="table-responsive table-bordered">
                                                <table class="table">
                                                    <thead class="bg-gray text-white">
                                                        <tr>
                                                            <th>SNo</th>
                                                            <th>Email Address</th>
                                                            <th>Primary Status</th>

                                                            <th>Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in email_list">
                                                            <td>{{$index+1}}</td>
                                                            <td>{{data.email_address}}</td>
                                                            <td>{{data.primary_emailaddress}}</td>
                                                            <td class="text-nowrap">
                                                                <button title="Edit" ng-click="emailaddress_edit(data.customer2emailaddress_gid)"
                                                                        class="btn btn-green btn-xs">
                                                                    <em class="fa fa-edit"></em>
                                                                </button>

                                                                <a class="btn btn-xs  btn-danger" ng-click="emailaddress_delete(data.customer2emailaddress_gid)"><span class="fa fa-trash-o"></span></a>

                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>

                        </div>
                    </div>
                </div>

                <div class="panel">
                    <div class="panel-heading">
                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Address Details </b></p>
                        <hr />
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-sm-4 col-md-4 col-lg-4">

                                    <strong class="ng-binding w3-text-indigo">
                                        <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="address_add()"><span class="fa fa-plus" />  Add Address</a>
                                    </strong>
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
