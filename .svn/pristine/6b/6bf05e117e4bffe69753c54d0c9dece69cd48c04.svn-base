
<h3>
    Add Product
</h3>
<div ng-controller="MstProductAddController">
    <div class="panel panel-primary">
        <div class="panel-heading">
            &nbsp;<span class="btn-label bg-primary">
                <i class="fa fa-plus "></i>
            </span> Add Product
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="addproduct" ng-submit="form.addproduct()" autocomplete="off" class="form-validate form-horizontal">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="row ">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <br />
                                    <label>Product Code &nbsp;<span style="color:red">*</span></label>

                                    <input ng-model="txtproduct_code" required=""
                                           tabindex="1" aria-required="true" aria-invalid="true" maxlength="200" class="form-control">

                                </div>
                                <div class="col-lg-8 col-md-8 col-sm-8">
                                    <br />
                                    <label>Product Name &nbsp;<span style="color:red">*</span></label>
                                    <input ng-model="txtproduct_name" required=""
                                           tabindex="1" aria-required="true" aria-invalid="true" maxlength="1000" class="form-control">
                                </div>
                            </div>
                        </div>
                        <div class="row ">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <br />
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label"> Sector <span style="color:red">*</span></label>
                                    <br />
                                    <select name="Sector" id="Sector" ng-options="m.businessunit_name for m in sector_list" chosen=""
                                            ng-model="cbosector" class="chosen-select form-control" required style="height:37px;">
                                        <option value=""></option>
                                    </select>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label"> Category <span style="color:red">*</span></label>
                                    <br />
                                    <select name="Category" id="Category" ng-options="m.valuechain_name for m in category_list" chosen=""
                                            ng-model="cbocategory" class="chosen-select form-control" required style="height:37px;">
                                        <option value=""></option>
                                    </select>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label"> CSA Category <span style="color:red">*</span></label>
                                    <br />
                                    <select name="CSACategory" id="CSACategory" ng-options="m.category_name for m in csacategory_list" chosen=""
                                            ng-model="cbocsacategory" class="chosen-select form-control" required style="height:37px;">
                                        <option value=""></option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="row ">
                            <br />
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="dd-handle ng-scope">
                                    <label class="text-primary" style="font-size:16px;">Variety / Commodity Details </label>
                                    <hr />
                                    <div class="row ">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                <label class="col-form-label"> Variety / Commodity <span style="color:red">*</span></label>

                                               <input type="text" name="variety_name" id="variety_name" ng-model="txtvariety_name"
                                                                    aria-invalid="true" maxlength="500" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                 <label class="col-form-label"> Botanical  Name </label>

                                               <input type="text" name="botanical_name" id="botanical_name" ng-model="txtbotanical_name"
                                                                    aria-invalid="true" maxlength="500" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                <label class="col-form-label"> Alternative Name  </label>

                                                <input type="text" name="alternative_name" id="alternative_name" ng-model="txtalternative_name"
                                                       aria-invalid="true" maxlength="500" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                <label class="col-form-label"> HSN code  </label>

                                                <input type="text" name="HSN_code" id="HSN_code" ng-model="txtHSN_code"
                                                       aria-invalid="true" maxlength="500" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                            </div> 
                                        </div>
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                <label class="col-form-label"> Type of Supply/Nature <span style="color:red">*</span></label>
                                                <select name="Vertical" id="Vertical" chosen="" ng-options="m.typeofsupplynature_name for m in typeofsupplynature_list"
                                                        ng-model="cbotypeofsupplynature" class="chosen-select form-control">
                                                    <option value=""></option>
                                                </select>
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                <label class="col-form-label"> Sector Classification <span style="color:red">*</span></label>
                                                <select name="Vertical" id="Vertical" chosen="" ng-options="m.sectorclassification_name for m in sectorclassification_list"
                                                        ng-model="cbosectorclassification" class="chosen-select form-control">
                                                    <option value=""></option>
                                                </select>
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                <label class="col-form-label"> Sector </label>
                                                <br />
                                                <select name="Sector" id="Sector" ng-options="m.businessunit_name for m in sector_list" chosen=""
                                                        ng-model="cboCommoditySector" class="chosen-select form-control" style="height:37px;" size="1">
                                                    <option value=""></option>
                                                </select>
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                <label class="col-form-label"> Industry/Category </label>
                                                <br />
                                                <select name="Category" id="Category" ng-options="m.valuechain_name for m in category_list" chosen=""
                                                        ng-model="cboCommodityCategory" class="chosen-select form-control" style="height:37px;" size="1">
                                                    <option value=""></option>
                                                </select>
                                            </div>
                                           
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <br />
                                            <div class="col-sm-9 col-md-9 col-lg-9">
                                                <label class="col-form-label"> Heading Description/Product </label>
                                                <input type="text" name="HSN_code" id="HSN_code" ng-model="txtCommodityheadingdesc"
                                                       aria-invalid="true" maxlength="500" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                            </div>
                                            <div class="col-sm-1 col-md-1 col-lg-1" style="padding-top:7px;">
                                                <br />
                                                <button class="btn btn-sm btn-default" style="background-color:#09385f;color:white;padding-left:9px;"
                                                        ng-click="variety_add()" ng-disabled="(cbotypeofsupplynature && cbosectorclassification && txtvariety_name) ?  false : true">
                                                    <span class="fa fa-plus" />
                                                </button>
                                            </div>
                                        </div>
                                    </div> 
                                    <div class="row">
                                        <br />
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="col-lg-12 col-md-12 col-sm-12">

                                                <table class="table">
                                                    <thead class="bg-gray text-white">
                                                        <tr>
                                                            <th>S.No</th>
                                                            <th> One API Code</th>
                                                            <th class="col-lg-6 col-md-6 col-sm-6 text-center">Variety / Commodity Details</th>
                                                            <th>Heading Description/Product</th>
                                                            <th>Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in variety_list">
                                                            <td>{{$index+1}}</td>
                                                            <td data-title="'One API Code'">{{data.api_code}}</td>
                                                            <td>
                                                                <div class="row">
                                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                                        <div class="col-lg-5 col-md-5 col-sm-5">
                                                                            <p><b>Variety / Commodity: </b>{{data.variety_name}} </p>
                                                                            <p><b>Botanical  Name: </b>{{data.botanical_name}} </p>
                                                                            <p><b>Alternative Name: </b>{{data.alternative_name}} </p>
                                                                            <p><b>HSN code: </b>{{data.hsn_code}} </p>
                                                                        </div>
                                                                        <div class="col-lg-5 col-md-5 col-sm-5">
                                                                            <p><b>Type of Supply/Nature: </b>{{data.typeofsupplynature_name}} </p>
                                                                            <p><b>Sector Classification: </b>{{data.sectorclassification_name}} </p>
                                                                            <p><b>Sector: </b>{{data.varietysector_name}} </p>
                                                                            <p><b>Industry/Category: </b>{{data.varietycategory_name}} </p>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </td>
                                                            <td>{{data.headingdesc_product}}</td>
                                                            <td class="text-nowrap">
                                                                <button title="GST Status" ng-click="varity_addgststatus(data.variety_gid)"
                                                                        class="btn btn-primary btn-xs ">
                                                                    <em class="fa fa-money"></em>
                                                                </button>
                                                                <button title="Trade Product Details" ng-click="varity_addtradeproduct(data.variety_gid)"
                                                                        class="btn btn-green btn-xs ">
                                                                    <em class="fa fa-product-hunt"></em>
                                                                </button>
                                                                <button title="Document Upload" ng-click="varity_adddocumentupload(data.variety_gid)"
                                                                        class="btn btn-info btn-xs ">
                                                                    <em class="fa fa-upload"></em>
                                                                </button>
                                                                <button title="Delete" ng-click="varity_delete(data.variety_gid)"
                                                                        class="btn btn-danger btn-xs ">
                                                                    <em class="fa fa-trash-o"></em>
                                                                </button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                            <br />
                                        </div>
                                        <br />
                                    </div>
                                    <br />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <br />
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="addproduct.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                </div>
                <br />
            </form>
        </div>
    </div>

    <script type="text/ng-template" id="/AddGSTStatus.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-money"></em>&nbsp;&nbsp;<label style="font-size:16px"> GST Status </label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="userform1" ng-submit="form.userform1()" autocomplete="off" class="form-validate mb-lg">

                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> IGST % <span style="color:red"> *</span></label>
                                            <br />
                                            <input type="number" ng-model="txtIGST_number" min="0" required tabindex="0" class="form-control" maxlength="10" />
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> SGST % <span style="color:red"> *</span></label>
                                            <br />
                                            <input type="number" ng-model="txtSGST_number" min="0" required tabindex="0" class="form-control" maxlength="10" />
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> CGST % <span style="color:red"> *</span></label>
                                            <br />
                                            <input type="number" ng-model="txtCGST_number" min="0" required tabindex="0" class="form-control" maxlength="10" />
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-6 col-md-6 col-sm-6">
                                            <label class="col-form-label"> Cess % <span style="color:red"> *</span></label>
                                            <br />
                                            <input type="number" ng-model="txtCess_number" min="0" required tabindex="0" class="form-control" maxlength="10" />
                                        </div>
                                        <div class="col-lg-6 col-md-6 col-sm-6">
                                            <label class="col-form-label"> w.e.f Date <span style="color:red"> *</span></label>
                                            <br />
                                            <p class="input-group">
                                                <input type="text" id="businessstart_date" name="businessstart_date" ng-model="txtwef_date" datepicker-popup="{{format}}"
                                                       style="height:36px;" is-open="open01" min="minDate" max="9999-12-31" datepicker-options="dateOptions"
                                                       close-text="Close" class="form-control" required placeholder="DD-MM-YYYY" />
                                                <span class="input-group-btn">
                                                    <button type="button" ng-click="calender01($event)" style="height:36px;" class="btn btn-default">
                                                        <em class="fa fa-calendar"></em>
                                                    </button>
                                                </span>
                                            </p>
                                        </div>

                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12 text-center">
                                        <button class="btn btn-sm btn-default" ng-disabled="userform1.$invalid" style="background-color:#09385f;color:white;"
                                                ng-click="submit()">
                                            <span class="fa fa-plus" />&nbsp;Add
                                        </button>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="table-responsive table-bordered">
                                                <div style="max-height:200px;overflow:auto;">
                                                    <table class="table">
                                                        <thead class="bg-gray text-white">
                                                            <tr>
                                                                <th>S.No</th>
                                                                <th>IGST %</th>
                                                                <th>SGST %</th>
                                                                <th>CGST %</th>
                                                                <th>Cess %</th>
                                                                <th>w.e.f date</th>
                                                                <th>Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in commoditygststatuslist">
                                                                <td>{{$index+1}}</td>
                                                                <td>{{data.IGST_percent}}</td>
                                                                <td>{{data.SGST_percent}}</td>
                                                                <td>{{data.CGST_percent}}</td>
                                                                <td>
                                                                    {{data.CESS_percent}}
                                                                </td>
                                                                <td> {{data.wef_date}}</td>
                                                                <td class="text-nowrap">
                                                                    <button title="Delete" ng-click="deletegst_details(data.commoditygststatus_gid)"
                                                                            class="btn btn-danger btn-xs">
                                                                        <em class="fa fa-trash-o"></em>
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />

                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>

    <script type="text/ng-template" id="/AddTradeProductDetails.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-product-hunt"></em>&nbsp;&nbsp;<label style="font-size:16px"> Trade Product Details </label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="tradeproduct" ng-submit="form.tradeproduct()" autocomplete="off" class="form-validate mb-lg">

                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-sm-6 col-md-6 col-lg-6">
                                            <label class="col-form-label"> Product <span style="color:red"> *</span></label>
                                            <select name="Vertical" id="Vertical" chosen="" required ng-options="m.loanproduct_name for m in Productlist"
                                                    ng-model="cboloanProduct" class="chosen-select form-control" ng-change="Onchangeproducttype()">
                                                <option value=""></option>
                                            </select>
                                        </div>
                                        <div class="col-sm-6 col-md-6 col-lg-6">
                                            <label class="col-form-label"> Sub Product <span style="color:red"> *</span></label>
                                            <select name="Vertical" id="Vertical" chosen="" required ng-options="m.loansubproduct_name for m in loansubproductlist"
                                                    ng-model="cboloansubproduct" class="chosen-select form-control">
                                                <option value=""></option>
                                            </select>
                                        </div>

                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-sm-6 col-md-6 col-lg-6">
                                            <label class="col-form-label"> Insurance company name </label>
                                            <select name="Vertical" id="Vertical" chosen="" ng-options="m.insurancecompany_name for m in insurancecompany_list"
                                                    ng-model="cboinsurancecompany" class="chosen-select form-control" ng-change="Onchangeinsurancecompany()">
                                                <option value=""></option>
                                            </select>
                                        </div>
                                        <div class="col-sm-6 col-md-6 col-lg-6">
                                            <label class="col-form-label"> Policy Name/Number <span ng-show="insurancecompanyselect" style="color:red"> *</span></label>
                                            <select name="Vertical" id="Vertical" chosen="" ng-options="m.policy_name for m in insurancePolicy_list"
                                                    ng-model="cboinsurancepolicy" class="chosen-select form-control" ng-required="insurancecompanyselect">
                                                <option value=""></option>
                                            </select>
                                        </div>
                                    </div>

                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12 text-center">
                                        <button class="btn btn-sm btn-default" ng-disabled="tradeproduct.$invalid" style="background-color:#09385f;color:white;"
                                                ng-click="tradedetailsubmit()">
                                            <span class="fa fa-plus" />&nbsp;Add
                                        </button>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="table-responsive table-bordered">
                                                <div style="max-height:200px;overflow:auto;">
                                                    <table class="table">
                                                        <thead class="bg-gray text-white">
                                                            <tr>
                                                                <th>S.No</th>
                                                                <th>Product</th>
                                                                <th>Sub Product</th>
                                                                <th>Insurance company name</th>
                                                                <th>Policy Name/Number</th>
                                                                <th>Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in commodityTradeProdctlist">
                                                                <td>{{$index+1}}</td>
                                                                <td>{{data.product_name}}</td>
                                                                <td>{{data.subproduct_name}}</td>
                                                                <td>{{data.insurancecompany_name}}</td>
                                                                <td>{{data.insurancepolicy_name}}</td>
                                                                <td class="text-nowrap">
                                                                    <button title="Delete" ng-click="deletetrade_details(data.commoditytradeproductdtl_gid)"
                                                                            class="btn btn-danger btn-xs">
                                                                        <em class="fa fa-trash-o"></em>
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />

                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>

    <script type="text/ng-template" id="/AddDocumentUpload.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-upload"></em>&nbsp;&nbsp;<label style="font-size:16px"> Document Upload </label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="DocumentUploadform" ng-submit="form.DocumentUploadform()" autocomplete="off" class="form-validate mb-lg">

                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label>As on date <span style="color:red"> *</span></label>
                                            <br />
                                            <p class="input-group">
                                                <input type="text" id="txtason_date" name="ason_date" ng-model="txtason_date" datepicker-popup="{{format}}"
                                                       style="height:36px;" is-open="open01" min-date="minDate" datepicker-options="dateOptions"
                                                       close-text="Close" class="form-control" required placeholder="DD-MM-YYYY" />
                                                <span class="input-group-btn">
                                                    <button type="button" ng-click="calender01($event)" style="height:36px;" class="btn btn-default">
                                                        <em class="fa fa-calendar"></em>
                                                    </button>
                                                </span>
                                            </p>
                                        </div>
                                        <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label class="col-form-label"> Commodity Report <span style="color:red"> *</span></label>
                                            <br />
                                            <input type="file" id="commodityfile" tabindex="5" multiple onchange="angular.element(this).scope().CommodityReportDocUpload(this.files, angular.element(this).scope().documentupload, this.name)"
                                                   ng-model="docCommodityReport" data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                        </div>
                                        <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label class="col-form-label"> Risk Analysis Report <span style="color:red"> *</span></label>
                                            <br />
                                            <input type="file" id="docRiskAnalysisfile" tabindex="5" multiple onchange="angular.element(this).scope().RiskAnalysisDocUpload(this.files, angular.element(this).scope().documentupload, this.name)"
                                                   ng-model="docRiskAnalysisreport" data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12 text-center">
                                        <button ng-disabled="DocumentUploadform.$invalid" class="btn btn-sm btn-default" style="background-color:#09385f;color:white;"
                                                ng-click="uploadsubmit()">
                                            <span class="fa fa-plus" />&nbsp;Add
                                        </button>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="table-responsive table-bordered">
                                                <div style="max-height:200px;overflow:auto;">
                                                    <table class="table">
                                                        <thead class="bg-gray text-white">
                                                            <tr>
                                                                <th>S.No</th>
                                                                <th>As on date</th>
                                                                <th>Commodity Report
                                                                 <a download="" href="" title="Download All" id="btnpdf3" ng-click="commoditydownloadall()">
                                                                                <em class="icon-arrow-down"></em>&nbsp;
                                                                            </a>
                                                                </th>
                                                                <th>Risk Analysis Report
                                                                    <a download="" href="" title="Download All" id="btnpdf3" ng-click="riskdownloadall()">
                                                                        <em class="icon-arrow-down"></em>&nbsp;
                                                                    </a>
                                                                </th>
                                                                <th>Uploaded By</th>
                                                                <th>Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in commodityDocumentUpload">
                                                                <td class="text-nowrap">{{$index+1}} </td>
                                                                <td class="text-nowrap">{{data.ason_date}}
                                                                </td>
                                                                <td class ="text-nowrap">
                                                                    <a download="" href="" id="btnpdf"
                                                                       ng-click="doc_downloads(data.commodityreport_filepath,data.commodityreport_filename)">{{data.commodityreport_filename}}</a>
                                                                    <button title="View" class="btn btn-info btn-xs" ng-click="documentviewer(data.commodityreport_filepath,data.commodityreport_filename)">
                                                                        <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                                    </button>
                                                                    </td>
                                                                    <td class="text-nowrap">
                                                                        <a download="" href="" ng-click="doc_downloads(data.riskanalysisreport_filepath,data.riskanalysisreport_filename)"
                                                                           id="btnpdf">{{data.riskanalysisreport_filename}}</a>
                                                                        <button title="View" class="btn btn-info btn-xs" ng-click="documentviewer(data.riskanalysisreport_filepath,data.riskanalysisreport_filename)">
                                                                            <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                                        </button>
                                                                    </td>
                                                                    <td>{{data.created_by}}</td>
                                                                    <td class="text-nowrap">
                                                                        <button title="Delete" ng-click="deletedoc_details(data.commoditydocument_gid)"
                                                                                class="btn btn-danger btn-xs">
                                                                            <em class="fa fa-trash-o"></em>
                                                                        </button>
                                                                    </td>
</tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />

                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>
</div>   