<h3>
    I-Assign Compose Mail 360

    <a href="#/app/iasnWomWorkOrderSummary">
        <button type="button" title="Back to Home" class="pull-right btn btn-sm btn-primary ">
            <em class="fa fa-arrow-circle-left fa-1x"></em>
        </button>
    </a>
</h3>
<style>
    #bordersty {
        border: 1px solid #ebebeb;
        padding: 10px;
        border-radius: 25px;
    }
</style>
<div ng-controller="iasnTrnComposeMail360">

    <div class="card">
        <br>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div id="bordersty" style="background-color: #FAF8F8;">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-3 col-md-3 col-sm-3">

                                    <label style="font-size:14px;" class="text-primary">
                                        Email To
                                    </label>
                                    <br>
                                    <span style="font-size:13px;">
                                        {{tomail_id}}
                                    </span>

                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3">

                                    <label style="font-size:14px;" class="text-primary">
                                        Email Sent Date
                                    </label>

                                    <br>
                                    <span style="font-size:13px;">
                                        {{email_date}}
                                    </span>

                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3">

                                    <label style="font-size:14px;" class="text-primary">
                                        Status
                                    </label>

                                    <br>
                                    <span style="font-size:13px;">
                                        {{email_status}}
                                    </span>

                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3">
                                    <label style="font-size:14px;" class="text-primary">
                                        Log Details
                                    </label>

                                    <br>
                                    <button class="btn btn-info" ng-click="logdetails()">
                                        <em class="fa fa-history"></em>

                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br>

                </div>

            </div>

        </div>
        <div class="row" ng-show="IsLogShow">
            <div class="col-lg-12 col-sm-12 col-md-12">
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <div class="panel">

                        <div class="panel-body ">
                            <div class="pd-sm-10">
                                <div class="row" ng-hide="transfershow">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                           
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12  ">
                                                    <p class="lead bb text-purple text-bold" style="font-size:16px;">Audit Log</p>

                                                </div>
                                            </div>

                                            <div class="table-responsive">
                                                <div style="max-height:300px;overflow:auto;">
                                                    <table class="table table-striped table-secondary">
                                                        <thead class="bg-gray text-white">
                                                            <tr width="100%">
                                                                <th align="center" scope="col">S.No</th>
                                                                <th align="center" scope="col">Workitem Response</th>
                                                                <th align="center" scope="col">Done By/On</th>

                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in auditlog_list" width="100%">
                                                                <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                                                <td align="left" scope="col" title="Workitem Response">{{data.action_taken}}</td>
                                                                <td align="left" scope="col" title="Done By/On">{{data.created_by}}/{{data.created_date}}</td>

                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <br>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="btn-group pull-right" >
                        <button type="button" class="btn  btn-sm" ng-click="signature()" title="Email Signature">
                            <em class="fa fa-clipboard text-info"></em> &nbsp;Signature
                        </button>
                        <button type="button" class="btn  btn-sm" ng-click="forward(composemail_gid,email_subject,tomail_id,ccmail_id,bccmail_id)" title="Forward">
                            <em class="fa fa-mail-forward (alias) text-info"></em>&nbsp;Forward
                        </button>
                        <button type="button" class="btn  btn-sm" ng-click="archivalWI(composemail_gid)" title="Archival">
                            <em class="fa fa-archive text-info"></em>&nbsp;Archival
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <br />
    </div>

    <div class="card" ng-repeat="data in referenceMail">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <table class="table mb-mails mb-view">
                        <tbody>
                            <tr>
                                <td>
                                    <div class="clearfix">
                                        <a href="#" class="pull-left">
                                            <img alt="" ng-src="{{user.picture}}"
                                                 class="media-box-object img-rounded thumb32 mb-mail-avatar" />
                                        </a>

                                        <div class="media-box-body">
                                            <span style="font-size:15px;">
                                                {{data.email_from}}
                                            </span><br>



                                            <span>
                                                <b>Sent:</b>
                                            </span>
                                            <span> {{data.email_date}} </span><br>
                                            <span>
                                                <b>To:</b> {{data.email_to}}
                                            </span><br>
                                            <span>
                                                <b>Cc:</b> {{data.email_cc}}
                                            </span><br>
                                            <span>
                                                <b>Bcc:</b> {{data.email_bcc}}
                                            </span>
                                        </div>

                                    </div>

                                    <div class="mb-mail-preview mt-lg">
                                        <h5>Subject:{{data.emailsubject}}</h5>
                                    </div>
                                    <p ng-bind-html=trustAsHtml(data.email_content)></p>
                                    <div class="mb-attachment">

                                        <ul class="list-inline">
                                            <li ng-repeat="att in data.MdlAttachmentList" style="padding:5px">
                                                <a ng-click="export(att.document_path,att.document_name)">
                                                    <em class="icon-arrow-down"></em>&nbsp; {{att.document_name}}
                                                </a>
                                            </li>
                                        </ul>
                                    </div>

                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>

        </div>
    </div>

    <div class="card">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <table class="table mb-mails mb-view">
                        <tbody>
                            <tr>
                                <td>
                                    <div class="clearfix">

                                        <a href="#" class="pull-left">
                                            <img alt="" ng-src="{{user.picture}}"
                                                 class="media-box-object img-rounded thumb32 mb-mail-avatar" />
                                        </a>

                                        <div class="media-box-body">
                                            <span style="font-size:15px;">
                                                {{frommail_id}}
                                            </span><br>

                                            <span>
                                                <b>Sent:</b>
                                            </span>
                                            <span> {{email_date}} </span><br>
                                            <span>
                                                <b>To:</b> {{tomail_id}}
                                            </span><br>
                                            <span>
                                                <b>Cc:</b> {{ccmail_id}}
                                            </span><br>
                                          
                                        </div>

                                    </div>

                                    <div class="mb-mail-preview mt-lg">
                                        <h5>Subject:{{email_subject}}</h5>
                                    </div>
                                    <p ng-bind-html=trustAsHtml(mailcontent)></p>
                                    <div class="mb-attachment">
                                        <ul class="list-inline">
                                            <li ng-repeat="att in attach_list">
                                                <a ng-click="export(att.document_path,att.document_name)">
                                                    <em class="icon-arrow-down"></em>&nbsp;  {{att.document_name}}
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>

        </div>
    </div>
    <br>
    <div>
        <div class="text-center">

            <button ng-click="back()" class="btn btn-info">
                <i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back
            </button>
        </div>
    </div>

    <script type="text/ng-template" id="/signatureContent.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-clipboard"></em>&nbsp; Email Signature</h2>
                <span flex></span>
                <md-button class="md-icon-button" ng-click="close()" title="Close">
                    <md-icon md-svg-src="app/img/icons/ic_close_24px.svg" aria-label="Close dialog"></md-icon>
                </md-button>
            </div>
        </md-toolbar>
        <div class="modal-body ng-scope">
            <div class="pd-sm-10">

                <div class="row">
                    <form name="mappingEditform" id="mappingEditform" novalidate="" class="form-validate mb-lg" autocomplete="off">

                        <div class="col-lg-12 col-sm-12 col-md-12">


                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <label> Email Signature</label>

                                    <div text-angular="" ng-model="EmailSignature"
                                         placeholder="Type Email Signature..." name="wysiswyg-editor" class="btn-group-justified"></div>
                                </div>
                            </div>
                            <br />
                            <div>
                                <div class="text-center">
                                    <button class="btn btn-success" ng-click="submit()" ng-disabled="!EmailSignature">
                                        <em class="fa fa-check"></em>
                                        <span>Submit</span>
                                    </button>
                                    <a class="btn btn-danger" ng-click="ok()">
                                        <em class="icon icon-close"></em>
                                        <span>Cancel</span>
                                    </a>
                                </div>
                            </div>
                        </div>

                    </form>
                </div>

            </div>

        </div>
    </script>

    <script type="text/ng-template" id="/archivalContent.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-archive"></em>&nbsp;Archival Work Item</h2>
                <span flex></span>

            </div>
        </md-toolbar>
        <div class="modal-body ng-scope">

            <form name="documentAddform" id="documentAddform" novalidate="" class="form-validate mb-lg" autocomplete="off">


                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">

                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <label><strong>Archival Type </strong></label>
                            <div class="radio c-radio">
                                <label>
                                    <input type="radio" name="types_of_archival" value="Customer" ng-checked="true" ng-click="onclickcustomer()"
                                           ng-change="onchangearchival(types_of_archival)"
                                           ng-model="archival.types_of_archival" />
                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Customer
                                </label>
                                <label>
                                    <input type="radio" name="types_of_archival" ng-change="onchangearchival(types_of_archival)"
                                           value="Specific" ng-model="archival.types_of_archival" ng-click="onclickspecific()" />
                                    <span class="fa fa-circle"></span>Specific
                                </label>

                            </div>

                        </div>

                    </div>
                </div>

                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <label class="col-form-label control-label"><strong>Customer <span style="color:red">*</span></strong></label>
                            <input type="text" name="customer" id="customer" ng-model="customer" required
                                   ng-keyup="complete(customer)" class="form-control" placeholder="Search Customer Name Here..." />

                            <ul class="list-group panel-scroll" style="height:100%; word-wrap: break-word;">
                                <li class="list-group-item" ng-repeat="m in customer_list"
                                    ng-click="fillTextbox(m.customer_gid,m.customername)">{{m.customername}}</li>
                            </ul>

                            <label style="color:red;">{{message}}</label>
                        </div>
                    </div>
                </div>
                <div class="row" ng-show="PnlSpecific">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-12 col-md-12 col-sm-12">

                            <label class="col-form-label control-label">
                                <strong>
                                    Sanction Ref No.
                                    <span style="color:red">*</span>
                                </strong>
                            </label>

                            <select id="sanction" name="sanction" chosen=""
                                    ng-options="m.sanctionrefno for m in sanctiondtl"
                                    ng-model="cbosanctionrefno" class="form-control">
                                <option value="">---Select Sanction Ref No.---</option>
                            </select>

                        </div>
                    </div>

                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">

                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <label class="col-form-label">Remarks &nbsp;<span style="color:red">*</span></label>
                            <textarea ng-model="archival.Remarks" required placeholder="Enter the Remarks" rows="3" maxlength="1000" class="form-control"
                                      id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                        </div>

                    </div>
                </div>
                <br />

                <div>
                    <div class="text-center">
                        <a class="btn btn-success" ng-click="ArchivalSubmit()" ng-disabled="documentAddform.$invalid">
                            <em class="fa fa-check"></em>
                            <span>Submit</span>
                        </a>
                        <button ng-click="close()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
            </form>
        </div>



    </script>

</div>