<h3>
    Case Allocation View
    <a href="#/app/caseAllocation">
        <button type="button" title="Back" class="pull-right btn btn-sm btn-info">
            <em class="fa fa-arrow-circle-left fa-1x"></em>
        </button>
    </a>
</h3>
<div ng-controller="caseAllocationViewcontroller">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <em class="fa fa-eye"></em>&nbsp; View Allocation Details
        </div>
        <div class="panel-body">
            <div class="row">
                <form name="sanctionform" id="sanctionform" novalidate="" class="form-validate mb-lg" autocomplete="off">

                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="dd-handle ng-scope bg-indigo-50">
                            <div class="row col-lg-12 col-sm-12 col-md-12  text-center">
                                <!--<label>Customer Name :</label>-->
                                <label class="text-primary" style="font-size:16px;">{{customerdetails.customerNameedit}} / <span class="text-pink">{{customer_urn}}</span></label>
                               
                            </div>
                            <br /><br />
                           
                            <div class="row">
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    <label>Requested By </label>
                                    <p>{{assigned_by}}</p>
                                </div>
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    <label>Requested Date </label>
                                    <p>{{assigned_date}}</p>
                                </div>
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    <label>Target Date </label>
                                    <p>{{target_date}}</p>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <label>Requested Remarks </label>
                                    <p ng-bind-html="AllocateExtRemarks"><p>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <label>Attached Document </label>
                                    <br />
                                    <p ng-show="documentNotUpload">---No Document Attached---</p>
                                    <div class="table-responsive table-bordered" ng-show="documentUpload">
                                        <table class="table">
                                            <!--<thead class="bg-indigo-300">
                                                <tr>
                                                    <th>S.No</th>
                                                    <th>Document Name</th>
                                                    <th>Action</th>
                                                </tr>
                                            </thead>-->
                                            <tbody>
                                                <tr ng-repeat="document in upload_list">
                                                    <td>{{$index+1}}</td>
                                                    <td>{{document.document_name}}</td>
                                                    <td>
                                                        <button title="Download" class="btn btn-green btn-sm"
                                                                ng-click="downloads(document.document_path,document.document_name)">
                                                            <em class="fa fa-download"></em>
                                                        </button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                
                            </div>
                            
                        </div>

                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div>
                                    <br />
                                    <div class="panel-body">
                                        <div class="container-fluid">
                                            <div class="row">
                                                <h4 class="page-header mt0 ng-scope text-primary">Customer Details</h4>
                                                <div class="animated fadeInLeft anim-delay-5">
                                                    <div class="row">
                                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                                            <div class="col-lg-4">
                                                                <label class="col-form-label">Customer Name</label>
                                                                <br />
                                                                <p style="font-size:14px;">{{customerdetails.customerNameedit}}</p>
                                                            </div>
                                                            <div class="col-lg-4">
                                                                <label class="col-form-label">URN No.</label>
                                                                <br />
                                                                <p style="font-size:14px;">{{customerdetails.customer_urnedit}}</p>
                                                            </div>
                                                            <div class="col-lg-4">
                                                                <label class="col-form-label">Contact Person</label>
                                                                <br />
                                                                <p style="font-size:14px;">{{customerdetails.contactPersonedit}}</p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                                            <div class="col-lg-4">
                                                                <label class="col-form-label">Contact Number</label>
                                                                <br />
                                                                <p style="font-size:14px;">{{customerdetails.mobileNoedit}}</p>
                                                            </div>
                                                            <div class="col-lg-4">
                                                                <label class="col-form-label">Address Line 1</label>
                                                                <br />
                                                                <p style="font-size:14px;">{{customerdetails.addressline1edit}}</p>
                                                            </div>
                                                            <div class="col-lg-4">
                                                                <label class="col-form-label">Address Line 2</label>
                                                                <br />
                                                                <p style="font-size:14px;">{{customerdetails.addressline2edit}}</p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                                            <div class="col-lg-4">
                                                                <label class="col-form-label">Vertical</label>
                                                                <br />
                                                                <p style="font-size:14px;">{{customerdetails.vertical_code}}</p>
                                                            </div>
                                                            <div class="col-lg-4">
                                                                <label class="col-form-label">Zonal Head</label>
                                                                <br />
                                                                <p style="font-size:14px;">{{customerdetails.zonal_name}}</p>
                                                            </div>
                                                            <div class="col-lg-4">
                                                                <label class="col-form-label">Business Head</label>
                                                                <br />
                                                                <p style="font-size:14px;">{{customerdetails.businesshead_name}}</p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">

                                                            <div class="col-lg-4">
                                                                <label class="col-form-label">Cluster Manager</label>
                                                                <br />
                                                                <p ng-model="vertical_code" style="font-size:14px;">{{customerdetails.cluster_manager_name}}</p>
                                                            </div>
                                                            <div class="col-lg-4">
                                                                <label class="col-form-label">Relationship Manager</label>
                                                                <br />
                                                                <p style="font-size:14px;">{{customerdetails.relationshipmgmt_name}}</p>
                                                            </div>
                                                            <div class="col-lg-4">
                                                                <label class="col-form-label">Credit Manager</label>
                                                                <br />
                                                                <p ng-model="vertical_code" style="font-size:14px;">{{customerdetails.creditmanager_name}}</p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <label class="ng-scope text-primary">Customer Gurantors</label>
                                                <div class="col-lg-12">
                                                    <div class="table-responsive table-bordered">
                                                        <table class="table" style="padding: 0px 0px">
                                                            <thead class="bg-indigo-300">
                                                                <tr>
                                                                    <th>S.No</th>
                                                                    <th>Gurantors Name</th>
                                                                    <th>Age</th>
                                                                    <th>Net Worth</th>
                                                                    <th>Basic of NW</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="guarantordata in customerguarantorlist">
                                                                    <td>{{$index+1}}</td>
                                                                    <td>{{guarantordata.guarantors_name}}</td>
                                                                    <td>{{guarantordata.guarantor_age}}</td>
                                                                    <td>{{guarantordata.networth}}</td>
                                                                    <td>{{guarantordata.basisofNW}}</td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <label class="ng-scope text-primary">Customer Promotors</label>
                                                <div class="col-lg-12">
                                                    <div class="table-responsive table-bordered">
                                                        <table class="table">
                                                            <thead class="bg-indigo-300">
                                                                <tr>
                                                                    <th>S.No</th>
                                                                    <th>Promotor Name</th>
                                                                    <th>Designation</th>
                                                                    <th>Age</th>
                                                                    <th>Mobile No.</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="promotordata in customerPromotorlist">
                                                                    <td>{{$index+1}}</td>
                                                                    <td>{{promotordata.promoter_name}}</td>
                                                                    <td>{{promotordata.designation}}</td>
                                                                    <td>{{promotordata.promoter_age}}</td>
                                                                    <td>{{promotordata.mobile}}</td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>

                                            </div>
                                            
                                            <br />
                                            <div class="row">
                                                <label class="ng-scope text-primary">Collateral Details</label>

                                                <div class="col-lg-12" ng-repeat="collateral in customerCollateral">
                                                    <a class="list-group-item">
                                                        <div class="media-box">

                                                            <div class="media-box-body clearfix">

                                                                <strong class="media-box-heading text-primary">
                                                                    <span class="circle circle-pink circle-lg text-left"></span>{{collateral.customer_name}}
                                                                </strong>

                                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                                    <br />

                                                                    <div class="row">

                                                                        <div class="col-lg-6 col-sm-6 col-md-6">
                                                                            <label>Type of Security </label>
                                                                            <br />
                                                                            <p>{{collateral.security_type}}</p>
                                                                        </div>
                                                                        <div class="col-lg-6 col-sm-6 col-md-6">
                                                                            <label>Account Status</label>
                                                                            <br />
                                                                            <p>{{collateral.account_status}}</p>
                                                                        </div>
                                                                    </div>
                                                                    <div class="row">
                                                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                                                            <label> Security Information </label>
                                                                            <br />
                                                                            <p>{{collateral.security_description}}</p>
                                                                        </div>

                                                                    </div>

                                                                </div>

                                                            </div>
                                                        </div>
                                                    </a>
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <label class="ng-scope text-primary">Sanction & Loan Details</label>
                                                <div class="col-lg-12">
                                                    <div class="table-responsive table-bordered">
                                                        <table class="table">
                                                            <thead class="bg-indigo-400">
                                                                <tr>
                                                                    <th></th>
                                                                    <th>S.No</th>
                                                                    <th colspan="2">Sanction Ref No.</th>
                                                                    <th colspan="2">Sanction Date</th>
                                                                    <th colspan="2">Sanction Amount</th>
                                                                    <!--<th colspan="3">Sanction Limit</th>-->
                                                                    <th colspan="2">Type of Facility</th>
                                                                    <th colspan="2">Tenure-Months</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody ng-repeat="loop in sanctiondetails">
                                                                <tr>
                                                                    <td>
                                                                        <a ng-hide="loop.expand" ng-click="loop.expand=true">
                                                                            <button title="Click to Expand Loan Details">
                                                                                <em class="fa fa-chevron-circle-down" style="font-size:14px;"></em>
                                                                            </button>
                                                                        </a>
                                                                        <a ng-show="loop.expand" ng-click="loop.expand=false">
                                                                            <button title="Click to Collapse Loan Details">
                                                                                <em class="fa fa-chevron-circle-up" style="font-size:14px;"></em>
                                                                            </button>
                                                                        </a>
                                                                    </td>
                                                                    <td> {{$index+1}} </td>
                                                                    <td colspan="2"> {{loop.sanction_refno}} </td>
                                                                    <td colspan="2"> {{loop.sanction_date}} </td>
                                                                    <td colspan="2">{{loop.sanction_amount}}</td>
                                                                    <!--<td colspan="3">{{loop.sanction_limit}}</td>-->
                                                                    <td colspan="2">{{loop.facility_type}}</td>
                                                                    <td colspan="2">{{loop.tenure_months}}</td>
                                                                </tr>

                                                                <tr ng-show="loop.expand" class="bg-indigo-300 text-white">
                                                                    <th></th>

                                                                    <th colspan="6"> Loan Ref.No </th>
                                                                    <th colspan="6"> Loan Facility Type </th>
                                                                    <!--<th colspan="4"> Vertical </th>-->
                                                                </tr>
                                                                <tr ng-show="loop.expand" class="bg-indigo-100" ng-repeat="nest in loop.loandetails">
                                                                    <td></td>

                                                                    <td colspan="6">  {{nest.loanref_no}}  </td>
                                                                    <td colspan="6"> {{nest.loan_title}}</td>
                                                                    <!--<td colspan="4"> {{nest.loan_title}} </td>-->
                                                                </tr>

                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>

                                            </div>
                                            <br />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="text-center">

                                <a class="btn btn-info" href="#/app/caseAllocation">
                                    <em class="fa fa-arrow-left"></em>
                                    <span>Back</span>
                                </a>
                            </div>
                        </div>
                        
                    </div>

                </form>
            </div>
        </div>
    </div>
</div>