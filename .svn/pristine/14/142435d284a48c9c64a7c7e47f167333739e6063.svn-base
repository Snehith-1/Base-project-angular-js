                        status: 'success',
                        pos: 'top-center',
                        timeout: 3000
                    });
                    $location.url('app/AgrMstSuprApplicationGeneralAdd?lstab=' + lstab);
                }
                else {
                    Notify.alert(resp.data.message, {
                        status: 'info',
