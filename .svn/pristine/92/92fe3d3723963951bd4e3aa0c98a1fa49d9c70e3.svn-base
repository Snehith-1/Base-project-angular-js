                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">

                <form novalidate="" name="userform1" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-9 col-md-9 col-sm-9">
                                <label><strong>Address Type </strong> <span style="color:red">*</span></label>
                                <select chosen="" ng-options="m as m.address_type for m in addresstype_list track by m.address_gid"
                                        ng-model="cboaddresstype" class="chosen-select form-control" required>
                                    <option value=""> ----- Select Address Type -----</option>
                                </select>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <label>Primary Status </label> <span style="color:red">*</span>
                                <div class="radio c-radio">
                                    <label>
                                        <input type="radio" id="primary" name="primaryaddress" required value="Yes" ng-model="rdbprimaryaddress" />
                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                    </label>
                                    <label>
                                        <input type="radio" id="primary" name="primaryaddress" required value="No" ng-model="rdbprimaryaddress" />
                                        <span class="fa fa-circle"></span>No
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-sm-12 col-sm-12 col-sm-12">
                                <label><strong>Address Line 1 </strong> <span style="color:red">*</span></label>
                                <input type="text" ng-model="txtaddressline1" tabindex="0" class="form-control" maxlength="300" required/>
                            </div>
                        </div>
                    </div>
                    <br/>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-sm-12 col-sm-12 col-sm-12">
                                <label><strong>Address Line 2 </strong></label>
                                <input type="text" ng-model="txtaddressline2" tabindex="0" class="form-control" maxlength="300" />
                            </div>
                        </div>
                    </div>
                    
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Land Mark </strong></label>
                                    <input type="text" ng-model="txtLand_Mark" tabindex="0" class="form-control" maxlength="100" />

                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Postal Code </strong> <span style="color:red">*</span></label>
                                    <input type="text" ng-model="txtpostal_code" tabindex="0" class="form-control" required
                                           ng-pattern="/^[0-9]*$/" ng-change="onchangepostal_code()" maxlength="6" />

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label>City </label>
                                    <input type="text" ng-model="txtcity" tabindex="0" class="form-control" disabled />
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label>Taluka</label>
                                    <input type="text" ng-model="txttaluka" tabindex="0" class="form-control" disabled />

                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label><strong>District</strong> <span style="color:red">*</span></label>
                                    <input type="text" ng-model="txtdistrict" tabindex="0" class="form-control" disabled />
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label>State </label> <span style="color:red">*</span>
                                    <br />
                                    <input type="text" ng-model="txtstate" tabindex="0" class="form-control" disabled />
                                </div>

                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label>Country</label> <span style="color:red">*</span>
                                    <input type="text" ng-model="txtcountry" tabindex="0" class="form-control" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/.,]*$/" />

                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="userform1.$invalid" ng-click="addressSubmit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>

                    </div>
                </form>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="/CICUpload.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-plus"></em>&nbsp;<label style="font-size:16px">Bureau Updates</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">

                <form novalidate="" name="userform1" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label><strong> Bureau Name  </strong> <span style="color:red">*</span></label>
                                <select name="BureauName" id="BureauName" chosen="" ng-options="m.bureauname_gid as m.bureauname_name for m in bureau_list"
                                        ng-model="bureau_gid" class="chosen-select form-control" required>
                                        <option value=""> -----Select Bureau Name -----</option>
                                    </select>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label> Bureau Score <span style="color:red"> *</span> </label>
                                <input type="text" ng-model="txtbureau_score" tabindex="0" class="form-control" required />
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label> Score As On </label>
                                <br />
                                <p class="input-group">
                                    <input type="date" id="SAO" name="SAO" ng-model="txtscore_on"
                                           style="width:200px;height:35px" class="form-control">
                                </p>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <label> Observations </label>
                                <br />
                                <textarea rows="2" placeholder="Enter Observations..." aria-multiline="true" tabindex="0" aria-invalid="false"
                                          ng-model="txtobservations" class="form-control"></textarea>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <label> Bureau Response </label>
                                <br />
                                <textarea rows="2" placeholder="Enter Bureau Response ..." aria-multiline="true" tabindex="0" aria-invalid="false"
                                          ng-model="txtbureau_response" class="form-control"></textarea>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="dd-handle ng-scope">
                                <br />
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Upload report </b></p>
                                <hr />
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="float-label">
                                        <label>Upload Documents<span style="color:red"> *</span></label>
                                        <br /><br />
                                        <input type="file" id="file" tabindex="5" onchange="angular.element(this).scope().CICDocumentUpload(this.files, angular.element(this).scope().documentupload, this.name)"
                                               ng-model="txtcic_document" data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                </div>
                                <br /><br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <br />
                                            <div class="table-responsive">
                                                <table class="table table-striped table-secondary">
                                                    <thead class="bg-gray text-white">
                                                        <tr width="100%">
                                                            <th scope="col">S.No</th>
                                                            <th scope="col">Document Name</th>
                                                            <th scope="col">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in cicuploaddoc_list">
                                                            <td title="S.No">{{$index+1}}</td>
                                                            <td title="Document Name">{{data.document_name}}</td>
                                                            <td data-title="Action">
                                                                <button title="Download" class="btn btn-green btn-sm"
                                                                        ng-click="downloads(data.document_path,data.document_name)">
                                                                    <em class="fa fa-download"></em>
                                                                </button>
                                                                <a title="Delete" class="btn btn-sm btn-danger" ng-click="uploaddocumentcancel(data.tmpcicdocument_gid)">
                                                                    <span class="icon icon-trash "></span>
                                                                </a>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>

                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="CICUploaddtlsUpdate.$invalid" ng-click="update_CICUploads()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>
