<h3>
    Customer Management - 360
</h3>
<div ng-controller="customerManagement360controller">

    <div class="card">
        <div class="card-heading bg-primary">
            <div class="card-title">{{customerdetails.customer_urnedit}} - {{customerdetails.customerNameedit}}</div>
        </div>


        <md-content class="md-padding">
            <md-tabs md-dynamic-height="" md-border-bottom="">
                <md-tab label="Customer Details">
                    <md-content class="md-padding">
                        <div class="">
                            <h4 class="text-pink">
                                Customer Details
                            </h4>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Customer Code</label>
                                        <br />
                                        <p>{{customerdetails.customerCodeedit}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Customer Name </label>
                                        <br />
                                        <p>{{customerdetails.customerNameedit}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Contact Person</label>
                                        <br />
                                        <p>{{customerdetails.contactPersonedit}}</p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Contact Number </label>
                                        <br />
                                        <p>{{customerdetails.mobileNoedit}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Address Line 1</label>
                                        <br />
                                        <p>{{customerdetails.addressline1edit}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Address Line 2</label>
                                        <br />
                                        <p>{{customerdetails.addressline2edit}}</p>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Zonal Head </label>
                                        <br />
                                        <p>{{customerdetails.zonal_name}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Business Head</label>
                                        <br />
                                        <p>{{customerdetails.businesshead_name}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Cluster Manager</label>
                                        <br />
                                        <p>{{customerdetails.cluster_manager_name}}</p>
                                    </div>
                                </div>
                                <div class="row">

                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Relationship Manager</label>
                                        <br />
                                        <p>{{customerdetails.relationshipmgmt_name}}</p>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Credit Manager</label>
                                        <br />
                                        <p>{{customerdetails.creditmanager_name}}</p>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </md-content>
                </md-tab>
                <md-tab label="Customer Collateral">
                    <md-content class="md-padding">

                        <div class="">
                            <h4 class="text-pink">
                                Customer Collateral Details
                            </h4>

                            <div class="panel-body">
                                <div class="row">
                                    
                                    <div class="col-lg-12" ng-repeat="collateral in customerCollateral">
                                        <a class="list-group-item">
                                            <div class="media-box">

                                                <div class="media-box-body clearfix">

                                                    <strong class="media-box-heading text-primary">
                                                        <span class="circle circle-pink circle-lg text-left"></span>{{$index+1}}
                                                    </strong>

                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                        <br />

                                                        <div class="row">

                                                            <div class="col-lg-6 col-sm-6 col-md-6">
                                                                <label>Type of Security </label>
                                                                <br />
                                                                <p>{{collateral.security_type}}</p>
                                                            </div>
                                                            <div class="col-lg-6 col-sm-6 col-md-6">
                                                                <label>Account Status</label>
                                                                <br />
                                                                <p>{{collateral.account_status}}</p>
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                                <label> Security Information </label>
                                                                <br />
                                                                <p>{{collateral.security_description}}</p>
                                                            </div>

                                                        </div>

                                                    </div>

                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </md-content>
                </md-tab>
                 
                <md-tab label="Customer promoter">
                    <md-content class="md-padding">
                        <div class="">
                            <h4 class="text-pink">
                                Customer promoter Details
                            </h4>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <button title="Add" class="btn btn-info btn-sm"
                                                ng-click="addpromotor()">
                                            <em class="fa fa-plus"></em>&nbsp;Add Promoter
                                        </button>
                                    </div>
                                </div>
                                <br />
                                <div class="table-responsive">
                                    <table datatable="ng" class="row-border hover">
                                        <thead>
                                            <tr class="bg-primary-light text-white">
                                                <th>S.NO</th>
                                                <th>Promoter Name</th>
                                                <th>Designation</th>
                                                <th>Age</th>
                                                <th>Mobile No.</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in customerPromotorlist">
                                                <td data-title="'S.No'">{{$index+1}}</td>
                                                <td data-title="'Promoter Name'">{{data.promoter_name}}</td>
                                                <td data-title="'Designation'">{{data.designation}}</td>
                                                <td data-title="'Age'">{{data.promoter_age}}</td>
                                                <td data-title="'Mobile No.'">{{data.mobile}}</td>
                                                <td>
                                                    <button title="Edit" class="btn btn-success btn-sm"
                                                            ng-click="editpromotor(data.customer2promotor_gid)">
                                                        <em class="fa fa-edit"></em>
                                                    </button>
                                                    <button title="delete" class="btn btn-danger btn-sm"
                                                            ng-click="deletepromotor(data.customer2promotor_gid)">
                                                        <em class="fa fa-trash"></em>
                                                    </button>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>

                            </div>

                        </div>
                    </md-content>
                </md-tab>
                <md-tab label="Customer Guarantors">
                    <md-content class="md-padding">

                        <div class="">
                            <h4 class="text-pink">
                                Customer Guarantors Details
                            </h4>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <button title="Add" class="btn btn-info btn-sm"
                                                ng-click="addgurantor()">
                                            <em class="fa fa-plus"></em>&nbsp;Add Guarantor
                                        </button>
                                    </div>
                                </div>
                                <br />
                                <div class="table-responsive">
                                    <table datatable="ng" class="row-border hover">
                                        <thead>
                                            <tr class="bg-primary-light text-white">
                                                <th>S.NO</th>
                                                <th>Guarantors Name</th>
                                                <th>Age</th>
                                                <th>Net worth</th>
                                                <th>Basis of NW (assessed/self certified)</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="gurantor in customerguarantorlist">
                                                <td data-title="'S.No'">{{$index+1}}</td>
                                                <td data-title="'Guarantors Name'">{{gurantor.guarantors_name}}</td>
                                                <td data-title="'Age'">{{gurantor.guarantor_age}}</td>
                                                <td data-title="'Net worth'">{{gurantor.networth}}</td>
                                                <td data-title="'Basis of NW (assessed/self certified)'">{{gurantor.basisofNW}}</td>
                                                <td>
                                                    <button title="Edit" class="btn btn-success btn-sm"
                                                            ng-click="editguarantor(gurantor.customer2guarantor_gid)">
                                                        <em class="fa fa-edit"></em>
                                                    </button>
                                                    <button title="Delete" class="btn btn-danger btn-sm"
                                                            ng-click="deleteguarantor(gurantor.customer2guarantor_gid)">
                                                        <em class="fa fa-trash"></em>
                                                    </button>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>

                        </div>
                    </md-content>
                </md-tab>
            </md-tabs>
        </md-content>
    </div>
    <div class="row">
        <div class="text-center" ng-switch="MyCustomer">
            <a class="btn btn-info" ng-switch-when="Y" href="#/app/myZonalCustomer">
                <em class="fa fa-arrow-left"></em>
                <span>Back</span>
            </a>
            <a class="btn btn-info" ng-switch-when="N" ng-click="back()">
                <em class="fa fa-arrow-left"></em>
                <span>Back</span>
            </a>
        </div>
    </div>
    <script type="text/ng-template" id="/promotorAddModalContent.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-plus"></em>&nbsp;Add Promoter Details</h2>
                <span flex></span>

            </div>
        </md-toolbar>

        <div class="modal-body ng-scope">
            <div class="pd-sm-10">

                <div class="row">
                    <form name="customerpromoterform" id="customerpromoterform" novalidate="" class="form-validate mb-lg" autocomplete="off">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtPromoterName" class="mda-form-control ng-pristine ng-valid ng-touched" required>
                                        <label>Promoter Name <span style="color:red">*</span></label>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtDesignation" name="txtDesignation" class="mda-form-control ng-pristine ng-valid ng-touched">
                                        <label>Designation </label>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtAge" type="number" class="mda-form-control ng-pristine ng-valid ng-touched" maxlength="3">
                                        <label>Age</label>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtmobile" type="number" class="mda-form-control ng-pristine ng-valid ng-touched" maxlength="11">
                                        <label>Mobile No.</label>
                                    </div>
                                </div>
                            </div>

                            <div class="class">
                                <div class="text-center">
                                    <button class="btn btn-success" ng-click="customerPromoterSubmit()" ng-disabled="customerpromoterform.$invalid">
                                        <em class="fa fa-check"></em>
                                        <span>Submit</span>
                                    </button>
                                    <a class="btn btn-danger" ng-click="ok()">
                                        <em class="fa fa-close"></em>
                                        <span>Cancel</span>
                                    </a>
                                </div>
                            </div>
                        </div>

                    </form>
                </div>

            </div>

        </div>
    </script>

    <script type="text/ng-template" id="/GuarantorsAddModalContent.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-plus"></em>&nbsp;Add Guarantor Details</h2>
                <span flex></span>

            </div>
        </md-toolbar>

        <div class="modal-body ng-scope">
            <div class="pd-sm-10">

                <div class="row">
                    <form name="customerGuarantorsAddform" id="customerGuarantorsAddform" novalidate="" class="form-validate mb-lg" autocomplete="off">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtGuarantorsName" name="txtGuarantorsName" class="mda-form-control ng-pristine ng-valid ng-touched" required>
                                        <label>Guarantors Name <span style="color:red">*</span></label>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtAge" type="number" class="mda-form-control ng-pristine ng-valid ng-touched" maxlength="3">
                                        <label>Age</label>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtNetWorth" name="txtNetWorth" class="mda-form-control ng-pristine ng-valid ng-touched">
                                        <label>Net worth  </label>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtBasisofNW" name="txtBasisofNW" class="mda-form-control ng-pristine ng-valid ng-touched">
                                        <label>Basis of NW (assessed/self certified) </label>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="text-center">
                                    <button class="btn btn-success" ng-click="customerGuarantorsSubmit()" ng-disabled="customerGuarantorsAddform.$invalid">
                                        <em class="fa fa-check"></em>
                                        <span>Submit</span>
                                    </button>
                                    <a class="btn btn-danger" ng-click="ok()">
                                        <em class="fa fa-close"></em>
                                        <span>Cancel</span>
                                    </a>
                                </div>
                            </div>
                        </div>

                    </form>
                </div>

            </div>

        </div>
    </script>

    <script type="text/ng-template" id="/promotorModalContent.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-edit"></em>&nbsp;Edit Promoter Details</h2>
                <span flex></span>

            </div>
        </md-toolbar>

        <div class="modal-body ng-scope">
            <div class="pd-sm-10">

                <div class="row">
                    <form novalidate="" name="customerpromoterEditform" ng-submit="customerpromoterEditform.submitForm1()" autocomplete="off"
                          class="form-validate form-horizontal">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group">
                                        <input ng-model="txtPromoterNameedit" class="mda-form-control ng-pristine ng-valid ng-touched" required>
                                        <label>Promoter Name <span style="color:red">*</span></label>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group">
                                        <input ng-model="txtDesignationedit" name="txtDesignation" class="mda-form-control ng-pristine ng-valid ng-touched">
                                        <label>Designation </label>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group">
                                        <input ng-model="txtAgeedit" type="number" class="mda-form-control ng-pristine ng-valid ng-touched" maxlength="3">
                                        <label>Age</label>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group">
                                        <input ng-model="txtmobileedit" type="number" class="mda-form-control ng-pristine ng-valid ng-touched" maxlength="11">
                                        <label>Mobile No.</label>
                                    </div>
                                </div>
                            </div>

                            <div class="class">
                                <div class="text-center">
                                    <button class="btn btn-success" ng-click="updatepromotor()" ng-disabled="customerpromoterEditform.$invalid">
                                        <em class="fa fa-check"></em>
                                        <span>Update</span>
                                    </button>
                                    <a class="btn btn-danger" ng-click="ok()">
                                        <em class="fa fa-close"></em>
                                        <span>Cancel</span>
                                    </a>
                                </div>
                            </div>
                        </div>


                    </form>
                </div>

            </div>

        </div>
    </script>

    <script type="text/ng-template" id="/gurantorModalContent.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-edit"></em>&nbsp;Edit Guarantors Details</h2>
                <span flex></span>

            </div>
        </md-toolbar>

        <div class="modal-body ng-scope">
            <div class="pd-sm-10">

                <div class="row">
                    <form name="customerGuarantorsEditform" id="customerGuarantorsEditform" novalidate="" class="form-validate mb-lg" autocomplete="off">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group">
                                        <input ng-model="txtGuarantorsNameedit" name="txtGuarantorsName" class="mda-form-control ng-pristine ng-valid" required>
                                        <label>Guarantor Name <span style="color:red">*</span></label>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group">
                                        <input ng-model="txtAgeedit" type="number" class="mda-form-control ng-pristine ng-valid ng-touched" maxlength="3">
                                        <label>Age</label>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group">
                                        <input ng-model="txtNetWorthedit" name="txtNetWorth" class="mda-form-control ng-pristine ng-valid ng-touched">
                                        <label>Net worth  </label>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group">
                                        <input ng-model="txtBasisofNWedit" name="txtBasisofNW" class="mda-form-control ng-pristine ng-valid ng-touched">
                                        <label>Basis of NW (assessed/self certified) </label>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="text-center">
                                    <button class="btn btn-success" ng-click="updateguarantor()" ng-disabled="customerGuarantorsEditform.$invalid">
                                        <em class="fa fa-check"></em>
                                        <span>Update</span>
                                    </button>
                                    <a class="btn btn-danger" ng-click="ok()">
                                        <em class="fa fa-close"></em>
                                        <span>Cancel</span>
                                    </a>
                                </div>
                            </div>
                        </div>

                    </form>
                </div>

            </div>

        </div>
    </script>
</div>