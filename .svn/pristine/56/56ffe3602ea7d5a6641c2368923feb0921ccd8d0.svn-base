<h3>
    Lawyer Details
</h3>
<div ng-controller="viewLawyercontroller">


    <div id="panelDemo8" class="panel panel-primary ">
        <div class="panel-heading">LAWYER DETAILS</div>

        <div class="panel-body">
            <div class="row" style="display:none;">
                <div class="col-lg-12 col-md-12 col-sm-12 ">
                    <div class="col-lg-6 col-md-6 col-sm-6 ">
                        <input name="lawyerregister_gid" id="lawyerregister_gid" style="font-size:12px"
                            placeholder="Enter lawyerregister_gid" ng-model="lawyerregister_gid" class="form-control"
                            required>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">

                    <div class="panel widget">
                        <div style="background-image: url('app/img/bg4.jpg')" class="panel-body text-center bg-center">
                            <div class="row row-table">
                                <div class="col-xs-11 text-white">
                                    <div ng-switch="employeedetails.employee_photo">
                                        <img src="app/img/user-bg1.jfif" alt="Image" ng-switch-when="N"
                                            class="img-thumbnail img-circle thumb96">
                                        <img ng-src="{{lawyerphoto_path}}" alt="Image" ng-switch-default
                                            class="img-thumbnail img-circle thumb96">
                                    </div>
                                    <h3 class="m0">{{UploadDocumentname.lawyer_name}}</h3>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Lawyer Ref No</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.lawyerref_no}}
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Enrolment No</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.enrolment_no}}
                        </div>
                       
                    </div>
                </div>
                <br/>
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">DOB </label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.dob}}
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Gender</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.gender}}
                        </div>
                       
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Mobile No</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.mobile_no}}
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Telephone No</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.telephone_no}}
                        </div>
                     
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Email Address</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.email_address}}
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Educational Qualification</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.educational_qualification}}
                        </div>
                        
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Enrolment Date with Bar Council</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.date_enrolment}}
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Experience at Bar Council</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.experience}}
                        </div>
                    </div>
                </div>
               
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Place of Empanelment</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.place_practice}}
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">PAN No</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.pan_no}}
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Aadhar No</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.aadhar_no}}
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Bank Name</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.bank_name}}
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Account No</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.account_no}}
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">IFSC Code</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.ifsc_code}}
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Address Line 1</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.address_line1}}
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Address Line 2</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.address_line2}}
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">Country</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.country}}
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            <label class="col-form-label">State / PinCode</label>
                        </div>
                        <div class="col-lg-3 col-sm-3 col-md-3">
                            {{UploadDocumentname.state}} / {{UploadDocumentname.postal_code}}
                        </div>
                    </div>
                </div>
                <br />

        </div>
        <div class="row">
            <div class="col-lg-12 col-sm-12 col-md-12">
                <div class="col-sm-4 col-md-4 col-lg-4">
                    <strong class="ng-binding w3-text-indigo">
                        <em class="fa fa-file text-primary fa-lg"></em>&emsp;
                        Uploaded Empanelment Certificate
                    </strong>
                </div>
            </div>
        </div>
        <div class="dd-handle ng-scope">
            <br />

            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="table-responsive">
                            <table class="table table-striped table-secondary">
                                <thead class="bg-gray text-white">
                                    <tr width="100%">
                                        <th scope="col">S.No</th>
                                        <th scope="col">Document Title</th>
                                        <th scope="col">Document Name</th>
                                        <th scope="col">Uploaded Details</th>
                                        <th scope="col">Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="data in filename_list" width="100%">
                                        <td title="S.No">{{$index+1}}</td>
                                        <td title="Document Title">{{data.document_type}}</td>
                                        <td title="Document Name">{{data.document_name}}</td>
                                        <td title="Uploaded Details">{{data.uploaded_by}} - {{data.updated_date}}</td>
                                        <td data-title="Action" title="Download">
                                            <a class="btn" download="" href="" id="btnpdf" ng-click="downloads(data.document_path,data.document_name)"><span
                                                    class="fa fa-download"></span></a>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
    <div class="panel-body text-center">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <button ng-click="viewBack()" class="btn btn-info"><i
                    class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
        </div>
    </div>

</div>

</div>