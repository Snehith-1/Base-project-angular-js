<h3>
    History
</h3>

<div ng-controller="MstSentbackCadtoCcHistoryController">
    <div class="panel">
        <div class="panel-heading" style="background-color:rgb(19,14,89);color:#ffffff">
            <h5> {{txtbasiccustomer_name}} / {{txtapplication_no}} </h5>
        </div>
        <br />
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="ng-scope dd-handle">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <label class="label label-pink" style="font-size:12px;padding-top:3px;"> CC Meeting to Credit Log : </label>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="table-responsive table-bordered">
                                    <table class="table">
                                        <thead class="bg-gray text-white">
                                            <tr style="font-size: 13px;">
                                                <th class="text-center">S.No</th>
                                                <th>Sent Back By</th>
                                                <th>Sent Back Date</th>
                                                <th>Reason </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in cctocreditlog_list">
                                                <td class="text-center">{{$index+1}}</td>
                                                <td>{{data.sentbacktocc_by}}</td>
                                                <td>{{data.sentbacktocc_date}}</td>
                                                <td>{{data.cctocredit_reason}}</td>
                                            </tr>

                                        </tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="ng-scope dd-handle">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <label class="label label-pink" style="font-size:12px;padding-top:3px;"> CAD to CC Meeting Log : </label>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="table-responsive table-bordered">
                                    <table class="table">
                                        <thead class="bg-gray text-white">
                                            <tr style="font-size: 13px;">
                                                <th class="text-center">S.No</th>
                                                <th>Sent Back By</th>
                                                <th>Sent Back Date</th>
                                                <th>Reason </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in cadtoccmeetinglog_list">
                                                <td class="text-center">{{$index+1}}</td>
                                                <td>{{data.sentbackcadtocc_by}}</td>
                                                <td>{{data.sentbackcadtocc_date}}</td>
                                                <td>{{data.cadtoccmeeting_reason}}</td>
                                            </tr>

                                        </tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="ng-scope dd-handle">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <label class="label label-pink" style="font-size:12px;padding-top:3px;"> CAD to Credit Log : </label>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="table-responsive table-bordered">
                                    <table class="table">
                                        <thead class="bg-gray text-white">
                                            <tr style="font-size: 13px;">
                                                <th class="text-center">S.No</th>
                                                <th>Sent Back By</th>
                                                <th>Sent Back Date</th>
                                                <th>Reason </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in cadtocreditlog_list">
                                                <td class="text-center">{{$index+1}}</td>
                                                <td>{{data.sentbackcadtocredit_by}}</td>
                                                <td>{{data.sentbackcadtocredit_date}}</td>
                                                <td>{{data.cadtocredit_reason}}</td>
                                            </tr>

                                        </tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="ng-scope dd-handle">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <p style="color:#141478;font-size: 15px;"><b> Schedule Meeting Details : </b></p>
                                <hr />
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="table-responsive table-bordered">
                                    <table class="table">
                                        <thead class="bg-gray text-white">
                                            <tr style="font-size: 13px;">
                                                <th class="text-center">S.No</th>
                                                <th>CC Meeting Title</th>
                                                <th>CC Meeting Number</th>
                                                <th>CC Meeting Date </th>
                                                <th>Meeting Time</th>
                                                <th>CC Meeting Mode</th>
                                                <th class="text-center">CC Meeting Details</th>
                                                <th class="text-center">CC Approval Details</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in ccmeetinghistorylog_list">
                                                <td class="text-center">{{$index+1}}</td>
                                                <td>{{data.ccmeeting_title}}</td>
                                                <td>{{data.ccmeeting_no}}</td>
                                                <td>{{data.ccmeeting_date}}</td>
                                                <td>{{data.ccmeeting_time}}</td>
                                                <td>{{data.ccmeeting_mode}}</td>
                                                <td class="text-center">
                                                    <button title="CC Meeting Details" type="button" class="btn btn-info"
                                                            ng-click="ccmeetingdetails_view(data.application_gid,data.ccschedulemeetinglog_gid)">
                                                        <em class="fa fa-eye"></em>
                                                    </button>
                                                </td>
                                                <td class="text-center">
                                                    <button title="CC Approval Details" type="button" class="btn btn-success"
                                                            ng-click="ccapprovaldetails_view(data.application_gid,data.ccschedulemeetinglog_gid)">
                                                        <em class="fa fa-check-square-o"></em>
                                                    </button>
                                                </td>
                                            </tr>

                                        </tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="ng-scope dd-handle">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <p style="color:#141478;font-size: 15px;"><b> Log - Schedule Meeting : </b></p>
                                <hr />
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12 col-md-12 col-lg-12">

                                <div class="table-responsive table-bordered">
                                    <table class="table" style="padding: 0px 0px">
                                        <thead>
                                            <tr class="bg-gray-light text-white">
                                                <th>S.No</th>
                                                <th>CC Meeting Title</th>
                                                <th>CC Meeting Number</th>
                                                <th>CC Meeting Date</th>
                                                <!--<th>CC Meeting Time</th>-->
                                                <th>CC Meeting Mode</th>
                                                <th>Meeting Scheduled By</th>
                                                <th>Meeting Scheduled Date</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in lblccschedule_list">
                                                <td data-title="'S.No'">{{$index+1}}</td>
                                                <td data-title="'CC Meeting Title'">{{data.ccmeeting_title}}</td>
                                                <td data-title="'CC Meeting Number'">{{data.ccmeeting_no}}</td>
                                                <td data-title="'CC Meeting Date'">{{data.ccmeeting_date}}</td>
                                                <!--<td data-title="'Time'">{{data.start_time}} - {{data.end_time}}</td>-->
                                                <td data-title="'CC Meeting Mode'">{{data.ccmeeting_mode}}</td>
                                                <td data-title="'Meeting Scheduled By'">{{data.created_by}}</td>
                                                <td data-title="'Meeting Scheduled Date'">{{data.created_date}}</td>
                                            </tr>
                                        </tbody>
                                    </table>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="dd-handle ng-scope">
                        <p style="font-size:15px;color:#930D59;"><b> CC Meeting Skip History</b></p>
                        <hr />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="table-responsive table-bordered">
                                    <table class="table">
                                        <thead class="bg-gray text-white">
                                            <tr style="font-size: 13px;">
                                                <th class="text-center">S.No</th>
                                                <th>CC Meeting Skip By</th>
                                                <th>CC Meeting Skip Date</th>
                                                <th class="text-center">Reason </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in ccmeetingskiphistory_list">
                                                <td class="text-center">{{$index+1}}</td>
                                                <td>{{data.ccmeetingskip_by}}</td>
                                                <td>{{data.ccmeetingskip_date}}</td>
                                                <td data-title="'CC Skipped Reason'" class="text-center">
                                                    <button ng-click="ccskipped_reason(data.ccmeetingskip_gid)" class="btn btn-info">
                                                        <em class="fa fa-eye"></em>
                                                    </button>
                                                </td>
                                            </tr>

                                        </tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="text-center">
                <br /><br />
                <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
            </div>
            <br />
        </div>
    </div>
    <br />
    <script type="text/ng-template" id="/viewccremarks.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-file-text-o"></em>&nbsp;<label style="font-size:16px">View Remarks</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>
        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="userform1" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="dd-handle ng-scope">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <label style="font-size:14px;color:darkblue;"><u> Remarks : </u></label>
                                        <br />
                                        <p>{{lblremarks}}</p>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="/viewschedulemeetingpopup.html">
