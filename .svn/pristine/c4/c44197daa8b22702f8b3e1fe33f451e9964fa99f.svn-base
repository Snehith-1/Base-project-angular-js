    <script type="text/ng-template" id="/viewapprovaldetailspopup.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-check-square-o"></em>&nbsp;<label style="font-size:16px">View Approval Details</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>
        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="userform1" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="dd-handle ng-scope">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <p style="font-size:15px;color:darkblue;"><b>Committee Members Attendance</b></p>
                                        <div class="table-responsive table-bordered">
                                            <table class="table">
                                                <thead class="bg-gray text-white">
                                                    <tr style="font-size: 13px;">
                                                        <th class="text-center">S.No</th>
                                                        <th>Group Name</th>
                                                        <th>Name</th>
                                                        <th class="text-center">Attendance </th>
                                                        <th class="text-center">Approval Status</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="list in ccmember_list">
                                                        <td class="text-center">{{$index+1}}</td>
                                                        <td>{{list.ccgroup_name}}</td>
                                                        <td>{{list.CCMember_name}}</td>
                                                        <td class="text-center">
                                                            <label style="color:green" ng-show="list.attendance_status=='P'">Present</label>
                                                            <label style="color:red" ng-show="list.attendance_status=='A'">Absent</label>
                                                        </td>
                                                        <td ng-hide="list.attendance_status=='A'" class="text-center">
                                                            {{list.approval_status}}
                                                            &nbsp;
                                                            <button ng-if="list.approval_status!='-'" title="View Remarks" type="button" class="btn btn-default" ng-click="cc_remarksview(list.ccmeeting2members_gid)">
                                                                <em class="fa fa-eye"></em>
                                                            </button>
                                                        </td>
                                                        <td ng-show="list.attendance_status=='A'" class="text-center">
                                                            -
                                                        </td>
                                                    </tr>

                                                </tbody>
                                            </table>
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <div class="dd-handle ng-scope">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <p style="font-size:15px;color:darkblue;"><b>Other Samunnati User Attendance</b></p>
                                        <div class="table-responsive table-bordered">
                                            <table class="table">
                                                <thead class="bg-gray text-white">
                                                    <tr style="font-size: 13px;">
                                                        <th class="text-center">S.No</th>
                                                        <th>Name</th>
                                                        <th class="text-center">Attendance</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in otheruser_list">
                                                        <td class="text-center">{{$index+1}}</td>
                                                        <td>{{data.employee_name}}</td>
                                                        <td class="text-center">
                                                            <label style="color:green" ng-show="data.attendance_status=='P'">Present</label>
                                                            <label style="color:red" ng-show="data.attendance_status=='A'">Absent</label>
                                                        </td>

                                                    </tr>

                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>                            
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>
