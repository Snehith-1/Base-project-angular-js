<h3>
    Add Template
</h3>
<div ng-controller="MstSanctionAddTemplateController">
    <div class="panel  panel-primary">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-plus"></i>
            </span>Add Template 
        </div> 
        <br/>
        <form novalidate="" name="templateformsubmit" ng-submit="form.submitForm1()" autocomplete="off" class="form-validate form-horizontal">
            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">  
                    <div class="col-lg-3 col-sm-3 col-md-3">
                        <label class="col-form-label"> Vertical </label>
                        <br />
                        <p>{{lblvertical_name}} / {{vertical_codeedit}}</p>
                    </div> 
                    <div class="col-lg-5 col-sm-5 col-md-5">
                    <label class="col-form-label"> Program <span style="color:red"> *</span></label>
                    <br />
                    <select name="program" id="program" chosen="" multiple ng-options="m.program_name for m in program_list" ng-maxlength="256"
                            ng-model="cboprogram" class="chosen-select form-control" required>
                        <option value=""></option>
                    </select>
                   </div>
                <div class="col-lg-4 col-sm-4 col-md-4">
                    <label><strong>Template Name <span style="color:red;">*</span></strong></label>
                    <input type="text" ng-model="templatename" required tabindex="0" class="form-control" /></div>
                    <div class="col-lg-1 col-sm-1 col-md-1"></div> 
                    <br>
                    <br>
                </div> 
            </div>
            <br>
     
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <label> Template Content <span style="color:red;">*</span></label>
                        <div>
                            <div id="froala-editor">
                            </div>
                            <!-- <div class="simple">
                                <textarea id="test" ng-show="editMode" froala ng-model="content" class="nw-editor__src"> </textarea>
                            </div> -->
                        </div>
                    </div>
                </div>
            </div>
            </br>
            <div class="row">
                <div class="text-center">
    
                    <button ng-click="submitTemplate()" ng-disabled="templateformsubmit.$invalid" class="btn btn-success">
                        <i class="fa fa-check"></i>&nbsp;&nbsp;Submit
                    </button>
    
                    <button ng-click="back()" class="btn btn-primary"><i class="fa fa-arrow-circle-left fa-1x"></i>&nbsp;&nbsp;Back</button>
                </div>
                <br>
            </div>
            
        </form>
        

    
</div>
<script type="text/ng-template" id="/addsmalltextbox.html">
    <div class="modal-header bg-primary ng-scope">

        <h4 id="myModalLabel" class="modal-title"> 
            <span class="btn-label bg-primary">
                <i class="fa fa-plus ">&nbsp; Small Textbox</i>
            </span> 
        </h4>
    </div>
    <div class="modal-body ng-scope">
        <form novalidate="" name="addsmalltextbox" ng-submit="form.addsmalltextbox()" autocomplete="off" class="form-validate form-horizontal">
            <div class="pd-sm-10">
                <div class="row">
                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                        <div class="row">
                            <div class="col-md-12 col-lg-12 col-sm-12">
                                <label>Field Type</label>&nbsp;<span style="color:red">*</span>
                                <div class="radio c-radio">
                                    <label>
                                        <input type="radio" id="Inputfield" name="Inputfield"  value="ManualCapturingField" 
                                        ng-model="rdbInputfield" required ng-change="Changedbfield()">
                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Manual Capturing Field
                                    </label>&nbsp;&nbsp;
                                    <label>
                                        <input type="radio" id="Inputfield" name="Inputfield" value="DBField"
                                        ng-change="Changedbfield('text')" ng-model="rdbInputfield" required />
                                        <span class="fa fa-circle"></span>DB Field
                                    </label>
                                </div>
                            </div>
                            
                        </div>
                        <div ng-show="rdbInputfield=='DBField'">
                            <br/>
                            <div class="row">
                                <div class="col-md-12 col-lg-12 col-sm-12">
                                    <label>Field Mapping </label>&nbsp;<span style="color:red">*</span>
                                    <select name="FieldMapping" id="FieldMapping" 
                                        ng-options="(m.fieldmapping_name + '/' + m.fieldmapping_code) for m in fieldmappinglist" 
                                        chosen=""  ng-model="cbofieldmapping" class="chosen-select form-control" style="height:37px;"
                                        ng-required="rdbInputfield=='DBField'" size="1">
                                        <option value=""> ----- Select Field Mapping -----</option>
                                    </select> 
                                </div> 
                            </div>
                            <br/>
                            <div class="row">
                                <div class="col-md-6 col-lg-6 col-sm-6"> 
                                    <label class="col-form-label"> Stage </label>
                                    <br /> 
                                    <p>{{cbofieldmapping.stage_fieldname}}</p>
                                </div> 
                                <div class="col-md-6 col-lg-6 col-sm-6"> 
                                    <label class="col-form-label"> Menu </label>
                                    <br />
                                    <p>{{cbofieldmapping.menu_fieldname}}</p>
                                </div> 
                            </div>
                            <br/>
                            <div class="row">
                                <div class="col-md-6 col-lg-6 col-sm-6"> 
                                    <label class="col-form-label"> Field </label>
                                    <br />
                                    <p>{{cbofieldmapping.fieldname}}</p>
                                </div> 
                                <div class="col-md-6 col-lg-6 col-sm-6" ng-show="cbofieldmapping.subfield_flag=='Y'"> 
                                    <label class="col-form-label"> Sub Field </label>
                                    <br />
                                    <p>{{cbofieldmapping.subfield_fieldname}}</p>
                                </div> 
                            </div>
                        </div>
                        <br />
                        <div ng-show="rdbInputfield=='ManualCapturingField'">
                            <div class="row ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <label>Group Name </label>&nbsp;<span style="color:red">*</span>
                                    <select name="department" id="department" 
                                    ng-options="m.input_fieldname for m in groupnamelist" chosen="" ng-change="onchangegroup()"
                                            ng-model="txtgroup_name" class="chosen-select form-control" style="height:37px;"
                                            ng-required="rdbInputfield=='ManualCapturingField'" size="1">
                                        <option value=""> ----- Select Group Name -----</option>
                                    </select> 
                                </div>
                            </div>
                            <br />
                            <div class="row ">
                                <div class="col-md-12 col-lg-12 col-sm-12"> 
                                    <label class="col-form-label"> Field Name  <span style="color:red"> *</span></label>
                                    <br />
                                    <input type="text"  ng-model="txttextbox_name"  ng-disabled="txtdisabled" tabindex="0" 
                                    class="form-control" ng-required="rdbInputfield=='ManualCapturingField'"  />
                                </div> 
                            </div>
                            <br />
                            <div class="row ">
                                <div class="col-md-9 col-lg-9 col-sm-9"> 
                                    <label class="col-form-label"> Placeholder  <span style="color:red"> *</span></label>
                                    <br />
                                    <input type="text"  ng-model="txtplaceholder" ng-disabled="txtdisabled" tabindex="0" 
                                    class="form-control" ng-required="rdbInputfield=='ManualCapturingField'"  />
                                </div>  
                                <div class="col-md-3 col-lg-3 col-sm-3"> 
                                    <label class="col-form-label"> Mandatory  <span style="color:red"> *</span></label>
                                    <br />
                                    <div class="radio c-radio" ng-hide="txtdisabled">
                                        <label>
                                            <input type="radio" id="Requiredcheck" name="Requiredcheck"  value="true"
                                             ng-model="rdbMandatory" ng-required="rdbInputfield=='ManualCapturingField'" />
                                            &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                        </label>
                                        <label>
                                            <input type="radio" id="Requiredcheck" name="Requiredcheck" value="false" 
                                            ng-model="rdbMandatory" ng-required="rdbInputfield=='ManualCapturingField'" />
                                            <span class="fa fa-circle"></span>No
                                        </label>
                                    </div>
                                    <div ng-show="txtdisabled"> 
                                        <input type="text"  ng-model="rdbMandatory" ng-disabled="txtdisabled" tabindex="0" class="form-control"  /> 
                                    </div>
                                 </div>
                            </div>
                            <br />
                            <div class="row ">
                                <div class="col-md-12 col-lg-12 col-sm-12"> 
                                    <label class="col-form-label"> Maxlength  <span style="color:red"> *</span></label>
                                    <br />
                                    <input type="text" ng-model="txtmaxlength" pattern="[0-9.]+" ng-disabled="txtdisabled" tabindex="0"
                                     class="form-control" ng-required="rdbInputfield=='ManualCapturingField'"  />
                                </div> 
                                
                            </div>
                        </div>
                       
                    </div>
                </div>

                <br />
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="addsmalltextbox.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                </div>
            </div>
            <br />
        </form>
    </div>
</script>

