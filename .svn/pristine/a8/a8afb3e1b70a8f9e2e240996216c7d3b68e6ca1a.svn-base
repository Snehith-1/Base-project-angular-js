        <h4 id="myModalLabel" class="modal-title"> 
            <span class="btn-label bg-primary">
                <i class="fa fa-plus ">&nbsp; Dropdown</i>
            </span> 
        </h4>
    </div>
    <div class="modal-body ng-scope">
        <form novalidate="" name="addlargetextbox" ng-submit="form.addlargetextbox()" autocomplete="off" class="form-validate form-horizontal">
            <div class="pd-sm-10">
                <div class="row">
                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                        <div class="row">
                            <div class="col-md-12 col-lg-12 col-sm-12">
                                <label>Field Type</label>&nbsp;<span style="color:red">*</span>
                                <div class="radio c-radio">
