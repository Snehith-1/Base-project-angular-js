                            <div class="col-sm-1 col-md-1 col-lg-1" style="padding-top:7px;">
                                <br />
                                <button class="btn btn-sm btn-default" style="background-color:#09385f;color:white;padding-left:9px;"
                                        ng-click="addlist()" ng-disabled="(txtPreviewText && txtValue) ?  false : true">
                                    <span class="fa fa-plus" />
                                </button>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-sm-12 col-lg-12 col-md-12">
                                <table class="table">
                                    <thead class="bg-gray text-white">
                                        <tr>
                                            <th>S.No</th>
                                            <th>Preview Text</th>
                                            <th>Value </th>
                                            <th ng-hide="txtdisabled">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="data in radiobuttonlistarray">
                                            <td>{{$index+1}}</td>
                                            <td>{{data.preview_text}}</td>
                                            <td>{{data.Value}}</td>
                                            <td ng-hide="txtdisabled" class="text-nowrap">

                                                <button title="Delete" ng-click="deletelist($index)"
                                                        class="btn btn-danger btn-xs ">
                                                    <em class="fa fa-trash-o"></em>
                                                </button>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

                <br />
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="addlargetextbox.$invalid && dropdownlistarray.length !=0" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                </div>
            </div>
            <br />
        </form>
    </div>
</script>

<script type="text/ng-template" id="/addCheckboxbutton.html">
    <div class="modal-header bg-primary ng-scope">

        <h4 id="myModalLabel" class="modal-title"> 
            <span class="btn-label bg-primary">
                <i class="fa fa-plus ">&nbsp; Checkbox</i>
            </span> 
        </h4>
    </div>
    <div class="modal-body ng-scope">
        <form novalidate="" name="addlargetextbox" ng-submit="form.addlargetextbox()" autocomplete="off" class="form-validate form-horizontal">
            <div class="pd-sm-10">
                <div class="row">
                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                        <div class="row ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <label>Group Name </label>&nbsp;<span style="color:red">*</span>
                                <select name="department" id="department" 
                                ng-options="m.input_fieldname for m in groupnamelist" chosen="" ng-change="onchangegroup()"
                                        ng-model="txtgroup_name" class="chosen-select form-control" style="height:37px;" required size="1">
                                    <option value=""> ----- Select Group Name -----</option>
                                </select> 
                            </div>
                        </div>
                        <br />
                        <div class="row ">
                            <div class="col-md-7 col-lg-7 col-sm-7"> 
                                <label class="col-form-label"> Field Name  <span style="color:red"> *</span></label>
                                <br />
                                <input type="text"  ng-model="txttextbox_name" ng-disabled="txtdisabled" tabindex="0" class="form-control" required  />
                            </div> 
                            <!-- <div class="col-md-4 col-lg-4 col-sm-4"> 
                                <label class="col-form-label"> Placeholder  <span style="color:red"> *</span></label>
                                <br />
                                <input type="text"  ng-model="txtplaceholder" ng-disabled="txtdisabled" tabindex="0" class="form-control" required  />
                            </div>   -->
                            <div class="col-md-5 col-lg-5 col-sm-5"> 
                                <label class="col-form-label"> Mandatory  <span style="color:red"> *</span></label>
                                <br />
                                <div class="radio c-radio" ng-hide="txtdisabled">
                                    <label>
                                        <input type="radio" id="Requiredcheck" name="Requiredcheck"  value="true" ng-model="rdbMandatory" required />
                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                    </label>
                                    <label>
                                        <input type="radio" id="Requiredcheck" name="Requiredcheck" value="false" ng-model="rdbMandatory" required />
                                        <span class="fa fa-circle"></span>No
                                    </label>
                                </div>
                                <div ng-show="txtdisabled"> 
                                    <input type="text"  ng-model="rdbMandatory" ng-disabled="txtdisabled" tabindex="0" class="form-control"  /> 
                                </div>
                             </div>
                        </div>
                        <br /> 
                        <div class="row" >
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <label class="col-form-label"> Preview Text  </label>
                                <textarea ng-model="txtPreviewText" rows="2" class="form-control"  ng-disabled="txtdisabled" style="resize:vertical;"
                                id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea> 
                            </div>
                            <!-- <div class="col-lg-4 col-md-4 col-sm-4">
                                <label> Value <span style="color:red">*</span></label>
                                <input type="text" ng-model="txtValue" required tabindex="0" class="form-control" /> 
                            </div> -->
                            
                        </div>
                        <br /> 
                    </div>
                </div>

                <br />
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="addlargetextbox.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                </div>
            </div>
            <br />
        </form>
    </div>
</script>
</div> 