                                    <select name="FieldMapping" id="FieldMapping" 
                                        ng-options="(m.fieldmapping_name + '/' + m.fieldmapping_code) for m in fieldmappinglist" 
                                        chosen=""  ng-model="cbofieldmapping" class="chosen-select form-control" style="height:37px;"
                                        ng-required="rdbInputfield=='DBField'" size="1">
                                        <option value=""> ----- Select Field Mapping -----</option>
                                    </select> 
