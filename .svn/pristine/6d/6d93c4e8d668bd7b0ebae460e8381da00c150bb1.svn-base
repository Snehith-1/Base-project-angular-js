<style>
    .space{ 
       margin: 10px 0px;
    }
</style>
<h3>
    Add Inbound Call
</h3>
<div ng-controller="MstInboundAddController as dp">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-plus"></i>
            </span>Add Inbound Call
        </div>
        <form novalidate="" name="addinbound" ng-submit="form.addinbound()" autocomplete="off" class="form-validate">
            <br>
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Basic Details </b></p>
                            <hr class="space" />
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-md-4 col-lg-4 col-sm-4">
                                        <label>Ticket Reference ID</label>
                                        <input type="text" class="form-control" placeholder="Auto Generated" disabled ng-model="txtticketrefid">
                                    </div>
                                    <div class="col-md-4 col-lg-4 col-sm-4">
                                        <label>Entity</label><span style="color:red;">&nbsp;*</span>
                                        <select class="chosen-select form-control" chosen="" id="entity" ng-options="m.entity_gid as m.entity_name  for m in entity_list" tabindex="0" ng-model="cboentity" required
                                                style="height:37px;" size="1">
                                            <option value=""></option>
                                        </select>
                                    </div>
                                    <div class="col-md-4 col-lg-4 col-sm-4">
                                        <label>Source of Contact</label><span style="color:red;">&nbsp;*</span>
                                        <select class="chosen-select form-control" chosen="" id="sourceofcontact" ng-options="m.sourceofcontact_gid as m.sourceofcontact_name for m in sourceofcontact_list" tabindex="0" ng-model="cbosourceofcontact" required
                                        style="height:37px;" size="1">
                                            <option value=""></option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-md-4 col-lg-4 col-sm-4">
                                        <label>Call Received Number</label><span style="color:red;">&nbsp;*</span>
                                        <select class="chosen-select form-control" chosen="" id="callreceivednumber" 
                                        ng-options="m.callreceivednumber_gid as  m.callreceivednumber_name for m in callreceivednumber_list" tabindex="0" ng-model="cbocallreceivednumber" required
                                        style="height:37px;" size="1">
                                            <option value=""></option>
                                        </select>
                                    </div>
                                    <div class="col-md-4 col-lg-4 col-sm-4">
                                        <label>Customer Type</label><span style="color:red;">&nbsp;*</span>
                                        <select class="chosen-select form-control" chosen="" id="customertype" tabindex="0" ng-model="cbocustomertype" required
                                                style="height:37px;" size="1">
                                            <option value="Existing">Existing</option>
                                            <option value="New">New</option>
                                            <option value="In Progress">In Progress</option>
                                            <option value="Others">Others</option>
                                            <option value=""></option>
                                        </select>
                                    </div>
                                    <div class="col-md-4 col-lg-4 col-sm-4">
                                        <label>Call Received Date</label>
                                        <input type="text" class="form-control" disabled tabindex="0" ng-model="txtcallreceived_date">
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-md-8 col-lg-8 col-sm-8">
                                        <label for="">Enquiry Name</label><span style="color:red;">&nbsp;*</span>
                                        <input type="text" ng-model="txtcaller_name" class="form-control" maxlength="150" style="text-transform:capitalize"  required>
                                    </div>
                                    <div class="col-md-4 col-lg-4 col-sm-4">
                                        <label for="">Internal Reference</label>
                                        <select class="chosen-select form-control" chosen="NA" id="internalreference" 
                                        ng-options="m.employee_gid as m.employee_name for m in employee_list" tabindex="0" ng-model="cbointernalreference" 
                                        style="height:37px;" size="1">
                                            <option value="">NA</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-md-8 col-lg-8 col-sm-8">
                                        <label for="">Company</label>
                                        <input type="text" ng-model="txtcallerassociate_company" style="text-transform:capitalize" class="form-control" maxlength="250">
                                    </div>
                                    <div class="col-md-4 col-lg-4 col-sm-4">
                                        <label for="">Office Landline Number</label>
                                        <input type="text" ng-model="txtoffice_landlineno" ng-pattern="/^[0-9]*$/" class="form-control" maxlength="11">
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-md-7 col-lg-7 col-sm-7">
                                        <div class="panel">
                                            <div class="panel-heading">
                                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Mobile Number(s)  </b></p>
                                                <hr class="space" class="space" />
                                            </div>
                                            <div class="panel-body" style="padding-top:0px;">
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-sm-4 col-md-4 col-lg-4" style="padding-right: 0px; width: 28%;">
                                                            <label><strong>Mobile Number </strong></label> <span style="color:red;">&nbsp;*</span>
                                                            <input ng-model="txtmobile_no" type="text" oninput="this.value = this.value.replace(/\D+/g, '')" 
                                                             maxlength= "10" minlength= "10" class="form-control">
                                                        </div>
                                                        <div class="col-sm-3 col-md-3 col-lg-3" style="padding: 0px 0px; width: 26%;">
                                                            <label>&nbsp;&nbsp;Primary Status </label> <span style="color:red">&nbsp;*</span>
                                                            <div class="radio c-radio">
                                                                <label>
                                                                    <input type="radio" id="primarymobileno" name="primarymobile_no" value="Yes" ng-model="rdbprimary_status" />
                                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                </label>
                                                                <label>
                                                                    <input type="radio" id="primarymobileno" name="primarymobile_no" value="No" ng-model="rdbprimary_status" />
                                                                    <span class="fa fa-circle"></span>No
                                                                </label>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-3 col-md-3 col-lg-3" style="padding: 0px 0px;width: 26%;">
                                                            <label>&nbsp;&nbsp;Whatsapp Status</label> <span style="color:red">&nbsp;*</span>
                                                            <div class="radio c-radio">
                                                                <label>
                                                                    <input type="radio" id="whatsapp_mobileno" name="whatsapp_mobileno" value="Yes" ng-model="rdbwhatsapp_status" />
                                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                </label>
                                                                <label>
                                                                    <input type="radio" id="whatsapp_mobileno" name="whatsapp_mobileno" value="No" ng-model="rdbwhatsapp_status" />
                                                                    <span class="fa fa-circle"></span>No
                                                                </label>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-2 col-md-2 col-lg-2" style="padding: 0px 0px;width: 20%;">
                                                            <label>&nbsp;&nbsp;SMS To</label> <span style="color:red">&nbsp;*</span>
                                                            <div class="radio c-radio">
                                                                <label>
                                                                    <input type="radio" id="sms_to" name="sms_to" value="Yes" ng-model="rdbsms_to" />
                                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                </label>
                                                                <label>
                                                                    <input type="radio" id="sms_to" name="sms_to" value="No" ng-model="rdbsms_to" />
                                                                    <span class="fa fa-circle"></span>No
                                                                </label>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>
                                                <br>
                                                <div class="row" align="center">
                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                        <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="add_mobileno()"><span class="fa fa-plus"> </span> Add </a>
                                                    </div>
                                                </div>
                                                <br>
                                                <div class="row">
                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                                            <div class="table-responsive table-bordered">
                                                                <div style="max-height:200px;overflow:auto;">
                                                                    <table class="table">
                                                                        <thead class="bg-gray text-white">
                                                                            <tr>
                                                                                <th>S.No</th>
                                                                                <th>Mobile Number</th>
                                                                                <th>Primary Status</th>
                                                                                <th>Whatsapp Status</th>
                                                                                <th>SMS To</th>
                                                                                <th>Action</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody>
                                                                            <tr ng-repeat="data in ibcallmobileno_list">
                                                                                <td>{{$index+1}}</td>
                                                                                <td>{{data.mobile_no}}</td>
                                                                                <td>{{data.primary_status}}</td>
                                                                                <td>{{data.whatsapp_status}}</td>
                                                                                <td>{{data.sms_to}}</td>
                                                                                <td class="text-nowrap">
                                                                                    <button title="Delete" ng-click="delete_mobileno(data.inboundcall2mobileno_gid)"
                                                                                            class="btn btn-danger btn-xs ">
                                                                                        <em class="fa fa-trash-o"></em>
                                                                                    </button>
                                                                                </td>
                                                                            </tr>
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-5 col-lg-5 col-sm-5">
                                        <div class="panel">
                                            <div class="panel-heading">
                                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Email Address(s) </b></p>
                                                <hr class="space" />
                                            </div>
                                            <div class="panel-body" style="padding-top:0px;">
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-sm-7 col-md-7 col-lg-7">
                                                            <label><strong> Email Address </strong><span style="color:red">&nbsp;*</span></label>
                                                            <input type="text" ng-model="txtemail_address" ng-pattern="appemail_pattern"  class="form-control">
                                                        </div>
                                                        <div class="col-sm-5 col-md-5 col-lg-5">
                                                            <label>Primary Status </label> <span style="color:red">&nbsp;*</span>
                                                            <div class="radio c-radio">
                                                                <label>
                                                                    <input type="radio" id="emailaddress" name="primaryemail_address" value="Yes" ng-model="rdbprimary_email" />
                                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                </label>
                                                                <label>
                                                                    <input type="radio" id="emailaddress" name="primaryemail_address" value="No" ng-model="rdbprimary_email" />
                                                                    <span class="fa fa-circle"></span>No
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row" align="center">
                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                        <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="add_emailaddress()"><span class="fa fa-plus"> </span> Add </a>
                                                    </div>
                                                </div>
                                                <br>
                                                <div class="row">
                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                                            <div class="table-responsive table-bordered">
                                                                <table class="table">
                                                                    <thead class="bg-gray text-white">
                                                                        <tr>
                                                                            <th>S.No</th>
                                                                            <th>Email Address</th>
                                                                            <th>Primary Status</th>
                                                                            <th>Action</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        <tr ng-repeat="data in ibcallemail_list">
                                                                            <td>{{$index+1}}</td>
                                                                            <td>{{data.email_address}}</td>
                                                                            <td>{{data.primary_status}}</td>
                                                                            <td class="text-nowrap">
                                                                                <a class="btn btn-xs  btn-danger" ng-click="delete_emailaddress(data.inboundcall2email_gid)"><span class="fa fa-trash-o"></span></a>
                                                                            </td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="dd-handle ng-scope">
                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Address Details </b><span style="color:red">&nbsp;*</span></p>
                            <hr class="space" />
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-sm-4 col-md-4 col-lg-4">
                                        <strong class="ng-binding w3-text-indigo">
                                            <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="address_add()"><span class="fa fa-plus"> </span>  Add Address</a>
                                        </strong>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="table-responsive table-bordered">
                                            <div style="max-height:200px;overflow:auto;">
                                                <table class="table">
                                                    <thead class="bg-gray text-white">
                                                        <tr width="100%">
                                                            <th scope="col">S.No</th>
                                                            <th scope="col">Address Type</th>
                                                            <th scope="col">Address</th>
                                                            <th> Primary Status</th>
                                                            <th scope="col">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in ibcalladdress_list" width="100%">
                                                            <td title="S.No">{{$index+1}}</td>
                                                            <td title="AddressType">{{data.addresstype_name}}</td>
                                                            <td title="Address">
                                                                {{data.addressline1}},{{data.addressline2}},<br />{{data.taluka}},{{data.district}},
                                                                <br />{{data.state}},{{data.country}},{{data.postal_code}}
                                                                <br /> <b>Latitude:</b> {{data.latitude}}
                                                                <br /> <b>Longitude:</b> {{data.longitude}}
                                                            </td>
                                                            
                                                            <td title="Primary Status">{{data.primary_status}}</td>                                                 
                                                            <td data-title="Action">                                                            
                                                                <button title="Delete" ng-click="address_delete(data.inboundcall2address_gid)"
                                                                        class="btn btn-danger btn-xs ">
                                                                    <em class="fa fa-trash-o"></em>
                                                                </button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />
                        </div>
                        <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                            <div class="row">
                                <br>
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-6 col-sm-6 col-md-6">
                                        <label>Enquiry Type</label><span style="color:red">&nbsp;*</span>
                                        <select class="chosen-select form-control" chosen="" id="call_type" tabindex="0" ng-model="cbocalltype" required
                                                style="height:37px;" size="1" ng-options="m.calltype_gid as m.calltype_name for m in calltype_list">
                                            <option value=""></option>
                                        </select>
                                    </div>
                                    <div class="col-lg-6 col-sm-6 col-md-6 ">
                                        <label>Function</label>
                                        <select class="chosen-select form-control" chosen="" id="function" tabindex="0" ng-model="cbofunction"
                                                style="height:37px;" size="1" ng-change="changefunctionstatus(telecallingfunction_name)" ng-options="m.telecallingfunction_gid as m.telecallingfunction_name for m in telecallingfunction_list">
                                            <option value=""></option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="row" ng-show="function_show">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <label>Function remarks</label><span style="color:red">&nbsp;*</span>
                                        <textarea class="form-control" rows="3" tabindex="0" ng-model="txtfunction_remarks" maxlength="500" style="resize:vertical;" placeholder="Enter Function remark"></textarea>
                                    </div>
                                </div>
                            </div>
                            <br>
                           
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <label>Requirement/Enquiry Title</label><span style="color:red">&nbsp;*</span>
                                        <textarea class="form-control" rows="3" tabindex="0" required ng-model="txtrequirement" maxlength="500" style="resize:vertical;" placeholder="Enter Requirement/Enquiry Title"></textarea>
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <label>Enquiry Detailed Remarks</label> <span style="color:red">&nbsp;*</span>
                                        <textarea class="form-control" rows="3" tabindex="0" required style="resize:vertical;" maxlength="5000" ng-model="txtenquiry_description" placeholder="Enter Enquiry Detailed Remarks"></textarea>
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <strong class="ng-binding w3-text-indigo">
                                            <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;"><span class="fa fa-phone-square fa-x"> </span> Enquiry Status</a><span style="color:red">&nbsp;*</span>
                                        </strong>    
                                        &nbsp;
                                        &nbsp;             
                                        <select name="call_closure_status" id="call_closure_status" required ng-model="cbocallclosure_status" class="form-control" style="height:32px;" size="1">
                                            <option value="">Select Enquiry Status</option>
                                            <option value="Assign">Assign</option>
                                            <option value="Follow Up">Follow Up</option>
                                            <option value="Closed">Closed</option>
                                        </select>
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4" ng-show="cbocallclosure_status == 'Assign'">
                                        <label>Assign Employee</label><span style="color:red">&nbsp;*</span>
                                        <select class="chosen-select form-control" chosen="" ng-options="m.employee_gid as m.employee_name for m in employee_list"
                                                id="assignemployee" tabindex="0" ng-model="cboassignemployee" style="height:37px;" size="1">
                                            <option value=""></option>
                                        </select>
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4" ng-show="cbocallclosure_status == 'Assign'">
                                        <label>TAT in Hrs</label>
                                        <input type="text" ng-model="txttat_hours" ng-pattern="/^[0-9]*$/" class="form-control" maxlength="2">                                     
                                    </div>
                                </div>
                            </div>
                            <br>


                        <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-4 col-sm-4 col-md-4" ng-show="cbocallclosure_status == 'Assign'">
                                        <label>Tag Employee</label>
                                        <select class="chosen-select form-control" chosen="" multiple ng-options="m.employee_name for m in employee_list" id="tagemployee" tabindex="0" ng-model="cbotagemployee"
                                                style="height:37px;" size="1">
                                            <option value=""></option>
                                        </select>
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4" ng-show="cbocallclosure_status == 'Assign'">
                                        <label>Allocated TAT Date</label><span style="color:red">&nbsp;*</span>
                                        <p class="input-group">
                                            <input type="text" id="allocated_tat" name="date" ng-model="txttat_date" datepicker-popup="{{dp.format}}"min-date="minDate"
                                                   style="height:36px;" is-open="dp.open3"  datepicker-options="dp.dateOptions" 
                                                   close-text="Close" class="form-control" 
                                                   ng-change="tatdate_change(txttat_date)" placeholder="DD-MM-YYYY" />
                                            <span class="input-group-btn">
                                                <button type="button" ng-click="dp.calender3($event)" style="height:36px;" class="btn btn-default">
                                                    <em class="fa fa-calendar"></em>
                                                </button>
                                            </span>
                                        </p>
                                     </div>
                                     <div class="col-lg-1 col-sm-1 col-md-1" ng-show="cbocallclosure_status == 'Assign'">
                                        <label for="">No.Of.days</label>
                                        <input id="allocated_days" ng-model="txttat_days" tabindex="0" class="form-control" disabled>

                                    
                                     </div>

                                </div>
                            </div>
                                                   

                            <div class="row" ng-show="cbocallclosure_status == 'Follow Up'">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>Follow Up Date</label>
                                        <p class="input-group">
                                            <input type="text" id="Followupdate" name="date" ng-model="txtfollowup_date" datepicker-popup="{{dp.format}}"min-date="minDate"
                                                   style="height:36px;" is-open="dp.open1" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                                   close-text="Close" class="form-control" placeholder="DD-MM-YYYY" />
                                            <span class="input-group-btn">
                                                <button type="button" ng-click="dp.calender1($event)" style="height:36px;" class="btn btn-default">
                                                    <em class="fa fa-calendar"></em>
                                                </button>
                                            </span>
                                        </p>
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>Follow Up Time</label>
                                        <input type="time" id="followupTime" ng-model="txtfollowup_time" tabindex="0" class="form-control" min="01:00" max="24:00"><i>hh:mm</i>
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4" style="padding-top: 3px;">
                                        <br>
                                        <strong class="ng-binding w3-text-indigo">
                                            <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white; height:35px; padding-top: 7px;" ng-click="add_followup()"><span class="fa fa-plus"> </span>  Add</a>
                                        </strong>
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="row" ng-show="cbocallclosure_status == 'Follow Up'">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="table-responsive table-bordered">
                                            <div style="max-height:200px;overflow:auto;">
                                                <table class="table">
                                                    <thead class="bg-gray text-white">
                                                        <tr width="100%">
                                                            <th scope="col">S.No</th>
                                                            <th scope="col">Follow Up Date</th>
                                                            <th scope="col">Follow Up Time</th>
                                                            <th scope="col">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in ibcallfollowup_list" width="100%">
                                                            <td title="S.No">{{$index+1}}</td>
                                                            <td title="Follow Up Date">{{data.followup_date}}</td>
                                                            <td title="Follow Up Time">{{data.followup_time}}</td>
                                                            <td data-title="Action">
                                                                <button title="Delete" ng-click="delete_followup(data.inboundcall2followup_gid)"
                                                                        class="btn btn-danger btn-xs ">
                                                                    <em class="fa fa-trash-o"></em>
                                                                </button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <label>Remarks by Inbound Team</label><span style="color:red">&nbsp;*</span>
                                        <textarea class="form-control" rows="3" tabindex="0" required style="resize:vertical;" ng-model="txtassignclosure_remarks" placeholder="Enter Remarks by Inbound Team"></textarea>
                                    </div>
                                </div>
                            </div>
                            <br>
                        </div>
                            </div>
                        </div>
            </div>
            <br>
            <br>
            <div>
                <div class="row text-center">
                    <!--<button type="button" class="btn btn-primary" ng-disabled="cboentity.$invalid" ng-click="save()"> <em class="fa fa-save"></em>&nbsp;&nbsp;Save as Draft&nbsp;&nbsp;</button>-->
                    <button type="button" class="btn btn-success btn-md" ng-disabled="addinbound.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>
                    <button type="button" ng-click="back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                </div>
                <br>
                <br>
            </div>
        </form>
    </div>
    <script type="text/ng-template" id="/addaddress.html">
        <div class="text-center">
            <md-toolbar>
                <div class="md-toolbar-tools">
                    <em style="font-size:16px" class="fa fa-plus"></em>&nbsp;<label style="font-size:16px">Add Address</label>
                </div>
            </md-toolbar>
        </div>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="addaddress" ng-submit="form.addaddress()" autocomplete="off" class="form-validate">
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-9 col-md-9 col-sm-9">
                                <label><strong>Address Type </strong> <span style="color:red"> *</span></label>
                                <select name="vertical_code" required="" ng-options="m.address_type for m in addresstype_list" style="height:37px;"
                                        ng-model="cboaddresstype" class="form-control " chosen=""
                                        size="1">
                                    <option value=""></option>
                                </select>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <label>Primary Status </label> <span style="color:red">*</span>
                                <div class="radio c-radio">
                                    <label>
                                        <input type="radio" id="primary" name="primaryaddress" required value="Yes" ng-model="rdbprimaryaddress" />
                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                    </label>
                                    <label>
                                        <input type="radio" id="primary" name="primaryaddress" required value="No" ng-model="rdbprimaryaddress" />
                                        <span class="fa fa-circle"></span>No
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-sm-12 col-sm-12 col-sm-12">
                                <label><strong>Address Line 1 </strong> <span style="color:red">*</span></label>
                                <input type="text" ng-model="txtaddressline1" tabindex="0" class="form-control" maxlength="300" required/>
                                <label style="font-weight:bolder;color:red" ng-show="geocodingFailed">Invalid Address!</label>
                            </div>
                        </div>
                    </div>
                    <br/>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-sm-12 col-sm-12 col-sm-12">
                                <label><strong>Address Line 2 </strong></label>
                                <input type="text" ng-model="txtaddressline2" tabindex="0" class="form-control" maxlength="300" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Land Mark </strong> </label>
                                    <input type="text" ng-model="txtLand_Mark" tabindex="0" class="form-control"  maxlength="100" />

                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Postal Code </strong> <span style="color:red">*</span></label>
                                    <input type="text" ng-model="txtpostal_code" tabindex="0" class="form-control" required
                                    ng-pattern="/^[1-9]{1}[0-9]{5}$/" ng-change="onchangepostal_code(); getGeoCoding();" autofocus required maxlength= "6" minlength= "6" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label>City</label>
                                    <input type="text" ng-model="txtcity" disabled ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" tabindex="0" class="form-control" />
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label>Taluka</label>
                                    <input type="text" ng-model="txttaluka" tabindex="0" class="form-control" disabled ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" />

                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label><strong>District</strong> </label>
                                    <input type="text" ng-model="txtdistrict" tabindex="0" class="form-control" disabled ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" />
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label>State </label>
                                    <br />
                                    <input type="text" ng-model="txtstate" tabindex="0" class="form-control" disabled ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" />
                                </div>

                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label>Country</label> <span style="color:red">*</span>
                                    <input type="text" ng-model="txtcountry" tabindex="0" class="form-control" required ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" />

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Latitude </strong></label>
                                    <input type="text" ng-model="txtlatitude" tabindex="0" class="form-control" disabled />
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Longitude </strong></label>
                                    <input type="text" ng-model="txtlongitude" tabindex="0" class="form-control" disabled />
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                <br>
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="addaddress.$invalid" ng-click="addressSubmit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                </div>
                <br>
            </div>
        </div>
    </script>
</div>
