<h3>
    Tenure
    <a href="#/app/MstHRLoanTenure">
        <button type="button" title="Back" class="pull-right btn btn-sm btn-primary ">
            <em class="fa fa-arrow-circle-left fa-1x"></em>
        </button>
    </a>
</h3>
<div ng-controller="MstHRLoanTenureAddController">
    <div class="panel panel-primary">
        <div class="panel-heading">
            &nbsp;<span class="btn-label bg-primary">
                <i class="fa fa-plus "></i>
            </span> Add Tenure
        </div>
        <div class="panel-body">
            <form novalidate="" name="Addtenure" ng-submit="form.Addtenure()" autocomplete="off" class="form-validate form-horizontal">
                <div class="dd-handle ng-scope">   
                    <div class="row">                                                                       
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <br>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label">HR Document Type &nbsp;<span style="color:red">*</span></label>
                                <br />
                                <select name="Document Type" id="Document Type" ng-options="m.hrloantypeoffinancialassistance_name for m in typeofdocument_list" chosen=""
                                    ng-model="cbohrdocument" class="chosen-select form-control" required style="height:37px;">
                                <option value=""></option>
                                </select>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">                                              
                                <label>LMS Code &nbsp;</label>
                                <input ng-model="txtlms_code" 
                                    tabindex="1" aria-required="true" aria-invalid="true" maxlength="1000" class="form-control">
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">                                              
                                <label>Bureau Code &nbsp;</label>
                                <input ng-model="txtbureau_code" 
                                    tabindex="1" aria-required="true" aria-invalid="true" maxlength="1000" class="form-control">
                            </div>
                        </div>                                                      
                    </div>
                    <br/>
                </div>
                <br/>
                <div class="text-center">                    
                    <button type="button" class="btn btn-success btn-md" ng-disabled="Addtenure.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                </div>
            </form>
        </div>
    </div>
</div>                                                                             
                                        
                                    
                          

                        

                    
           