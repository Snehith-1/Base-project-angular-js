                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <div class="mda-form-group">
                                <label>State </label> <span style="color:red">*</span>
                                <br />
                                <select name="state_code" required ng-options="m.state_name for m in state_list"
                                        ng-model="cbostate" class="chosen-select form-control" chosen="" style="height:37px;"
                                        size="1">
                                    <option value=""> -----Select State-----</option>
                                </select>
                            </div>

                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <div class="mda-form-group">
                                <label>Country</label> <span style="color:red">*</span>
                                <input type="text" ng-model="txtcountry" required tabindex="0" class="form-control" />

                            </div>
                        </div>
                    </div>
                </div>
                <br />
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="userform1.$invalid" ng-click="addressSubmit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>

                </div>
            </form>
        </div>
    </div>
</script>
<script type="text/ng-template" id="/StaticMapAndPhotosView.html">
