<style>
    .nav.nav-pills .active > a {
        background-color: rgb(19,14,89);
        background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);
        color: #ffffff;
    }
</style>
<h3>
    Credit Underwriting
</h3>
<div ng-controller="AgrTrnSuprCreditExistingBankDtlEditController as form">
    <div class="panel">
        <div class="panel-heading" style="background-color:rgb(19,14,89);background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);color:#ffffff">
            <h5> Edit Existing Bank Facility </h5>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="panel-body">
                        <div class="row">
                            <form name="form.formValidate" ng-submit="form.submitForm()" novalidate="" class="form-validate form-horizontal">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="row">
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <div class="dd-handle ng-scope">
                                                <div class="row">
                                                    <div class="col-sm-12 col-md-12 col-lg-12">
                                                        <p class="text-primary" style="font-size:14px;"><b> Existing Bank Details </b></p>
                                                        <hr />
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> Bank Name <span style="color:red"> *</span></label>
                                                            <br />
                                                            <select name="bankdtl_name" id="bankdtl_name" chosen="" ng-options="m.bankdtl_gid as m.bankdtl_name for m in bankdtl_list"
                                                                    type="text" class="form-control parsley-error" ng-model="cboBankName" required="">
                                                                <option value=""> ----- Select From Dropdown -----</option>
                                                            </select>
                                                            <span ng-show="form.validateInput('bankdtl_name', 'required')" class="text-danger">This field is required</span>
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> Facility Type </label>
                                                            <br />
                                                            <select name="credit_underwriting_facility_type" id="credit_underwriting_facility_type" chosen="" ng-options="m.creditunderwritingfacilitytype_gid as m.credit_underwriting_facility_type for m in creditunderwritingfacilitytype_list"
                                                                    type="text" class="form-control parsley-error" ng-model="cboFacilityType">
                                                                <option value=""> ----- Select From Dropdown -----</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <div ng-controller="MstCreditExistingBankDtlEditController as dp">
                                                                <label class="col-form-label"> Facility Sanctioned On </label>
                                                                <br />
                                                                <p class="input-group">
                                                                    <input type="text" id="FacilitySanctionedOn" name="FacilitySanctionedOn" ng-model="$parent.txtfacilitysanction_date" datepicker-popup="{{dp.format}}"
                                                                           style="height:36px;" is-open="dp.open1" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                                                           close-text="Close" class="form-control" placeholder="DD-MM-YYYY" />
                                                                    <span class="input-group-btn">
                                                                        <button type="button" ng-click="dp.calender1($event)" style="height:36px;" class="btn btn-default">
                                                                            <em class="fa fa-calendar"></em>
                                                                        </button>
                                                                    </span>
                                                                </p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-sm-12 col-md-12 col-lg-12">
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> Type of existing funded </label>
                                                            <br />
                                                            <select name="fundedtypeindicator_name" id="fundedtypeindicator_name" chosen="" ng-options="m.fundedtypeindicator_gid as m.fundedtypeindicator_name for m in fundedtypeindicator_list"
                                                                    type="text" class="form-control parsley-error" ng-model="cboTypeofexistingfunded">
                                                                <option value=""> ----- Select From Dropdown -----</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> Sanctioned Limit</label>
                                                            <br />
                                                            <input type="text" ng-model="txtSanctioned_Limit" name="SanctionedLimit" id="SanctionedLimit" tabindex="0" class="form-control" maxlength="15"
                                                                   ng-change="SanctionedLimit()" ng-pattern="/^[0-9,]+(\.[0-9]{1,2})?$/" step="0.01" style="text-align:right;" />
                                                            <span style="font-style:italic;">Number in words:</span>
                                                            <span id="words_totalamount4" style="color:#003c96fc;" class="text-bold"></span>
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> Instalment Frequency </label>
                                                            <br />
                                                            <select name="creditinstalmentfrequency_name" id="creditinstalmentfrequency_name" chosen="" ng-options="m.creditinstalmentfrequency_gid as m.creditinstalmentfrequency_name for m in creditinstalmentfrequency_list"
                                                                    type="text" class="form-control parsley-error" ng-model="cboInstalmentfrequency">
                                                                <option value=""> ----- Select From Dropdown -----</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-sm-12 col-md-12 col-lg-12">
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> Instalment amount </label>
                                                            <br />
                                                            <input type="text" ng-model="txtInstalment_amount" name="Instalmentamount" id="Instalmentamount" tabindex="0" class="form-control" maxlength="15"
                                                                   ng-change="Instalmentamount()" ng-pattern="/^[0-9,]+(\.[0-9]{1,2})?$/" step="0.01" style="text-align:right;" />
                                                            <span style="font-style:italic;">Number in words:</span>
                                                            <span id="words_totalamount1" style="color:#003c96fc;" class="text-bold"></span>
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> Outstanding Amount </label>
                                                            <br />
                                                            <input type="text" ng-model="txtOutstanding_amount" name="Outstandingamount" id="Outstandingamount" tabindex="0" class="form-control" maxlength="15"
                                                                   ng-change="Outstandingamount()" ng-pattern="/^[0-9,]+(\.[0-9]{1,2})?$/" step="0.01" style="text-align:right;" />
                                                            <span style="font-style:italic;">Number in words:</span>
                                                            <span id="words_totalamount2" style="color:#003c96fc;" class="text-bold"></span>
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <div ng-controller="MstCreditExistingBankDtlEditController as dp">
                                                                <label class="col-form-label"> Record Date </label>
                                                                <br />
                                                                <p class="input-group">
                                                                    <input type="text" id="RecordDate" name="RecordDate" ng-model="$parent.txtRecord_Date" datepicker-popup="{{dp.format}}"
                                                                           style="height:36px;" is-open="dp.open2" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                                                           close-text="Close" class="form-control" placeholder="DD-MM-YYYY" />
                                                                    <span class="input-group-btn">
                                                                        <button type="button" ng-click="dp.calender2($event)" style="height:36px;" class="btn btn-default">
                                                                            <em class="fa fa-calendar"></em>
                                                                        </button>
                                                                    </span>
                                                                </p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-sm-12 col-md-12 col-lg-12">
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> Overdue amount </label>
                                                            <br />
                                                            <input type="text" ng-model="txtOverdue_amount" name="Overdueamount" id="Overdueamount" tabindex="0" class="form-control" maxlength="15"
                                                                   ng-change="Overdueamount()" ng-pattern="/^[0-9,]+(\.[0-9]{1,2})?$/" step="0.01" style="text-align:right;" />
                                                            <span style="font-style:italic;">Number in words:</span>
                                                            <span id="words_totalamount3" style="color:#003c96fc;" class="text-bold"></span>
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> Overdue if any (DPD) </label>
                                                            <br />
                                                            <input type="text" ng-model="txtoverdue_dpd" name="Overduedpd" id="Overduedpd" tabindex="0" class="form-control" maxlength="15"
                                                                   ng-change="Overduedpd()" ng-pattern="/^[0-9,]+(\.[0-9]{1,2})?$/" step="0.01" style="text-align:right;" />
                                                            <span style="font-style:italic;">Number in words:</span>
                                                            <span id="words_totalamount5" style="color:#003c96fc;" class="text-bold"></span>
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> Account Classification </label>
                                                            <br />
                                                            <select name="creditaccountclassification_name" id="creditaccountclassification_name" chosen="" ng-options="m.creditaccountclassification_gid as m.creditaccountclassification_name for m in creditaccountclassification_list"
                                                                    type="text" class="form-control parsley-error" ng-model="cboAccountClassification">
                                                                <option value=""> ----- Select From Dropdown -----</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <label>Remarks </label>
                                                            <textarea style="resize:vertical;" rows="4" name="Remarks" id="Remarks" placeholder="Enter Remarks..." aria-multiline="true" tabindex="0" aria-invalid="false" maxlength="3000"
                                                                      ng-model="txtremarks" class="form-control"></textarea>
                                                            <br />
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />

                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <br />
                                            <div class="text-center">
                                                <button type="submit" class="btn btn-success btn-md" ng-click="update_ExistingBankacctdtl()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                                                <button ng-click="ExistingBankacctdtl_Back()" type="button" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <br />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>