                    <br />
                    <div class="row" ng-show="transferlist">
                        <
                        <div class="col-sm-12 col-lg-12 col-md-12">
                            <label>Allocated Details </label>
                            <div class="table-responsive">
                                <table class="table table-striped table-secondary">
                                    <thead>
                                        <tr>
                                            <th align="center" scope="col">S.No</th>
                                            <th align="center" scope="col">Assigned Member Name</th>
                                            <th align="center" scope="col">Assigned Date/Time</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="data in allocatedlist">
                                            <td align="left" scope="col" title="S.No">
                                                {{$index+1}}
                                            </td>
                                            <td align="left" scope="col" title="Assigned Member">
                                                {{data.assigned_membername}}
                                            </td>


                                            <td align="left" scope="col" title="Assigned Date/Time">
                                                {{data.assigned_date}}
                                            </td>


                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <br />
                </div>
                <br />
                <div>
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="managerformsubmit.$invalid" ng-click="allocateSubmit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Allocate</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                    <br />
                </div>
            </form>
        </div>
    </script>
</div>
