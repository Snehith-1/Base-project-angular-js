<h3>
   Customer - Misdata
</h3>
<div ng-controller="customer2misdataController">
    <div class="row text-center">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <label style="font-size:16px;color:darkcyan;" class="col-form-label w3-text-indigo">Customer Name &nbsp;:&nbsp;&nbsp;</label><b>{{customer_name}}</b>
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <label style="font-size:16px;color:darkcyan;" class="col-form-label w3-text-indigo">URN &nbsp;:</label>&nbsp;&nbsp;&nbsp;<b>{{urn}}</b>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <br />
            <div class="col-lg-11 col-sm-11 col-md-11">
                <p class="bb text-green text-bold">Loan Details</p>
                <!--<div class="panel-body table-responsive">-->
                <table id="statictable" class="row-border hover">
                    <thead>
                        <tr class="bg-success text-white">
                            <th>Account No</th>
                            <th>Disbursement Date</th>
                            <th>Maturity Data</th>
                            <th>OD Days</th>
                            <th>Last payment</th>
                            <th>Schedule Payment</th>
                            <th>Tenure / Frequency</th>
                           
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="Loan in mdlMisdataimport">
                            <td data-title="'Name''">{{Loan.account_no}}</td>
                            <td data-title="'Age''">{{Loan.disbursement_date}}</td>
                            <td data-title="'Designation''">{{Loan.maturity_date}}</td>
                            <td data-title="'Mobile No'">{{Loan.od_days}}</td>
                            <td data-title="'Designation''">{{Loan.last_payment}}</td>
                            <td data-title="'Mobile No'">{{Loan.schedulde_payment}}</td>
                            <td data-title="'Designation''">{{Loan.tenure}}</td>
                            <!--<td data-title="'Mobile No'">{{Loan.od_days}}</td>-->
                        </tr>
                    </tbody>
                </table>
                <!--</div>-->
            </div>
            </div>
        </div>
    <br/>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="text-center">
              <button type="button" class="btn btn-success btn-md" ng-disabled="userform.$invalid" ng-click="requestSubmit(requestcompliance_gid)"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp; Process</button>&nbsp;&nbsp;
             
                <button ng-click="requestback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Close</button>&nbsp;&nbsp;
            </div>
        </div>
    </div>
    </div>
