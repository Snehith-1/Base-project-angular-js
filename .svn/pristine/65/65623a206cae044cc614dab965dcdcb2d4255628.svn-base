<h3>
    Closed Call
</h3>
<div ng-controller="MstTeleCallingCloseController as dp">
    <div class="panel">
        <form novalidate="" name="closecall" ng-submit="form.closecall()" autocomplete="off" class="form-validate">
            <div class="panel-heading bg-primary">
                <h5><i class="fa fa-eye"></i> &nbsp; Closed Call </h5>
            </div>
            <div class="modal-body ng-scope">
                <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                    <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Completed Details </b></p>
                    <hr class="space" />
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Completed By</label><br>
                                {{txtcompleted_by}}
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Completed Date</label><br>
                                {{txtcallclosure_status}}
                            </div>
                            <div class="col-lg-6 col-sm-6 col-md-6">
                                <label>Completed Remarks</label><br>
                                {{txtcompleted_remarks}}
                            </div>

                        </div>
                    </div>
                    <br>
                </div>
                <div class="dd-handle ng-scope">
                    <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Assigned Details </b></p>
                    <hr class="space" />
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Ticket Reference ID</label><br>
                                {{txtticket_refid}}
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Enquiry Status</label><br>
                                {{txtcallclosure_status}}
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Assigned Employee</label><br>
                                {{txtassignemployee_name}}
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Entity</label><br>
                                {{txtentity_name}}
                            </div>
                        </div>
                    </div>
                    <br>
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Source of Contact</label><br>
                                {{txtsourceofcontact_name}}
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Call Received Number</label><br>
                                {{txtcallreceivednumber_name}}
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Customer Type</label><br>
                                {{txtcustomer_type}}
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Call Received Date</label><br>
                                {{txtcallreceived_date}}
                            </div>
                        </div>
                    </div>
                    <br>
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label for="">Enquiry Name</label><br>
                                {{txtcaller_name}}
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label for="">Internal Reference</label> <br>
                                {{txtinternalreference_name}}
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label for="">Office Landline Number</label><br>
                                {{txtoffice_landlineno}}
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label for="">Company</label><br>
                                {{txtcallerassociate_company}}
                            </div>
                        </div>
                    </div>
                    <br>
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Enquiry Type</label><br>
                                {{txtcalltype_name}}
                            </div>
                            
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Tagged Employee</label><br>
                                {{txttagemployee_name}}
                            </div>
                           
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Allocated TAT (days)</label><br>
                                {{txttat_days}}
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Allocated TAT Date</label><br>
                                {{txttat_date}}
                            </div>
                        </div>
                    </div>
                    <br>
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>TAT in Hrs</label><br>
                                {{txttat_hours}}
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Function</label><br>
                                {{txtfunction_name}}
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3">
                                <label>Function Remarks</label><br>
                                {{txtfunction_remarks}}
                            </div>
                        </div>
                    </div>
                    <br>
                </div>
                        <div class="dd-handle ng-scope">
                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Post Closure Details </b></p>
                            <hr class="space" />
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-3 col-sm-3 col-md-3">
                                        <label>Call Closure Status </label><span style="color:red;">&nbsp; *</span>
                                        <select name="closecall" id="closecall" required ng-model="cboclosurestatus" ng-change="changeclosurestatus(cboclosurestatus)" class="form-control" style="height:32px;" size="1">
                                            <option value="">Select Call Closure Status</option>
                                            <option value="Closed">Closed</option>
                                            <option value="Extend Follow Up">Extend Follow-Up</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row" ng-show="followup_show">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-3 col-sm-3 col-md-3">
                                        <label>Follow Up Date</label>
                                        <p class="input-group">
                                            <input type="text" id="Followupdate" name="date" ng-model="txtfollowup_date" datepicker-popup="{{dp.format}}"
                                                   style="height:36px;" is-open="dp.open1" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                                   close-text="Close" class="form-control" placeholder="DD-MM-YYYY" />
                                            <span class="input-group-btn">
                                                <button type="button" ng-click="dp.calender1($event)" style="height:36px;" class="btn btn-default">
                                                    <em class="fa fa-calendar"></em>
                                                </button>
                                            </span>
                                        </p>
                                    </div>
                                    <div class="col-lg-3 col-sm-3 col-md-3">
                                        <label>Follow Up Time</label>
                                        <input type="time" ng-model="txtfollowup_time" tabindex="0" class="form-control" min="01:00" max="24:00"><i>hh:mm</i>
                                    </div>
                                    <div class="col-lg-6 col-sm-6 col-md-6">
                                        <label>Follow Up Remarks</label>
                                        <textarea style="resize:vertical;" class="form-control" rows="3" tabindex="0" ng-model="txtremarks" maxlength="500" style="resize:vertical;" placeholder="Enter Remarks"></textarea>
                                    </div>
                                </div>
                            </div>
                            <div class="row" ng-show="Closed_show">
                                <div class="col-sm-12 col-lg-12 col-md-12">
                                    <div class="col-sm-12 col-lg-12 col-md-12">
                                        <label>Closed Remarks </label><span style="color:red;">&nbsp; *</span>
                                        <textarea  style="resize:vertical;" ng-model="closedremarks" placeholder="Enter Remarks" rows="3" class="form-control"
                                                  id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br><br />
                        <div class="row text-center">
                            <button type="button" class="btn btn-success btn-md" ng-disabled="closecall.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>
                            <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                        </div>
                        <br>
                    </div>
</form>
</div>
</div>
