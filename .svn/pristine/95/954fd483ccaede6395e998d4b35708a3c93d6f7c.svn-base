<h3>
    Institution Grouping
</h3>
<div ng-controller="MstSAOnboardingInstitutionRenewalGroupingController">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="panel panel-default">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div style="color :#000000 ;font-size:14px;font-weight :bold;background-color:#3a4ccb;color:white;padding-top:10px;padding-bottom:10px;">
                            <h5>&nbsp;&nbsp; <em class="fa fa-users" style="font-size: 18px;"></em>&nbsp;&nbsp;Institution Grouping Details</h5>
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="table-responsive">
                                <table datatable="ng" class="table table-hover table-striped">
                                    <thead>
                                        <tr class="text-white" style="background-color:#09385f;">
                                            <th> S.No </th>
                                            <th> SBA Auto-Generated ID </th>
                                            <th> Samfin Code</th>
                                            <th> Samagro Code</th>
                                            <th> Samunnati Associate Name </th>
                                            <th> SBA Type</th>
                                            <th> SBA Entity Type</th>
                                            <th> Referred By</th>
                                            <th> Origination</th>
                                            <th> Status</th>
                                            <th> Renewal Status</th>
                                            <th> RM Tagged</th>
                                            <th> Next Renewal Date</th>
                                            <th class="text-center">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="data in saOnboardSummary_list">
                                            <td>{{$index+1}}</td>
                                            <td>{{data.sa_autogeneratedid}}</td>
                                            <td>{{data.samfin_code}}</td>
                                            <td>{{data.samagro_code}}</td>
                                            <td>{{data.sa_associatename}}</td>
                                            <td>{{data.satype_name}}</td>
                                            <td>{{data.saentitytype_name}}</td>
                                            <td>{{data.referred_by}}</td>
                                            <td>{{data.recordsource}}</td>
                                            <td>{{data.approvalstatus}}</td>
                                            <td>{{data.renewal_status}}</td>
                                            <td>{{data.sa_updated_by}}</td>
                                            <td>{{data.agreementrenewal_date}}</td>

                                            <td class="nowrap" data-title="'Action'" style="text-align: center;">
                                                <button title="View" ng-click="renewalinstitution(data.sacontactinstitution_gid,data.saentitytype_gid)" class="btn btn-info">
                                                    <em class="fa fa-eye"></em>
                                                </button>

                                                <button title="Renewal" ng-hide="data.renewal_flag == 'Y'" ng-click="renewal(data.sacontactinstitution_gid,data.saentitytype_gid);"  class="btn btn-info btn-sm" style="background-color:#0828c7;">
                                                    <em class="icon-refresh"></em>
                                                </button>
                                            </td>
                                        </tr>


                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="text-center">
                        <button ng-click="back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                    </div>
                    <br />
                </div>
            </div>
        </div>
    </div>
</div>
