    <script type="text/ng-template" id="/Receivableadd.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-plus"></em>&nbsp;<label style="font-size:16px">Add Receivable</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>
        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="receivableaddform" ng-submit="form.receivableaddform()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label><strong> Date </strong> <span style="color:red"> *</span> </label>
                                <p class="input-group">
                                    <input type="text" id="receivable_date" name="receivable_date" ng-model="txtreceivable_date" datepicker-popup="{{format}}"
                                           style="height:36px;" is-open="open16" min-date="minDate" datepicker-options="dateOptions"
                                           close-text="Close" class="form-control" required placeholder="DD-MM-YYYY" />
                                    <span class="input-group-btn">
                                        <button type="button" ng-click="calenderopen16($event)" style="height:36px;" class="btn btn-default">
                                            <em class="fa fa-calendar"></em>
                                        </button>
                                    </span>
                                </p>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label"> 01-30 days </label>
                                <input type="text" ng-model="txtonetothirty_days" class="form-control" ng-pattern="/^[0-9]*$/">
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label"> 31-60 days </label>
                                <input type="text" ng-model="txtthirtyonetosixty_days" class="form-control" ng-pattern="/^[0-9]*$/">
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label"> 61-90 days </label>
                                <input type="text" ng-model="txtsixtyonetoninety_days" class="form-control" ng-pattern="/^[0-9]*$/">
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label"> 90 days </label>
                                <input type="text" ng-model="txtninety_days" class="form-control" ng-pattern="/^[0-9]*$/">
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                            </div>
                        </div>
                    </div>
                    <br /><br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="receivableaddform.$invalid" ng-click="receivable_submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>

                    </div>
                </form>
            </div>
        </div>
    </script>

</div>