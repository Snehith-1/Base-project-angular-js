<h3>
    &nbsp;&nbsp;Unreconciliation View
</h3>

<style>
    #bordersty {
        border: 1px solid #ebebeb;
        padding: 10px;
        border-radius: 25px;
    }
</style>
<style>
    .buttonstyle {
        border: none;
        background-color: rgb(255, 255, 255);
        color: #141478;
    }
</style>
<div ng-controller="osdTrnUnreconciliationTransactionViewController">
    <div class="panel">
        <div class="row">
            <div class="col-lg-12 col-sm-12 col-md-12">
                <div class="col-lg-11 col-sm-11 col-md-11">
                    <h3 style="color:#141478;"> &nbsp;&nbsp; Unreconciliation View </h3>
                    <hr />
                </div>
                <div class="col-lg-1 col-sm-1 col-md-1">
                    <br />
                    <a>
                        <button type="button" title="Back to Summary" ng-click="Back()" class="pull-right btn btn-sm btn-primary ">
                            <em class="fa fa-arrow-circle-left fa-1x"></em>
                        </button>
                    </a>

                </div>

            </div>
        </div>


        <div style="margin-bottom:-10px;">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div id="bordersty" style="background-color: #FAF8F8;">
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                        <label class="text-primary">
                                            Reference  Number
                                        </label>
                                        <br>
                                        <span>
                                            {{lblbanktransc_gid}}
                                        </span>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                        <label class="text-primary">
                                            Bank Name
                                        </label>
                                        <br>
                                        <span>
                                            {{lblbank_name}}
                                        </span>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                        <label class="text-primary">
                                            Cheque No.
                                        </label>
                                        <br>
                                        <span>
                                            {{lblchq_no}}
                                            <!-- <label class="text-primary">
                                                Customer Reference No.
                                            </label>
                                            <br>
                                            <span>
                                                {{lblcustomer_refno}} -->
                                        </span>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                        <label class="text-primary">
                                            Branch Name
                                        </label>
                                        <br>
                                        <span>
                                            {{lblbranch_name}}
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                        <label class="text-primary">
                                            Credit/Debit
                                        </label>
                                        <br>
                                        <span>
                                            {{lblcr_dr}}<br>
                                        </span>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                        <label class="text-primary">
                                            Transaction Value
                                        </label>
                                        <br>
                                        <span>
                                            {{lbltransc_value}}
                                        </span>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                        <label class="text-primary">
                                            Transaction Particulars
                                        </label>
                                        <br>
                                        <span>
                                            {{lbltransact_particulars}}
                                        </span>
                                        <!-- <label class="text-primary">
                                            Transaction Balance
                                        </label>
                                        <br>
                                        <span>
                                            {{lbltransc_balance}}
                                        </span> -->
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                        <label class="text-primary">
                                            Remarks
                                        </label>
                                        <br>
                                        <span>
                                            {{lblremarks}}
                                        </span>
                                    </div>

                                </div>

                                <br />
                            </div>
                            <br />
                        </div>
                        <br />

                    </div>
                </div>
            </div>
            <br>
        </div>

        <br />

    </div>

    <div class="row">
        <div class="col-lg-12 col-sm-12 col-md-12">
            <div class="dd-handle ng-scope">
                <p class="text-primary" style="font-size:16px;">
                    <b>
                        &nbsp;&nbsp; BRS - Assigned History
                    </b>

                </p>
            </div>

        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-12 col-sm-12 col-md-12">
            <div class="col-lg-12 col-sm-12 col-md-12">
                <div class="table-responsive">
                    <table class="table table-striped table-secondary">
                        <thead class="bg-gray text-white">
                            <tr width="100%">
                                <th>S.No</th>
                                <th>Reference No</th>
                                <th>Assigned On</th>
                                <th> Assigned By</th>
                                <th> Assigned To</th>
                                <th> Assigned Remarks</th>

                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="data in assignedlist">
                                <td data-title="'S.No'" class="text-center"> {{$index+1}} </td>
                                <td data-title="'Reference No'"> {{data.banktransc_gid}} </td>
                                <td data-title="'Assigned On'"> {{data.tagged_date}} </td>
                                <td data-title="'Assigned By'">{{data.assigned_by}} </td>
                                <td data-title="'Assigned To'">{{data.taggedmember_name}} </td>
                                <td data-title="'Tagged Remarks' ">{{data.tagged_remarks}} </td>

                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-12 col-sm-12 col-md-12">
            <div class="dd-handle ng-scope">
                <p class="text-primary" style="font-size:16px;">
                    <b>
                        &nbsp;&nbsp; BRS - Transferred History
                    </b>

                </p>
            </div>

        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-12 col-sm-12 col-md-12">
            <div class="col-lg-12 col-sm-12 col-md-12">
                <div class="table-responsive">
                    <table class="table table-striped table-secondary">
                        <thead class="bg-gray text-white">
                            <tr width="100%">
                                <th>S.No</th>
                                <th>Reference No</th>
                                <th>Transferred On</th>
                                <th> Transferred By</th>
                                <th> Transferred To</th>
                                <th> Transferred Reason</th>

                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="data in transferlist">
                                <td data-title="'S.No'" class="text-center"> {{$index+1}} </td>
                                <td data-title="'Reference No'"> {{data.banktransc_gid}} </td>
                                <td data-title="'Transferred On'"> {{data.transfered_date}} </td>
                                <td data-title="'Transferred By'">{{data.transfer_by}} </td>
                                <td data-title="'Transferred To'">{{data.transfer_toname}} </td>
                                <td data-title="'Transferred Reason' ">{{data.transfer_reason}} </td>

                            </tr>
                        </tbody>
                    </table>
                </div>

            </div>
        </div>

    </div>
    <br />


    <div style="margin-bottom:6px;" ng-show="lblallocated_status=='Pending'">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div style="border: 1px solid #EEEEEE;">
                        <br />
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div>
                                <!--   <em class="fa fa-user" style="color:#141478;"></em> &nbsp; -->
                                <p style="color:#141478;font-size: 17px;"><em class="fa fa-th-list" style="color:#141478;"></em> &nbsp;&nbsp;<b>Status Updation </b></p>
                                <hr />
                            </div>
                        </div>
                        <br />
                        <form novalidate="" name="userform" autocomplete="off" class="form-validate mb-lg">
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-1 col-md-1 col-sm-1">
                                            <label> Status </label><span style="color:red"> * </span>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <select name="employeeList" id="employeeList"
                                                    ng-model="cborm_status" class="chosen-select form-control" required>
                                                <option value="Adjust against loan outstanding">Adjust against loan outstanding</option>
                                                <!-- <option value="Refund to client's bank account">Refund to client's bank account</option> -->
                                                <!-- <option value="Others">Others</option> -->

                                            </select>
                                        </div>
                                        <div class="col-lg-2 col-md-2 col-sm-2">
                                            <label> Upload Document </label>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <div class="input-group form-group">
                                                <input type="file" id="file" ng-model="test_document"
                                                       multiple onchange="angular.element(this).scope().uploadattachment(this.files, angular.element(this).scope().documentupload, this.name)"
                                                       data-classbutton="btn btn-default" data-classinput="form-control" data-button-text="Select" />
                                            </div>
                                        </div>
                                        <br />

                                    </div>
                                </div>

                            </div>
                            <br />
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12 form-group">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <!-- <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label> Remarks </label><span style="color:red"> * </span>
                                        </div>
                                        // Froala Editor
                                        <div id="froala-editor" class="col-sm-11 col-lg-11 col-md-11">
                                        </div> -->
                                        <div class="col-sm-11 col-lg-11 col-md-11 froalaheight">
                                            <label> Remarks </label><span style="color:red"> * </span>
                                            <textarea ng-model="txtremarks" placeholder="Enter Remarks" rows="3" class="form-control" required
                                                      id="input_13" ng-trim="true" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                            <!-- <textarea placeholder="Enter Remarks..." required tabindex="3"
                                            froala ng-model="txtremarks" class="form-control" rows="3"></textarea> -->

                                        </div>

                                    </div>

                                </div>
                            </div>


                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12" ng-show="uploaddocument!=null">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="table-responsive">
                                                <table class="table table-striped table-secondary">
                                                    <thead class="bg-gray text-white">
                                                        <tr width="100%">
                                                            <th scope="col">S.No</th>
                                                            <th scope="col">Document Name</th>
                                                            <th scope="col">
                                                                Action&nbsp;&nbsp;&nbsp;
                                                                <a download="" href="" title="Download All" id="btnpdf1" ng-click="Udownloadall(lblfilepath,lblfilename)">
                                                                    <em class="icon-arrow-down"></em>&nbsp;
                                                                </a>
                                                            </th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in uploaddocument">
                                                            <td title="S.No">{{$index+1}}</td>
                                                            <td title="Document Name">{{data.document_name}}</td>
                                                            <td data-title="Action">
                                                                <button title="Download" class="btn btn-default btn-sm"
                                                                        ng-click="download(data.document_path,data.document_name)">
                                                                    <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                    <em class="fa fa-download"></em>
                                                                </button>
                                                                <button title="Remove" ng-click="UploadDocCancel(data.id)" class="btn btn-danger btn-sm ">
                                                                    <i class="fa fa-close"></i>
                                                                </button>
                                                                <button title="View" class="btn btn-info btn-sm" ng-click="documentviewer(data.document_path,data.document_name)">
                                                                    <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                                </button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <br>

                            <br />
                            <div class="row">
                                <div class="text-center">
                                    <button ng-click="Submit()" ng-disabled="userform.$invalid" class="btn btn-success btn-md"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>
                                    <button  ng-click="Sendbacktobrs()" style="background-color:#706710;" class="btn btn-success btn-md"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Not My Customer</button>
                                    <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                                </div>
                            </div>
                        </form>

                        <br /><br />
                    </div>
                </div>
            </div>
        </div>
        <br /><br />
    </div>

    <div style="border:1px solid #ebebeb" ng-show="lblallocated_status=='Completed'">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <br />
                    <p style="color:#141478;font-size: 16px;"> &nbsp;&nbsp; <b> Status Details</b></p>
                    <hr />
                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <label> Status </label>
                        <br />
                        {{lblrm_status}}
                        <label></label>
                    </div>
                </div>
            </div>

        </div>
        <br />
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <label> Remarks </label>
                        <br />
                        <!-- <div id="lblrm_remarks" class="fr-view dd-handle" style="max-height:500px; overflow-y: auto;">
                            {{lblrm_remarks}}
                        </div> -->
                        {{lblrm_remarks}}
                        <label></label>
                    </div>
                </div>
            </div>

        </div>
        <br />
        <div class="row">
            <div class="col-lg-12 col-sm-12 col-md-12">
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="table-responsive">
                            <table class="table table-striped table-secondary">
                                <thead class="bg-gray text-white">
                                    <tr width="100%">
                                        <th scope="col">S.No</th>
                                        <th scope="col">
                                            Document Name&nbsp;&nbsp;&nbsp;
                                            <a download="" href="" title="Download All" id="btnpdf1" ng-click="Udownloadall(lblfilepath,lblfilename)">
                                                <em class="icon-arrow-down"></em>&nbsp;
                                            </a>
                                        </th>
                                        <th scope="col">Created Date</th>
                                        <th scope="col">Uploaded By</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="data in uploaddocument">
                                        <td title="S.No">{{$index+1}}</td>
                                        <td title="Document Name">

                                            <button title="Download" class="btn btn-default btn-sm"
                                                    ng-click="download(data.document_path,data.document_name)">
                                                <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                <em class="icon-arrow-down"></em>&nbsp; {{data.document_name}}
                                            </button>

                                            <!-- <button title="Download Document"
                                                    ng-click="download(data.document_path,data.document_name)"  
                                                    class="buttonstyle">
                                                <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                <em class="icon-arrow-down"></em>&nbsp; {{data.document_name}}
                                            </button> -->
                                            <!--<a download="" href="" id="btnpdf" ng-click="download(data.document_path,data.document_name)">
                                                <em class="icon-arrow-down"></em>&nbsp; {{data.document_name}}
                                            </a>-->
                                        </td>
                                        <td title="Document Name">{{data.created_date}}</td>
                                        <td title="Document Name">{{data.created_by}}</td>

                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <br />
        <br />
    </div>

    <br />
    <script type="text/ng-template" id="/sentbacktobrs.html">
        <div class="modal-header bg-primary ng-scope">
            <h4 id="myModalLabel" class="modal-title">
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-arrow-circle-left"></i>
                </span> Sent Back to BRS Unrecon
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="sentbacktobrsreassign" ng-submit="form.sentbacktobrsreassign()" autocomplete="off" class="form-validate form-horizontal">
                <div class="pd-sm-10">
                    <div class="row ">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="float-label">
                                <label>Reason</label><span style="color:red;"> *</span>
                                <textarea class="form-control" rows="3" tabindex="0" required ng-model="txtsendbackreason" style="resize:vertical;" placeholder="Enter Reason"></textarea>
                            </div>
                        </div>
                    </div>
                    <div>
                        <br />
                        <div class="text-center">
                            <button type="button" class="btn btn-success btn-md" ng-disabled="sentbacktobrsreassign.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                            <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                        </div>
                    </div>
                    <br />
                </div>
            </form>
        </div>
    </script>
</div>


<br />
<br />
