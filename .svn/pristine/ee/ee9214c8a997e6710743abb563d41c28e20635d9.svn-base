<style>
    @font-face {
        font-family: 'cabrito contrast cond bold';
        src: url('Fonts/cabritocontrastcondbold.otf');
        src: url('Fonts/cabritocontrastcondbold.otf') format('opentype');
    }

    .cabritofontclass {
        font-family: 'cabrito contrast cond bold';
        font-weight: bold;
    }

    .responsive {
        max-width: 100%;
        height: auto;
        padding-top: 10px;
    }

    .circle {
        height: 75px;
        width: 80px;
        background-color: #008053;
        border-radius: 50%;
        display: inline-block;
    }

    .buttonstyclr {
        background-color: #d5591e;
        background-image: linear-gradient(316deg, #6DBF67 0%, #008053 70%);
    }

    /* Mobile View */
    @media screen and (min-width:200px) and (max-width:300px) {
        .mblcenter {
            text-align: center;
        }
    }

    @media only screen and (max-width:764px) {
        .space1 {
            text-align: center;
            margin-left: 25%;
        }
    }

    @media only screen and (max-width:764px) {
        .space1 {
            text-align: center;
            margin-left: 10%;
        }
    }

    @media screen and (min-width:300px) and (max-width:400px) {
        .mblcenter {
            text-align: center;
        }
    }

    @media screen and (min-width:400px) and (max-width:447px) {
        .mblcenter {
            text-align: center;
        }
    }

    @media screen and (min-width:448px) and (max-width:480px) {
        .mblcenter {
            text-align: center;
        }
    }

    @media screen and (min-width:481px) and (max-width:500px) {
        .mblcenter {
            text-align: center;
        }
    }

    @media screen and (min-width:500px) and (max-width:600px) {
        .mblcenter {
            text-align: center;
        }
    }

    @media screen and (min-width:600px) and (max-width:607px) {
        .mblcenter {
            text-align: center;
        }
    }

    @media screen and (min-width:608px) and (max-width:726px) {
        .mblcenter {
            text-align: center;
        }

        .panelresponse {
            align-content: center;
        }
    }

    @media screen and (min-width:727px) and (max-width:767px) {
        .mblcenter {
            text-align: center;
        }
    }

    @media screen and (min-width:768px) and (max-width:800px) {
        .mblcenter {
            text-align: center;
        }
    }

    @media screen and (min-width:801px) and (max-width:900px) {
        .mblcenter {
            text-align: center;
        }
    }

    @media screen and (min-width:900px) and (max-width:1157px) {
        .mblcenter {
            text-align: center;
        }
    }

    .flex-container {
        display: flex;
        flex-wrap: wrap;
        flex-direction: row;
    }
</style>
<div class="limiter" ng-controller="LoginFormController">
    <div style="background-image:url('app/img/Login and Error page img/bg img.png'); background-repeat: no-repeat; background-size: 100% 100% ;
    height:100vh;
    margin: 0px auto 0;
    position: relative;">
        <div>
            <br /><br />
            <div class="row mblcenter">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-4 col-md-4 col-sm-4">

                    </div>
                    <div class="col-lg-1 col-md-1 col-sm-1">
                        <img src="app/img/Login and Error page img/Logo 1.png" alt="Image" class="responsive" style="width:60px;height:100px;" />
                    </div>
                    <div class="col-lg-1 col-md-1 col-sm-1">
                        <img src="app/img/Login and Error page img/Logo 2.png" alt="Image" class="responsive" style="width:80px;height:110px;" />
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2">
                        &nbsp;&nbsp; <img src="app/img/Login and Error page img/Logo 3.png" alt="Image" class="responsive" style="width:190px;height:100px;" />
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2">
                        &nbsp;&nbsp; <img src="app/img/Login and Error page img/Logo 4.png" alt="Image" class="responsive" style="width:160px;" />
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2">
                        <img src="app/img/Login and Error page img/Logo 5.png" alt="Image" class="responsive" style="width:200px;height:100px;" />
                    </div>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 flex-container">
                    <div class="col-lg-1 col-md-1 col-sm-1">

                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6">
                        <img src="app/img/Login and Error page img/BG login.png" class="responsive" alt="Image">
                    </div>
                    <div class="col-lg-1 col-md-1 col-sm-1">

                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 space1" ng-show=!showotp>
                        <br /><br /><br />
                        <div class="card" style="height:226px;width:360px;border-radius: 16px;">
                            <div class="text-center">
                                <br />
                                <img src="app/img/Samunnati Login Logo.svg" style="height:80%;width:70%"
                                     alt="Image" class="block-center img-rounded" /><br /><br />
                                <p style="color:#008053;font-size:20px;"><b> Making Markets Work for <br /> Small Holder Farmers </b></p>
                            </div>
                            <br /><br /><br />
                            <div class="card-offset">

                                <button type="button" style="font-size:18px;color:white;padding-top:6px;left:135px;" ng-click="adclick()" class="btn circle buttonstyclr">
                                    Log in
                                </button>
                            </div>
                            <div class="card-body">

                            </div>
                        </div>

                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 space1" ng-show=showotp>
                        <br /><br /><br />
                        <div class="block-center mt-xl wd-xl">
                            <div class="panel" style="border-radius: 5%;">
                                <br />
                                <div class="text-center">
                                    <img src="app/img/Samunnati Login Logo.svg" style="height:80%;width:70%"
                                         alt="Image" class="block-center img-rounded" />

                                </div>
                                <div style="color: red; text-align:center">
                                    <p>
                                        Due to an error in SSO Login the page has been Redirected here.
                                        <br>
                                        Login by using OTP
                                    </p>
                                </div>
                                <div ng-show="isemailexist" style="color: green; text-align:center">
                                    <p>
                                        {{isemailmessage}}
                                    </p>
                                </div>
                                <div class="panel-body">
                                    <form role="form" ng-submit="otplogin()" name="otploginForm" novalidate="" class="form-validate mb-lg" ng-show=otpswitch>
                                        <div class="form-group has-feedback">
                                            <input id="exampleInputEmail1" type="email" name="user_name" placeholder="Enter Mail Address" autocomplete="off" ng-model="mailid" required="" tabindex="0" class="form-control" />
                                            <span class="fa fa-envelope form-control-feedback text-muted"></span>
                                        </div>
                                        <button type="submit" class="btn btn-block btn-success mt-lg">Send OTP</button>
                                    </form>
                                    <form role="form" ng-submit="verify_otp()" name="otpverificationForm" novalidate="" class="form-validate mb-lg" ng-show=!otpswitch>
                                        <div class="form-group has-feedback">
                                            <input id="exampleInputEmail1" type="email" name="user_name" placeholder="Enter Mail Address" autocomplete="off" ng-model="mailid" disabled tabindex="0" class="form-control" />
                                            <span class="fa fa-envelope form-control-feedback text-muted"></span>
                                        </div>
                                        <div class="form-group has-feedback">
                                            <input id="exampleInputPassword1" type="password" name="otp" placeholder="Enter OTP" autocomplete="off" ng-model="password" required="" tabindex="0" class="form-control" />
                                            <span class="fa fa-lock form-control-feedback text-muted"></span>
                                            <div>

                                            </div>
                                        </div>
                                        <div>
                                            <div>OTP Expires In T- <span id="time" style="color:red;">00:60</span> seconds</div>
                                        </div>

                                        <div>
                                            <div class="col-lg-6 col-sm-6 col-md-6">
                                                <button type="submit" class="btn btn-block btn-success mt-lg" id="verify_otp">Verify OTP</button>
                                            </div>
                                            <div class="col-lg-6 col-sm-6 col-md-6">
                                                <button type="button" class="btn btn-block btn-success mt-lg" ng-click="otplogin()" id="resend_otp">Resend OTP</button>
                                            </div>

                                        </div>
                                    </form>
                                </div>
                            </div>

                        </div>

                    </div>
                </div>

            </div>
            <!-- <button type="button" style="font-size:18px;color:white;padding-top:6px;left:135px;" ng-click="adclick2()" class="btn circle buttonstyclr">
                Otp<br>Login
            </button> -->
            <!-- <button><a href="http://D:/OneDrive%20-%20Vcidex%20Solutions%20Pvt%20Ltd/Web/EMS/V1/app/pages/otplogin.html"></a>Otplogin</button> -->
            <br /><br /><br />
            <div class="row">
                <div class="col-lg-8 col-md-8 col-sm-8">
                    <div style="background-color:#008053;">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <div style="padding-top:10px;">
                                        <a href="https://www.facebook.com/samunnatifinance/" style="text-decoration: none;" target="_blank">
                                            <img src="app/img/Login and Error page img/Fb.png"
                                                 alt="Image" class="block-center img-rounded" />
                                        </a>
                                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                        <a href="https://twitter.com/SamunnatiFin" style="text-decoration: none;" target="_blank">
                                            <img src="app/img/Login and Error page img/Twitter01.png"
                                                 alt="Image" class="block-center img-rounded" />
                                        </a>
                                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                        <a href="https://www.youtube.com/channel/UC_ETSDwfqOgw-RTpVwSoXUQ" style="text-decoration: none;" target="_blank">
                                            <img src="app/img/Login and Error page img/YouTube01.png"
                                                 alt="Image" class="block-center img-rounded" />
                                        </a>
                                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                        <a href="https://www.instagram.com/samunnati" style="text-decoration: none;" target="_blank">
                                            <img src="app/img/Login and Error page img/Insta.png"
                                                 alt="Image" class="block-center img-rounded" />
                                        </a>
                                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                        <a href="https://www.linkedin.com/company/samunnati-financial-intermediation-and-services-private-limited" style="text-decoration: none;" target="_blank">
                                            <img src="app/img/Login and Error page img/Linkedin01.png"
                                                 alt="Image" class="block-center img-rounded" />
                                        </a>
                                    </div>
                                </div>
                                <div class="col-lg-1 col-md-1 col-sm-1">
                                </div>
                                <div class="col-lg-5 col-md-5 col-sm-5" style="padding-top:7px;">
                                    <p style="color:#356338;font-size: 16px;color:white;"><em class="fa fa-copyright"></em> 2021 Samunnati. All Rights Reserved.</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-md-4">

                </div>
            </div>
            <br /><br /><br />
        </div>
