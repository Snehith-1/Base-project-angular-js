        <script type="text/ng-template" id="/Otpverify.html">
            <div class="modal-header bg-primary ng-scope">

                <h4 class="modal-title">
                    <span class="btn-label bg-primary">
                    </span>OTP Verification
                </h4>
            </div>
            <div class="modal-body ng-scope">
                <form novalidate="" name="Otpverify" ng-submit="form.otpverify()" autocomplete="off" class="form-validate form-horizontal">
                    <div class="pd-sm-10">
                        <div class="row">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div class="row ">
                                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                        <div class="mda-form-group float-label">
                                            <input id="exampleInputOTP" ng-pattern="/^[0-9]*$/" type="tel" name="OTP" placeholder="Enter your OTP"
                                                   ng-model="login.account.mobile_number" required="" tabindex="0" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div>
                        <br />
                        <div class="text-center">
                            <button type="button" class="btn btn-success btn-md" ng-disabled="otplogin.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Send Otp</button>&nbsp;&nbsp;
                            <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                        </div>
                    </div>
                    <br />
                </form>
            </div>
        </script>

    </div>
</div>




