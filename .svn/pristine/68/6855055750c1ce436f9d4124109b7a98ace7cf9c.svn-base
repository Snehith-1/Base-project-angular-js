                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label"> Over All Limit </label>
                                <br />
                                {{txtoverall_limit}}
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label"> Cap Limit </label>
                                <br />
                                {{txtcap_limit}}
                            </div>
                            <br />
                        </div>
                    </div>
                    <br />
                </div>
                <br />
            </div>

            <div class="col-lg-12 col-sm-12 col-md-12">
                <div class="dd-handle ng-scope">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <b style="color:darkblue;font-size:15px;">
                                Bureau Score
                            </b>
                            <hr />
                        </div>
                    </div>
                    <div class="dd-handle ng-scope" ng-repeat="data in bureauscore_list">
                        <div class="row">
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-3 col-sm-3 col-md-3">
                                        <label style="font-weight:bold">Bureau Name</label>
                                        <br />
                                        {{data.bureauname_name}}
                                    </div>
                                    <div class="col-lg-3 col-sm-3 col-md-3">
                                        <label style="font-weight:bold">Bureau Score</label>
