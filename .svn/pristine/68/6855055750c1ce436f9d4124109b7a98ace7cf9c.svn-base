        <div class="text-center">
            <button type="button" class="btn btn-info btn-md" ng-click="back()"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>&nbsp;&nbsp;
        </div>
    </div>
        <br /> <br />
    </div>
    <script type="text/ng-template" id="/bureaudocuments.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-file-text-o"></em>&nbsp;<label style="font-size:16px">Bureau Documents</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">

                <form novalidate="" name="userform1" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                           
                                <div class="table-responsive">
                                    <table class="table table-striped table-secondary">
                                        <thead class="bg-gray text-white">
                                            <tr width="100%">
                                                <th scope="col">S.No</th>
                                                <th scope="col">Document Name</th>
                                                <th scope="col">Uploaded By / On </th>
                                                <th scope="col">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in bureaudoc_list">
                                                <td title="S.No">{{$index+1}}</td>
                                                <td title="Document Name">{{data.document_name}}</td>
                                                <td title="Document Name">{{data.created_by}} / {{data.created_date}} </td>
                                                <td data-title="Action" class="nowrap">
                                                    <button title="Download" class="btn btn-green btn-sm"
                                                            ng-click="download_bureaudoc(data.document_path,data.document_name)">
                                                        <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                        <em class="fa fa-download"></em>
                                                    </button>
                                                    <button title="View" class="btn btn-info btn-sm" ng-click="documentviewer(data.document_path,data.document_name)">
                                                        <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                    </button>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                           
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="/StaticMapAndPhotosView.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-image"></em>&nbsp;<label style="font-size:16px">Static Map and Photos</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>
        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            <p class="text-primary" style="font-size:14px;"><b>&nbsp; Static Map </b></p>
                            <img ng-src="{{staticmapImgUrl}}">
                        </div>
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            <p class="text-primary" style="font-size:14px;"><b>&nbsp; Photos </b></p>
                            <div ng-repeat="item in photoUrlList" ng-show="photoFound">
                                <img ng-src="{{item}}">
                                <br />
                                <br />
                            </div>
                            <label style="font-weight:bolder;color:red" ng-show="photoNotFound">Photos not found!</label>
                        </div>
                    </div>
                </div>
                <br />
                <div class="text-center">
                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                </div>
            </div>
        </div>
    </script>

</div>