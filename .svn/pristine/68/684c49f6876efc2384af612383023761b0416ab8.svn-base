<h3>
    Courier Management
</h3>
<div ng-controller="idasCourierEdit as dp">
    <div id="panelDemo8" class="panel panel-primary ">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-plus"></i>
            </span>Edit Courier
        </div>

        <div class="panel-body">
            <div class="row" style="display:none;">
                <div class="col-lg-12 col-md-12 col-sm-12 ">
                    <div class="col-lg-6 col-md-6 col-sm-6 ">
                        <input name="customer_gid" id="customer_gid" style="font-size:12px"
                               placeholder="Enter Customer gid" ng-model="customer_gid" class="form-control">
                    </div>
                </div>
            </div>

            <div class="modal-body ng-scope">
                <form novalidate="" name="userform" ng-submit="form.submitLoan()" autocomplete="off"
                      class="form-validate mb-lg">


                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <div align="left">
                                    <label>Courier Ref No :</label>
                                    <label ng-model="courierref_no" class="col-form-label text-primary">
                                        <strong>{{courierref_no}}</strong>
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br/>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label class="col-form-label control-label">
                                    <strong>
                                        Courier Type &nbsp;<span style="color:red">*</span>
                                    </strong>
                                </label>
                                <select name="courier_type" id="courier_type" disabled ng-change="OnChangeCourierType(courier_type)"
                                        dropzone="" ng-model="courier_type"
                                        required="" class="chosen-select form-control" size="1">
                                    <option value="">-----Courier Type----</option>
                                    <option value="Physical Inward">Physical Inward</option>
                                    <option value="Physical Outward">Physical Outward</option>
                                    <option value="Courier Inward">Courier Inward</option>
                                    <option value="Courier Outward">Courier Outward</option>
                                </select>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>If Available Customer <span style="color:red">*</span> </label>
                                <div class="radio c-radio">
                                    <label>
                                        &nbsp;&nbsp; <input type="radio" name="rdbcustomeredit" value="Yes" ng-model="rdbcustomeredit" required ng-click="rdbcustomer_yes()" />
                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                    </label>
                                    <label>
                                        <input type="radio" name="rdbcustomeredit" value="No" ng-model="rdbcustomeredit" required ng-click="rdbcustomer_no()"  />
                                        <span class="fa fa-circle"></span>No
                                    </label>
                                </div>
                            </div>
                          
                                <div ng-show="customerno">
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Customer Name </label> <span style="color:red">*</span>
                                        <input text type="text" ng-model="customernameedit" rows="1" tabindex="0"
                                               min="0" aria-required="true" aria-invalid="true" class="form-control" />
                                    </div>
                                </div>
                                <div ng-show="customeryes">
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Customer </label> <span style="color:red">*</span>
                                        <select id="customername" name="editcustomergid" dropzone="" chosen=""
                                                ng-model="customername" 
                                                ng-options="data.customer_gid as data.customername for data in customer_list"
                                                class="form-control" ng-change="onselectedchangecustomer(customername)">
                                            <option value=""> Select Customer </option>
                                        </select>
                                    </div>
                                </div>
                         
                        </div>
                    </div>
                    <br />

                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div ng-show="customeryes" class="col-lg-4 col-md-4 col-sm-4">
                                <label><strong>Sanction Ref No</strong></label>
                                <select id="cbosanctionGid" name="cbosanctionGid" dropzone="" chosen=""
                                        ng-model="cbosanctionGid"
                                        ng-options="data.sanction_Gid as data.sanctionrefno for data in sanctiondtl"
                                        class="form-control">
                                    <option value=""> Select Sanction Ref No. </option>
                                </select>

                            </div>

                            <div ng-show="customerno" class="col-lg-4 col-md-4 col-sm-4">
                                <label><strong>Sanction Ref No</strong> </label>
                                <input text type="text" ng-model="sanctionref_no" rows="1" tabindex="0" disabled
                                       min="0" aria-required="true" aria-invalid="true" class="form-control" />
                            </div>

                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label ng-show="courier_type=='Courier Inward'"><strong>Date of Recepit of courier </strong></label>
                                <label ng-show="courier_type=='Courier Outward'"><strong>Date of Courier </strong></label>
                                <label ng-show="courier_type=='Physical Inward'"><strong>Date of Document Inward </strong></label>
                                <label ng-show="courier_type=='Physical Outward'"><strong>Date of Document Outward </strong></label>
                                <p class="input-group">
                                    <input type="text" id="txtdate_of_courier" name="date" ng-model="date_of_courier"
                                           datepicker-popup="{{dp.format}}" style="height:30px;" is-open="dp.open1"
                                           min-date="dp.minDate" datepicker-options="dp.dateOptions" close-text="Close"
                                           class="form-control" required />
                                    <span class="input-group-btn">
                                        <button type="button" ng-click="dp.calender1($event)" style="height:30px;"
                                                class="btn btn-default">
                                            <em class="fa fa-calendar"></em>
                                        </button>
                                    </span>
                                </p>

                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Document Type </label>
                                <!--<input type="text" ng-model="txtdocument_type" tabindex="0" class="form-control" />-->
                                <input text type="text" ng-model="document_type" rows="1" tabindex="0" min="0"
                                       aria-required="true" aria-invalid="true" class="form-control" required />

                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">

                        <div class="col-lg-8 col-sm-8 col-md-8">
                            <div class="row">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <div class="mda-form-group float-label">
                                            <label ng-show="courier_type=='Physical Inward' || courier_type=='Physical Outward'">Document Handover By </label>
                                            <label ng-show="courier_type=='Courier Inward'">Courier Despatched By </label> 
                                            <label ng-show="courier_type=='Courier Outward'">Courier Initiated By </label>
                                             <span style="color:red">*</span>
                                           
                                            <select name="sender_name" chosen="" multiple="multiple" ng-options="m.employee_name for m in employee_list"
                                            ng-model="sender_name" class="chosen-select form-control" required></select>

                                        </div>

                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <div class="mda-form-group float-label">
                                            <label ng-show="courier_type=='Physical Inward' || courier_type=='Physical Outward'">Document Handover To </label>
                                            <label ng-show="courier_type=='Courier Inward'">Courier Handover to </label>
                                            <label ng-show="courier_type=='Courier Outward'">Courier Addressed To</label>
                                             <span style="color:red">*</span>
                                            <select id="handover_name" name="handover_name" required  chosen="" ng-model="handover_name"
                                                    ng-options="data.employee_name for data in employee_list" multiple="multiple"
                                                    class="chosen-select form-control">
                                                
                                            </select>

                                        </div>

                                    </div>


                                </div>
                            </div>

                            <div class="row">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <label>Courier Company Name </label>
                                        <select id="courier_company_name" name="courier_company_name"
                                         ng-disabled="courier_type=='Physical Inward' || courier_type=='Physical Outward'"
                                        chosen=""
                                        ng-model="courier_company_name" 
                                        ng-options="emp.couriercompany_name as emp.couriercompany_name for emp in couriercompany_list"
                                        class="form-control" >
                                       <option value=""> Select Courier Company </option>
                                    </select>
                                      
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <label>POD No </label>
                                        <input text type="text" ng-model="pod_no" id="pod_no" ng-disabled="courier_type=='Physical Inward' || courier_type=='Physical Outward'" rows="1" tabindex="0" min="0"
                                               aria-required="true" aria-invalid="true" class="form-control" />

                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4">

                            <label for="input_13">Address</label>
                            <textarea ng-model="address" rows="6" maxlength="1000" class="form-control"
                                      id="input_13" ng-trim="false" aria-multiline="true" required tabindex="0"
                                      aria-invalid="true"></textarea>


                        </div>


                    </div>

                    <br>
                    <div class="row">
                        <div class="col-lg-12 col-sm-2 col-md-12">
                            <!--<div class="col-lg-4 col-md-4 col-sm-4">

                                  <label class="col-form-label control-label"><strong>Acknowledgement Status</strong></label><br />

                                  <div class="radio c-radio">
                                      <label>
                                          <input type="radio"  name="ack_status" value="Pending" ng-change="OnChangeAckStatus(ack_status)" ng-model="ack_status" />
                                          &nbsp;&nbsp;<span class="fa fa-circle"></span>Pending
                                      </label>
                                      <label>
                                          <input type="radio"  name="ack_status"  ng-change="OnChangeAckStatus(ack_status)" value="Acknowledged" ng-model="ack_status" />
                                          <span class="fa fa-circle"></span>Acknowledged
                                      </label>

                                  </div>
                            </div>-->
                            <div ng-if="ack_status=='Acknowledged'">
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label class="col-form-label control-label">
                                        <strong>
                                            Acknowledgement Date &nbsp;<span style="color:red">*</span>
                                        </strong>
                                    </label>
                                    <p class="input-group">
                                        <input type="text" id="ack_date" name="date" ng-model="$parent.ack_date"
                                               datepicker-popup="{{dp.format}}" style="height:30px;" is-open="dp.open2"
                                               min-date="dp.minDate" datepicker-options="dp.dateOptions" close-text="Close"
                                               class="form-control" />
                                        <span class="input-group-btn">
                                            <button type="button" ng-click="dp.calender2($event)" style="height:30px;"
                                                    class="btn btn-default">
                                                <em class="fa fa-calendar"></em>
                                            </button>
                                        </span>
                                    </p>
                                </div>

                            </div>

                        </div>
                    </div>
                    <br>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">

                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <label><strong>Remarks </strong></label>
                                <textarea rows="3" placeholder="Enter Remarks..." aria-multiline="true" tabindex="0"
                                          aria-invalid="false" ng-model="remarks" class="form-control"></textarea>

                            </div>
                        </div>

                    </div>
                    <br />

                    <p class="lead bb text-purple text-bold" style="font-size:16px;">Upload Document</p>
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label>Document Title</label>&nbsp;<span style="color:red">*</span>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">

                                <input text type="text" ng-model="txtdocument_title" rows="1" tabindex="0" min="0" aria-required="true"
                                       aria-invalid="true" class="form-control" maxlength="256" />
                            </div>
                        </div>
                    </div>
                    <br>
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label>File Upload</label>
                            </div>

                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <input type="file" id="commonupload" ng-model="common_document" ng-disabled="!txtdocument_title"
                                       onchange="angular.element(this).scope().commondocumentupload(this.files, angular.element(this).scope().commondocumentupload, this.name)"
                                       data-classbutton="btn btn-default" data-classinput="form-control inline"
                                       data-button-text="Select"
                                       accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="text-center">
                                    <div class="dd-handle ng-scope">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-secondary">
                                                        <thead class="bg-gray text-white">
                                                            <tr width="100%">
                                                                <th align="center" scope="col">S.No</th>
                                                                <th align="center" scope="col">Document Title</th>
                                                                <th align="center" scope="col">Document Name</th>
                                                                <th align="center" scope="col">Created By</th>
                                                                <th align="center" scope="col">Created Date</th>
                                                                <th align="center" scope="col">Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in uploadDoc_list" width="100%">
                                                                <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                                                <td align="left" scope="col" title="Document Title">
                                                                    {{data.document_title}}
                                                                </td>
                                                                <td align="left" scope="col" title="Document Name">
                                                                    {{data.document_name}}
                                                                </td>
                                                                <td align="left" scope="col" title="Created By">
                                                                    {{data.created_by}}
                                                                </td>
                                                                <td align="left" scope="col" title="Created Date">
                                                                    {{data.created_date}}
                                                                </td>
                                                                <td align="left" scope="col" data-title="'Action'">
                                                                    <button title="Download Document"
                                                                            ng-click="downloadsdocument(data.document_path,data.document_name)"
                                                                            class="btn btn-info"><a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                        <em class="fa fa-download"></em>
                                                                    </button>
                                                                    <button title="Delete Document"
                                                                            ng-click="deletedocument(data.courierdocument_gid)" style="display:none;"
                                                                            class="btn btn-danger">
                                                                        <em class="fa fa-close"></em>
                                                                    </button>

                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div>
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" 
                            ng-click="update()">
                        <i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update
                    </button>&nbsp;&nbsp;
                    <button ng-click="courierback()" class="btn btn-info">
                        <i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back
                    </button>
                </div>
            </div>

        </div>
    </div>


</div>