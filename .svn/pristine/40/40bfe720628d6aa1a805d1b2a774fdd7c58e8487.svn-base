<style>
    .nav.nav-pills .active > a {
        background-color: rgb(19,14,89);
        background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);
        color: #ffffff;
    }
</style>
<h3>
    Credit Underwriting
</h3>
<div ng-controller="AgrTrnCreditSuppliersDtlEditController as form">
    <div class="panel">
        <div class="panel-heading" style="background-color:rgb(19,14,89);background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);color:#ffffff">
            <h5> Edit Supplier Details </h5>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="panel-body">
                        <div class="row">
                            <form name="form.formValidate" ng-submit="form.submitForm()" novalidate="" class="form-validate form-horizontal">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="row">
                                                            <div class="col-sm-12 col-md-12 col-lg-12 dd-handle ng-scope">
                                                                <div>
                                                                    <div class="row"> 
                                                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                                                        <p class="text-primary" style="font-size:14px;"><b> Supplier Details </b></p>
                                                                        <hr />
                                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                                            <label class="col-form-label"> Supplier Name <span style="color:red"> *</span></label>
                                                                            <br />
                                                                            <select name="SupplierName" id="SupplierName" chosen="" ng-options="m.supplier_gid as m.supplier_name for m in supplier_list" 
                                                                            type="text" class="form-control parsley-error" required="" ng-model="cboSupplierName">
                                                                                <option value=""> ----- Select From Dropdown -----</option>    
                                                                            </select>
                                                                            <span ng-show="form.validateInput('SupplierName', 'required')" class="text-danger">This field is required</span>
                                                                        </div>
                                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                                            <label class="col-form-label"> Relationship Vintage (years) <span style="color:red"> *</span></label>
                                                                            <br />
                                                                            <input type="text" ng-model="txtRelationshipVintageYear" name="RelationshipVintage" id="RelationshipVintage" required="" ng-pattern="/^[0-9+]*$/" tabindex="0" class="form-control" maxlength="4" />
                                                                            <span ng-show="form.validateInput('RelationshipVintage', 'required')" class="text-danger">This field is required</span>
                                                                        </div>
                                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                                            <label class="col-form-label"> Relationship Vintage (months) <span style="color:red"> *</span></label>
                                                                            <br />
                                                                            <input type="text" ng-model="txtRelationshipVintagemonth" name="RelationshipVintagemonth" id="RelationshipVintagemonth" required="" ng-pattern="/^[0-9+]*$/" tabindex="0" class="form-control" maxlength="2" />
                                                                            <span ng-show="form.validateInput('RelationshipVintagemonth', 'required')" class="text-danger">This field is required</span>
                                                                        </div>
                                                                        </div>
                                                                    </div>
                                                                    <br/>
                                                                    <div class="row"> 
                                                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                                <div ng-controller="MstCreditSuppliersDtlEditController as dp">
                                                                                    <label class="col-form-label"> Start Date <span style="color:red"> *</span></label>
                                                                                    <br />
                                                                                    <p class="input-group">
                                                                                        <input type="text" id="StartDate" name="StartDate" ng-model="$parent.txtStart_Date" datepicker-popup="{{dp.format}}"
                                                                                               style="height:36px;" is-open="dp.open1" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                                                                               close-text="Close" class="form-control" placeholder="DD-MM-YYYY" required="" />
                                                                                        <span class="input-group-btn">
                                                                                            <button type="button" ng-click="dp.calender1($event)" style="height:36px;" class="btn btn-default">
                                                                                                <em class="fa fa-calendar"></em>
                                                                                            </button>
                                                                                        </span>
                                                                                    </p>
                                                                                </div>
                                                                                <span ng-show="form.validateInput('StartDate', 'required')" class="text-danger">This field is required</span>
                                                                            </div>
                                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                                <div ng-controller="MstCreditSuppliersDtlEditController as dp">
                                                                                    <label class="col-form-label"> End Date <span style="color:red"> *</span></label>
                                                                                    <br />
                                                                                    <p class="input-group">
                                                                                        <input type="text" id="EndtDate" name="EndtDate" ng-model="$parent.txtEnd_Date" datepicker-popup="{{dp.format}}"
                                                                                               style="height:36px;" is-open="dp.open2" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                                                                               close-text="Close" class="form-control" placeholder="DD-MM-YYYY" required="" />
                                                                                        <span class="input-group-btn">
                                                                                            <button type="button" ng-click="dp.calender2($event)" style="height:36px;" class="btn btn-default">
                                                                                                <em class="fa fa-calendar"></em>
                                                                                            </button>
                                                                                        </span>
                                                                                    </p>
                                                                                </div>
                                                                                <span ng-show="form.validateInput('EndtDate', 'required')" class="text-danger">This field is required</span>
                                                                            </div>
                                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                                <label class="col-form-label"> Purchase Amount <span style="color:red"> *</span></label>
                                                                                <br />
                                                                                <input id="Purchaseamt" name="Purchaseamt" ng-model="txtPurchaseamt" maxlength="15" class="form-control ng-pristine ng-valid ng-touched"
                                                                                       required ng-change="Purchaseamt()" ng-pattern="/^[0-9,]+(\.[0-9]{1,2})?$/" step="0.01" style="text-align:right;">
                                                                                <span style="font-style:italic;">Number in words:</span>
                                                                                <span id="words_totalamount1" style="color:#003c96fc;" class="text-bold"></span>
                                                                              <span ng-show="form.validateInput('Purchaseamt', 'required')" class="text-danger">This field is required</span>
                                                                            </div>
                                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                                <label class="col-form-label"> Bank Debit Amount <span style="color:red"> *</span></label>
                                                                                <br />
                                                                                <input id="BankDebitamt" name="BankDebitamt" ng-model="txtBankDebitamt" maxlength="15" class="form-control ng-pristine ng-valid ng-touched"
                                                                                       required ng-change="BankDebitamt()" ng-pattern="/^[0-9,]+(\.[0-9]{1,2})?$/" step="0.01" style="text-align:right;">
                                                                                <span style="font-style:italic;">Number in words:</span>
                                                                                <span id="words_totalamount2" style="color:#003c96fc;" class="text-bold"></span>
                                                            <span ng-show="form.validateInput('BankDebitamt', 'required')" class="text-danger">This field is required</span>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <br/>
                                                                    <div class="row"> 
                                                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                                                            <div class="col-sm-12 col-md-12 col-lg-12">
                                                                                <label>Relationship with the Supplier <span style="color:red"> *</span></label>
                                                                                <textarea style="resize:vertical;" rows="4" name="RelationshipwiththeSupplier" id="RelationshipwiththeSupplier" placeholder="Enter Relationship with the Supplier..." aria-multiline="true" tabindex="0" aria-invalid="false" maxlength="1000"
                                                                                          ng-model="txtrelationship_supplier" class="form-control"  required=""></textarea>
                                                                                          <span ng-show="form.validateInput('RelationshipwiththeSupplier', 'required')" class="text-danger">This field is required</span>
                                                                                <br />
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                    </div>

                                                    <br/>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <br />
                                            <div class="text-center">
                                                <button type="submit" class="btn btn-success btn-md"  ng-click="update_supplierdtl()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                                                <button ng-click="supplierdtl_Back()" type="button" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <br />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>