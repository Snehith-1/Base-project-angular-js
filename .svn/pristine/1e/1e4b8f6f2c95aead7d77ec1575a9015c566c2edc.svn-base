<h3>
    Assignment
</h3>
<style>
    .picColor:hover {
        background-color: transparent;
        box-shadow: none;
        color: #144A6E;
        border: 1px solid #A59F9D;
        transform: translateY(-3px);
        box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2);
        text-decoration: underline;
        text-decoration-color: #141478;
    }

    .tab {
        overflow: hidden;
        border: 1px solid #ccc;
        background-color: #f8f7f7;
        height: 48px;
    }

        /* Style the buttons inside the tab */
        .tab button {
            background-color: inherit;
            float: left;
            border: none;
            outline: none;
            cursor: pointer;
            padding: 14px 16px;
            transition: 0.3s;
            font-size: 14px;
            font-weight: bold;
        }

            /* Change background color of buttons on hover */
            .tab button:hover {
                background-color: rgb(226, 221, 221);
                text-decoration: underline;
                height: 56px;
            }

    /* Style the tab content */
    .tabcontent {
        display: none;
        padding: 6px 12px;
        border: 1px solid #fff;
        border-top: none;
    }

    .tab button.active {
        background-color: #d5591e;
        background-image: linear-gradient(316deg, rgb(236 195 100) 0%, rgb(171 225 166) 74%);
    }
</style>
<script>
    function openApplication(evt, ApplicationName) {
        var i, tabcontent, tablinks;
        tabcontent = document.getElementsByClassName("tabcontent");
        for (i = 0; i < tabcontent.length; i++) {
            tabcontent[i].style.display = "none";
        }
        tablinks = document.getElementsByClassName("tablinks");
        for (i = 0; i < tablinks.length; i++) {
            tablinks[i].className = tablinks[i].className.replace(" active", "");
        }
        document.getElementById(ApplicationName).style.display = "block";
        evt.currentTarget.className += " active";
    }

    // Get the element with id="defaultOpen" and click on it
    document.getElementById("defaultOpen").click();

</script>
<div ng-controller="MstAppassignedAssignmentSummaryController">
    <div class="row">
        <div class="col-lg-10 col-md-10 col-sm-10">
            <div class="tab nowrap">
                <button class="tablinks" ng-click="pending_applications()" style="color:#141478;font-size:13px">
                    <em class="fa fa-user fa-x" style="color:#141478;font-size: 15px;"></em> &nbsp;  Pending Assignment
                    &nbsp; <div class="pull-right label" style="background-color:#141478;padding-top:4px;font-size: 8px;padding-bottom:4px;">{{pending_count}}</div>
                </button>
                <button class="tablinks" onclick="openApplication(event, 'Assigned Application')" id="defaultOpen" ng-click="assigned_applications()" style="color:#871490;font-size:13px">
                    <em class="fa fa-group fa-x" style="color:#871490;font-size: 15px;"></em> &nbsp;  Assigned Application
                    &nbsp; <div class="pull-right label" style="background-color:#871490;padding-top:4px;font-size: 8px;padding-bottom:4px;">{{assigned_count}}</div>
                </button>
                <button class="tablinks" ng-click="submittedto_cc()" style="color:#00796b;font-size:13px">
                    <em class="fa fa-file-text fa-x" style="color:#00796b;font-size: 15px;"></em> &nbsp; Submitted to CC
                    &nbsp; <div class="pull-right label" style="background-color:#00796b;padding-top:4px;font-size: 8px;padding-bottom:4px;">{{submittedtocc_count}}</div>
                </button>

                <button class="tablinks" ng-click="ccapproved()" style="color:#800000;font-size:13px">
                    <em class="fa fa-file-text fa-x" style="color:#800000;font-size: 15px;"></em> &nbsp; CC Approved
                    &nbsp; <div class="pull-right label" style="background-color:#800000;padding-top:4px;font-size: 8px;padding-bottom:4px;">{{ccapproved_count}}</div>
                </button>
                <button class="tablinks" ng-click="rejected()" style="color:#FF0000;font-size:13px">
                    <em class="fa icon-close fa-x" style="color:#FF0000;font-size: 15px;"></em> &nbsp; Rejected & Hold
                    &nbsp; <div class="pull-right label" style="background-color:#FF0000;padding-top:4px;font-size: 8px;padding-bottom:4px;">{{rejected_count}}</div>
                </button>
            </div>
        </div>
        <div class="col-lg-2 col-sm-2 col-md-2">
            <div class="list-group-item picColor" style="height:50px;">
                <div class="media-box">
                    <div class="media-box-body clearfix">
                        <div class="media-box-heading" style="padding-top:6px;">
                            <div class="pull-right label" style="background-color:#C10B68;padding-top:6px;padding-bottom:4px;font-size:8px;">{{lstotalcount}}</div>
                            <div style="font-size:14px;color:#C10B68;"><b> Total Applications </b></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div class="panel" id="Assigned Application">
        <br />
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="table-responsive">
                        <table datatable="ng" class="table table-hover table-striped">
                            <thead>
                                <tr class="text-white" style="background-color:#09385f;">
                                    <th style="display:none;" class="text-center nowrap"> S.No</th>
                                    <th class="nowrap"> Submitted Date </th>
                                    <th class="nowrap"> Submitted By </th>
                                    <th class="nowrap"> Application Number</th>
                                    <th class="nowrap"> Applicant Name </th>
                                    <th class="nowrap"> Assigned By</th>
                                    <th class="nowrap"> Assigned To</th>
                                    <th class="nowrap"> Assigned Date </th>
                                    <!--<th> Approved Date</th>-->
                                    <th class="nowrap"> Type </th>
                                    <th class="text-center nowrap">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in appassignment_list">
                                    <td data-title="'S.No'" style="display:none;" class="text-center"> {{$index+1}} </td>
                                    <td data-title="'Submitted Date'">{{data.created_date}}</td>
                                    <td data-title="'Submitted By'"> {{data.created_by}} </td>
                                    <td data-title="'Application Number'"> {{data.application_no}} </td>
                                    <td data-title="'Applicant Name'"> {{data.customer_name}} </td>
                                    <td data-title="'Assigned By'"> {{data.creditassigned_by}} </td>
                                    <td data-title="'Assigned To'"> {{data.creditassigned_to}} </td>
                                    <td data-title="'Assigned Date'"> {{data.creditassigned_date}} </td>
                                    <!--<td data-title="'Approved Date'"> {{data.headapproval_date}} </td>-->
                                    <td data-title="'Application Type'" ng-if="data.renewal_flag=='Y'">
                                        <label class="label label-pink" style="font-size:10px;"> Renewal </label>
                                    </td>
                                    <td data-title="'Application Type'" ng-if="data.enhancement_flag=='Y'">
                                        <label class="label label-success" style="font-size:10px;">Enhancement </label>
                                    </td>
                                    <td data-title="'Application Type'" ng-if="data.renewal_flag=='N' && data.enhancement_flag=='N'">
                                        <label class="label label-primary" style="font-size:10px;"> New </label>
                                    </td>
                                    <td class="nowrap" data-title="'Action'" style="text-align: center;">
                                        <button title="View" ng-click="applcreation_view(data.application_gid)" class="btn btn-info">
                                            <em class="fa fa-eye"></em>
                                        </button>
                                        <button title="Reassign Application" ng-click="reassignapplication(data.creditgroup_gid,data.application_gid)" class="btn btn-pink">
                                            <em class="fa fa-external-link"></em>
                                        </button>
                                        <button title="Assessed Score" ng-click="GradingTool_add(data.application_gid)" class="btn btn-primary">
                                            <em class="fa fa-asterisk"></em>
                                        </button>
                                        <button title="Visit Updates" ng-click="VisitReport_add(data.application_gid)" class="btn btn-danger">
                                            <em class="fa fa-file-pdf-o"></em>
                                        </button>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

        <br />
    </div>
    <script type="text/ng-template" id="/reassigncreditapproval.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-external-link"></em>&nbsp;<label style="font-size:16px">Reassign Application</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="assignform" ng-submit="form.submitassign()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-6 col-md-6 col-sm-6" ng-show="(approvedcreditgroup!=true)">
                                        <label>Credit Group <span style="color:red"> *</span>  </label>
                                        <select name="creditgroup" id="creditgroup" ng-options="m.creditgroup_gid as m.creditgroup_name for m in creditgrouplist" ng-change="creditgroup_change(cbocreditgroup)"
                                                ng-model="cbocreditgroup" class="form-control " chosen="">
                                            <option value=""> </option>
                                        </select>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6" ng-show="(approvedcreditgroup==true)">
                                        <label>Credit Group <span style="color:red"> *</span>  </label>
                                        <input type="text" ng-model="cbocreditgroupname" tabindex="0" class="form-control ng-pristine ng-untouched ng-valid" disabled="" aria-invalid="false">
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <label> Credit Manager - CM (L1) <span style="color:red"> *</span> </label>
                                        <select name="creditmanager" id="creditmanager" chosen="" ng-options="m.employee_gid as m.employee_name for m in creditmanagerlist"
                                                ng-model="cbocreditmanager" class="form-control" required></select>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-6 col-md-6 col-sm-6" ng-show="approvedregionalcredit!=true">
                                        <label> Regional Credit Manger - RCM (L2) <span style="color:red"> *</span> </label>
                                        <select name="regionalcredit" id="regionalcredit" chosen="" ng-options="m.employee_gid as m.employee_name for m in regionalcreditlist"
                                                ng-model="cboregionalcredit" class="form-control" required></select>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6" ng-show="approvedregionalcredit==true">
                                        <label> Regional Credit Manger - RCM (L2) <span style="color:red"> *</span> </label>
                                        <input type="text" ng-model="cboregionalcreditname" tabindex="0" class="form-control ng-pristine ng-untouched ng-valid" disabled="disabled" aria-invalid="false">

                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6" ng-show="approvednationalcredit!=true">
                                        <label> National Credit Manager - NCM (L3) <span style="color:red"> *</span> </label>
                                        <select name="nationalcredit" id="nationalcredit" chosen="" ng-options="m.employee_gid as m.employee_name for m in nationalcreditlist"
                                                ng-model="cbonationalcredit" class="form-control" required></select>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6" ng-show="approvednationalcredit==true">
                                        <label> National Credit Manager - NCM (L3) <span style="color:red"> *</span> </label>
                                        <input type="text" ng-model="cbonationalcreditname" tabindex="0" class="form-control ng-pristine ng-untouched ng-valid" disabled="disabled" aria-invalid="false">
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-6 col-md-6 col-sm-6" ng-show="approvedcredithead!=true">
                                        <label> Credit Head - CH (L4) <span style="color:red"> *</span> </label>
                                        <select name="credithead" id="credithead" chosen="" ng-options="m.employee_gid as m.employee_name for m in creditheadlist"
                                                ng-model="cbocreditheadgid" class="form-control" required></select>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6" ng-show="approvedcredithead==true">
                                        <label> Credit Head - CH (L4) <span style="color:red"> *</span> </label>
                                        <input type="text" ng-model="cbocreditheadname" tabindex="0" class="form-control ng-pristine ng-untouched ng-valid" disabled="disabled" ng-if="approvedcredithead==true" aria-invalid="false">
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <label> Remarks <span style="color:red"> *</span></label>
                                        <textarea ng-model="txtreassignremarks" rows="2" class="form-control" style="resize:vertical;"
                                                  id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true" required></textarea>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row" ng-show="showreassignedlist">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="row" style="margin-bottom:-10px;">
                                            <div id="panelDemo20" class="panel">
                                                <div class="panel-heading" style="background: rgb(49,43,168);
                                background: linear-gradient(90deg, rgba(49,43,168,1) 0%, rgba(9,121,103,1) 37%, rgba(200,241,67,1) 95%);color:#ffffff">Reassigned History</div>
                                                <div class="panel-body">
                                                    <div ui-scroll-viewport="" style="height:400px; overflow-y: auto;overflow-x: hidden;" class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="dd-handle ng-scope">
                                                                <div class="row" ng-show="showreassignedlist">
                                                                    <div class="col-sm-12 col-lg-12 col-md-12">                                                                        
                                                                        <div class="table-responsive">
                                                                            <table class="table table-striped table-secondary">
                                                                                <thead>
                                                                                    <tr>
                                                                                        <th align="center" scope="col">S.No</th>
                                                                                        <th align="center" scope="col">Reassigned Information</th>
                                                                                        <th align="center" scope="col">Reassigned By</th>
                                                                                        <th align="center" scope="col">Reassigned Date & Time</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody>
                                                                                    <tr ng-repeat="data in reassignedloglist">
                                                                                        <td align="left" scope="col" title="S.No">
                                                                                            {{$index+1}}
                                                                                        </td>
                                                                                        <td align="left" scope="col" title="Reassigned information">
                                                                                            <p ng-show="data.showcreditgroup_name"><b>Credit Group -</b> {{data.creditgroup_name}} to {{data.reassignto_creditgroup_name}}</p>
                                                                                            <p ng-show="data.showcreditmanger_name"><b>Credit Manager -</b> {{data.creditmanger_name}} to {{data.reassignto_creditmanger_name}}</p>
                                                                                            <p ng-show="data.showcreditregionalmanager_name"><b>Regional Credit Manger -</b> {{data.creditregionalmanager_name}} to {{data.reassignto_creditregionalmanager_name}}</p>
                                                                                            <p ng-show="data.showcreditnationalmanager_name"><b>National Credit Manager -</b> {{data.creditnationalmanager_name}} to {{data.reassignto_creditnationalmanager_name}}</p>
                                                                                            <p ng-show="data.showcredithead_name"><b>Credit Head -</b> {{data.credithead_name}} to {{data.reassignto_credithead_name}}</p>
                                                                                            <p><b>Reassigned Remarks -</b> {{data.reassign_remarks}}</p>
                                                                                        </td>
                                                                                        <td align="left" scope="col" title="Reassigned By">
                                                                                            {{data.created_by}}
                                                                                        </td>
                                                                                        <td align="left" scope="col" title="Reassigned Date/Time">
                                                                                            {{data.created_date}}
                                                                                        </td>
                                                                                    </tr>
                                                                                </tbody>
                                                                            </table>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                        <br />
                        <div class="text-center">
                            <button type="button" class="btn btn-success" ng-disabled="assignform.$invalid" ng-click="reassign_application()"><i class="fa fa-check"></i>&nbsp;&nbsp;Reassign</button>&nbsp;&nbsp;
                            <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                            <br />
                        </div>
                </form>
            </div>
        </div>
    </script>
</div>