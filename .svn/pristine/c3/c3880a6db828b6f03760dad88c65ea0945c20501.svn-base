<h3>
    Sanction Dashboard
</h3>
<div ng-controller="idasTrnSanctionDashboard as dp">
    <div class="card">
        <div class="card-heading bg-primary">
            <div class="card-title">{{sanctionrefnoEdit}}</div>
        </div>


        <md-content class="md-padding">
            <md-tabs md-dynamic-height="" md-border-bottom="">
                 
                <md-tab label="Basic Sanction Info">
                    <md-content class="md-padding">
                        <div class="">
                            <h4 class="text-pink">
                                Basic Sanction Info
                            </h4>
                            <div class="panel-body">
                                <div class="modal-body ng-scope">
                                    <div class="pd-sm-10">
                                        <form novalidate="" name="userform5" ng-submit="form.submitForm1()" autocomplete="off" class="form-validate form-horizontal">
                                            <a class="list-group-item" style="background-color:#dcdcdc3b;">
                                                <div class="media-box">
                                                    <div class="row">
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label>Entity </label>
                                                            <br />
                                                            {{entityedit}}

                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label>Sanction Ref No. </label>
                                                            <br />
                                                            {{txtsanctionrefnoEdit}}

                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label>Sanction Date </label>
                                                            <br />
                                                            {{SanctionDateEdit}}
                                                        </div>

                                                    </div>
                                                    <br />
                                                    <div class="row">
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label>Sanction Amount </label>
                                                            <br />
                                                            {{txtSanctionAmountEdit}}
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label>Sanction Limit </label>
                                                            <br />
                                                            {{txtSanctionLimitEdit}}
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">

                                                            <label class="col-form-label control-label">Loan Facility Type </label>
                                                            <br />
                                                            {{cboFacilityTypeEdit}}
                                                        </div>

                                                    </div>
                                                    <br />
                                                    <div class="row">
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label>Vertical </label>
                                                            <br />
                                                            {{verticalCodeEdit}}

                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label>Collateral Security </label>
                                                            <br />
                                                            {{txtcollateralEdit}}

                                                        </div>

                                                    </div>
                                                </div>
                                            </a>
                                            <br/>
                                            <label class="text-primary">Customer Info</label>
                                            <div class="row">
                                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                        <label class="col-form-label control-label"><strong>Customer </strong></label>
                                                        <br />
                                                        {{customerNameEdit}}

                                                    </div>
                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                        <label class="col-form-label control-label"><strong>Zonal Head</strong></label>
                                                        <br />

                                                        {{zonalHeadNameEdit}}
                                                    </div>
                                                    <div class="col-lg-4 col-md-4 col-sm-4">

                                                        <label class="col-form-label control-label"><strong>Business Head</strong></label>
                                                        <br />
                                                        {{businessHeadNameEdit}}

                                                    </div>
                                                </div>

                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                        <label class="col-form-label control-label"><strong>Cluster Manager</strong></label>
                                                        <br />
                                                        {{clusterManagerEdit}}
                                                    </div>
                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                        <label class="col-form-label control-label"><strong>Relationship Manager</strong></label>
                                                        <br />
                                                        {{relationshipmgmtEdit}}
                                                    </div>
                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                        <label class="col-form-label control-label"><strong>Credit Manager</strong></label>
                                                        <br />
                                                        {{creditManagerEdit}}
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                        <label class="col-form-label control-label"><strong>Address </strong></label>
                                                        <br />
                                                        {{addressline1}}

                                                    </div>
                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                        <label class="col-form-label control-label"><strong>State </strong></label>
                                                        <br />
                                                        {{txtstate}}
                                                    </div>
                                                    <div class="col-lg-4 col-md-4 col-sm-4">

                                                        <label class="col-form-label control-label"><strong>Pincode </strong></label>
                                                        <br />
                                                        {{pincode}}
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                        <label class="col-form-label control-label"><strong>Contact Person </strong></label>
                                                        <br />
                                                        {{contact_person}}

                                                    </div>
                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                        <label class="col-form-label control-label"><strong>Email ID </strong></label>
                                                        <br />
                                                        {{email_id}}
                                                    </div>
                                                    <div class="col-lg-4 col-md-4 col-sm-4">

                                                        <label class="col-form-label control-label"><strong>Contact Number </strong></label>
                                                        <br />
                                                        {{contact_number}}
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                                    <!--<div class="col-lg-4 col-md-4 col-sm-4">
                                                        <label class="col-form-label control-label"><strong>RM Name </strong></label>
                                                        <br />
                                                        {{relationshipmgmtEdit}}

                                                    </div>-->
                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                        <label class="col-form-label control-label"><strong>RM Email ID </strong></label>
                                                        <br />
                                                        {{txtrm_emailid}}
                                                        <!--<input id="txtemailInput" ng-model="txtrm_emailid" required type="email"
                                                        class="form-control ng-pristine ng-valid ng-touched">-->
                                                    </div>
                                                    <div class="col-lg-4 col-md-4 col-sm-4">

                                                        <label class="col-form-label control-label"><strong>RM Phone No. </strong></label>
                                                        <br />
                                                        {{txtrm_phoneno}}
                                                        <!--<input id="txtphoneInput" ng-model="txtrm_phoneno" required type="text"
                                                        class="form-control ng-pristine ng-valid ng-touched">-->
                                                    </div>
                                                </div>
                                            </div>

                                            <br />

                                            <div class="ng-scope">
                                                <div class="text-center">

                                                    <button ng-click="sanctionback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                                                </div>
                                            </div>

                                        </form>
                                    </div>

                                </div>
                            </div>

                        </div>
                    </md-content>
                </md-tab>
                <md-tab label="Sanction details 1">
                    <md-content class="md-padding">

                        <div class="">
                            <h4 class="text-pink">
                                Sanction Details 1
                            </h4>

                            <div class="panel-body">
                                <div class="modal-body ng-scope">
                                    <div class="pd-sm-10">
                                        <div class="row">
                                            <div class="col-lg-3 col-sm-4 col-md-4 form-group">
                                                <label>Earlier Sanction Reference  </label>
                                                <input id="txtezrlierInput" ng-model="$parent.earliersancrefnoEdit" required type="text"
                                                       class="form-control ng-pristine ng-valid ng-touched">
                                            </div>
                                            <div class="col-lg-3 col-sm-4 col-md-4 form-group">
                                                <label>Approval Authority <!--<span style="color:red">*</span>--></label>
                                                <input ng-model="$parent.txtapprovalauthority" tabindex="0" required
                                                       class="form-control ng-pristine ng-valid ng-touched" style="">
                                            </div>
                                            <div class="col-lg-3 col-md-4 col-sm-4">
                                                <label>CC Approval Date <!--<span style="color:red;">*</span>--></label>
                                                <p class="input-group">
                                                    <input type="text" datepicker-popup="{{dp.format}}" ng-model="$parent.txtccapproval_date" is-open="dp.open1"
                                                           datepicker-options="dp.dateOptions" ng-required="true"
                                                           close-text="Close" class="form-control" />
                                                    <span class="input-group-btn">
                                                        <button type="button" ng-click="dp.calender1($event)" class="btn btn-default">
                                                            <em class="fa fa-calendar"></em>
                                                        </button>
                                                    </span>
                                                </p>
                                            </div>
                                            <div class="col-lg-3 col-md-4 col-sm-4 form-group">
                                                <label>Nature of Proposal <!--<span style="color:red">*</span>--></label>
                                                <input ng-model="$parent.txtnatureofproposal" tabindex="0" required
                                                       class="form-control ng-pristine ng-valid ng-touched" style="">
                                            </div>

                                        </div>
                                        <div class="row">
                                            <div class="col-lg-4 col-md-4 col-sm-4 form-group">
                                                <label>Classification of MSME <!--<span style="color:red;">*</span>--></label>
                                                <input ng-model="$parent.txtEditclassificationofMSME" tabindex="0" required
                                                       class="form-control ng-pristine ng-valid ng-touched" style="">
                                            </div>
                                            <div class="col-lg-4 col-md-4 col-sm-4 form-group">
                                                <label>Validity <!--<span style="color:red">*</span>--></label>
                                                <input ng-model="$parent.txtEditValidity" type="text" class="form-control ng-pristine ng-valid ng-touched">
                                            </div>
                                            <div class="col-lg-4 col-sm-4 col-md-4 form-group">
                                                <label>Expiry Date <!--<span style="color:red">*</span>--></label>
                                                <p class="input-group">
                                                    <input type="text" datepicker-popup="{{dp.format}}" ng-model="$parent.txtEditExpiryDate" is-open="dp.open2"
                                                           datepicker-options="dp.dateOptions" ng-required="true"
                                                           close-text="Close" class="form-control" />
                                                    <span class="input-group-btn">
                                                        <button type="button" ng-click="dp.calender2($event)" class="btn btn-default">
                                                            <em class="fa fa-calendar"></em>
                                                        </button>
                                                    </span>
                                                </p>
                                            </div>

                                        </div>

                                        <div class="row">
                                            <div class="col-lg-4 col-sm-4 col-md-4 form-group">
                                                <label>Review Date <!--<span style="color:red">*</span>--></label>
                                                <p class="input-group">
                                                    <input type="text" datepicker-popup="{{dp.format}}" ng-model="$parent.txtEditReviewDate" is-open="dp.open3"
                                                           datepicker-options="dp.dateOptions" ng-required="true"
                                                           close-text="Close" class="form-control" />
                                                    <span class="input-group-btn">
                                                        <button type="button" ng-click="dp.calender3($event)" class="btn btn-default">
                                                            <em class="fa fa-calendar"></em>
                                                        </button>
                                                    </span>
                                                </p>
                                            </div>
                                            <div class="col-lg-4 col-md-4 col-sm-4 form-group">
                                                <label>Constitution <!--<span style="color:red">*</span>--></label>
                                                <input id="txtRevisedLimit" ng-model="$parent.txtEditconstitution" type="text"
                                                       class="form-control ng-pristine ng-valid ng-touched" required />
                                            </div>
                                            <div class="col-lg-4 col-md-4 col-sm-4 form-group">
                                                <label>Sanction Letter Authorized Signatory <!--<span style="color:red">*</span>--></label>

                                                <select id="authorizedsignatory" name="authorizedsignatory" dropzone="" ng-model="$parent.cboEditauthorizedsignatory" required=""
                                                        ng-options="data.employee_id as data.employee_name for data in employee_list"
                                                        class="form-control ng-valid ng-touched ng-valid-parse">
                                                    <option value=""> Select Employee Name </option>

                                                </select>

                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-4 col-md-4 col-sm-4 form-group">
                                                <label>Existing Limit <!--<span style="color:red">*</span>--></label>
                                                <div class="form-group float-label">
                                                    <input id="txtExistingLimit" ng-model="$parent.txtEditexistingLimit" type="text"
                                                           class="form-control ng-pristine ng-valid ng-touched" required />

                                                </div>
                                            </div>
                                            <div class="col-lg-4 col-md-4 col-sm-4 form-group">
                                                <label>Additional & Proposed Limit <!--<span style="color:red">*</span>--></label>
                                                <input id="txtRevisedLimit" ng-model="$parent.txtEditAdditProposedLimit" type="text"
                                                       class="form-control ng-pristine ng-valid ng-touched" required />

                                            </div>
                                            <div class="col-lg-4 col-md-4 col-sm-4 form-group">
                                                <label>Overall Limit <!--<span style="color:red">*</span>--></label>
                                                <input id="txtRevisedLimit" ng-model="$parent.txtEditOverallLimit" type="text"
                                                       class="form-control ng-pristine ng-valid ng-touched" required />

                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-lg-6 col-md-6 col-sm-6 form-group">
                                                <label>Purpose <!--<span style="color:red">*</span>--></label>
                                                <input id="txtExistingLimit" ng-model="$parent.txtEditpurpose" type="text"
                                                       class="form-control ng-pristine ng-valid ng-touched" required />
                                            </div>

                                            <div class="col-lg-6 col-md-6 col-sm-6 form-group">
                                                <label>Tenure-Months <!--<span style="color:red">*</span>--></label>
                                                <input id="txtRevisedLimit" ng-model="$parent.txtEdittenureMonths" type="text"
                                                       class="form-control ng-pristine ng-valid ng-touched" required />
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-lg-6 col-md-6 col-sm-6 form-group">
                                                <label>Repayment-Principal <!--<span style="color:red">*</span>--></label>
                                                <input id="txtExistingLimit" ng-model="$parent.txtEditRepaymentPrinicipal" type="text"
                                                       class="form-control ng-pristine ng-valid ng-touched" required />
                                            </div>

                                            <div class="col-lg-6 col-md-6 col-sm-6 form-group">
                                                <label>Repayment-Interest <!--<span style="color:red">*</span>--></label>
                                                <input id="txtRevisedLimit" ng-model="$parent.txtEditRepInterest" type="text"
                                                       class="form-control ng-pristine ng-valid ng-touched" required />
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-6 col-md-6 col-sm-6 form-group">
                                                <label>Primary Security <!--<span style="color:red">*</span>--></label>
                                                <input id="txtExistingLimit" ng-model="$parent.txtEditPrimarySecurity" type="text"
                                                       class="form-control ng-pristine ng-valid ng-touched" required />

                                            </div>

                                            <!--<div class="col-lg-6 col-md-6 col-sm-6 form-group">
                                                <label>Collateral Security </label>
                                                <input id="txtRevisedLimit" ng-model="$parent.txtEditCollateralSecurity" type="text"
                                                       class="form-control ng-pristine ng-valid ng-touched" required />
                                            </div>-->
                                        </div>
                                        <br />
                                        <div class="ng-scope">
                                            <div class="text-center">
                                                <button type="button" class="btn btn-success btn-md" ng-click="sanctiondetails1Update()" ng-disabled="usersanctiondetails1.$invalid"><i class="fa fa-arrow-circle-right"></i> &nbsp;&nbsp;Update</button>
                                                <button ng-click="sanctionback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                                            </div>
                                        </div>

                                    </div>
                                </div>




                            </div>
                        </div>
                    </md-content>
                </md-tab>
                <md-tab label="Sanction Details 2">
                    <md-content class="md-padding">
                        <div class="">
                            <h4 class="text-pink">
                                Sanction Details 2
                            </h4>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Security Cheque- Bank Name <!--<span style="color:red">*</span>--></label>
                                        <div class="form-group float-label">
                                            <input id="txtExistingLimit" ng-model="$parent.txtEditSecurityBank" type="text"
                                                   class="form-control ng-pristine ng-valid ng-touched" required />

                                        </div>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Security Cheque-Account Number <!--<span style="color:red">*</span>--></label>
                                        <div class="form-group float-label">
                                            <input id="txtRevisedLimit" ng-model="$parent.txtEditSecurityAccount" type="text"
                                                   class="form-control ng-pristine ng-valid ng-touched" required />

                                        </div>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Margin <!--<span style="color:red">*</span>--></label>
                                        <div class="form-group float-label">
                                            <input id="txtRevisedLimit" ng-model="$parent.txtEditMargin" type="text"
                                                   class="form-control ng-pristine ng-valid ng-touched" required />

                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Rate of Interest <!--<span style="color:red">*</span>--></label>
                                        <div class="form-group float-label">
                                            <input id="txtExistingLimit" ng-model="$parent.txtEditRateofInterest" type="text"
                                                   class="form-control ng-pristine ng-valid ng-touched" required />

                                        </div>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Penal Interest <!--<span style="color:red">*</span>--></label>
                                        <div class="form-group float-label">
                                            <input id="txtRevisedLimit" ng-model="$parent.txtEditPenalInterest" type="text"
                                                   class="form-control ng-pristine ng-valid ng-touched" required />

                                        </div>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Processing Fee <!--<span style="color:red">*</span>--></label>
                                        <div class="form-group float-label">
                                            <input id="txtRevisedLimit" ng-model="$parent.txtEditProcessingFee" type="text"
                                                   class="form-control ng-pristine ng-valid ng-touched" required />

                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <label>Bank and Cheque No. <!--<span style="color:red">*</span>--></label>
                                        <div class="form-group float-label">
                                            <input id="txtExistingLimit" ng-model="$parent.txtEditBankCheque" type="text"
                                                   class="form-control ng-pristine ng-valid ng-touched" required />

                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <label>Cheque Realization Date <!--<span style="color:red">*</span>--></label>
                                        <p class="input-group">
                                            <input type="text" datepicker-popup="{{dp.format}}" ng-model="$parent.txtEditChequeRealization" is-open="dp.open7"
                                                   datepicker-options="dp.dateOptions" ng-required="true"
                                                   close-text="Close" class="form-control" />
                                            <span class="input-group-btn">
                                                <button type="button" ng-click="dp.calender7($event)" class="btn btn-default">
                                                    <em class="fa fa-calendar"></em>
                                                </button>
                                            </span>
                                        </p>

                                    </div>

                                </div>

                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <label>Documentation Charge & Client Visit Charge <!--<span style="color:red">*</span>--></label>
                                        <div class="form-group float-label">
                                            <input id="txtRevisedLimit" ng-model="$parent.txtEditDocumentationCharge" type="text"
                                                   class="form-control ng-pristine ng-valid ng-touched" required />

                                        </div>
                                    </div>

                                    <div class="col-lg-6 col-md-6 col-sm-6">
                                        <label>GST Number <!--<span style="color:red">*</span>--></label>
                                        <div class="form-group float-label">
                                            <input id="txtExistingLimit" ng-model="$parent.txtEditGSTNumber" type="text"
                                                   class="form-control ng-pristine ng-valid ng-touched" required />

                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-6 col-sm-6 col-md-6 form-group">
                                        <label>Mode of Operations <!--<span style="color:red">*</span>--></label>
                                        <textarea name="specificcondition" placeholder="Enter Mode of Operations..." required
                                                  ng-model="$parent.txtEditModeOperation" class="form-control" rows="4"></textarea>
                                    </div>
                                    <div class="col-lg-6 col-sm-6 col-md-6 form-group">
                                        <label>Specific Conditions <!--<span style="color:red">*</span>--></label>
                                        <textarea name="specificcondition" placeholder="Enter Specific Conditions..." required
                                                  ng-model="$parent.txtEditspecificcondition" class="form-control" rows="4"></textarea>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-4 col-sm-4 col-md-4 form-group">
                                        <label>Date of Receipt of Docs for Vetting <!--<span style="color:red">*</span>--></label>
                                        <p class="input-group">
                                            <input type="text" datepicker-popup="{{dp.format}}" ng-model="$parent.receiptdocsDateEdit" is-open="dp.open4"
                                                   datepicker-options="dp.dateOptionsEdit" ng-required="true"
                                                   close-text="Close" class="form-control" />
                                            <span class="input-group-btn">
                                                <button type="button" ng-click="dp.calender4($event)" class="btn btn-default">
                                                    <em class="fa fa-calendar"></em>
                                                </button>
                                            </span>
                                        </p>
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4 form-group">
                                        <label>NACH Form <!--<span style="color:red">*</span>--></label>
                                        <input id="txtExistingLimit" ng-model="$parent.txtEditNACHForm" type="text"
                                               class="form-control ng-pristine ng-valid ng-touched" required />
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <div class="form-group">
                                            <label>Escrow Account <!--<span style="color:red">*</span>--></label>
                                            <div class="col">
                                                <p class="mda-radio">
                                                    <input type="radio" name="radio_escrow" ng-model="$parent.cboEditEscrowAccount" value="Applicable">
                                                    <em class="bg-teal-500"></em> Applicable
                                                </p>
                                            </div>
                                            <div class="col">
                                                &nbsp; &nbsp;
                                            </div>
                                            <div class="col">
                                                <p class="mda-radio">
                                                    <input type="radio" name="radio_escrow" ng-model="$parent.cboEditEscrowAccount" value="Not Applicable">
                                                    <em class="bg-teal-500"></em> Not Applicable
                                                </p>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                                <div class="row">
