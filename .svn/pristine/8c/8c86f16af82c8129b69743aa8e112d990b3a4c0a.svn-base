    <script type="text/ng-template" id="/addcontactdetails.html">
        <div class="modal-header bg-green-200 ng-scope">
            <h4 id="myModalLabel" class="modal-title">
                <span class="btn-label bg-green-200">
                    <em class="fa fa-plus"></em>
                    Add Details
                </span>
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <div class="pd-sm-10">
                <div class="row">
                    <form name="contactdetailsform" id="contactdetailsform" novalidate="" class="form-validate mb-lg" autocomplete="off">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtname" class="mda-form-control ng-pristine ng-valid ng-touched" required>
                                        <label>Name <span style="color:red">*</span></label>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtlocation" name="txtlimit" class="mda-form-control ng-pristine ng-valid ng-touched">
                                        <label>Location <span style="color:red">*</span></label>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtmobile" class="mda-form-control ng-pristine ng-valid ng-touched" maxlength="10">
                                        <label>Mobile No. <span style="color:red">*</span></label>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="mda-form-group float-label">

                                    </div>
                                </div>
                            </div>

                            <div class="class">
                                <div class="text-center">
                                    <a class="btn btn-success" ng-click="contactdetailsformsubmit()" ng-disabled="contactdetailsform.$invalid">
                                        <em class="fa fa-check"></em>
                                        <span>Submit</span>
                                    </a>
                                    <a class="btn btn-danger" ng-click="ok()">
                                        <em class="fa fa-close"></em>
                                        <span>Cancel</span>
                                    </a>
                                </div>
                            </div>
                        </div>

                    </form>
                </div>

            </div>

        </div>
    </script>

