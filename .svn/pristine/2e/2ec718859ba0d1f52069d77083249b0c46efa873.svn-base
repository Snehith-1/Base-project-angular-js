                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>Request Ref.No</label>
                                        <br>
                                        {{request_refno}}
                                    </div>

                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>Reopened By</label>
                                        <br>
                                        {{raised_by}}
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>Reopened By Number</label>
                                        <br>
                                        {{employee_mobileno}}
                                    </div>

                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>Reopened Date/Time</label>
                                        <br>
                                        {{reopened_date}}
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                        <label>Reopen Remarks : </label>
                                        <br />
                                        {{reopen_reason}}
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>
                                            Reporting To
                                        </label>
                                        <br>
                                        {{level_one}}
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>Reporting To Number</label>
                                        <br>
                                        {{employee_number}}
                                    </div>
                                </div>
                            </div>
                            <br />
                            <p class="lead bb text-purple text-bold" style="font-size:16px;">Reopen Document</p>
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="text-center">
                                        <div class="dd-handle ng-scope">
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-secondary">
                                                            <thead class="bg-gray text-white">
                                                                <tr width="100%">
                                                                    <th align="center" scope="col">S.No</th>
                                                                    <th align="center" scope="col">Document Name</th>
                                                                    <th align="center" scope="col">Reopened By</th>
                                                                    <th align="center" scope="col">Reopened Date</th>
                                                                    <th align="center" scope="col">
                                                                        Action&nbsp;&nbsp;&nbsp;
                                                                        <a download="" href="" title="Download All" id="btnpdf1" ng-click="Rdownloadall(rofilepath,rofilename)">
                                                                            <em class="icon-arrow-down"></em>&nbsp;
                                                                        </a>
                                                                    </th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in reopenrequestdocumentdtl" width="100%">
                                                                    <td align="left" scope="col" title="S.No">
                                                                        {{$index+1}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Document Name">
                                                                        {{data.document_name}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Reopened By">
                                                                        {{data.created_by}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Reopened Date">
                                                                        {{data.created_date}}
                                                                    </td>
                                                                    <td align="left" scope="col" data-title="Action">
                                                                        <button title="Download Document"
                                                                                ng-click="downloads(data.document_path,data.document_name)"
                                                                                class="btn btn-green">
                                                                            <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                            <em class="fa fa-download"></em>
                                                                        </button>
                                                                        <button title="View" class="btn btn-info" ng-click="documentviewer(data.document_path,data.document_name)">
                                                                            <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                                        </button>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                            </div>
                            <br />
                            <p class="lead bb text-purple text-bold" style="font-size:16px;">Transfer History</p>

                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="text-center">
                                        <div class="dd-handle ng-scope">
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-secondary">
                                                            <thead class="bg-gray text-white">
                                                                <tr width="100%">
                                                                    <th align="center" scope="col">S.No</th>
                                                                    <th align="center" scope="col">Assigned Team</th>
                                                                    <th align="center" scope="col">Assigned By</th>
                                                                    <th align="center" scope="col">Assigned Date/Time</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in transferlistdtlreopen" width="100%">
                                                                    <td align="left" scope="col" title="S.No">
                                                                        {{$index+1}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Assigned Team">
                                                                        {{data.assigned_team}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Assigned Member">
                                                                        {{data.assigned_to}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Assigned Date/Time">
                                                                        {{data.assigned_date}}
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                            </div>
                            <br />
                            <p class="lead bb text-purple text-bold" style="font-size:16px;">Allocation History</p>

                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="text-center">
                                        <div class="dd-handle ng-scope">
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-secondary">
                                                            <thead class="bg-gray text-white">
                                                                <tr width="100%">
                                                                    <th align="center" scope="col">S.No</th>
                                                                    <th align="center" scope="col">Assigned Manager</th>
                                                                    <th align="center" scope="col">Assigned Date/Time</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in allocatelistdtlreopen" width="100%">
                                                                    <td align="left" scope="col" title="S.No">
                                                                        {{$index+1}}
                                                                    </td>

                                                                    <td align="left" scope="col" title="Assigned Member">
                                                                        {{data.allocate_managername}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Assigned Date/Time">
                                                                        {{data.assigned_date}}
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-md-12 col-lg-12 col-sm-12">
                                    <div class="panel">
                                        <div class="panel-heading">
                                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Business Unit Status Details </b></p>
                                            <hr />
                                        </div>
                                        <br />
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <div class="table-responsive table-bordered">
                                                        <div style="max-height:200px;overflow:auto;">
                                                            <table class="table">
                                                                <thead class="bg-gray text-white">
                                                                    <tr>
                                                                        <th>SNo</th>
                                                                        <th>Business Unit Status</th>
                                                                        <th>Created By</th>
                                                                        <th>Created Date </th>
                                                                        <!--<th>Action</th>-->
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr ng-repeat="list in businessstatusunitmyactivitycompleted_list">
                                                                        <td>{{$index+1}}</td>
                                                                        <td>{{list.businessactivity_status}}</td>
                                                                        <td>{{list.created_by}}</td>
                                                                        <td>{{list.created_date}}</td>
                                                                        <!--<td class="text-nowrap">
                                                                            <button title="Delete" ng-click="businessstatus_delete(list.businessstatusactivity_gid)"
                                                                                    class="btn btn-danger btn-xs ">
                                                                                <em class="fa fa-trash-o"></em>
                                                                            </button>
                                                                        </td>-->
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <br />
                                    </div>
                                </div>
                            </div>
                            <br />
