<h3>
    My Activity 360
</h3>
<style>
    #bordersty {
        border: 1px solid #ebebeb;
        padding: 10px;
        border-radius: 25px;
    }
</style>
<style>
      .buttonstyle {
                border:none;
                background-color: rgb(255, 255, 255);
                color:#141478;

    }
</style>
<div ng-controller="osdTrnMyActivity360">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-retweet"></i>
            </span> My Activity 360
            <button ng-show="completeddtlsback" type="button" title="Back" class="pull-right btn btn-sm btn-default" ng-click="CompletedBack()">
                <em class="fa fa-chevron-circle-left text-danger fa-1x">&nbsp;Back</em>
            </button>
            <button ng-show="closeddtlsback" type="button" title="Back" class="pull-right btn btn-sm btn-default" ng-click="ClosedBack()">
                <em class="fa fa-chevron-circle-left text-danger fa-1x">&nbsp;Back</em>
            </button>
            <button ng-show="rejecteddtlsback" type="button" title="Back" class="pull-right btn btn-sm btn-default" ng-click="ClosedBack()">
                <em class="fa fa-chevron-circle-left text-danger fa-1x">&nbsp;Back</em>
            </button>
            <button ng-show="cancelleddtlsback" type="button" title="Back" class="pull-right btn btn-sm btn-default" ng-click="ClosedBack()">
                <em class="fa fa-chevron-circle-left text-danger fa-1x">&nbsp;Back</em>
            </button>

        </div>


        <md-content class="md-padding" ng-show="osd_View">
            <md-tabs md-dynamic-height="" md-border-bottom="">
                <md-tab label="Request Details">
                    <md-content class="md-padding">
                        <!-- Closed details-->
                        <div ng-show="closeddtls">

                            <br />
                            <h4 class="text-pink">
                                <em class="fa fa-times-circle text-danger fa-lg"></em> Closed Details
                            </h4>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>
                                            Closed By
                                        </label>
                                        <br>
                                        {{closed_by}}
                                    </div>

                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>Closed Date</label>
                                        <br>
                                        {{closed_date}}
                                    </div>

                                </div>
                            </div>



                        </div>
                        <!--Completed Details-->
                        <div ng-show="completeddtls">

                            <br />
                            <h4 class="text-pink">
                                <em class="fa fa-folder-open text-danger fa-lg"></em> Completed Details
                            </h4>
                            <br />

                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>
                                            Completed By
                                        </label>
                                        <br>
                                        {{completed_by}}
                                    </div>

                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label>Completed Date</label>
                                        <br>
                                        {{completed_date}}
                                    </div>

                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="col-lg-8 col-md-8 col-sm-8">
                                        <label><strong>Completed Remarks</strong> </label>
                                        <br>
                                        {{completed_remarks}}
                                    </div>
                                </div>
                            </div>
                            <br />
                            <p class="lead bb text-purple text-bold" style="font-size:16px;">Completed Document</p>
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="text-center">
                                        <div class="dd-handle ng-scope">
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-secondary">
                                                            <thead class="bg-gray text-white">
                                                                <tr width="100%">
                                                                    <th align="center" scope="col">S.No</th>
                                                                    <th align="center" scope="col">Document Name</th>
                                                                    <th align="center" scope="col">Created By</th>
                                                                    <th align="center" scope="col">Created Date</th>
                                                                    <th align="center" scope="col">
                                                                        Action
                                                                        &nbsp;&nbsp;&nbsp;
                                                                        <a download="" href="" title="Download All" id="btnpdf1" ng-click="downloadall(comfilepath,comfilename)">
                                                                            <em class="icon-arrow-down"></em>&nbsp;
                                                                        </a>
                                                                    </th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in completerequestdocumentdtl" width="100%">
                                                                    <td align="left" scope="col" title="S.No">
                                                                        {{$index+1}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Document Name">
                                                                        {{data.document_name}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Created By">
                                                                        {{data.created_by}}
                                                                    </td>
                                                                    <td align="left" scope="col" title="Created Date">
                                                                        {{data.created_date}}
                                                                    </td>

                                                                    <td align="left" scope="col" data-title="Action">
                                                                        <button title="Download Document"
                                                                                ng-click="downloads(data.document_path,data.document_name)"
                                                                                class="btn btn-green">
                                                                            <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                            <em class="fa fa-download"></em>
                                                                        </button>
                                                                        <button title="View" class="btn btn-info" ng-click="documentviewer(data.document_path,data.document_name)">
                                                                            <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                                        </button>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                            </div>


                            <br />
                        </div>
                        <div ng-show="Rejectdtls">
                            <h4 class="text-pink">
                                <em class="fa fa-ban text-danger fa-lg"></em> Rejected Details
                            </h4>
                            <div class="panel-body">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>
                                                    Rejected By
                                                </label>
                                                <br>
                                                {{rejected_by}}
                                            </div>

                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>Rejected Date</label>
                                                <br>
                                                {{rejected_date}}
                                            </div>

                                        </div>
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                            <div class="col-lg-8 col-md-8 col-sm-8">
                                                <label><strong>Rejected Remarks</strong> </label>
                                                <br>
                                                {{rejected_remarks}}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--Cancelled Details-->
                        <div ng-show="canceldtls">
                            <h4 class="text-pink">
                                <em class="fa fa-ban text-danger fa-lg"></em> Cancelled Details
                            </h4>
                            <div class="panel-body">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>
                                                    Cancelled By
                                                </label>
                                                <br>
                                                {{raised_by}}
                                            </div>

                                            <div class="col-lg-4 col-sm-4 col-md-4">
                                                <label>Cancelled Date</label>
                                                <br>
                                                {{cancel_date}}
                                            </div>

                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>

                        <!--Reopen Details Starts-->
                        <div ng-show="reopendetails">
