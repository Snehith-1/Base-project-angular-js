                                        <table class="table table-hover table-striped">
                                            <thead class="bg-gray text-white">
                                                <tr>
                                                    <th scope="col">S.No</th>
                                                    <th scope="col">Cheque Number</th>
                                                    <th scope="col">Account Number</th>
                                                    <th scope="col">Primary Status</th>
                                                    <th scope="col">Merged Banking Entity</th>
                                                    <th scope="col">Cheque Type</th>
                                                    <th scope="col">CTS Enabled</th>
                                                    <!-- <th>Action</th> -->
                                                    <th align="center" scope="col">
                                                        Action&nbsp;&nbsp;&nbsp;
                                                        <a download="" href="" title="Download All" id="btnpdf1" ng-click="downloadall()">
                                                            <em style="font-size: 1.5em;" class="icon-arrow-down"></em>&nbsp;
                                                        </a>
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in cheque_list" width="100%">
                                                    <td title="S.No">{{$index+1}}</td>
                                                    <td title="Document Name">{{data.cheque_no}}</td>
                                                    <td title="Document Name">{{data.account_number}}</td>
                                                    <td>{{data.primary_status}}</td>
                                                    <td>{{data.mergedbankingentity_name}}</td>
                                                    <td>{{data.cheque_type}}</td>
                                                    <td>{{data.cts_enabled}}</td>
                                                    <td data-title="Action">
                                                        <button title="Download" class="btn btn-green btn-xs"
                                                                ng-click="download_doc(data.document_path,data.document_name)">
                                                            <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                            <em class="fa fa-download"></em>
                                                        </button>

                                                        <button title="View" ng-click="cheque_view(data.creditor2cheque_gid)" class="btn btn-info btn-xs ">
                                                            <em class="fa fa-eye"></em>
                                                        </button>

                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>


                                <div ui-scroll-viewport="" style="overflow-y: auto;overflow-x: hidden;" class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
