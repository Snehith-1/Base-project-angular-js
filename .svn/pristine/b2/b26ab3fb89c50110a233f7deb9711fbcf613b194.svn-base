                                <textarea ng-model="txtcreditrating_link" rows="4" class="form-control" style="resize:vertical;" disabled
                                          id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>

                    </div>
                </form>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="/companybankdtl.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-edit"></em>&nbsp;&nbsp;<label style="font-size:16px"> Edit Bank Details </label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="panel-body">
                                <div class="row">
                                    <form novalidate="" name="submitForm" ng-submit="form.submitForm()" autocomplete="off" class="form-validate mb-lg">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="row">
                                                <div class="row">
                                                    <div class="col-sm-12 col-md-12 col-lg-12">
                                                        <p class="text-primary" style="font-size:14px;"><b> Bank Account Details </b></p>
                                                        <hr />
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> IFSC Code <span style="color:red"> *</span></label>
                                                            <br />
                                                            <input type="text" ng-model="txtIFSC_Code" name="IFSCCode" id="IFSCCode" required=""
                                                                   ng-change="IFSCValidation()" tabindex="0" class="form-control" />
                                                            <label class="fa fa-check" style="color:green" ng-show="ifscvalidation"></label>&nbsp;<label style="font-weight:bolder;color:green" ng-show="ifscvalidation">Verified</label>
                                                            <span ng-show="form.validateInput('IFSCCode', 'required')" class="text-danger">This field is required</span>
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> Bank Name </label>
                                                            <br />
                                                            <input type="text" ng-model="txtBank_Name" name="BankName" id="BankName" tabindex="0" class="form-control" disabled />
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> Branch Name <span style="color:red"> *</span></label>
                                                            <br />
                                                            <input type="text" ng-model="txtBranch_Name" name="BranchName" id="BranchName" tabindex="0" class="form-control" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-sm-12 col-md-12 col-lg-12">
                                                        <div class="col-lg-8 col-md-8 col-sm-8">
                                                            <label class="col-form-label"> Branch Address <span style="color:red"> *</span></label>
                                                            <br />
                                                            <input type="text" ng-model="txtBank_Address" name="BankAddress" id="BankAddress" tabindex="0" class="form-control"  />
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> MICR Code </label>
                                                            <br />
                                                            <input type="text" ng-model="txtMICR_Code" name="MICRCode" id="MICRCode" tabindex="0" class="form-control" disabled />
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-sm-12 col-md-12 col-lg-12">
                                                        <div class="col-lg-6 col-md-6 col-sm-6">
                                                            <label class="col-form-label"> Bank Account Number <span style="color:red"> *</span></label>
                                                            <br />
                                                            <input type="text" ng-model="txtbankacct_no" name="BankAccountNumber" id="BankAccountNumber" required="" ng-pattern="/^([A-Z0-9]{9,18})$/" tabindex="0" class="form-control" maxlength="30" />
                                                            <span ng-show="form.validateInput('BankAccountNumber', 'required')" class="text-danger">This field is required</span>
                                                            <label class="fa fa-check" style="color:green" ng-show="bankaccvalidation"></label>&nbsp;<label style="font-weight:bolder;color:green" ng-show="bankaccvalidation">Verified</label>
                                                            <label ng-if="bankaccvalidation == false" style="font-weight:bolder;color:red">Not Verified</label>
                                                        </div>
                                                        <div class="col-lg-6 col-md-6 col-sm-6">
                                                            <label class="col-form-label"> Confirm Account Number<span style="color:red"> *</span></label>
                                                            <br />
                                                            <input type="text" ng-model="txtconfirmbankacct_no" name="ConfirmBankAccountNumber" id="ConfirmBankAccountNumber" ui-validate="'$value==txtbankacct_no'" ui-validate-watch="'txtconfirmbankacct_no'" ng-pattern="/^([A-Z0-9]{9,18})$/" required="" tabindex="0" class="form-control" maxlength="30" ng-change="BankAccValidation()" />
                                                            <span ng-show="form.formValidate.ConfirmBankAccountNumber.$dirty &amp;&amp; form.formValidate.ConfirmBankAccountNumber.$error.validator" class="text-danger">Bank Account number does not match</span>
                                                            <span ng-show="form.validateInput('ConfirmBankAccountNumber', 'required')" class="text-danger">This field is required</span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-sm-12 col-md-12 col-lg-12">
                                                        <div class="col-lg-6 col-md-6 col-sm-6">
                                                            <label class="col-form-label"> Account Holder Name <span style="color:red"> *</span></label>
                                                            <br />
                                                            <input type="text" ng-model="txtacctholder_name" name="accounttholdername" id="accounttholdername" required="" tabindex="0" class="form-control" />
                                                            <span ng-show="form.validateInput('accounttholdername', 'required')" class="text-danger">This field is required</span>
                                                        </div>
                                                        <div class="col-lg-6 col-md-6 col-sm-6">
                                                            <label class="col-form-label"> Account Type <span style="color:red"> *</span></label>
                                                            <br />
                                                            <select name="AccountType" id="AccountType" chosen="" ng-options="m.bankaccounttype_gid as m.bankaccounttype_name for m in accounttype_list"
                                                                    type="text" class="form-control parsley-error" required="" ng-model="cboAccountType">
                                                                <option value=""> ----- Select From Dropdown -----</option>
                                                            </select>
                                                            <span ng-show="form.validateInput('AccountType', 'required')" class="text-danger">This field is required</span>
                                                        </div>

                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-sm-12 col-md-12 col-lg-12">
                                                        <div class="col-lg-6 col-md-6 col-sm-6">
                                                            <label> Joint Account <span style="color:red"> *</span></label>
                                                            <div class="radio c-radio">
                                                                <label>
                                                                    <input type="radio" id="JointAccount" name="JointAccount" value="Yes" ng-model="rdbJoint_Account" required="" />
                                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                </label>
                                                                <label>
                                                                    <input type="radio" id="JointAccount" name="JointAccount" ng-change="change()" value="No" ng-model="rdbJoint_Account" required="" />
                                                                    <span class="fa fa-circle"></span>No
                                                                </label>
                                                            </div>
                                                            <span ng-show="form.validateInput('JointAccount', 'required')" class="text-danger">This field is required</span>
                                                        </div>
                                                        <div class="col-lg-6 col-md-6 col-sm-6" ng-show="rdbJoint_Account=='Yes'">
                                                            <label class="col-form-label"> Joint Account Holder Name  <span style="color:red"> *</span></label>
                                                            <br />
                                                            <input type="text" ng-model="txtjointacctholder_name" name="jointacctholdername" id="jointacctholdername" tabindex="0" class="form-control" maxlength="50" />
                                                        </div>
                                                        <div class="col-lg-6 col-md-6 col-sm-6" ng-show="rdbJoint_Account!='Yes'">
                                                            <label class="col-form-label"> Joint Account Holder Name</label>
                                                            <br />
                                                            <input type="text" ng-model="test" disabled name="jointacctholdername" id="jointacctholdername" class="form-control" maxlength="50" />
                                                        </div>

                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-sm-12 col-md-12 col-lg-12">

                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label> Is Cheque Book Facility Available  <span style="color:red"> *</span></label>
                                                            <div class="radio c-radio">
                                                                <label>
                                                                    <input type="radio" id="CheckBook" name="CheckBook" value="Yes" ng-model="rdbCheque_Book" required="" />
                                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                </label>
                                                                <label>
                                                                    <input type="radio" id="CheckBook" name="CheckBook" value="No" ng-change="change()" ng-model="rdbCheque_Book" required="" />
                                                                    <span class="fa fa-circle"></span>No
                                                                </label>
                                                            </div>
                                                            <span ng-show="form.validateInput('CheckBook', 'required')" class="text-danger">This field is required</span>
                                                        </div>
                                                        <div class="col-sm-4 col-md-4 col-lg-4">
                                                            <label>Primary Status <span style="color:red"> *</span> </label>
                                                            <br />
                                                            <div class="radio c-radio">
                                                                <label>
                                                                    <input type="radio" id="primarystatus" name="primarystatus" value="Yes" ng-model="rdbprimarystatus" />
                                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                </label>
                                                                <label>
                                                                    <input type="radio" id="primarystatus" name="primarystatus" value="No" ng-model="rdbprimarystatus" />
                                                                    <span class="fa fa-circle"></span>No
                                                                </label>
                                                            </div>
                                                        </div>
                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                            <label class="col-form-label"> Account Open Date </label>
                                                            <br />
                                                            <p class="input-group">
                                                                <input type="text" id="AccountOpenDate" name="AccountOpenDate" ng-model="txtAccountOpen_Date" datepicker-popup="{{format}}"
                                                                       style="height:36px;" is-open="open01" min-date="minDate" datepicker-options="dateOptions"
                                                                       close-text="Close" class="form-control" placeholder="DD-MM-YYYY" />
                                                                <span class="input-group-btn">
                                                                    <button type="button" ng-click="calender01($event)" style="height:36px;" class="btn btn-default">
                                                                        <em class="fa fa-calendar"></em>
                                                                    </button>
                                                                </span>
                                                            </p>
                                                        </div>
                                                    </div>

                                                </div>
                                                <br />

                                            </div>
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <br /> <br />
                                                    <div class="text-center">
                                                        <button type="submit" class="btn btn-success btn-md" ng-disabled="submitForm.$invalid" ng-click="update_Bankacctdtl()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                                                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="/HeadOfficeConfirmation.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="icon-check"></em>&nbsp;<label style="font-size:16px"> Head Office Confirmation </label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="userform1" ng-submit="form.submitHeadoffice()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="text-center">
                                <h4> Do you want to confirm this GST Number belongs to head office? </h4>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="headoffice_submit()" class="btn btn-success"><em class="fa fa-check"></em>&nbsp;&nbsp;Confirm</button>
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>

</div>