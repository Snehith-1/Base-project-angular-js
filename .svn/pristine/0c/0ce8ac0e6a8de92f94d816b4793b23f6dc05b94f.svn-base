<h3>
    Credit Admin
</h3>
<div ng-controller="MstCreditAdminSummaryController">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <button type="button" title="Add Credit Admin" class="btn btn-info btn-sm" ng-click="addcreditadmin()">
                <span class="btn-label">
                    <i class="fa fa-plus"></i>
                </span>Add Credit Admin
            </button>
        </div>
    </div>
    <br />
    <div class="panel">
        <br />
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="table-responsive">
                        <table class="table table-hover table-striped">
                            <thead>
                                <tr class="text-white" style="background-color:#09385f;">
                                    <th style="display:none;"> S.No </th>
                                    <th> Created Date </th>
                                    <th> Credit Admin Code </th>
                                    <th> Credit Admin Name </th>
                                    <th> Credit Admin Role </th>
                                    <th> Created By </th>
                                    <th class="text-center">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td style="display:none;"></td>
                                    <td> 04-06-2021 </td>
                                    <td>CODE 001</td>
                                    <td>NAME</td>
                                    <td>ROLE</td>
                                    <td>SELF</td>
                                    <td class="nowrap" data-title="'Action'" style="text-align: center;" ng-switch="data.status">
                                        <button title="Edit" ng-click="editcreditadmin()" class="btn btn-green">
                                            <em class="fa fa-edit"></em>
                                        </button>
                                        <button title="Delete" ng-click="delete(data.credittype_gid)" class="btn btn-danger">
                                            <em class="icon icon-trash "></em>
                                        </button>
                                    </td>
                                </tr>
                                <!--<tr ng-repeat="data in credittype_data">
                                    <td data-title="'S.No'" style="display:none;" class="text-center"> {{$index+1}} </td>
                                    <td data-title="' Credit Admin Code'">{{data.created_date}}</td>
                                    <td data-title="'Credit Admin Name'"> {{data.credittype_name}} </td>
                                    <td data-title="'Role'"> {{data.lms_code}} </td>
                                    <td data-title="'Bureau Code'"> {{data.bureau_code}} </td>
                                    <td data-title="'Created By'"> {{data.created_by}} </td>                                 
                                    <td class="nowrap" data-title="'Action'" style="text-align: center;" ng-switch="data.status">
                                        <button title="Edit" ng-click="editcreditadmin(data.credittype_gid)" class="btn btn-green">
                                            <em class="fa fa-edit"></em>
                                        </button>
                                        <button title="Delete" ng-click="delete(data.credittype_gid)" class="btn btn-danger">
                                            <em class="icon icon-trash "></em>
                                        </button>
                                    </td>
                                </tr>-->

                            </tbody>

                        </table>
                    </div>
                </div>
            </div>
        </div>
        <br />
    </div>
    <script type="text/ng-template" id="/addcreditadmin.html">
        <div class="modal-header bg-primary ng-scope">

            <h4 id="myModalLabel" class="modal-title">
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-plus "></i>
                </span> Add Credit Admin
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="Addcreditadmin" ng-submit="form.Addcreditadmin()" autocomplete="off" class="form-validate form-horizontal">
                <div class="pd-sm-10">
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="row ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtcreditadmin_code" ng-pattern="/^[ A-Za-z0-9)(}​​​​​​​​{​​​​​​​​\]\['\\/,.]*$/" maxlength="255" required="" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                        <label>Credit Admin Code*</label>
                                    </div>
                                </div>
                            </div>

                            <div class="row ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="mda-form-group float-label">
                                        <label>Credit Admin Name *</label>
                                        <select chosen="" ng-options="m as m.entity_name for m in entitylist track by m.entity_gid"
                                                ng-model="creditverticalhead" class="chosen-select form-control" required></select>
                                    </div>
                                </div>
                            </div>

                            <div class="row ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtcreditadmin_role" ng-pattern="/^[ A-Za-z0-9)(}​​​​​​​​{​​​​​​​​\]\['\\/,.]*$/" maxlength="10" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                        <label>Credit Admin Role *</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div>
                        <br />
                        <div class="text-center">
                            <button type="button" class="btn btn-success btn-md" ng-disabled="Addcreditadmin.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                            <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                        </div>
                    </div>
                    <br />
                </div>
            </form>
        </div>
    </script>
    <script type="text/ng-template" id="/editcreditadmin.html">
        <div class="modal-header bg-primary ng-scope">

            <h4 id="myModalLabel" class="modal-title">
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-edit "></i>
                </span>Edit Credit Admin
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="Editcreditadmin" ng-submit="form.Editcreditadmin()" autocomplete="off" class="form-validate form-horizontal">
                <div class="pd-sm-10">
                    <div class="row ">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="row ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="mda-form-group">
                                        <input ng-model="txteditcredittype_name" ng-pattern="/^[ A-Za-z0-9)(}​​​​​​​​{​​​​​​​​\]\['\\/,.]*$/" maxlength="8" required="" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                        <label>Credit Admin Code *</label>
                                    </div>
                                </div>
                            </div>

                            <div class="row ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="mda-form-group">
                                         <label>Credit Admin Name *</label>
                                        <select chosen="" ng-options="m as m.entity_name for m in entitylist track by m.entity_gid"
                                                ng-model="creditverticalhead" class="chosen-select form-control" required></select>
                                    </div>
                                </div>
                            </div>

                            <div class="row ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="mda-form-group">
                                        <input ng-model="txteditbureau_code" ng-pattern="/^[ A-Za-z0-9)(}​​​​​​​​{​​​​​​​​\]\['\\/,.]*$/" maxlength="30" required="" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                        <label>Credit Admin Role *</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="Editcreditadmin.$invalid" ng-click="update()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
                <br />
            </form>
        </div>
    </script>
    
</div>