<a href="#/app/SysMstTask">
    <button type="button" title="Back" class="pull-right btn btn-sm btn-primary">
        <em class="fa fa-arrow-circle-left fa-1x"></em>
    </button>
</a>
<h3>
    Task View
</h3>
<div ng-controller="SysMstTaskViewController as Ctrl" class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        <div id="panelDemo8" class="panel panel-primary ">
            <div class="panel-heading">
                <h5><span><i class="fa fa-eye"></i></span>&nbsp;&nbsp;
                View Task</h5>
            </div>

            <div class="panel">
                <div class="panel-body">

                    <div id="panelDemo8" class="panel">
                    <div class="panel-heading" style="background-color: #cfcccb;color:#000000;">
                            <b>Basic Details</b>
                        </div>
                        <div class="panel">
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Task Code </label>

                                                    <p> {{lbltask_code}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Task Name</label>

                                                    <p> {{lbltask_name}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">LMS Code</label>

                                                    <p> {{lbllms_code}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold"> Bureau Code</label>

                                                    <p> {{lblbureau_code}} </p>
                                                </div>
                                            </div>
                                        </div>                                      
                                    </div>
                                    <br />
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-6 col-sm-6 col-md-6">
                                                    <label style="font-weight:bold">Assigned To </label>

                                                    <p> {{lblassignedto_name}} </p>
                                                </div>
                                                <div class="col-lg-6 col-sm-6 col-md-6">
                                                    <label style="font-weight:bold">Escalation Mail To</label>

                                                    <p> {{lblescalationmailto_name}} </p>
                                                </div>                                              
                                            </div>
                                        </div>                                      
                                    </div>
                              
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                               
                                                <div class="col-lg-6 col-sm-6 col-md-6">
                                                    <label style="font-weight:bold">Task Description</label>

                                                    <p> {{lbltask_description}} </p>
                                                </div>
                                                <div class="col-lg-6 col-sm-6 col-md-6">
                                                    <label style="font-weight:bold"> TAT</label>

                                                    <p> {{lbltat}} </p>
                                                </div>
                                            </div>
                                        </div>                                      
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="text-center">
                    <button type="button" class="btn btn-info btn-md" ng-click="back()"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>&nbsp;&nbsp;
                </div>
                <br />
            </div>
        </div>
    </div>
</div>






