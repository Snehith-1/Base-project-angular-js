    <script type="text/ng-template" id="/CCApprovedColendingVerification.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-eye"></em>&nbsp;<label style="font-size:16px">Co-lending Details</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="userform1" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="dd-handle ng-scope">
                                <div class="row">
                                    <div class="col-sm-12 col-md-12 col-lg-12">
                                        <div>
                                            <label> Applicability <span style="color:red"> *</span></label>
                                            <div class="radio c-radio">
                                                <label>
                                                    <input type="radio" id="Applicability" name="Applicability" value="Yes" ng-model="rdbccapplicability" required="" />
                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                </label>
                                                <label>
                                                    <input type="radio" id="Applicability" name="Applicability" value="No" ng-model="rdbccapplicability" required="" />
                                                    <span class="fa fa-circle"></span>No
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div>
                                            <label>Remarks <span style="color:red"> *</span></label>
                                            <textarea style="resize:vertical;" rows="4" name="Remarks" id="Remarks" placeholder="Enter Remarks..." aria-multiline="true" tabindex="0" aria-invalid="false" maxlength="1000"
                                                      ng-model="txtccverification_remarks" class="form-control"></textarea>

                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div>
                                            <div class="dd-handle ng-scope">
                                                <p class="text-primary" style="font-size:14px;"><b> Upload Co-lending Document </b></p>
                                                <hr />
                                                <div class="col-lg-8 col-md-8 col-sm-8">
                                                    <div class="mda-form-group float-label">
                                                        <input ng-model="txtdocument_title" tabindex="0" maxlength="75" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                                        <label>Document Title </label>
                                                    </div>
                                                </div>

                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <div class="mda-form-group float-label">
                                                        <strong>Upload Documents</strong>
                                                        <input type="file" id="ccverificationfile" tabindex="5" multiple onchange="angular.element(this).scope().ccverificationdocumentUpload(this.files, angular.element(this).scope().documentupload, this.name)"
                                                               ng-model="ccverificationfile" data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                        <div ui-scroll-viewport="" style="height:160px; overflow-y: auto;overflow-x: hidden;" class="row">
                                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                                <div class="table-responsive">
                                                                    <table class="table table-striped table-secondary">
                                                                        <thead class="bg-gray text-white">
                                                                            <tr width="100%">
                                                                                <th scope="col">S.No</th>
                                                                                <th scope="col">Document Title</th>
                                                                                <th scope="col">Document Name</th>
                                                                                <th align="center" scope="col">
                                                                                    Action&nbsp;&nbsp;&nbsp;
                                                                                    <a download="" href="" title="Download All" id="btnpdf2" ng-click="downloadall_verificationdoc()()">
                                                                                        <em class="icon-arrow-down"></em>&nbsp;
                                                                                    </a>
                                                                                </th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody>
                                                                            <tr ng-repeat="data in ccapprovedverificationdoc_list">
                                                                                <td title="S.No">{{$index+1}}</td>
                                                                                <td title="Document Name">{{data.document_title}}</td>
                                                                                <td title="Document Name">{{data.document_name}}</td>
                                                                                <td data-title="Action">
                                                                                    <button title="Download" class="btn btn-green btn-xs"
                                                                                            ng-click="downloads(data.document_path,data.document_name)">
