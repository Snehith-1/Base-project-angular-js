<h3>
    Scanned Document
</h3>
<div ng-controller="MstCadDeferralDochecklistController">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="panel panel-default">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div style="color :#000000 ;font-size:14px;font-weight :bold;">
                            <div class="col-lg-9 col-md-9 col-sm-9" style="background-color:#191370;color:white;padding-top:10px;padding-bottom:10px;">
                                <h5>{{txtbasiccustomer_name}}</h5>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3" style="background-color:#3a4ccb;color:white;padding-top:10px;padding-bottom:10px;">
                                <h5>{{txtapplication_no}}</h5>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row panel-body">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="dd-handle ng-scope">
                            <p class="text-primary" style="font-size:14px;"><b> Deferral Document </b></p>
                            <hr class="space" />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="table-responsive">
                                            <table class="table table-striped table-secondary">
                                                <thead class="bg-gray nowrap">
                                                    <tr width="100%">
                                                        <th style="color:#000000;">S.No</th>
                                                        <th style="color:#000000;">Document Type</th>
                                                        <th style="color:#000000;">Name of the Document</th>
                                                        <th style="color:#000000;">Initial Documents submitted</th>
                                                        <th style="color:#000000;">Signed Documents</th>
                                                        <th style="color:#000000;">Physical Documents</th> 
                                                        <th style="color:#000000;">Deferral Status</th>
                                                        <th style="color:#000000;">Due Date</th>
                                                        <th style="color:#000000;">Tagged Date & Time</th>
                                                        <th style="color:#000000;">Extended Due Date</th>
                                                        <th style="color:#000000;">Status</th>
                                                        <th style="text-align: center;width:8%;color:#000000;"> Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="nowrap">
                                                    <tr ng-repeat="data in ScannedDocumentlist">
                                                        <td title="S.No">{{$index+1}}</td>
                                                        <td title="Document Type">{{data.documenttype_code}}</td>
                                                        <td title="Name of the Document">{{data.documenttype_name}}</td>
                                                        <td title="Initial Documents submitted">
                                                            <button title="View" ng-click="deferraldoc_view(data.groupdocumentchecklist_gid,'N')" class="btn btn-info">
                                                                <em class="fa fa-eye"></em>
                                                            </button> 
                                                        </td>
                                                        <td title="Signed Documents">
                                                            <button title="View" ng-show="data.scanned_documentcount > 0" ng-click="deferraldoc_view(data.groupdocumentchecklist_gid,'Y')" class="btn btn-info">
                                                                <em class="fa fa-eye"></em>
                                                            </button> 
                                                        </td>
                                                        <td title="Physical Documents">
                                                            <button title="View" ng-click="defferaldoc_view()" disabled class="btn btn-info">
                                                                <em class="fa fa-eye"></em>
                                                            </button>
                                                        </td> 
                                                        <td title="Deferral Status" ng-switch="data.deferraltag_status">
                                                            <span ng-switch-when="0">Tagged</span>
                                                        </td>
                                                        <td title="Due Date">{{data.due_date}}  </td>
                                                        <td title="Tagged Date & Time"> {{data.taggeddate}}</td>
                                                        <td title="Extended Due Date"> {{data.extendeddue_date}}</td>
                                                        <td title="Status">{{data.overall_docstatus}}</td>
                                                        <td>
                                                            <button title="Query" ng-click="raise_query(data.groupdocumentchecklist_gid)" class="btn btn-primary">
                                                                <em class="fa fa-comment"></em>
                                                            </button>
                                                            <button title="Initiate Extension/waiver" ng-click="extension_waiver(data.groupdocumentchecklist_gid)" class="btn btn-success">
                                                                <em class="fa fa-external-link-square"></em>
                                                            </button>
                                                            <button title="Deferral History" ng-click="defferal_history(data.groupdocumentchecklist_gid)" class="btn btn-purple">
                                                                <em class="fa fa-history"></em>
                                                            </button>
                                                            <button title="Document Confirmation" ng-click="doc_confirmation(data.groupdocumentchecklist_gid,data.deferraltag_status,data.documentconfirmation_remarks,data.overall_docstatus)" class="btn btn-green">
                                                                <i class="icon icon-check"></i>
                                                            </button>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row panel-body">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="dd-handle ng-scope">
                            <p class="text-primary" style="font-size:14px;"><b> Covenant Document </b></p>
                            <hr class="space" />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="table-responsive">
                                            <table class="table table-striped table-secondary">
                                                <thead class="bg-gray nowrap">
                                                    <tr width="100%">
                                                        <th style="color:#000000;">S.No</th>
                                                        <th style="color:#000000;">Document Type</th>
                                                        <th style="color:#000000;">Name of the Document</th>
                                                        <th style="color:#000000;">Initial Documents submitted</th>
                                                        <th style="color:#000000;">Signed Documents</th>
                                                        <th style="color:#000000;">Physical Documents</th> 
                                                        <th style="color:#000000;">Deferral Status</th>
                                                        <th style="color:#000000;">Due Date</th>
                                                        <th style="color:#000000;">Tagged Date & Time</th>
                                                        <th style="color:#000000;">Extended Due Date</th>
                                                        <th style="color:#000000;">Covenant Periods</th>
                                                        <th style="color:#000000;">Status</th>
                                                        <th style="text-align: center;width:8%;color:#000000;"> Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="nowrap">
                                                    <tr ng-repeat="data in ScannnedCovenantDocumentlist">
                                                        <td title="S.No">{{$index+1}}</td>
                                                        <td title="Document Type">{{data.documenttype_code}}</td>
                                                        <td title="Name of the Document">{{data.documenttype_name}}</td>
                                                        <td title="Initial Documents submitted">
                                                            <button title="View" ng-click="deferraldoc_view(data.groupdocumentchecklist_gid,'N')" class="btn btn-info">
                                                                <em class="fa fa-eye"></em>
                                                            </button> 
                                                        </td>
                                                        <td title="Signed Documents">
                                                            <button title="View" ng-show="data.scanned_documentcount > 0" ng-click="deferraldoc_view(data.groupdocumentchecklist_gid,'Y')" class="btn btn-info">
                                                                <em class="fa fa-eye"></em>
                                                            </button> 
                                                        </td>
                                                        <td title="Physical Documents">
                                                            <button title="View" ng-click="defferaldoc_view()" disabled class="btn btn-info">
                                                                <em class="fa fa-eye"></em>
                                                            </button>
                                                        </td> 
                                                        <td title="Deferral Status" ng-switch="data.deferraltag_status">
                                                            <span ng-switch-when="0">Tagged</span>
                                                        </td>
                                                        <td title="Due Date">{{data.due_date}}  </td>
                                                        <td title="Tagged Date & Time"> {{data.taggeddate}}</td>
                                                        <td title="Extended Due Date"> {{data.extendeddue_date}}</td>
                                                        <td title="Covenant Periods"> 
                                                            <a ng-click="addcovenantperiods(data.groupdocumentchecklist_gid,data.documenttype_name,data.covenant_periods,'Y')">
                                                                {{data.covenant_periods}}
                                                            </a>
                                                        </td>
                                                        <td title="Status">{{data.overall_docstatus}}</td>
                                                        <td>
                                                            <button title="Query" ng-click="raise_query(data.groupdocumentchecklist_gid)" class="btn btn-primary">
                                                                <em class="fa fa-comment"></em>
                                                            </button>
                                                            <button title="Initiate Extension/waiver" ng-click="extension_waiver(data.groupdocumentchecklist_gid)" class="btn btn-success">
                                                                <em class="fa fa-external-link-square"></em>
                                                            </button>
                                                            <button title="Deferral History" ng-click="defferal_history(data.groupdocumentchecklist_gid)" class="btn btn-purple">
                                                                <em class="fa fa-history"></em>
                                                            </button>
                                                            <button title="Document Confirmation" ng-click="doc_confirmation(data.groupdocumentchecklist_gid,data.deferraltag_status,data.documentconfirmation_remarks,data.overall_docstatus)" class="btn btn-green">
                                                                <i class="icon icon-check"></i>
                                                            </button>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>

                </div>
                <br />
                <div class="row">
                    <div class="text-center">
                        <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                    </div>
                </div>
                <br /><br />
            </div>
        </div>
    </div>
    <script type="text/ng-template" id="/TagToDeferral.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px"></em>&nbsp;<label style="font-size:16px"><em class="icon-tag"></em> Tag to deferral </label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>
        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="tagdefferaladd" ng-submit="form.tagdefferaladd()" autocomplete="off" class="form-validate">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <div class="mda-form-group">
                                    <label>Criticality</label>
                                    <br />
                                    <p>{{txtdocumentseverity_name}}</p>
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <div class="mda-form-group">
                                    <label>Tracking ID </label>
                                    <br />
                                    <input disabled="disabled" placeholder="Auto Generated" class="form-control" />
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <div class="mda-form-group">
                                    <label>Tagged To <span style="color:red">*</span></label>
                                    <br />
                                    <select class="chosen-select form-control" id="TaggedTo" name="TaggedTo" tabindex="0" ng-model="cbotaggedto" required
                                            style="height:37px;" size="1">
                                        <option value="">---Select Tagged To---</option>
                                        <option value="Original Document">Original Document</option>
                                        <option value="Scanned Copy">Scanned Copy</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label>Due Date <span style="color:red">*</span></label>
                                    <br />
                                    <p class="input-group">
                                        <input type="date" id="SAO" name="SAO" ng-model="txtdue_date" required
                                               style="width:260px;height:35px" class="form-control" />
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="dd-handle">
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-6 col-md-6 col-sm-6">
                                            <p style="font-size:14px;color:darkred;"> Checkpoint </p>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <p style="font-size:14px;color:darkred;"> Requirement Status </p>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                        </div>
                                    </div>
                                </div>
                                <div ui-scroll-viewport="" style="height:180px; overflow-y: auto;overflow-x: hidden;">
                                    <div class="row" ng-repeat="data in Checklist">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="col-lg-6 col-md-6 col-sm-6">
                                                <p style="font-size:14px;"> {{data.checklist_name}} </p>
                                            </div>
                                            <div class="col-lg-4 col-md-4 col-sm-4">
                                                <input type="checkbox" id="c1"
                                                       value={{data.checklistchecked}} ng-disabled="{{data.checklistchecked}}" ng-model="data.checklistchecked">
                                            </div>
                                            <div class="col-lg-4 col-md-4 col-sm-4">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <label class="col-form-label">Remarks by CAD </label> <span style="color:red">*</span>
                                <textarea style="resize:vertical;" ng-model="txtcad_remarks" maxlength="5000" rows="3" class="form-control"
                                          required id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>

                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="tagdefferaladd.$invalid" ng-click="tagdoc_submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                    <br />
                </form>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="/document_view.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-file-text-o"></em>&nbsp;<label style="font-size:16px"> Documents</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="userform1" ng-submit="form.userform1()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">

                            <div class="table-responsive">
                                <table class="table table-striped table-secondary">
                                    <thead class="bg-gray text-white">
                                        <tr width="100%">
                                            <th scope="col">S.No</th>
                                            <th scope="col">Document Title</th>
                                            <th scope="col">Document Name</th>
                                            <th scope="col">Uploaded By / On </th>
                                            <th scope="col">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="data in scanneduploaddocumentlist">
                                            <td title="S.No">{{$index+1}}</td>
                                            <td title="Document Title">{{data.documenttype_name}}</td>
                                            <td title="Document Name">{{data.file_name}}</td>
                                            <td title="Uploaded By / On">{{data.created_by}} / {{data.created_date}} </td>
                                            <td data-title="Action">
                                                <button title="Download" class="btn btn-green btn-sm"
                                                        ng-click="download_doc(data.file_path,data.file_name)">
                                                    <em class="fa fa-download"></em>
                                                </button>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>
