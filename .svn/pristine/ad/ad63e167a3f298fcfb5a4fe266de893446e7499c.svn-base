    <script type="text/ng-template" id="/defferal_docupload.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-file-text-o"></em>&nbsp;<label style="font-size:16px"> Document Upload </label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="userform1" ng-submit="form.userform1()" autocomplete="off" class="form-validate mb-lg">
                    <div class="dd-handle ng-scope">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-sm-5 col-md-5 col-lg-5">
                                    <label>Document Title </label>
                                    <input type="text" ng-model="txtdocument_name" tabindex="0" class="form-control" maxlength="50" disabled />
                                </div>
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    <div class="float-label">
                                        <strong> Upload Documents </strong>
                                        <br /><br />
                                        <input type="file" id="scannedfile" tabindex="5" multiple onchange="angular.element(this).scope().scannedDocumentUpload(this.files, angular.element(this).scope().documentupload, this.name)"
                                               ng-model="cbodocumentname" data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="table-responsive table-bordered">
                                        <div style="max-height:200px;overflow:auto;">
                                            <table class="table">
                                                <thead class="bg-gray text-white">
                                                    <tr>
                                                        <th>S.No</th>
                                                        <th>Document Title</th>
                                                        <th>Document Name</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="list in scanneduploaddocumentlist">
                                                        <td>{{$index+1}}</td>
                                                        <td>{{list.documenttype_name}}</td>
                                                        <td>{{list.file_name}}</td>
                                                        <td class="text-nowrap">
                                                            <button title="Delete" ng-click="defdoc_delete(list.scanneddocument_gid)"
                                                                    class="btn btn-danger btn-xs ">
                                                                <em class="fa fa-trash-o"></em>
                                                            </button>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br />
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="/DocumentConfirmation.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-check"></em>&nbsp;Document Confirmation</h2>
                <span flex></span>

            </div>
        </md-toolbar>

        <div class="modal-body ng-scope">
            <form name="documentConfirmationform" id="documentConfirmationform" novalidate="" class="form-validate mb-lg" autocomplete="off">
                <div class="row">
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <label class="col-form-label control-label"><strong> Final Remarks <span style="color:red;">*</span></strong></label><br />
                        <select name="txtfinalremarks" ng-model="cbofinalstatus" class="form-control ng-valid ng-touched ng-valid-parse"
                                size="1" ng-change="onchangeremarks(cbofinalstatus)">
                            <option value="">--- Select Final Remarks ---</option>
                            <option value="Query Cleared">Query Cleared </option>
                            <option value="Deferral Taken">Deferral Taken </option>
                            <option value="Others">Others </option>
                        </select>

                        <span style="color:red;" ng-show="showerrordiv">
                            <em class="fa fa-warning"></em>&nbsp;{{errormsg}}
                        </span>
                    </div>
                    <br /> <br /> <br /><br />
                    <div class="col-sm-12 col-md-12 col-lg-12" ng-show="other_remarks">
                        <textarea ng-model="scanfinal_remarks" placeholder="Final Remarks - Conversation" rows="2" maxlength="1000"
                                  class="form-control" aria-required="true" style="resize:vertical;"
                                  id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                    </div>
                </div>
                <br /> <br />
                <div clas="row">
                    <div class="text-center">
                        <a class="btn btn-success" ng-click="docconfirmation_Submit()" ng-disabled="btnconfirmation">
                            <em class="fa fa-check"></em>
                            <span>Document Confirm</span>
                        </a>
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
            </form>
        </div>
    </script>


    <script type="text/ng-template" id="/DocumentCompleted.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-check"></em>&nbsp;Document Confirmation</h2>
                <span flex></span>

            </div>
        </md-toolbar>

        <div class="modal-body ng-scope">
            <form name="documentConfirmationform" id="documentConfirmationform" novalidate="" class="form-validate mb-lg" autocomplete="off">
                <div class="row">
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <label class="col-form-label control-label"><strong> Final Status </strong></label><br />
                        <p>{{lbloverallstatus}}</p>
                    </div>
                    <br /> <br />
                    <div class="col-sm-12 col-md-12 col-lg-12" ng-show="lbldocumentconfirmation_remarks!=''">
                        <label>Final Remarks</label>
                        <textarea ng-model="lbldocumentconfirmation_remarks" placeholder="Final Remarks - Conversation" rows="2" maxlength="1000"
                                  class="form-control" aria-required="true" style="resize:vertical;" disabled
                                  id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                    </div>
                </div>
                <br /> <br />
                <div clas="row">
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
            </form>
        </div>
    </script>

    <script type="text/ng-template" id="/MovedtosignedDoc.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-arrow-right"></em>&nbsp;&nbsp;<label style="font-size:16px"> Moved to Signed Documents</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="userform1" ng-submit="form.userform1()" autocomplete="off" class="form-validate mb-lg">

                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">

                            <div class="table-responsive">
                                <table class="table table-striped table-secondary">
                                    <thead class="bg-gray text-white">
                                        <tr width="100%">
                                            <th>
                                                <input type="checkbox" id="c3" ng-model="covenantall" ng-change="checkall(covenantall,true);">
                                                Check All
                                            </th>
                                            <th scope="col">Document Title</th>
                                            <th scope="col">Document Name</th>
                                            <th scope="col">Uploaded By / On </th>
                                            <th scope="col">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody style="max-height:300px;overflow:auto;">
                                        <tr ng-repeat="data in scanneduploaddocumentlist">
                                            <td>
                                                &nbsp;
                                                <input type="checkbox" id="checkbox_{{$index}}"
                                                       value={{data.checked}} ng-checked="{{data.checked}}" ng-model="data.checked">
                                            </td>
                                            <td title="Document Title">{{data.documenttype_name}}</td>
                                            <td title="Document Name">{{data.file_name}}</td>
                                            <td title="Uploaded By / On">{{data.created_by}} / {{data.created_date}} </td>
                                            <td data-title="Action">
                                                <button title="Download" class="btn btn-green btn-sm"
                                                        ng-click="download_doc(data.file_path,data.file_name)">
                                                    <em class="fa fa-download"></em>
                                                </button>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="yesmovedtosigned()" class="btn btn-success"><i class="fa icon-check"></i>&nbsp;&nbsp;Submit</button>
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>

    <script type="text/ng-template" id="/AddCovenantPeriods.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-plus"></em>&nbsp;&nbsp;<label style="font-size:16px"> Covenant Periods </label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="userform1" ng-submit="form.userform1()" autocomplete="off" class="form-validate mb-lg">

                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                             
                            <div>
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label>Document Name</label>
                                            <br />
                                            <p>{{lbldocumentname}}</p>
                                        </div>
                                        <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label>Covenant Periods</label>
                                            <br />
                                            <p>{{lblcovenantperiods}}</p>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                            <div class="table-responsive" >
                                <table class="table table-striped table-secondary">
                                    <thead class="bg-gray text-white">
                                        <tr width="100%">
                                            <th scope="col">Covenant Periods</th>
                                            <th scope="col">Start Date</th>
                                            <th scope="col">End Date</th>
                                            <th scope="col">Submission Date</th>
                                            <th scope="col">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody style="max-height:300px;overflow:auto;">
                                        <tr ng-repeat="data in scannedcovenantperiodlist">
                                            <td title="Covenant Periods">{{data.covenant_periods}}</td>
                                            <td title="Start Date">{{data.covenant_startdate | date:'dd-MM-yyyy'}}</td>
                                            <td title="End Date">{{data.covenant_enddate | date:'dd-MM-yyyy'}}</td>
                                            <td data-title="Submission Date">{{data.covenant_submissiondate | date:'dd-MM-yyyy'}}</td>
                                            <td data-title="Remarks">
                                                <button title="Delete" ng-click="covenantperiod_delete(data.covenantperioddtl_gid)"
                                                        class="btn btn-danger btn-xs ">
                                                    <em class="fa fa-trash-o"></em>
                                                </button>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>
</div>
