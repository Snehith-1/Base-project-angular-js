<h3>
    External Visit Report - Cancel
     <a href="#/app/caseAllocation">
        <button type="button" title="Back" class="pull-right btn btn-sm btn-info">
            <em class="fa fa-arrow-circle-left fa-1x"></em>
        </button>
    </a>
</h3>
<div ng-controller="externalvisitReportCancelcontroller as dp">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <em class="fa fa-file"></em>&nbsp; Generate Visit Report
        </div>
        <div class="panel-body">
            <div class="row">
                <form name="Visitreportform" id="Visitreportform" novalidate="" class="form-validate mb-lg" autocomplete="off">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="row col-lg-12 col-sm-12 col-md-12  text-center">
                            <!--<label>Customer Name :</label>-->
                            <label class="text-primary" style="font-size:16px;">{{clientName}}</label>
                            <br />
                            URN No:&nbsp;<label class="text-pink" style="font-size:16px;">{{customer_urn}}</label>
                            <br />
                            <div class="row text-center">
                                <div class="col-lg-4 col-md-4 col-sm-4">

                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">

                                    <div class="form-group">

                                        <div class="col">
                                            <label>Risk Code <span style="color:red">*</span></label> &emsp;
                                            <p class="mda-radio">
                                                <input ng-model="cboriskcode" type="radio" name="radio_status" value="Green" required>
                                                <em class="bg-teal-500"></em><span class="label label-success">Green</span>
                                            </p>
                                        </div>
                                        <div class="col">
                                            &nbsp; &nbsp;
                                        </div>
                                        <div class="col">
                                            <p class="mda-radio">
                                                <input ng-model="cboriskcode" type="radio" name="radio_status" value="Amber" required>
                                                <em class="bg-teal-500"></em><span class="label label-warning">Amber</span>
                                            </p>
                                        </div>
                                        <div class="col">
                                            &nbsp; &nbsp;
                                        </div>
                                        <div class="col">
                                            <p class="mda-radio">
                                                <input ng-model="cboriskcode" type="radio" name="radio_status" value="Red" required>
                                                <em class="bg-teal-500"></em><span class="label label-danger">Red</span>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">

                                </div>

                            </div>
                        </div>
                        <br />
                        <div class="row">

                            <div class="col-lg-4 col-sm-6 col-md-6 form-group float-label">
                                <label>Constitution <span style="color:red">*</span></label>
                                <input ng-model="txtconstitution" type="text"
                                       class="form-control ng-pristine ng-valid ng-touched" required>

                            </div>
                            <div class="col-lg-4 col-md-6 col-sm-6">
                                <label>Dealing with since <span style="color:red">*</span> </label>
                                <p class="input-group">
                                    <input type="text" datepicker-popup="{{dp.format}}" ng-model="txtfirstdisb_date" is-open="dp.open5"
                                           datepicker-options="dp.dateOptions" ng-required="true"
                                           close-text="Close" class="form-control" />
                                    <span class="input-group-btn">
                                        <button type="button" ng-click="dp.calender5($event)" class="btn btn-default">
                                            <em class="fa fa-calendar"></em>
                                        </button>
                                    </span>
                                </p>

                                <!--<p class="input-group">
                                    <input type="text" datepicker-popup="{{dp.format}}" ng-model="txtincorporated_date" is-open="dp.open2"
                                           datepicker-options="dp.dateOptions" ng-required="true"
                                           close-text="Close" class="form-control" />
                                    <span class="input-group-btn">
                                        <button type="button" ng-click="dp.calender2($event)" class="btn btn-default">
                                            <em class="fa fa-calendar"></em>
                                        </button>
                                    </span>
                                </p>-->
                            </div>
                            <div class="col-lg-4 col-md-6 col-sm-6">
                                <label>Date of the Visit <span style="color:red">*</span></label>
                                <p class="input-group">
                                    <input type="text" datepicker-popup="{{dp.format}}" ng-model="visitdate" is-open="dp.open1"
                                           datepicker-options="dp.dateOptions" ng-required="true"
                                           close-text="Close" class="form-control" />
                                    <span class="input-group-btn">
                                        <button type="button" ng-click="dp.calender1($event)" class="btn btn-default">
                                            <em class="fa fa-calendar"></em>
                                        </button>
                                    </span>
                                </p>
                                <!--<p class="input-group">
                                    <input type="text" datepicker-popup="{{dp.format}}" ng-model="visitdate" is-open="dp.open1"
                                           datepicker-options="dp.dateOptions" ng-required="true"
                                           close-text="Close" class="form-control" />
                                    <span class="input-group-btn">
                                        <button type="button" ng-click="dp.calender1($event)" class="btn btn-default">
                                            <em class="fa fa-calendar"></em>
                                        </button>
                                    </span>
                                </p>-->
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-6 col-sm-6 col-md-6">
                                <div class="form-group float-label">
                                    <label>Business Vintage <span style="color:red">*</span></label>
                                    <input ng-model="txtbusiness_vintage" type="text"
                                           class="form-control ng-pristine ng-valid ng-touched" required>
                                </div>
                            </div>

                            <div class="col-lg-6 col-sm-6 col-md-6">
                                <div class="form-group float-label">
                                    <label> Business Sector <span style="color:red">*</span></label>
                                    <input ng-model="txtbusiness_sector" type="text"
                                           class="form-control ng-pristine ng-valid ng-touched" required>
                                </div>
                            </div>
                        </div>
                        <div class="row">

                            <div class="col-lg-6 col-sm-6 col-md-6">
                                <label>Type of Loan  <span style="color:red">*</span></label>
                                <select name="approval_authority" ng-model="cbotypeof_loan" required=""
                                        class="form-control ng-valid ng-touched ng-valid-parse" size="1">
                                    <option value=""> Select Type of Loan </option>
                                    <option value="Fresh"> Fresh</option>
                                    <option value="Renewal"> Renewal</option>
                                    <option value="Enhancement"> Enhancement</option>
                                    <option value="Restructured"> Restructured</option>

                                </select>
                            </div>
                            <div class="col-lg-6 col-sm-6 col-md-6">
                                <label>Type of Risk Review  <span style="color:red">*</span></label>
                                <select name="approval_authority" ng-model="cborisk_reviewtype" required=""
                                        class="form-control ng-valid ng-touched ng-valid-parse" size="1">
                                    <option value=""> Select Type of Risk Review </option>
                                    <option value="Mandatory visit"> Mandatory visit</option>
                                    <option value="Reassessment visit"> Reassessment visit</option>
                                    <option value="Continuous visit"> Continuous visit</option>
                                    <option value="Loan covenant visit"> Loan covenant visit</option>

                                </select>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 form-group">
                                <label class="text-primary" style="text-decoration:underline;">
                                    Address of the Client / office visited
                                </label>
                            </div>
                            <div class="col-lg-12 col-sm-12 col-md-12 form-group">
                                <div class="col-lg-6 col-md-6 col-sm-6">
                                    <label>Registered Office Address: <span style="color:red">*</span></label>
                                    <textarea name="specificcondition" placeholder="Enter Registered Office Address..." required
                                              ng-model="txtregister_address" class="form-control" rows="3"></textarea>
                                </div>
                                <div class="col-lg-6 col-md-6 col-sm-6">
                                    <label>Actual / Present Address:  <span style="color:red">*</span></label>
                                    <textarea name="specificcondition" placeholder="Enter Actual / Present Address..." required
                                              ng-model="txtactual_address" class="form-control" rows="3"></textarea>
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 form-group">
                                <label class="text-primary" style="text-decoration:underline;">
                                    Name/s of the representative met,
                                    contact details (email & mob no to be
                                    captured) :
                                </label>
                            </div>
                            <div class="col-lg-12 col-sm-12 col-md-12 form-group">
                                <div class="col-lg-6 col-md-6 col-sm-6">
                                    <label>Contact Details 1: <span style="color:red">*</span></label>
                                    <textarea name="specificcondition" placeholder="Enter Contact Details 1..." required
                                              ng-model="txtcontact_dtl1" class="form-control" rows="3"></textarea>
                                </div>
                                <div class="col-lg-6 col-md-6 col-sm-6">
                                    <label>Contact Details 2:  <span style="color:red">*</span></label>
                                    <textarea name="specificcondition" placeholder="Enter Contact Details 2..." required
                                              ng-model="txtcontact_dtl2" class="form-control" rows="3"></textarea>
                                </div>
                            </div>
                            <!--<div class="col-lg-4 col-sm-4 col-md-4">
                                <div class="form-group float-label">
                                    <label>Contact Person <span style="color:red">*</span></label>
                                    <input ng-model="txtcontact_person" class="form-control ng-pristine ng-valid ng-touched" required>
                                </div>
                            </div>

                            <div class="col-lg-4 col-sm-4 col-md-4">
                                <div class="form-group float-label">
                                    <label>Email ID <span style="color:red">*</span></label>
                                    <input ng-model="txtemail_ID" type="email"
                                           class="form-control ng-pristine ng-valid ng-touched" required>
                                </div>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                <div class="form-group float-label">
                                    <label>Contact No. <span style="color:red">*</span></label>
                                    <input ng-model="txtcontact_number" type="number" maxlength="10"
                                           class="form-control ng-pristine ng-valid ng-touched" required>
                                </div>
                            </div>-->
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="form-group float-label">
                                        <label>Latitude <span style="color:red">*</span></label>
                                        <input ng-model="txtlattitude" type="text"
                                               class="form-control ng-pristine ng-valid ng-touched" required>
                                    </div>
                                </div>

                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="form-group float-label">
                                        <label>Longitude <span style="color:red">*</span></label>
                                        <input ng-model="txtlongitude" type="text"
                                               class="form-control ng-pristine ng-valid ng-touched" required>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <label class="ng-scope text-primary">Sanction & Loan Details</label>
                                <div class="col-lg-12">
                                    <div class="table-responsive table-bordered">
                                        <table class="table">
                                            <thead class="bg-indigo-400">
                                                <tr>
                                                    <th></th>
                                                    <th>S.No</th>
                                                    <th colspan="2">Sanction Ref No.</th>
                                                    <th colspan="2">Sanction Date</th>
                                                    <th colspan="2">Sanction Amount</th>
                                                    <th colspan="2">Sanction Limit</th>
                                                    <th colspan="2">Type of Facility</th>
                                                    <th colspan="2">Tenure-Months</th>
                                                </tr>
                                            </thead>
                                            <tbody ng-repeat="loop in sanctiondetails">
                                                <tr>
                                                    <td>
                                                        <a ng-hide="loop.expand" ng-click="loop.expand=true">
                                                            <button title="Click to Expand Loan Details">
                                                                <em class="fa fa-chevron-circle-down" style="font-size:14px;"></em>
                                                            </button>
                                                        </a>
                                                        <a ng-show="loop.expand" ng-click="loop.expand=false">
                                                            <button title="Click to Collapse Loan Details">
                                                                <em class="fa fa-chevron-circle-up" style="font-size:14px;"></em>
                                                            </button>
                                                        </a>
                                                    </td>
                                                    <td> {{$index+1}} </td>
                                                    <td colspan="2"> {{loop.sanction_refno}} </td>
                                                    <td colspan="2"> {{loop.sanction_date}} </td>
                                                    <td colspan="2">{{loop.sanction_amount}}</td>
                                                    <td colspan="2">{{loop.sanction_limit}}</td>
                                                    <td colspan="2">{{loop.facility_type}}</td>
                                                    <td colspan="2">{{loop.tenure_months}}</td>
                                                </tr>

                                                <tr ng-show="loop.expand" class="bg-indigo-300 text-white">
                                                    <th></th>

                                                    <th colspan="6"> Loan Ref.No </th>
                                                    <th colspan="6"> Loan Facility Type </th>
                                                    <!--<th colspan="4"> Vertical </th>-->
                                                </tr>
                                                <tr ng-show="loop.expand" class="bg-indigo-100" ng-repeat="nest in loop.loandetails">
                                                    <td></td>

                                                    <td colspan="6">  {{nest.loanref_no}}  </td>
                                                    <td colspan="6"> {{nest.loan_title}}</td>
                                                    <!--<td colspan="4"> {{nest.loan_title}} </td>-->
                                                </tr>

                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>

                        </div>
                        <!--<div class="row">

                            <div class="col-lg-6 col-sm-6 col-md-6">
                                <div class="form-group float-label">
                                    <label>Limit Sanctioned <span style="color:red">*</span></label>
                                    <input ng-model="txtsanctioned_limit" class="form-control ng-pristine ng-valid ng-touched" required>
                                </div>
                            </div>
                            <div class="col-lg-6 col-sm-6 col-md-6">
                                <div class="form-group float-label">
                                    <label>Tenure/Credit Period <span style="color:red">*</span></label>
                                    <input ng-model="txttenure_period" class="form-control ng-pristine ng-valid ng-touched" required>
                                </div>
                            </div>
                        </div>-->
                        <br />

                        <div class="row">
                            <!--<div class="col-lg-4 col-sm-3 col-md-3 form-group float-label">
                                <label>Relationship Started from</label>
                                 <p>{{firstdisb_date}}</p>
                            </div>-->
                            <div class="col-lg-6 col-sm-6 col-md-6">


                                <div class="form-group float-label">
                                    <label>Primary/secondary value chain <span style="color:red">*</span></label>
                                    <input ng-model="txtprimary_chain" class="form-control ng-pristine ng-valid ng-touched" required>
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="form-group">
                                    <label>Genetic Code Complied <span style="color:red">*</span></label>
                                    <div class="col">
                                        <p class="mda-radio">
                                            <input type="radio" name="genetic_code" ng-model="cbogenetic_code" value="Yes">
                                            <em class="bg-teal-500"></em> Yes
                                        </p>
                                    </div>
                                    <div class="col">
                                        &nbsp; &nbsp;
                                    </div>
                                    <div class="col">
                                        <p class="mda-radio">
                                            <input type="radio" name="genetic_code" ng-model="cbogenetic_code" value="No">
                                            <em class="bg-teal-500"></em> No
                                        </p>
                                    </div>

                                </div>
                            </div>
                            <!--<div class="col-lg-6 col-sm-6 col-md-6">
                                <div class="form-group float-label">
                                    <label>Client business Vintage (in years) <span style="color:red">*</span></label>
                                    <input ng-model="txtbusiness_client" class="form-control ng-pristine ng-valid ng-touched" required>
                                </div>
                            </div>-->
                        </div>
                        <div class="row">

                            <div class="col-lg-4 col-md-4 col-sm-4 form-group">
                                <label ng-model="assigned_RMD">RMD person’s name visited </label>
                                <p ng-model="assignedRMD_gid">{{assigned_RMD}}</p>
                                <!--<select id="cboRMD_name" name="RMD_name" chosen="" ng-options="m.employee_name for m in employee_list"
                                        ng-model="cboRMD_gid" class="chosen-select form-control">
                                    <option value="">select Employee Name</option>
                                </select>-->

                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4 form-group">
                                <label>RM Name </label>
                                <p>{{relationship_managername}}</p>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                <div class="form-group float-label">
                                    <label>Credit Manager’s Name </label>
                                    <p>{{credit_managername}}</p>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-6 col-sm-6 col-md-6 form-group float-label">
                                <label>PPA Name </label>
                                <input ng-model="txtPPA_name" type="text" class="form-control ng-pristine ng-valid ng-touched" disabled>
                            </div>

                            <div class="col-lg-6 col-sm-6 col-md-6 form-group">
                                <label>Where the visit was done <span style="color:red">*</span></label>
                                <select name="approval_authority" chosen="" multiple="multiple" ng-options="m.visit_type for m in visittypedtl"
                                        ng-model="cbovisit_done" class="chosen-select form-control" required></select>
                                <!--<select name="approval_authority" ng-model="cbovisit_done" required=""
                                        class="form-control ng-valid ng-touched ng-valid-parse" size="1">
                                    <option value=""> Select visit </option>
                                    <option value="Branch Office"> Branch Office</option>
                                    <option value="Head Office"> Head Office</option>
                                    <option value="Regional Office"> Regional Office</option>
                                    <option value="Groups"> Groups</option>
                                    <option value="Warehouse"> Warehouse</option>
                                    <option value="Processing units/factories/Manufacturing units"> Processing units/factories/Manufacturing units</option>
                                    <option value="Supplier/ vendor place"> Supplier/ vendor place</option>
                                    <option value="Collateral/Securities verification"> Collateral/Securities verification.</option>
                                </select>-->
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                <div class="form-group float-label">
                                    <label>Purpose of loan/end use <span style="color:red">*</span></label>
                                    <input ng-model="txtpurposeof_loan" class="form-control ng-pristine ng-valid ng-touched" required>
                                </div>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                <div class="form-group float-label">
                                    <label>Requested loan amount by client <span style="color:red">*</span></label>
                                    <span style="color:#ff001882;" class="text-bold"> Accept Number Format Only</span>
                                    <input id="requestloan_amount" ng-model="txtrequestedloan_byclient" class="form-control ng-pristine ng-valid ng-touched"
                                           ng-change="requestedloanamountChange()" required style="text-align:right;">
                                    <span style="font-style:italic;">Number in words:</span>
                                    <span id="words_requestedloan" style="color:#003c96fc;" class="text-bold"></span>
                                </div>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                <div class="form-group float-label">
                                    <label>Sanctioned loan amount by credit <span style="color:red">*</span></label>
                                    <input ng-model="txtsantionloan_bycredit" disabled style="text-align:right;" class="form-control ng-pristine ng-valid ng-touched">
                                    <!--<p ng-model="txtsantionloan_bycredit">{{txtsantionloan_bycredit}}</p>-->
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4 col-sm-3 col-md-3 form-group float-label">
                                <label>Disbursement Date</label>
                                <p class="input-group">
                                    <input type="text" datepicker-popup="{{dp.format}}" ng-model="txtdisbursement_date" is-open="dp.open4"
                                           datepicker-options="dp.dateOptions" ng-required="true"
                                           close-text="Close" class="form-control" />
                                    <span class="input-group-btn">
                                        <button type="button" ng-click="dp.calender4($event)" class="btn btn-default">
                                            <em class="fa fa-calendar"></em>
                                        </button>
                                    </span>
                                </p>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                <div class="form-group float-label">
                                    <label>Disbursement amount <span style="color:red">*</span></label>
                                    <span style="color:#ff001882;" class="text-bold"> Accept Number Format Only</span>
                                    <input id="disbursement_amount" ng-model="txtdisbursement_amount" class="form-control ng-pristine ng-valid ng-touched"
                                           ng-change="disbursementchange()" required style="text-align:right;">
                                    <span style="font-style:italic;">Number in words:</span>
                                    <span id="words_disbursementamount" style="color:#003c96fc;" class="text-bold"></span>
                                </div>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                <div class="form-group float-label">
                                    <label>Total loan outstanding as on date <span style="color:red">*</span></label>
                                    <span style="color:#ff001882;" class="text-bold"> Accept Number Format Only</span>
                                    <input id="totalloan_oustanding" ng-model="txttotalloan_oustanding" class="form-control ng-pristine ng-valid ng-touched"
                                           ng-change="totalloanoustandingChange()" required style="text-align:right;">
                                    <span style="font-style:italic;">Number in words:</span>
                                    <span id="words_totalloan" style="color:#003c96fc;" class="text-bold"></span>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4 col-sm-3 col-md-3 form-group float-label">
                                <label>Overdue if any(Amount & No of days overdue)</label>
                                <input ng-model="txtoverdue" class="form-control ng-pristine ng-valid ng-touched" required>

                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                <div class="form-group">
                                    <label>Repayment track on Existing borrowings other than Samunnati <span style="color:red">*</span></label>
                                    <div class="col">
                                        <p class="mda-radio">
                                            <input type="radio" name="repayment_track" ng-model="cborepayment_track" value="Regular">
                                            <em class="bg-teal-500"></em> Regular
                                        </p>
                                    </div>
                                    <div class="col">
                                        &nbsp; &nbsp;
                                    </div>
                                    <div class="col">
                                        <p class="mda-radio">
                                            <input type="radio" name="repayment_track" ng-model="cborepayment_track" value="Irregular">
                                            <em class="bg-teal-500"></em> Irregular
                                        </p>
                                    </div>
                                    <br />
                                    <input ng-model="txtrepayment_borrowings" class="form-control ng-pristine ng-valid ng-touched" required>
                                </div>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                <div class="form-group">
                                    <label>Basic records maintained <span style="color:red">*</span></label>
                                    <div class="col">
                                        <p class="mda-radio">
                                            <input type="radio" name="basic_records" ng-model="cbobasic_records" value="Yes">
                                            <em class="bg-teal-500"></em> Yes
                                        </p>
                                    </div>
                                    <div class="col">
                                        &nbsp; &nbsp;
                                    </div>
                                    <div class="col">
                                        <p class="mda-radio">
                                            <input type="radio" name="basic_records" ng-model="cbobasic_records" value="No">
                                            <em class="bg-teal-500"></em> No
                                        </p>
