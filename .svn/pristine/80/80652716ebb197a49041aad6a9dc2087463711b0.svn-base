
<h3>
    Samunnati Associate Onboarding
</h3>
<div ng-controller="MstSAOnboardingIndividualRenewalEditController as dp">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-plus"></i>
            </span>Add Samunnati Associate - Individual
        </div>
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12">
                <md-content class="md-padding">
                    <div class="dd-handle ng-scope">
                        <form novalidate="" name="submitindividual" ng-submit="form.submitindividual()"
                              autocomplete="off" class="form-validate">
                            <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <md-content class="md-padding">
                                        <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                                            <div class="panel-heading">
                                                <p class="text-primary" style="font-size:16px;"><b> Basic Details </b></p>
                                                <hr />
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                                        <label>RM Reporting To </label><br>
                                                        {{txtRM}}
                                                    </div>
                                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                                        <label>SA Type </label><br>
                                                        {{satype_name}}
                                                    </div>
                                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                                        <label>SA Entity Type <span style="color:red">*</span></label><br>
                                                        <select name="saentitytype_name" id="saentitytype_name" chosen=""
                                                                ng-options="m.saentitytype_gid as m.saentitytype_name for m in applicationadd_list"
                                                                ng-model="cbosaentitytype" ng-change="entityname_change(cbosaentitytype)" class="chosen-select form-control">
                                                            <option value=""></option>
                                                        </select>
                                                    </div>
                                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                                        <label> RM Name </label><br>
                                                        {{lblRM}}
                                                    </div>

                                                </div>
                                            </div>
                                            <br>
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">

                                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                                        <label> Origination </label>
                                                        <br />
                                                        {{lblorigination}}
                                                    </div>
                                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                                        <label>Referred By </label><br>
                                                        {{txtreferredby}}

                                                    </div>
                                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                                        <label>Registration Fee-UTR Number  </label><br>
                                                        {{txtutrno}}

                                                    </div>
                                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                                        <label>Present Occupation </label><br>
                                                        {{present_occupation}}
                                                    </div>
                                                </div>
                                            </div>
                                            <br>
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">

                                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                                        <label> Work Experience (years) </label>
                                                        <br />
                                                        {{work_experience}}
                                                    </div>
                                                    <div class="col-lg-3 col-md-3 col-sm-3">
                                                        <label>Exp in Agri business Industry </label><br>
                                                        {{Expagri_business}}

                                                    </div>

                                                </div>
                                            </div>

                                            <br>
                                        </div>
                                    </md-content>
                                </div>
                            </div>

                            <div class="panel">
                                <br>
                                <div class="panel-heading">
                                    <p class="text-primary" style="font-size:16px;">
                                        <b>
                                            &nbsp;&nbsp; Contact Person
                                            Details
                                        </b>
                                    </p>
                                    <hr />
                                </div>
                                <br>
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">

                                        <div class="col-sm-4 col-md-4 col-lg-4">
                                            <label>
                                                Samunnati Associate First Name <span style="color:red">*</span>
                                            </label><br>
                                            <input type="text" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/]*$/"
                                                   maxlength="100" ng-model="txtsacontact_person_first_name"
                                                   class="form-control" required />
                                        </div>
                                        <div class="col-sm-4 col-md-4 col-lg-4">
                                            <label>Samunnati Associate Middle Name </label><br>
                                            <input type="text" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/]*$/"
                                                   maxlength="100" ng-model="txtsacontact_person_middle_name"
                                                   class="form-control" />
                                        </div>
                                        <div class="col-sm-4 col-md-4 col-lg-4">
                                            <label>
                                                Samunnati Associate Last Name<span style="color:red">*</span>
                                            </label><br>
                                            <input type="text" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/]*$/"
                                                   maxlength="100" ng-model="txtsacontact_person_last_name"
                                                   class="form-control" required />
                                        </div>
                                    </div>
                                </div>
                                <br>
                                <div class="row">

                                    <div class="col-lg-12 col-md-12 col-sm-12">

                                        <div class="col-sm-4 col-md-4 col-lg-4">
                                            <label>Aadhaar Number <span style="color:red">*</span></label><br>
                                            <p class="input-group">
                                                <!--<input type="password" ng-model="txtaadhar_no" maxlength="15" tabindex="0" required class="form-control" pattern="^[0-9]{1}[0-9]{3}[0-9]{4}[0-9]{4}$" />-->
                                                <input type="{{inputType}}" ng-model="txtindividual_aadharnumber" maxlength="12" tabindex="0" required class="form-control" pattern="^[0-9]{1}[0-9]{3}[0-9]{4}[0-9]{4}$" />
                                                <span class="input-group-btn">
                                                    <button type="button" style="height:35px;" ng-show="eye" ng-click="show()" class="btn btn-default "><em class="fa fa-eye"></em></button>
                                                    <button type="button" style="height:35px;" ng-show="eyeslash" ng-click="noshow()" class="btn btn-default "><em class="fa fa-eye-slash"></em></button>
                                                </span>
                                            </p>
                                            <!--<input id='cardNumber' type="numeric"  maxlength="14"  minlength="14"
                           ng-model="txtindividual_aadharnumber" class="form-control" required />
                    -->
                                        </div>



                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <!--ng-show="havepan"-->
                                            <label class="col-form-label"> PAN  </label> <span style="color:red">*</span>
                                            <br />
                                            <input type="text" ng-model="txtpan_no"
                                                   ng-pattern="/^[A-Z]{5}[0-9]{4}[A-Z]{1}$/" maxlength="10" tabindex="0"
                                                   class="form-control" ng-change="PANValidation()" />
                                            <label class="fa fa-check" style="color:green"
                                                   ng-show="panvalidationind"></label>&nbsp;<label style="font-weight:bolder;color:green"
                                                                                                   ng-show="panvalidationind">Verified</label>
                                        </div>
                                        <div class="col-lg-4 col-sm-4 col-md-4">
                                            <label>Gender </label><br>
                                            <select name="Select Gender" id="Gender" ng-model="cbogender" class="form-control" style="height:32px;" size="1">
                                                <option value="">Select Gender</option>
                                                <option value="Male">Male</option>
                                                <option value="Female">Female</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <br>
                                <!--<br />-->

                                <br />
                                <div class="row" ng-show="havenotpan">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">

                                                <p class="text-primary" style="font-size:16px;">
                                                    <b>
                                                        &nbsp;&nbsp; Customer
                                                        do not have the PAN and he/she submitted Form 60 and it fulfils
                                                        the following,
                                                    </b>
                                                </p>
                                                <hr />
                                                <div ng-show="view_nopanreasons">
                                                    <table class="table">
                                                        <tbody>
                                                            <tr ng-repeat="data in panabsencereason_list">
                                                                <td width="10%">
                                                                    &nbsp;
                                                                    <input type="checkbox" id="c1"
                                                                           value={{data.checked}} ng-model="data.checked">
                                                                </td>
                                                                <td width="90%">{{data.panabsencereason}}</td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                    <br />
                                                    <!--<div class="text-center">
                                <button type="button" class="btn btn-success btn-md" ng-click="pandtl_submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                                <button ng-click="pandtl_close()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                            </div>-->
                                                </div>
                                                <div ng-hide="view_nopanreasons">
                                                    <table class="table">
                                                        <tbody>
                                                            <tr ng-repeat="data in panabsencereason_list">
                                                                <td width="10%">
                                                                    &nbsp;
                                                                    <input type="checkbox" id="c1"
                                                                           value={{data.checked}} ng-model="data.checked"
                                                                           disabled>
                                                                </td>
                                                                <td width="90%">{{data.panabsencereason}}</td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                    <br />
                                                </div>
                                                <br />

                                                <p class="text-primary" style="font-size:16px;">
                                                    <b>
                                                        &nbsp;&nbsp; Form-60
                                                        Document Upload
                                                    </b>
                                                </p>
                                                <hr />
                                                <div class="col-lg-6 col-sm-6 col-md-6">
                                                    <div class="float-label">
                                                        <strong>Upload Documents</strong>
                                                        <input type="file" id="panform60file" tabindex="5"
                                                               onchange="angular.element(this).scope().IndividualPANForm60DocumentUpload(this.files, angular.element(this).scope().documentupload, this.name)"
                                                               ng-model="txtindividualpanform60_document"
                                                               data-classbutton="btn btn-default"
                                                               data-classinput="form-control inline"
                                                               data-button-text="Select" />
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                                            <br />
                                                            <div class="table-responsive">
                                                                <table class="table table-striped table-secondary">
                                                                    <thead class="bg-gray text-white">
                                                                        <tr width="100%">
                                                                            <th scope="col">S.No</th>
                                                                            <th scope="col">Document Name</th>
                                                                            <th scope="col">Action</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        <tr ng-repeat="data in contactpanform60_list">
                                                                            <td title="S.No">{{$index+1}}</td>
                                                                            <td title="Document Name">
                                                                                {{data.document_name}}
                                                                            </td>
                                                                            <td data-title="Action">
                                                                                <button title="Download"
                                                                                        class="btn btn-green btn-sm"
                                                                                        ng-click="downloads(data.document_path,data.document_name)">
                                                                                    <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                                    <em class="fa fa-download"></em>
                                                                                </button>

                                                                                <a title="Delete"
                                                                                   class="btn btn-sm btn-danger"
                                                                                   ng-click="IndividualPANForm60DocumentDelete(data.sacontact2panform60_gid)">
                                                                                    <span class="icon icon-trash "></span>
                                                                                </a>
                                                                            </td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12 col-lg-12 col-sm-12">
                                        <div class="col-md-6 col-lg-6 col-sm-6">
                                            <div class="panel">
                                                <div class="panel-heading">
                                                    <p class="text-primary" style="font-size:16px;">
                                                        <b>
                                                            &nbsp;&nbsp;
                                                            Mobile Number(s)
                                                        </b>
                                                    </p>
                                                    <hr />
                                                </div>
                                                <div class="panel-body">
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-sm-5 col-md-5 col-lg-5">
                                                                <label><strong>Mobile Number </strong></label> <span style="color:red">*</span>
                                                                <input ng-model="txtmobile_no" type="text" tabindex="0"
                                                                       maxlength="10" ng-pattern="/^[0-9]*$/"
                                                                       aria-required="true" aria-invalid="true"
                                                                       class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched">

                                                            </div>
                                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                                <label>Primary Status </label> <span style="color:red">*</span>
                                                                <div class="radio c-radio">
                                                                    <label>
                                                                        <input type="radio" id="primarymobileno"
                                                                               name="primarymobile_no" value="Yes"
                                                                               ng-model="rdbprimarymobile_no"
                                                                               ng-change="ngchange()" />
                                                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                    </label>
                                                                    <label>
                                                                        <input type="radio" id="primarymobileno"
                                                                               name="primarymobile_no" value="No"
                                                                               ng-model="rdbprimarymobile_no"
                                                                               ng-change="ngchange()" />
                                                                        <span class="fa fa-circle"></span>No
                                                                    </label>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                                <label>Whatsapp Number </label> <span style="color:red">*</span>
                                                                <div class="radio c-radio">
                                                                    <label>
                                                                        <input type="radio" id="whatsappmobileno"
                                                                               name="whatsappmobile_no" value="Yes"
                                                                               ng-model="rdbwhatsappmobile_no"
                                                                               ng-change="ngchange()" />
                                                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                    </label>
                                                                    <label>
                                                                        <input type="radio" id="whatsappmobileno"
                                                                               name="whatsappmobile_no" value="No"
                                                                               ng-model="rdbwhatsappmobile_no"
                                                                               ng-change="ngchange()" />
                                                                        <span class="fa fa-circle"></span>No
                                                                    </label>
                                                                </div>
                                                            </div>

                                                            <div class="col-lg-1 col-md-1 col-sm-1 align-top">
                                                                <br />
                                                                <a class="btn btn-sm btn-default"
                                                                   style="background-color:#09385f;color:white;"
                                                                   ng-click="add_mobileno()">
                                                                    <span class="fa fa-plus" />
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                    <div class="row">
                                                        <div class="col-lg-12">
                                                            <div class="col-lg-12">
                                                                <div class="table-responsive table-bordered">
                                                                    <div style="max-height:200px;overflow:auto;">
                                                                        <table class="table">
                                                                            <thead class="bg-gray text-white">
                                                                                <tr>
                                                                                    <th>S.No</th>
                                                                                    <th>Mobile No</th>
                                                                                    <th>Primary Status</th>
                                                                                    <th>Whatsapp Number </th>
                                                                                    <th>Action</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                                <tr ng-repeat="list in individualmobileno_list">
                                                                                    <td>{{$index+1}}</td>
                                                                                    <td>{{list.samobile_no}}</td>
                                                                                    <td>{{list.saprimary_status}}</td>
                                                                                    <td>{{list.sawhatsapp_no}}</td>
                                                                                    <td class="text-nowrap">
                                                                                        <button title="Delete"
                                                                                                ng-click="delete_mobileno(list.sacontact2mobileno_gid)"
                                                                                                class="btn btn-danger btn-xs ">
                                                                                            <em class="fa fa-trash-o"></em>
                                                                                        </button>
                                                                                    </td>
                                                                                </tr>
                                                                            </tbody>
                                                                        </table>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6 col-lg-6 col-sm-6">
                                            <div class="panel">
                                                <div class="panel-heading">
                                                    <p class="text-primary" style="font-size:16px;">
                                                        <b>
                                                            &nbsp;&nbsp;
                                                            Email Address(s)
                                                        </b>
                                                    </p>
                                                    <hr />
                                                </div>
                                                <div class="panel-body">
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-sm-6 col-md-6 col-lg-6">
                                                                <label>
                                                                    <strong>Email Address </strong><span style="color:red">*</span>
                                                                </label>
                                                                <input type="text" ng-model="txtemail_address"
                                                                       pattern="[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,3}$"
                                                                       class="form-control">
                                                            </div>
                                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                                <label>
                                                                    Primary Status <span style="color:red">*</span>
                                                                </label>
                                                                <div class="radio c-radio">
                                                                    <label>
                                                                        <input type="radio" id="emailaddress"
                                                                               name="primaryemail_address" value="Yes"
                                                                               ng-model="rdbprimaryemail_address"
                                                                               ng-change="ngchange()" />
                                                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                    </label>
                                                                    <label>
                                                                        <input type="radio" id="emailaddress"
                                                                               name="primaryemail_address" value="No"
                                                                               ng-model="rdbprimaryemail_address"
                                                                               ng-change="ngchange()" />
                                                                        <span class="fa fa-circle"></span>No
                                                                    </label>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-2 col-md-2 col-lg-2"></div>
                                                            <div class="col-lg-1 col-md-1 col-sm-1 align-top">
                                                                <br />
                                                                <a class="btn btn-sm btn-default"
                                                                   style="background-color:#09385f;color:white;"
                                                                   ng-click="add_emailaddress()">
                                                                    <span class="fa fa-plus" />
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                    <div class="row">
                                                        <div class="col-lg-12">
                                                            <div class="col-lg-12">
                                                                <div class="table-responsive table-bordered">
                                                                    <table class="table">
                                                                        <thead class="bg-gray text-white">
                                                                            <tr>
                                                                                <th>S.No</th>
                                                                                <th>Email Address</th>
                                                                                <th>Primary Status</th>
                                                                                <th>Action</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody>
                                                                            <tr ng-repeat="list in saOnboardemailaddress_list">
                                                                                <td>{{$index+1}}</td>
                                                                                <td>{{list.saemail_address}}</td>
                                                                                <td>{{list.saprimary_status}}</td>
                                                                                <td class="text-nowrap">
                                                                                    <a class="btn btn-xs  btn-danger"
                                                                                       ng-click="delete_emailaddress(list.sacontact2email_gid)">
                                                                                        <span class="fa fa-trash-o"></span>
                                                                                    </a>

                                                                                </td>
                                                                            </tr>
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="dd-handle ng-scope">
                                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Address Details </b></p>
                                <hr />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-sm-4 col-md-4 col-lg-4">

                                            <strong class="ng-binding w3-text-indigo">
                                                <a class="btn btn-sm btn-default"
                                                   style="background-color:#09385f;color:white;"
                                                   ng-click="address_add()"><span class="fa fa-plus" /> Add Address</a>
                                            </strong>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="table-responsive table-bordered">
                                                <div style="max-height:200px;overflow:auto;">
                                                    <table class="table">
                                                        <thead class="bg-gray text-white">
                                                            <tr width="100%">
                                                                <th scope="col">S.No</th>
                                                                <th scope="col">Address Type</th>
                                                                <th scope="col">Address</th>
                                                                <th> Primary Status</th>
                                                                <th> Static Map / Photos</th>
                                                                <th scope="col">Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in contactindividualaddress_list">
                                                                <td title="S.No">{{$index+1}}</td>
                                                                <td title="AddressType">{{data.addresstype_name}}</td>
                                                                <td title="Address">
                                                                    {{data.addressline1}},{{data.addressline2}},<br />{{data.taluka}},{{data.district}},
                                                                    <br />{{data.state}},{{data.country}},{{data.postal_code}}
                                                                    <br /> <b>Latitude:</b> {{data.latitude}}
                                                                    <br /> <b>Longitude:</b> {{data.longitude}}
                                                                </td>
                                                                <td title="PrimaryStatus">{{data.primary_status}}</td>
                                                                <td title="Static Map and Photos">
                                                                    <button title="View"
                                                                            ng-click="StaticMapAndPhotos_View(data.latitude, data.longitude, data.addressline1, data.addressline2, data.postal_code)"
                                                                            class="btn btn-xs btn-info">
                                                                        <em class="fa fa-eye"></em>
                                                                    </button>
                                                                </td>
                                                                <td data-title="Action">
                                                                    <a class="btn btn-xs btn-danger"
                                                                       ng-click="delete_address(data.sacontact2address_gid)">
                                                                        <span class="fa fa-trash-o"></span>
                                                                    </a>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />


                            </div>
                            <br />
                            <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <md-content class="md-padding">
                                        <div class="dd-handle ng-scope">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <br />
                                                    <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Application Fee Details </b></p>
                                                    <hr />
