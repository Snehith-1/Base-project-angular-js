    <div class="list-group">
        <div class="card md-padding bg-green-100" style="width:80%;margin:auto;">
            <div class="card-subheader text-center w3-green ">
                Application Details
            </div>
            <div class="card-body bg-light-green-200">
                <form class="form-horizontal p-20">
                    <div class="form-group">
                        <div class="col-sm-3">Application Code  :</div>
                        <div class="col-sm-3">
                            <strong>{{data.application_code}}</strong>
                        </div>
                        <div class="col-sm-3">Application  :</div>
                        <div class="col-sm-3">
                            <strong>{{data.application_name}}</strong>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-3">Handled By  :</div>
                        <div class="col-sm-3">
                            <strong>{{data.department_name}}</strong>
                        </div>
                        <div class="col-sm-3">Current Status  :</div>
                        <div class="col-sm-3">
                            <strong>{{data.current_status}}</strong>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-3">Team   : </div>
                        <div class="col-sm-3">
                            <strong>{{data.team_name}}</strong>
                        </div>

                    </div>
                </form>
            </div>
            <div class="card-footer text-center">
                <div class="label label-success">Active</div>
            </div>

        </div>
    </div>
    <br />
    <div class="panel panel-default" ng-hide="true">
        <div class="panel-heading">Information Gallery</div>
        <div class="panel-body">

            <div class="row">
                <div class="col-md-6" ng-repeat="datas in data.applications">
                    <p class="lead bb">Application Details</p>
                    <form class="form-horizontal p-20 card md-padding bg-blue-grey-100">
                        <div class="form-group">
                            <div class="col-sm-4">Application ID:</div>
                            <div class="col-sm-8">
                                <strong>{{datas.app_code}}</strong>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-4">Application :</div>
                            <div class="col-sm-8">
                                <strong>{{datas.app_name}}</strong>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-4">Department :</div>
                            <div class="col-sm-8">
                                <strong>{{datas.dept}}</strong>
                            </div>
                        </div>
                        <div class="form-group ">
                            <div class="col-sm-4">
                                Team Name :
                            </div>
                            <div class="col-sm-8">
                                <strong>{{datas.team_name}}</strong>
                            </div>
                        </div>
                       
                        <div class="form-group">
                            <div class="col-sm-4">Status</div>
                            <div class="col-sm-8">
                                <strong >{{datas.current_status}}</strong>
                            </div>
                        </div>
                       
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>