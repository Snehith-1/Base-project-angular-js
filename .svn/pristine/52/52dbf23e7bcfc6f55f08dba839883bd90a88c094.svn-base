<h3>
    Cheque Management
</h3>
<div ng-controller="AgrMstChequeApprovalCompletedController">
    <div class="panel">
        <br>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <p style="color:#09385f;font-size: 17px;"><b>Approval Summary</b></p>
                    <hr />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="maker()" style="width:200px;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;"> <b> {{MakerPendingCount -- MakerFollowUpCount}}</b></div>
                        <b style="font-size: 14px;"> Maker </b>
                    </button>
                    <br />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="checker()" style="width:200px;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;"> <b> {{CheckerPendingCount -- CheckerFollowUpCount}}</b></div>
                        <b style="font-size: 14px;"> Checker </b>
                    </button>
                    <br />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="approval()" style="width:200px;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;"> <b> {{ApproverPendingCount}}</b></div>
                        <b style="font-size: 14px;">Approval</b>
                    </button>
                    <br />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="approvalcompleted()" style="width:200px;height:50px;border:1px solid rgb(179, 177, 177); background-color: #84cbdb;">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;"> <b> {{CompletedCount}}</b></div>
                        <b style="font-size: 14px;">Approval Completed</b>
                    </button>
                    <br />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">

                </div>
            </div>
        </div>
        <div class="row">
            <br>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="table-responsive">
                        <table datatable="ng" class="table table-hover table-striped">
                            <thead>
                                <tr class="text-white nowrap" style="background-color:#09385f;">
                                    <th> S.No </th>
                                    <th> Application Number</th>
                                    <th> Customer URN </th>
                                    <th> Customer Name </th>
                                    <!--<th> Customer URN </th>-->
                                    <th> Approved Date </th>
                                    <th> Sanction Reference Number </th>
                                    <!--<th> Approval Status </th>
                                    <th> AC Group </th>
                                    <th> CAD Group </th>
                                    <th> CAD Accepted Date </th>
                                    <th> CAD Accepted By </th>-->
                                    <th class="text-center">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in approvercompleted_list">
                                    <td data-title="'S.No'"> {{$index+1}} </td>
                                    <td data-title="'Application Number'"> {{data.application_no}} </td>
                                    <td data-title="'Customer URN'"> {{data.customer_urn}} </td>
                                    <td data-title="'Customer Name'"> {{data.customer_name}} </td>
                                    <!--<td data-title="'Customer URN'"> {{data.customer_urn}} </td>-->
                                    <td data-title="'AC Approved Date'"> {{data.approver_approveddate}} </td>
                                    <td data-title="'Sanction Reference Number'"> {{data.sanction_refno}} </td>
                                    <!--<td data-title="'Approval Status'"> {{data.approval_status}} </td>
                                    <td data-title="'AC Group'"> {{data.ccgroup_name}} </td>
                                    <td data-title="'CAD Group'"> {{data.cadgroup_name}} </td>
                                    <td data-title="'CAD Accepted Date'"> {{data.cadaccepted_date}} </td>
                                    <td data-title="'CAD Accepted By'"> {{data.cadaccepted_by}} </td>-->
                                    <td align="center" class="nowrap">
                                        <button title="View" ng-click="view(data.application_gid)" class="btn btn-info">
                                            <em class="fa fa-eye"></em>
                                        </button>
                                        <button title="Process" ng-click="approved_process(data.application_gid, data.created_by, data.lsmakersummary_flag)" style="background-color:#871490;" class="btn btn-pink">
                                            <em class="fa fa-spinner"></em>
                                        </button>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <br />
    </div>
</div>