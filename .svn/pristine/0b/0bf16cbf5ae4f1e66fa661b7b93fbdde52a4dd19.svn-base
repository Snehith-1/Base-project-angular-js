                    <div>
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div ui-scroll-viewport="" style="max-height:200px;overflow-y: auto;" class="row">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="table-responsive table-bordered">
                                                <table class="table">
                                                    <thead class="bg-gray text-white">
                                                        <tr>
                                                            <th>S.No</th>
                                                            <th>Upload Document Name</th>
                                                            <th align="center" scope="col">
                                                                Action&nbsp;&nbsp;&nbsp;
                                                                <a download="" href="" title="DownloadAll" id="btnpdf" ng-click="download_allenquiry(lmfilepath,lmfilename)">
                                                                    <em class="icon-arrow-down"></em>&nbsp;
                                                                </a>
                                                            </th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in enquirydocument_list">
                                                            <td>{{$index+1}}</td>
                                                            <td>{{data.document_name}}</td>
                                                            <td class="nowrap" data-title="'Action'">
                                                                <button title="Downloadmillet" class="btn btn-green btn-sm"
                                                                        ng-click="enquirydocument_downloads(data.document_path,data.document_name)">
                                                                    <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                    <em class="fa fa-download"></em>
                                                                </button>
                                                                <button title="View" class="btn btn-info btn-sm"
                                                                        ng-click="documentviewerenquiry(data.document_path,data.document_name)">
                                                                    <a download="" href="" style="display:none;"></a>
                                                                    <em class="fa fa-file"></em>
                                                                </button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                    <br>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="dd-handle ng-scope">
                    <div>
                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Upload </b></p>
                        <hr />
                    </div>
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div ui-scroll-viewport="" style="max-height:200px;overflow-y: auto;" class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="table-responsive table-bordered">
                                            <table class="table">
                                                <thead class="bg-gray text-white">
                                                    <tr>
                                                        <th>Document Title</th>
                                                        <th>Upload Document Name</th>
                                                        <th>Document Upload By</th>
                                                        <th>Document Upload Date</th>
                                                        <th align="center" scope="col">
                                                            Action&nbsp;&nbsp;&nbsp;
                                                            <a download="" href="" title="Download All" id="btnpdf2" ng-click="downloads_all(lsfilepath1,lsfilename1)">
                                                                <em class="icon-arrow-down"></em>&nbsp;
                                                            </a>
                                                        </th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in Uploadcallproofdocument_list">
                                                        <td>{{data.document_title}}</td>
                                                        <td>{{data.document_name}}</td>
                                                        <td>{{data.created_by}}</td>
                                                        <td>{{data.created_date}}</td>
                                                        <td class="nowrap" data-title="'Action'">

                                                            <!--<button title="Cancel" class="btn btn-danger btn-sm"
                                                        ng-click="recproof_cancel(data.ibcallproofdocupload_gid)">
                                                    <em class="icon icon-trash"></em>
                                                </button>-->
                                                            <button title="Download" class="btn btn-green btn-sm"
                                                                    ng-click="recproof_downloads(data.document_path,data.document_name)">
                                                                <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                <em class="fa fa-download"></em>
                                                            </button>
                                                            <button title="View" class="btn btn-info btn-sm"
                                                                    ng-click="documentviewer(data.document_path,data.document_name)">
                                                                <a download="" href="" style="display:none;"></a>
                                                                <em class="fa fa-file"></em>
                                                            </button>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                                <br>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="dd-handle ng-scope">
                    <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Post Closure Details </b></p>
                    <hr class="space" />
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-3 col-sm-3 col-md-3">
                                <label>Final Lead Closure Status </label><span style="color:red;">&nbsp; *</span>
                                <select name="closecall" id="closecall" required ng-model="cboclosurestatus" ng-change="changeclosurestatus(cboclosurestatus)" class="form-control" style="height:32px;" size="1">
                                    <option value="">---- Final Lead Closure Status ----</option>
                                    <option value="Rejected">Rejected</option>
                                    <option value="Extend Follow Up">Extend Follow-Up</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row" ng-show="followup_show">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-3 col-sm-3 col-md-3">
                                <label>Follow Up Date</label><span style="color:red;">&nbsp; *</span>
                                <p class="input-group">
                                    <input type="text" id="Followupdate" name="date" ng-model="txtfollowup_date" datepicker-popup="{{dp.format}}"
                                           style="height:36px;" is-open="dp.open1" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                           close-text="Close" class="form-control" placeholder="DD-MM-YYYY" />
                                    <span class="input-group-btn">
                                        <button type="button" ng-click="dp.calender1($event)" style="height:36px;" class="btn btn-default">
                                            <em class="fa fa-calendar"></em>
                                        </button>
                                    </span>
                                </p>
                            </div>
                            <div class="col-lg-3 col-sm-3 col-md-3">
                                <label>Follow Up Time</label><span style="color:red;">&nbsp; *</span>
                                <input type="time" ng-model="txtfollowup_time" tabindex="0" class="form-control" min="01:00" max="24:00"><i>hh:mm</i>
                            </div>
                            <div class="col-lg-6 col-sm-6 col-md-6">
                                <label>Follow Up Remarks</label><span style="color:red;">&nbsp; *</span>
                                <textarea id="follow_i" class="form-control" rows="3" tabindex="0" ng-model="txtremarks" maxlength="500" style="resize:vertical;" placeholder="Enter Remarks"></textarea>
                            </div>
                        </div>
                    </div>
                    <div class="row" ng-show="Rejected_show">
                        <div class="col-sm-12 col-lg-12 col-md-12">
                            <div class="col-sm-12 col-lg-12 col-md-12">
                                <label>Rejected Remarks </label><span style="color:red;">&nbsp; *</span>
                                <textarea id="close_i" class="form-control" rows="3" tabindex="0" ng-model="closedremarks" maxlength="500" style="resize:vertical;" placeholder="Enter Remarks"></textarea>
                            </div>
                        </div>
                    </div>
                </div>
                <br><br />
                <div class="row text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="closecall.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>
                    <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                </div>
                <br>
            </div>
</form>
</div>
</div>
