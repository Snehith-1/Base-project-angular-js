<a href="#/app/idasMstTemplateSummary">
    <button type="button" title="Back" class="pull-right btn btn-sm btn-primary">
        <em class="fa fa-arrow-circle-left fa-1x"></em>
    </button>
</a>
<h3>
    View - Template Details
</h3>

<div ng-controller="viewTemplateDetailsController">

    <div class="row">
        <div class="col-lg-12 col-sm-12 col-md-12">
            <div class="col-lg-5 col-sm-5 col-md-5">
                <label>Template Name</label><br />
                <p>{{template_name}}</p>
            </div>
            <div class="col-lg-1 col-sm-1 col-md-1"></div>
            <div class="col-lg-5 col-sm-5 col-md-5">
                <label>Template Type</label><br />
                <p>{{templatetype_name}}</p>
            </div>
            <br>
            <br>
        </div>
    </div>


    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">

            <div class="col-lg-12 col-md-12 col-sm-12">
                <label>Template Content</label></br>

                <p ng-bind-html="template_content"></p>

            </div>
        </div>
    </div>
    </br>

    <div class="row">
        <div class="text-center">
            <button ng-click="back()" class="btn btn-primary"><i class="fa fa-arrow-circle-left fa-1x"></i>&nbsp;&nbsp;Back</button>
        </div>
        <br>
    </div>

</div>


