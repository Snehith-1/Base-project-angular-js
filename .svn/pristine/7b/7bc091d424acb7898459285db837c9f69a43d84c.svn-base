<div id="sidebar-new" class="menu-container">
    <div class="menu">
        <h4>MENU<span ng-click="collapse()"><i class="fa fa-close"></i></span></h4>
        <ul>
            <li ng-repeat="levelone in menuItems">
                <a ng-href="{{$state.href(levelone.sref, levelone.params)}}"><span><i class="{{levelone.icon}}"></i></span>{{levelone.text}}<span><i class="fa fa-chevron-right fa-sm"></i></span></a>
                <ul>
                    <li ng-repeat="leveltwo in levelone.submenu">
                        <a ng-href="{{$state.href(leveltwo.sref, leveltwo.params)}}">{{leveltwo.text}}<span><i class="fa fa-chevron-right"></i></span></a>
                        <ul>
                            <li ng-repeat="levelthree in leveltwo.submenu">
                            <a ng-href="{{$state.href(levelthree.sref,levelthree.params)}}" ng-click="fullcollapse()">{{levelthree.text}}</a></li>
                        </ul>
                    </li>
                </ul>
            </li>
        </ul>
    </div>
</div>