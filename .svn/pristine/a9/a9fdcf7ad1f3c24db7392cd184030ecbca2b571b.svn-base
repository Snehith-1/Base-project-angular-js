<h3>
    Credit Group Mapping
</h3>
<div ng-controller="MstCreditMappingDetailsController">
 
 


        <div class="dd-handle ng-scope">
            <p class="text-primary" style="font-size:14px;"><b>&nbsp;  Credit Group Mapping Log Details </b></p>
            <hr />
            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <div class="table-responsive">
                        <table class="table table-striped table-secondary">
                            <thead class="bg-gray text-white">
                                <tr width="100%">
                                <tr class="text-white" style="background-color:#09385f;">
                                    <th align="center" width="5%" scope="col">S.No</th>
                                    <th align="center" width="35%"scope="col">Old Record</th>
                                    <th align="center" width="35%" scope="col">Updated Record</th>
                                    <th align="center"width="15%"scope="col">Updated by</th>
                                    <th align="center"width="15%"scope="col">Update date</th>

                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in creditmappingloglist ">
                                    <td align="left" scope="col" title="S.No">
                                        {{$index+1}}
                                    </td>
                                    <td align="left" scope="col" title="Reassigned information">
                                        <p ng-show="data.showcreditgroup_name == true "><b>Credit Group Name -</b> {{data.old_creditgroup_name}} </p>
                                        <p ng-show="data.showcredit2credithead_name"><b>Credit Head -</b> {{data.credithead_name}} </p>
                                        <p ng-show="data.showcredit2nationalmanager_name"><b>National Credit Manager -</b> {{data.nationalmanager_name}} </p>
                                        <p ng-show="data.showcreditr2regionalmanager_name"><b>Regional Credit Manger  -</b> {{data.regionalmanager_name}} </p>
                                        <p ng-show="data.showcredit2creditmanager_name"><b>Credit Manager -</b> {{data.creditmanager_name}} </p>

                                    </td>
                                    <td align="left" scope="col" title="Reassigned information">
                                        <p ng-show="data.showcreditgroup_name"><b>Credit Group Name -</b> {{data.creditgroup_name}} </p>
                                        <p ng-show="data.showcredit2credithead_name"><b>Credit Head -</b> {{data.new_credithead_name}} </p>
                                        <p ng-show="data.showcredit2nationalmanager_name"><b>National Credit Manager -</b> {{data.new_nationalmanager_name}} </p>
                                        <p ng-show="data.showcreditr2regionalmanager_name"><b>Regional Credit Manger  -</b> {{data.new_regionalmanager_name}} </p>
                                        <p ng-show="data.showcredit2creditmanager_name"><b>Credit Manager -</b> {{data.new_creditmanager_name}} </p>

                                    </td>
                                    <td align="left" scope="col" title="Reassigned By">
                                        {{data.created_by}}
                                    </td>
                                    <td align="left" scope="col" title="Reassigned Date/Time">
                                        {{data.created_date}}
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <br />
        </div>
        <br />
     
 
    <div class="row">
        <div class="text-center">
            <br />
            <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
        </div>
        <br /><br />
    </div>
      
</div>   