<h3>
    BRE Template
</h3>
<div ng-controller="MstRETemplateController">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <button type="button" title="Add Template" ng-hide="lsoneapipage=='OneApi'"class="btn btn-info btn-sm" ng-click="addtemplate()">
                <span class="btn-label">
                    <i class="fa fa-plus"></i>
                </span> Add Template
            </button>
        </div>
    </div>
    <br />
    <!--Summary-->
    <div class="panel">
        <br />
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="table-responsive">
                        <table datatable="ng" class="table table-hover table-striped">
                            <thead>
                                <tr class="text-white" style="background-color:#09385f;">
                                    <th> S.No </th>
                                    <th> One API Code</th>
                                    <th> Template Code  </th>
                                    <th>Template Name </th>
                                    <th ng-hide="lsoneapipage=='OneApi'"> Created By </th>
                                    <th ng-hide="lsoneapipage=='OneApi'"> Created Date </th>
                                    <th class="text-center" ng-hide="lsoneapipage=='OneApi'">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in ruletemplate_list ">
                                    <td data-title="'S.No'"> {{$index+1}} </td>
                                    <td data-title="'One API Code'">{{data.api_code}}</td>
                                    <td data-title="'Template Code'">{{data.template_code}}</td>
                                    <td data-title="'Template name'">{{data.template_name}}</td>
                                    <td data-title="'Created By'" ng-hide="lsoneapipage=='OneApi'">{{data.created_by}}</td>
                                    <td data-title="'Created Date'" ng-hide="lsoneapipage=='OneApi'">{{data.created_date}} </td>
                                    <td class="nowrap" data-title="'Action'" ng-hide="lsoneapipage=='OneApi'" style="text-align: center;">
                                        <button title="config" ng-click="configtemplate(data.ruletemplatemaster_gid)" class="btn btn-green">
                                            <em class="fa fa-key"></em>
                                        </button>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <br />
    </div>
    </div>

   