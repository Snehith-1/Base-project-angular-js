


<h3>
    Customer
</h3>
<div ng-controller="mstcustomersummarycontroller">
    <div>
        <button type="button" title="Add Customer" class="btn btn-info btn-sm" ng-click="addcustomer()">
            <span class="btn-label">
                <i class="fa fa-plus"></i>
            </span>Add Customer
        </button>
    </div>
    <br />
    <div class="panel">
        <div class="panel-body">

            <tabset justified="true">
                <tab ng-click="pending()" id="id1" select="tab.activeTabId = 'pending'" active="tabpending">
                    <tab-heading title="Common Customer">
                        <em class="fa fa-clock-o fa-1x"></em>
                    Common Customer
                        <label class="label label-pink" ng-if="lstmpcount != '0'">{{lstmpcount}}</label>
                    </tab-heading>
