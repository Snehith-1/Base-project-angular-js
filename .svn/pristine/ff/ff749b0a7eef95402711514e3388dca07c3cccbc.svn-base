                    <br>
                    <div class="table-responsive">
                        <table  datatable="ng" class="table hover table-striped ">
                            <thead>
                                <tr class="bg-primary text-white">
                                    <th style="display:none;">S.NO</th>
                                    <th class="nowrap">Created Date</th>
                                    <th> URN </th>
                                    <th>Customer Name </th>

                                    <th>Vertical</th>
                                    <th>RM Name</th>
                                    <th>Created By</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in customer_data">
                                    <td data-title="'S.No'" style="display:none;">{{$index+1}}</td>
                                    <td data-title="'Created Date'" class="nowrap">{{data.created_date}}</td>
                                    <td data-title="'Customer URN'">{{data.customer_urn}}</td>
                                    <td data-title="'Customer'">{{data.customername}}</td>
                                    <td data-title="'Vertical Code'">{{data.vertical_code}}</td>
                                    <td data-title="'Relationship Manager'">{{data.relationshipMgmtGid}}</td>
                                    <td data-title="'Created By'">{{data.created_by}}</td>
                                    <td class="text-nowrap">
                                        <button class="btn btn-green" ng-click="customerEdit(data.customer_gid)">
                                            <span class="fa fa-edit"></span>
                                        </button>
                                        <button class="btn btn-info" ng-click="customerView(data.customer_gid)">
                                            <span class="fa fa-eye"></span>
                                        </button>
                                    </td>

                                </tr>
                            </tbody>
                        </table>
                    </div>

                </tab>

            </tabset>
        </div>
    </div>
    <script type="text/ng-template" id="/tagcustomer.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-tag"></em>&nbsp;Tag Customer</h2>
                <span flex></span>

            </div>
        </md-toolbar>

        <div class="modal-body ng-scope">
            <div class="pd-sm-10">

                <div class="row">
                    <form name="mappingEditform" id="mappingEditform" novalidate="" class="form-validate mb-lg" autocomplete="off">

                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <h4>Do you want tag the customer to CAD?</h4>

                                    <label class="text-center text-bold">Customer Name : </label> <span> {{customer_name}}</span>
                                    <br />
                                    <label class="text-center text-bold">Customer URN : </label> <span>{{customer_urn}} </span>
                                </div>
                               
                            </div>
                            <br />
                            <div class="class">
                                <div class="text-center">

                                    <a class="btn btn-success" ng-click="confirm()">
                                        <em class="fa fa-check"></em>
                                        <span>Confirm</span>
                                    </a>
                                    <a class="btn btn-danger" ng-click="ok()">
                                        <em class="fa fa-close"></em>
                                        <span>Cancel</span>
                                    </a>
                                </div>
                            </div>
                        </div>

                    </form>
                </div>

            </div>

        </div>
    </script>
</div>
