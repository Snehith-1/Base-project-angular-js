                    <br>
                    <div class="table-responsive">
                        <table  datatable="ng" class="table hover table-striped">
                            <thead>
                                <tr class="bg-primary text-white">
                                    <th style="display:none;">S.NO</th>
                                    <th class="nowrap">Created Date</th>
                                    <th> URN </th>
                                    <th>Customer Name </th>

                                    <th>Vertical</th>
                                    <th>RM Name</th>
                                    <th>Created By</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in temp_customer">
                                    <td data-title="'S.No'" style="display:none;">{{$index+1}}</td>
                                    <td data-title="'Created Date'" class="nowrap">{{data.created_date}}</td>
                                    <td data-title="'Customer URN'">{{data.customer_urn}}</td>
                                    <td data-title="'Customer'">{{data.customername}}</td>
                                    <td data-title="'Vertical Code'">{{data.vertical_code}}</td>
                                    <td data-title="'Relationship Manager'">{{data.relationshipMgmtGid}}</td>
                                    <td data-title="'Created By'">{{data.created_by}}</td>
                                    <td class="text-nowrap">
                                        <button class="btn btn-green" title="Tag" ng-click="tagcustomer(data.customer_gid,data.customer_urn,data.customername)">
                                            <span class="fa fa-tag"></span>
                                        </button>
                                        <button class="btn btn-info" ng-click="customertmpView(data.customer_gid)">
                                            <span class="fa fa-eye"></span>
                                        </button>
                                    </td>

                                </tr>
                            </tbody>
                        </table>
                    </div>
                </tab>
                <tab id="id2" ng-click="completed()" select="tab.activeTabId ='completed'" active="tabcompleted">

                    <tab-heading title="CAD Customer">
                        <em class="fa fa-clock-o fa-1x"></em>  CAD Customer
                        <label class="label label-pink" ng-if="lsmstcount != '0'">{{lsmstcount}}</label>
                    </tab-heading>
