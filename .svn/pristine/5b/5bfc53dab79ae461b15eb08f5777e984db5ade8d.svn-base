<style>
    .nav.nav-pills .active > a {
        background-color: rgb(19,14,89);
        background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);
        color: #ffffff;
    }
</style>
<h3>
    Base Details of Company
</h3>
<div ng-controller="AgrBaseDetailsViewController as dp">
    <div class="panel">
        <div class="panel-heading" style="background-color:rgb(19,14,89);background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);color:#ffffff">
            <em class="fa fa-eye"></em>&nbsp; Base Details View
            <button type="button" title="Window Close" class="pull-right btn btn-sm btn-default" ng-click="close()">
                <em class="fa fa-close text-danger fa-1x">&nbsp;Close</em>
            </button>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12" style="overflow-x: hidden;">
                            <div class="row" style="margin-bottom:-10px;">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div id="panelDemo9" class="panel panel-primary">
                                        <div class="panel-heading" style="background-color: #d5591e;
                                            background-image: linear-gradient(316deg, rgb(238 181 49) 0%,  rgb(171 225 166) 74%);color:#000000;"><b> Company Details </b></div>
                                        <div class="panel-body">

                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">

                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> CIN </label>
                                                                <br />
                                                                <p>{{cin}}</p>
                                                            </div>
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> Legal Name </label>
                                                                <br />
                                                                <p>{{legal_name}}</p>
                                                            </div>
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> E-Filing Status </label>
                                                                <br />
                                                                <p>{{efiling_status}}</p>
                                                            </div>
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> Incorporation Date </label>
                                                                <br />
                                                                <p>{{incorporation_date}}</p>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> Paid-up Capital </label>
                                                                <br />
                                                                <p>{{paid_up_capital}}</p>
                                                            </div>
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> Sum of Charges </label>
                                                                <br />
                                                                <p>{{sum_of_charges}}</p>
                                                            </div>
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> Authorized Capital </label>
                                                                <br />
                                                                <p>{{authorized_capital}}</p>
                                                            </div>
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> Active Compliance </label>
                                                                <br />
                                                                <p>{{active_compliance}}</p>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> CIRP Status </label>
                                                                <br />
                                                                <p>{{cirp_status}}</p>
                                                            </div>
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> Registered Address </label>
                                                                <br />
                                                                <p>{{registered_address}}</p>
                                                            </div>
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> Type of Entity </label>
                                                                <br />
                                                                <p>{{classification}}</p>
                                                            </div>
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> Status </label>
                                                                <br />
                                                                <p>{{status}}</p>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> CIN Change (If any) </label>
                                                                <br />
                                                                <p>{{next_cin}}</p>
                                                            </div>
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> Last AGM Date </label>
                                                                <br />
                                                                <p>{{last_agm_date}}</p>
                                                            </div>
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> Last Filing with ROC </label>
                                                                <br />
                                                                <p>{{last_filing_date}}</p>
                                                            </div>
                                                            <div class="col-lg-3 col-md-3 col-sm-3">
                                                                <label> Email </label>
                                                                <br />
                                                                <p>{{email}}</p>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>

                                            <div class="row" ng-show="authorizedsignatories_list!=null">
                                                <div class="col-lg-12 col-sm-12 col-md-12">

                                                    <div class="dd-handle ng-scope">
                                                        <p class="text-primary" style="font-size:14px;"><b>&nbsp;Authorized Signatories </b></p>
                                                        <hr />
                                                        <div class="row">
                                                            <div class="col-lg-12 col-md-12 col-sm-12">

                                                                <div class="row">
                                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                                        <div class="table-responsive">
                                                                            <table ng-table="tableParams2" class="table table-striped table-bordered">
                                                                                <thead style="background-color:#dde6e9;">
                                                                                    <tr>
                                                                                        <th style="width:5%;text-align:center;">S.No</th>
                                                                                        <th> PAN </th>
                                                                                        <th> DIN </th>
                                                                                        <th> Name </th>
                                                                                        <th> Designation </th>

                                                                                        <th> DIN Status </th>
                                                                                        <th> Gender </th>
                                                                                        <th> Date of Birth </th>
                                                                                        <th> Age </th>

                                                                                        <th> Date of Appointment </th>
                                                                                        <th> Date of Appointment for Current Designation </th>
                                                                                        <th> Date of Cessation </th>
                                                                                        <th> Nationality </th>

                                                                                        <th> DSC Status </th>
                                                                                        <th> DSC Expiry Date </th>
                                                                                        <th> Father Name </th>
                                                                                        <th> Address </th>


                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody>
                                                                                    <tr ng-repeat="item in authorizedsignatories_list" width="100%">
                                                                                        <td title="S.No" style="text-align:center;">{{$index+1}}</td>
                                                                                        <td title="PAN">{{item.pan}} </td>
                                                                                        <td title="DIN">{{item.din}} </td>
                                                                                        <td title="Name">{{item.name}}</td>
                                                                                        <td title="Designation">{{item.designation}} </td>

                                                                                        <td title="DIN Status">{{item.din_status}} </td>
                                                                                        <td title="Gender">{{item.gender}} </td>
                                                                                        <td title="Date of Birth">{{item.date_of_birth}} </td>
                                                                                        <td title="Age">{{item.age}} </td>

                                                                                        <td title="Date of Appointment">{{item.date_of_appointment}} </td>
                                                                                        <td title="Date of Appointment for Current Designation">{{item.date_of_appointment_for_current_designation}} </td>
                                                                                        <td title="Date of Cessation">{{item.date_of_cessation}} </td>
                                                                                        <td title="Nationality">{{item.nationality}} </td>

                                                                                        <td title="DSC Status">{{item.dsc_status}} </td>
                                                                                        <td title="DSC Expiry Date">{{item.dsc_expiry_date}} </td>
                                                                                        <td title="Father Name">{{item.father_name}} </td>
                                                                                        <td title="Address">
                                                                                            {{item.address.address_line1}}, {{item.address.address_line2}}, <br>
                                                                                            {{item.address.city}}, {{item.address.state}}, {{item.address.pincode}}, {{item.address.country}},                                                                               
                                                                                        </td>
                                                                                    </tr>
                                                                                </tbody>
                                                                            </table>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                            </div>
                                                        </div>
                                                    </div>


                                                </div>
                                            </div>

                                            <div class="row" ng-show="opencharges_list!=null">
                                                <div class="col-lg-12 col-sm-12 col-md-12">

                                                    <div class="dd-handle ng-scope">
                                                        <p class="text-primary" style="font-size:14px;"><b>&nbsp;Open Charges </b></p>
                                                        <hr />
                                                        <div class="row">
                                                            <div class="col-lg-12 col-md-12 col-sm-12">

                                                                <div class="row">
                                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                                        <div class="table-responsive">
                                                                            <table ng-table="tableParams2" class="table table-striped table-bordered">
                                                                                <thead style="background-color:#dde6e9;">
                                                                                    <tr>
                                                                                        <th style="width:5%;text-align:center;">S.No</th>
                                                                                        <th> ID </th>
                                                                                        <th> Date </th>
                                                                                        <th> Holder Name </th>
                                                                                        <th> Amount </th>
                                                                                        <th> Type </th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody>
                                                                                    <tr ng-repeat="item in opencharges_list" width="100%">
                                                                                        <td title="S.No" style="text-align:center;">{{$index+1}}</td>
                                                                                        <td title="ID">{{item.id}} </td>
                                                                                        <td title="Date">{{item.date}} </td>
                                                                                        <td title="Holder Name">{{item.holder_name}}</td>
                                                                                        <td title="Amount">{{item.amount}} </td>
                                                                                        <td title="Type">{{item.type}} </td>
                                                                                    </tr>
                                                                                </tbody>
                                                                            </table>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                            </div>
                                                        </div>
                                                    </div>


                                                </div>
                                            </div>


                                        </div>
                                    </div>
                                </div>
                            </div>                           

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>
