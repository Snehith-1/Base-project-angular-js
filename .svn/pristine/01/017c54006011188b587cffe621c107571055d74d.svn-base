<h3>
    Buyer / Supplier Approval
</h3>
<div ng-controller="AgrMstCustomerApprovalSummaryController">
    <div class="card"> 
        <div class="row">
            <br/>
            <div class="col-lg-5 col-sm-5 col-md-5">
            </div> 
            <div class="col-lg-3 col-sm-3 col-md-3">
                <div class="list-group-item">
                    <div class="media-box">
                        <div class="pull-left">
                            <span class="fa-stack">
                                <em class="fa fa-circle fa-stack-2x text-danger"></em>
                                <em class="fa fa-thumbs-down fa-stack-1x fa-inverse text-white"></em>
                            </span>
                        </div>
                        <div class="media-box-body clearfix">
                            <div class="media-box-heading">
                                <a ng-click="rejectedsummary()" style="font-size:14px;" title="Go To Rejected Buyer/Supplier Applications" class="text-primary m0">Rejected Buyer/Supplier Applications</a>
                            </div>

                        </div>
                    </div>
                </div>
            </div>  
            <div class="col-lg-3 col-sm-3 col-md-3">
                <div class="list-group-item">
                    <div class="media-box">
                        <div class="pull-left">
                            <span class="fa-stack">
                                <em class="fa fa-circle fa-stack-2x text-success"></em>
                                <em class="fa fa-thumbs-up fa-stack-1x fa-inverse text-white"></em>
                            </span>
                        </div>
                        <div class="media-box-body clearfix">
                            <div class="media-box-heading">
                                <a href="#/app/AgrMstOnboardingApprovalCompleted" style="font-size:14px;" title="Go To Approved Buyer/Supplier Applications" class="text-primary m0">Approved Buyer/Supplier Applications</a>
                            </div>

                        </div>
                    </div>
                </div>
            </div> 
            <div class="col-lg-1 col-sm-1 col-md-1">
            </div> 
        </div>
        <md-content class="md-padding">
            <md-tabs md-dynamic-height="" md-border-bottom="" md-selected="selectedIndex"> 
                <md-tab label="Buyer Onboarding - Pending" md-on-select="GetBuyeronboardingPendinglist()">
                    <md-tab-label>
                        Buyer Onboarding - Pending
                        &emsp;<i class='pull-right label label-primary'>{{approvalcount.BuyerApprovalPending}}</i>
                    </md-tab-label>
                    <md-tab-body>
                        <md-content class="md-padding">
                            <div class="">
                                <h4 class="text-pink">
                                    Buyer Onboarding - Pending
                                </h4>

                                <div class="panel-body">

                                    <div class="table-responsive">
                                        <table datatable="ng" class="table table-hover table-striped">
                                            <thead class="nowrap">
                                                <tr class="text-white" style="background-color:#09385f;">
                                                    <th> S.No </th>
                                                    <!--<th> Buyer ID </th>-->
                                                    <th> Customer/Buyer Name </th>
                                                    <!--<th> Vertical </th>-->
                                                    <th style="width:10%;"> Applicant Type </th>
                                                    <th> Submitted Date </th>
                                                    <th> Created By</th>
                                                    <th class="text-center" style="width:14%">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in buyerapplicationlist">
                                                    <td data-title="'S.No'" class="text-center"> {{$index+1}}</td>
                                                    <!--<td data-title="'Buyer ID'">  {{data.application_no}} </td>-->
                                                    <td data-title="'Applicant Name">  {{data.customer_name}} </td>
                                                    <!--<td data-title="'Vertical">  {{data.vertical_name}} </td>-->
                                                    <td data-title="'Applicant Type" style="width:10%;">  {{data.applicant_type}} </td>
                                                    <td data-title="'Submitted Date'">
                                                        {{data.submittedto_approval}}
                                                    </td>
                                                    <td>{{data.created_by}}</td>
                                                    <td class="nowrap" data-title="'Action'" style="text-align: center;width:14%;" ng-switch="data.application_status">
                                                        <button title="Approval" ng-click="applcreation_view(data.application_gid)" class="btn btn-success">
                                                            <em class="fa fa-check-square-o"></em>
                                                        </button>
                                                        <button title="Edit" ng-click="applcreation_edit(data.application_gid)" class="btn btn-green">
                                                            <em class="icon-pencil"></em>
                                                        </button>
                                                        <!--<button title="Limit Management" ng-click="onboardappdetailinfo(data.application_gid)" class="btn bg-pink-400">
                                                            <em class="fa fa-retweet text-white"></em>
                                                        </button>-->
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                </div>
                            </div>
                        </md-content>
                    </md-tab-body>
                    
                </md-tab> 

                <md-tab label="Supplier Onboarding - Pending" md-on-select="GetsupplieronboardingPendinglist()">
                    <md-tab-label>
                        Supplier Onboarding - Pending
                        &emsp;<i class='pull-right label label-primary'>{{approvalcount.SupplierApprovalPending}}</i>
                    </md-tab-label>
                    <md-tab-body>
                        <md-content class="md-padding">
                            <div class="">
                                <h4 class="text-pink">
                                    Supplier Onboarding - Pending
                                </h4>
                                <div class="panel-body">
                                    <div class="table-responsive">
                                        <table datatable="ng" class="table table-hover table-striped">
                                            <thead class="nowrap">
                                                <tr class="text-white" style="background-color:#09385f;">
                                                    <th> S.No </th>
                                                    <!--<th> Supplier ID </th>-->
                                                    <th> Customer Name </th>
                                                    <!--<th> Vertical </th>-->
                                                    <th style="width:10%;"> Applicant Type </th>
                                                    <th> Submitted Date </th>
                                                    <th> Created By</th>
                                                    <th class="text-center" style="width:14%">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in supplierapplicationlist">
                                                    <td data-title="'S.No'" class="text-center">  {{$index+1}}</td>
                                                    <!--<td data-title="'Buyer ID'">  {{data.application_no}} </td>-->
                                                    <td data-title="'Applicant Name">  {{data.customer_name}} </td>
                                                    <!--<td data-title="'Vertical">  {{data.vertical_name}} </td>-->
                                                    <td data-title="'Applicant Type" style="width:10%;">  {{data.applicant_type}} </td>
                                                    <td data-title="'Submitted Date'">
                                                        {{data.submittedto_approval}}
                                                    </td>
                                                    <td>{{data.created_by}}</td>
                                                    <td class="nowrap" data-title="'Action'" style="text-align: center;width:14%;" ng-switch="data.application_status">
                                                        <button title="Approval" ng-click="suprapplcreation_view(data.application_gid)" class="btn btn-success">
                                                            <em class="fa fa-check-square-o"></em>
                                                        </button>
                                                        <button title="Edit" ng-click="suprapplcreation_edit(data.application_gid)" class="btn btn-green">
                                                            <em class="icon-pencil"></em>
                                                        </button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                </div>

                            </div>
                        </md-content>
                    </md-tab-body>
                    
                </md-tab>
                 
            </md-tabs>
        </md-content>
    </div>
</div>
 