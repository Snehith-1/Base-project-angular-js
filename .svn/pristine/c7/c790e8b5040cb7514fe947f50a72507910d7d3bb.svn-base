
<div ng-controller="welcomecontroller">
    <div class="md-welcome half-float">
        <div class="md-welcome-msg">
            <div class="container container-md">
                <h2 class="animated fadeInDown anim-delay-10 text-white">Samunnati Financial Intermediation and Services</h2>
                <h3 class="animated fadeInLeft anim-delay-15 text-white">Lawyer Portal</h3>
            </div>
            <div class="half-float-bottom">
                <div ng-switch="lawyer_photo">
                    <img  src="app/img/user/lawyer.jfif"  alt="Image" ng-switch-when="N"
                        class="img-thumbnail img-circle thumb128">
                    <img ng-src="{{lawyer_photo}}" alt="Image" ng-switch-default
                         class="img-thumbnail img-circle thumb128">
                </div>
            </div>

        </div>
    </div>
    <br /><br />
    <div class="widget">
        <div class="text-center">
            <h3 class="m0">{{lawyer_name}} / {{lawyeruser_code}}</h3>
            <p class="text-muted">{{educational_qualification}}</p>
            <p> </p>
        </div>
        <br />
        <div class="row">
            <div class="col-lg-3 col-sm-6" ng-click="dashboard()" title="Legal Services">
                <!-- START widget-->
                <div class="panel widget bg-indigo-300">
                    <div class="row row-table">
                        <div class="col-xs-4 text-center bg-indigo-500 pv-lg">
                            <em class="fa fa-file fa-3x"></em>
                        </div>
                        <div class="col-xs-8 pv-lg">
                            <div class="h2 mt0">{{count_compliance}}</div>
                            <div>Legal Services</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-sm-6" ng-click="legalSR()" title="Legal SR">
                <!-- START widget-->
                <div class="panel widget bg-red-A200">
                    <div class="row row-table">
                        <div class="col-xs-4 text-center bg-red-600 pv-lg">
                            <em class="fa fa-ticket fa-3x"></em>
                        </div>
                        <div class="col-xs-8 pv-lg">
                            <div class="h2 mt0">
                                {{count_legalSR}}
                            </div>
                            <div class="text-uppercase">Legal SR</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-12" ng-click="invoice()" title="Invoice">
                <!-- START widget-->
                <div class="panel widget bg-green">
                    <div class="row row-table">
                        <div class="col-xs-4 text-center bg-green-dark pv-lg">
                            <em class="icon-notebook fa-3x"></em>
                        </div>
                        <div class="col-xs-8 pv-lg">
                            <div class="h2 mt0">{{count_invoice}}</div>
                            <div class="text-uppercase">Invoice</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-12">
                <!-- START date widget-->
                <div class="panel widget">
                    <div class="row row-table">
                        <div class="col-xs-4 text-center bg-purple-500 pv-lg">
                            <!-- See formats: https://docs.angularjs.org/api/ng/filter/date-->
                            <now format="MMMM" class="text-sm"></now>
                            <br />
                            <now format="d" class="h2 mt0"></now>
                        </div>
                        <div class="col-xs-8 pv-lg bg-purple-100">
                            <now format="EEEE" class="text-uppercase"></now>
                            <br />
                            <now format="h:mm" class="h2 mt0"></now>
                            <now format="a" class="text-sm"></now>
                        </div>
                    </div>
                </div>
                <!-- END date widget    -->
            </div>
        </div>
