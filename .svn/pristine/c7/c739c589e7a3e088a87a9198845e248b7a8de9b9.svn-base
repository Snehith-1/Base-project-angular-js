<h3>Employee Summary</h3>
<style>
  .picColor:hover {
    background-color: transparent;
    box-shadow: none;
    color: #144a6e;
    border: 1px solid #a59f9d;
    transform: translateY(-3px);
    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2);
    text-decoration: underline;
    text-decoration-color: #141478;
  }

  .tab {
    overflow: hidden;
    border: 1px solid #ccc;
    background-color: #f8f7f7;
    height: 48px;
  }

  /* Style the buttons inside the tab */
  .tab button {
    background-color: inherit;
    float: left;
    border: none;
    outline: none;
    cursor: pointer;
    padding: 14px 16px;
    transition: 0.3s;
    font-size: 14px;
    font-weight: bold;
  }

  /* Change background color of buttons on hover */
  .tab button:hover {
    background-color: rgb(226, 221, 221);
    text-decoration: underline;
    height: 56px;
  }

  /* Style the tab content */
  .tabcontent {
    display: none;
    padding: 6px 12px;
    border: 1px solid #fff;
    border-top: none;
  }

  .tab button.active {
    background-color: #d5591e;
    background-image: linear-gradient(
      316deg,
      rgb(236 195 100) 0%,
      rgb(171 225 166) 74%
    );
  }
</style>
<div ng-controller="SysMstEmployeeRelievingController">
  <div>
    <button
      type="button"
      title="Add Employee"
      class="btn btn-info bt-sm"
      ng-click="employee_add()"
    >
      <span class="btn-label">
        <i class="fa fa-plus"></i>
      </span>
      Add Employee
    </button>
    <button
      type="button"
      class="btn btn-success pull-right"
      ng-click="exportemployee()"
      title="Export Excel"
    >
      <i class="fa fa-file-excel-o"></i>
      <a download="" href="" id="btnpdf" style="display:none;"></a></button
    >&nbsp;
  </div>
  <div class="" style="text-align: right"></div>
  <br />
  <div id="panelDemo13" class="panel panel-default">
    <div class="panel-body">
      <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
          <div class="col-lg-2 col-md-6 col-sm-12">
            <p style="color: #09385f; font-size: 17px">
              <b>Relieving Users</b>
            </p>
            <hr />
          </div>
          <div class="col-lg-2 col-md-6 col-sm-12">
            <button
              type="button"
              class="btn btn-labeled a1"
              ng-click="pendingSummary()"
              style="
                width: 200px;
                height: 50px;
                border: 1px solid rgb(179, 177, 177);
              "
            >
              <b style="font-size: 16px">Pending-Onboarding</b>
            </button>
            <br />
          </div>
          <div class="col-lg-2 col-md-6 col-sm-12">
            <button
              type="button"
              class="btn btn-labeled"
              ng-click="ActiveSummary()"
              style="
                width: 200px;
                height: 50px;
                border: 1px solid rgb(179, 177, 177);
              "
            >
              <b style="font-size: 16px">Active Users</b>
            </button>
            <br />
          </div>
          <div class="col-lg-2 col-md-3 col-sm-3">
            <button
              type="button"
              class="btn btn-labeled"
              ng-click="RelieveingSummary()"
              style="
                width: 200px;
                height: 50px;
                border: 1px solid rgb(179, 177, 177);
                background-color: #84cbdb;
              "
            >
              <b style="font-size: 16px">Relieving Users</b>
            </button>
            <br />
          </div>
          <div class="col-lg-2 col-md-6 col-sm-12">
            <button
              type="button"
              class="btn btn-labeled"
              ng-click="InactiveSummary()"
              style="
                width: 200px;
                height: 50px;
                border: 1px solid rgb(179, 177, 177);
              "
            >
              <b style="font-size: 16px">Inactive Users</b>
            </button>
            <br />
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
          <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="table-responsive">
              <table datatable="ng" class="table table-hover table-striped">
                <thead>
                  <tr
                    class="text-white nowrap"
                    style="background-color: #09385f"
                  >
                    <th>S.No</th>
                    <th>Entity</th>
                    <th>Employee Code</th>
                    <th>Employee Name</th>
                    <th>Base Location</th>
                    <th>Department / Function</th>
                    <th>Reporting To</th>
                    <th>Relieving Date</th>
                    <th>Status</th>
                    <th style="text-align: center">Action</th>
                  </tr>
                </thead>
                <tbody>
                  <tr ng-repeat="data in employeerelieving_list" class="nowrap">
                    <td data-title="'S.No'" class="text-center">
                      {{$index+1}}
                    </td>
                    <td data-title="'Entity'">{{data.entity_name}}</td>
                    <td data-title="'Employee Code'">{{data.user_code}}</td>
                    <td data-title="'Employee Name'">{{data.employee_name}}</td>
                    <td data-title="'Base Location'">
                      {{data.baselocation_name}}
                    </td>
                    <td data-title="'Department / Function'">
                      {{data.department_name}}
                    </td>
                    <td data-title="'Reporting To'">
                      {{data.employeereporting_to}}
                    </td>
                    <td data-title="'Relieved Date'">{{data.relive_date}}</td>
                    <td data-title="'Status'">{{data.user_access}}</td>
                    <td>
                      <button
                        type="button"
                        class="btn btn-info"
                        ng-click="employee_view(data.employee_gid)"
                        title="View"
                      >
                        <em class="fa fa-eye"></em>
                      </button>
                      <!--<button
                        type="button"
                        class="btn btn-green"
                        ng-click="relive_edit(data.employee_gid)"
                        title="Edit"
                      >
                        <em class="icon-pencil"></em>
                      </button>-->
                      <!--<button
                        type="button"
                        class="btn btn-warning"
                        ng-click="employee_deactive(data.employee_gid)"
                        title="Deactivate"
                      >
                        <em class="fa fa-thumbs-down"></em>
                      </button>-->
                    </td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!--reset password popup-->
  <script type="text/ng-template" id="/empresetpassowrd.html">
    <div class="modal-header bg-primary ng-scope">

        <h4 id="myModalLabel" class="modal-title">
            &emsp;
            <span class="btn-label bg-primary">
                <i class="fa fa-lock "></i>
            </span>RESET PASSWORD
        </h4>
    </div>
    <div class="modal-body ng-scope">
        <form novalidate="" name="userform" ng-submit="form.submitForm1()" autocomplete="off" class="form-validate form-horizontal">
            <div class="pd-sm-10">

                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-sm-1"></div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <strong class="mt0">Employee Code</strong>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <input class="form-control" type="text" ng-model="employee_code" maxlength="20" name="productgroupcode" tabindex="0" readonly />
                        </div>

                    </div>
                </div>
                <br />

                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-sm-1"></div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <strong class="mt0">Employee Name</strong>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <input class="form-control" type="text" ng-model="employee_name" maxlength="30" name="productgroupname" tabindex="1" readonly />
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-sm-1"></div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <strong class="mt0">Password</strong><span class="text-danger">*</span>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <input type="password" name="Password" required="" maxlength="20" placeholder="Enter Password"
                                   ng-pattern="/(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[\W])/" ng-model="Password" class="form-control" />
                            <span ng-show="userform.Password.$dirty &amp;&amp; userform.Password.$error.required" class="text-danger">This field is required</span>
                            <span ng-show="userform.Password.$dirty &amp;&amp; userform.Password.$error.pattern" class="text-danger">Password should contain: "Atleast one upper case letter, one lower case letter, one special character, one number and minimum eight characters."</span>
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-sm-1"></div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <strong class="mt0">Confirm Password</strong><span class="text-danger">*</span>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <input type="password" name="retypepassword" required="" maxlength="20" placeholder="Confirm Password"
                                   ui-validate="'$value==Password'" ui-validate-watch="'Password'" ng-model="retypepassword" class="form-control" />
                            <span ng-show="userform.retypepassword.$dirty &amp;&amp; userform.retypepassword.$error.validator" class="text-danger">Password does Not match</span>
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="userform.$invalid" ng-click="passwordSubmit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
            </div>
        </form>
    </div>
  </script>

  <!--reset password popup-->
  <script type="text/ng-template" id="/empcodeupdate.html">
    <div class="modal-header bg-primary ng-scope">

        <h4 id="myModalLabel" class="modal-title">
            &emsp;
            <span class="btn-label bg-primary">
                <i class="fa fa-user "></i>
            </span>UPDATE EMPLOYEE CODE
        </h4>
    </div>
    <div class="modal-body ng-scope">
        <form novalidate="" name="userform" ng-submit="form.submitForm1()" autocomplete="off" class="form-validate form-horizontal">
            <div class="pd-sm-10">



                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-sm-1"></div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <strong class="mt0">Employee Name</strong>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <input class="form-control" type="text" ng-model="employee_name" maxlength="30" name="productgroupname" tabindex="1" readonly />
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-sm-1"></div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <strong class="mt0">Current Employee Code</strong>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <input class="form-control" type="text" ng-model="employee_code" maxlength="20" name="productgroupcode" tabindex="0" readonly />
                        </div>

                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-sm-1"></div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <strong class="mt0">New Employee Code</strong><span class="text-danger">*</span>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <input class="form-control" type="text" ng-model="newemp_code" maxlength="20" name="newemp_code" placeholder="New Employee Code" tabindex="0" required="" />
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-sm-1"></div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <strong class="mt0">Confirm Employee Code</strong><span class="text-danger">*</span>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <input type="text" name="confirmemp_code" required="" maxlength="225" placeholder="Confirm Employee Code"
                                   ui-validate="'$value==newemp_code'" ui-validate-watch="'newemp_code'" ng-model="confirmemp_code" class="form-control" />
                            <span ng-show="userform.confirmemp_code.$dirty &amp;&amp; userform.confirmemp_code.$error.validator" class="text-danger">Employee Code does Not match</span>
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="userform.$invalid" ng-click="passwordSubmit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
            </div>
        </form>
    </div>
  </script>

  <script type="text/ng-template" id="/relivingdetails.html">
    <div class="modal-header bg-primary ng-scope">

        <h4 id="myModalLabel" class="modal-title">
            &emsp;
            <span class="btn-label bg-primary">
                <i class="fa fa-edit "></i>
            </span>RELIEVING DETAILS
        </h4>
    </div>
    <div class="modal-body ng-scope">
        <form novalidate="" name="userform" ng-submit="form.submitForm1()" autocomplete="off" class="form-validate form-horizontal">
            <div class="pd-sm-10">
                <br>
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <strong class="mt0">Relieving Date</strong><span class="text-danger">*</span>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <p class="input-group">
                                <input type="text" id="relive_date" name="relive_date" ng-model="txtrelive_date" datepicker-popup="{{format}}"
                                       style="height:36px;" is-open="open03" min-date="minDate" datepicker-options="dateOptions"
                                       close-text="Close" class="form-control" placeholder="DD-MM-YYYY" />
                                <span class="input-group-btn">
                                    <button type="button" ng-click="calender03($event)" style="height:36px;" class="btn btn-default">
                                        <em class="fa fa-calendar"></em>
                                    </button>
                                </span>
                            </p>
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <label>Reason <span style="color:red;"> *</span> </label>
                        <div class="mda-form-group">
                            <textarea ng-model="txtrelivereason" style="resize:vertical;" required rows="3"
                            aria-multiline="true" tabindex="0" aria-invalid="false" class="mda-form-control" placeholder="Enter Reason">
                            </textarea>
                        </div>
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="userform.$invalid" ng-click="reliveSubmit()"><i class="fa fa-arrow-circle-up"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
                <br>
            </div>
        </form>
    </div>
  </script>

  <!--Employee History popup-->
  <script type="text/ng-template" id="/employee_history.html">
    <div class="modal-header bg-primary ng-scope">

        <h4 id="myModalLabel" class="modal-title">
            &emsp;
            <span class="btn-label bg-primary">
                <i class="fa fa-history "></i>
            </span>Employee History
        </h4>
    </div>
  </script>
</div>
