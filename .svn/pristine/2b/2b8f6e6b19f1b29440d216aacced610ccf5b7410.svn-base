<h3>
    New Service Ticket
</h3>
<div ng-controller="newServiceTicketcontroller">
    <div id="panelDemo8" class="panel panel-primary">
        <div class="panel-heading">
            Raise Service Ticket
        </div>
        <div>
            <div class="panel">
                <form novalidate="" name="userformsubmit" ng-submit="form.submitForm1()" autocomplete="off" class="form-validate form-horizontal">

                    <div class="panel-body">
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-6 col-md-6 col-sm-6">
                                    <div class="mda-form-group ">
                                        <select name="category" dropzone="" ng-model="category" ng-change="onselectedchange(category)" required=""
                                                class="mda-form-control ng-valid ng-touched  ng-valid-parse" size="1">
                                            <option data-ng-repeat="data in category_list" value="{{data.category_gid}}">{{data.category_name}}</option>
                                        </select>
                                        <label>Select Category *</label>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-6 col-sm-6">
                                    <div class="mda-form-group">
                                        <select name="subcategory" ng-model="subcategory" ng-change="onselectedchangesubcategory(subcategory)"
                                                required="" class="mda-form-control ng-valid ng-touched ng-valid-parse" size="1">
                                            <option data-ng-repeat="data in subcategory_list" value="{{data.subcategory_gid}}">{{data.subcategory_name}}</option>
                                        </select>
                                        <label>Select Sub Category *</label>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-6 col-md-6 col-sm-6">
                                    <div class="mda-form-group">
                                        <select name="type" ng-model="type" size="1" required=""
                                                class="mda-form-control ng-valid ng-touched  ng-valid-parse">
                                            <option data-ng-repeat="data in type_list" value="{{data.type_gid}}">{{data.type_name}}</option>
                                        </select>
                                        <label>Select Type *</label>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-6 col-sm-6">
                                    <div class="mda-form-group">
                                        <label>Ticket Raised For *</label>
                                        <div class="col">
                                            <label class="mda-radio">
                                                <input type="radio" name="radio_selfothers" ng-model="radio_selfothers" ng-change="onchangeself(radio_selfothers)"
                                                       ng-click="isShowHide('hide')" value="Self">
                                                <em class="bg-teal-500"></em> Self
                                            </label>
                                        </div>
                                        <div class="col">
                                            &nbsp; &nbsp;
                                        </div>
                                        <div class="col">
                                            <label class="mda-radio">
                                                <input type="radio" name="radio_selfothers" ng-model="radio_selfothers" ng-click="isShowHide('show')" ng-change="onchangeself(radio_selfothers)" value="Others">
                                                <em class="bg-teal-500"></em> Others
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-6 col-md-6 col-sm-6">

                                </div>

                                <div class="col-lg-6 col-md-6 col-sm-6">
                                    <div class="mda-form-group" ng-show="showval">
                                        <select id="employee_dropdown" ng-model="employee" ng-change="onselectedchangeemployee(employee)"
                                                class="mda-form-control ng-touched" size="1">
                                            <option data-ng-repeat="data in employee_list" value="{{data.employee_gid}}">{{data.employee_name}}</option>
                                        </select>
                                        <label>Select Employee Name</label>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row" ng-show="hidecontact" id="contact_details">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-6 col-md-6 col-sm-6">
                                    <div class="mda-form-group">
                                        <input type="text" ng-model="txtcontact_number" required=""
                                               class="mda-form-control ng-pristine ng-valid ng-touched" tabindex="0" aria-invalid="false">
                                        <label>Contact Number</label>
                                        <!--<span class="mda-input-group-addon">
                                            <em class="fa fa-phone text-primary fa-lg"></em>
                                        </span>-->
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-6 col-sm-6">
                                    <div class="mda-form-group">
                                        <!--<span class="mda-input-group-addon">
                                            <em class="fa fa-envelope fa-lg text-primary"></em>

                                        </span>-->
                                        <input type="text" ng-model="txtemail_address" required=""
                                               class="mda-form-control ng-pristine ng-untouched ng-valid" tabindex="0" aria-invalid="false">

                                        <label>Email Address</label>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="mda-form-group float-label">
                                        <input name="txt_title" id="exampleEmail" ng-model="txt_title"
                                               class="mda-form-control ng-pristine ng-valid ng-touched" required>
                                        <label>Ticket Subject</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-sm-12 ">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="mda-form-group float-label">
                                        <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                            <label for="input_13">Ticket Description </label>
                                            <textarea ng-model="txt_remarks" rows="3" md-maxlength="1000" class=" md-input ng-valid-md-maxlength ng-touched ng-dirty ng-valid-parse"
                                                      name="txt_remarks" id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                        </md-input-container>
                                    </div>
                                </div>
                            </div>
                        </div>



                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-sm-3 col-md-3 col-lg-3">
                                    <strong class="ng-binding w3-text-indigo">
                                        <em class="fa fa-file text-primary fa-lg"></em>&emsp;
                                        UPLOAD DOCUMENT
                                    </strong>
                                </div>
                            </div>
                        </div>
                        <div class="dd-handle ng-scope">
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-2 col-md-2 col-sm-2">
                                            <label class="col-form-label "><strong>File Upload</strong></label>
                                        </div>

                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <input type="file" id="addupload" ng-model="test_document" onchange="angular.element(this).scope().upload(this.files, angular.element(this).scope().documentupload, this.name)"
                                                   data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="table-responsive">
                                            <table class="table table-striped table-secondary">
                                                <thead class="bg-gray text-white">
                                                    <tr width="100%">
                                                        <th scope="col">S.No</th>
                                                        <th scope="col">Document Name</th>
                                                        <th scope="col">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in filename_list" width="100%">
                                                        <td title="'S.No'">{{$index+1}}</td>
                                                        <td title="'Document Name'">{{data.filename}}</td>
                                                        <td data-title="'Action'">
                                                            <a class="btn btn-xs btn-danger" ng-click="document_cancelclick(data.id,data)"><span class="fa fa-close"></span>Cancel</a>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <br />

                        <div class="row" ng-show="hideapproval" id="approval">
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-3"></div>
                                        <div class="col-lg-6">
                                            <div class="panel widget bg-grey-300">
                                                <div class="panel-body text-center">
                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                        <p style="text-align:left;font-size:13px; color:red">* Note : The Selected Category will go for the following Approvals,</p>
                                                        <p style="text-align:left;font-size:13px;">Department Approval :<strong> {{approvaldetails.department_approval}}</strong> </p>
                                                        <p style="text-align:left;font-size:13px;">Management Approval :<strong> {{approvaldetails.management_approval}}</strong> </p>
                                                        <p style="text-align:left;font-size:13px;">Service Approval :<strong> {{approvaldetails.service_approval}}</strong> </p>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                      
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="panel-footer">
                            <div class="text-center">
                                <div>
                                    <button ng-click="serviceticket_submit()" ng-disabled="userformsubmit.$invalid" class="btn btn-success btn-md">

                                        <i class="fa fa-arrow-circle-right">&nbsp;&nbsp;</i>Submit
                                    </button>
                                    <a class="btn btn-info" ng-click="back()">
                                        <i class="fa fa-arrow-left" style="font-size:15px;">
                                            &nbsp;Back
                                        </i>
                                    </a>
                                </div>
                            </div>
                        </div>
                </form>
            </div>
        </div>
    </div>
    <script type="text/ng-template" id="/myModalContent.html">

        <div class="modal-header bg-green-200 ng-scope">

            <h4 id="myModalLabel" class="modal-title">

                <span class="btn-label bg-green-200">

                </span>View Approval Details
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <div class="pd-sm-10">
                <div class="row ">
                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <label style="color:black">This Selected Category will go for the following Approvals</labe</label>

                        </div>
                    </div>
                </div>
            </div>
            <br />
            <div class="pd-sm-10">
                <div class="row ">
                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            <label>Department Approval</label>
                        </div>

                        <div class="col-lg-6 col-sm-6 col-md-6">
                            {{subcategory.department_approval}}
                        </div>
                    </div>
                </div>
            </div>
            <div class="pd-sm-10">
                <div class="row ">
                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            <label>Management Approval</label>
                        </div>
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            {{subcategory.management_approval}}
                        </div>
                    </div>
                </div>
            </div>
            <div class="pd-sm-10">
                <div class="row ">
                    <div class="col-sm-12 col-sm-12 col-sm-12 ">
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            <label>Service Approval</label>
                        </div>
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            {{subcategory.service_approval}}
                        </div>
                    </div>
                </div>
            </div>
            <br /><br />

            <div class="text-center">
                <button ng-click="ok()" class="btn btn-primary"><i class="fa fa-check">&nbsp;&nbsp;</i>Ok</button>
            </div>


        </div>
    </script>
    <script>
        $("#file-upload-button").change(function () {
            var fileName = $(this).val().replace('C:\\fakepath\\', '');
            $("#file-upload-filename").html(fileName);
        });
    </script>
</div>