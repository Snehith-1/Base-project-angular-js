<style>
    .space {
        margin: 10px 0px;
    }
</style>
<h3>
    Company Details
</h3>
<div ng-controller="AgrMstSuprCADInstitutionDtlAddController as dp">
    <div class="panel">
        <div class="panel-heading" style="background-color:rgb(19,14,89);background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);color:#ffffff">
            <h5> Add Company Details </h5>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <br />
                    <form name="InstitutiondetailsSave" ng-submit="form.submitForm1()" novalidate="" autocomplete="off" class="form-validate form-horizontal">
                        <div>
                            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <br />
                                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Company Details </b></p>
                                        <hr />
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> ERP Id </label> <span style="color:red">*</span>
                                            <br />
                                            <div class="radio c-radio">
                                                <label>
                                                    <input type="radio" id="URN" name="URN" value="Yes" ng-model="rdburn_status" ng-change="onselectedurn_yes()" required />
                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                </label>
                                                <label>
                                                    <input type="radio" id="URN" name="URN" value="No" ng-model="rdburn_status" ng-change="onselectedurn_yes()" required />
                                                    <span class="fa fa-circle"></span>No
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4" ng-show="URN_yes">
                                            <label class="col-form-label"> ERP Id </label> <span style="color:red">*</span>
                                            <br />
                                            <input type="text" ng-model="txt_urn" tabindex="0" class="form-control" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" maxlength="25" />
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <label class="col-form-label"> Legal/Trade Name <span style="color:red"> *</span> </label>
                                            <br />
                                            <input type="text" ng-model="txtcompany_name" tabindex="0" class="form-control" maxlength="300" required ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/.,]*$/" />
                                        </div> 
                                    </div>
                                </div>
                                <br />
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12">  
                                            <div class="col-lg-6 col-md-6 col-sm-6">
                                                <label class="col-form-label"> Stakeholder Type <span style="color:red"> *</span></label>
                                                <br />
                                                <select name="user_type" ng-options="m as m.user_type for m in usertype_list track by m.usertype_gid"
                                                ng-model="cboStakeholdertype" class="chosen-select form-control" chosen="" style="height:37px;" required
                                                ng-change="getPANbasedGST()" size="1">
                                            <option value=""></option>
                                                </select>
                                            </div>
                                            <div class="col-lg-6 col-md-6 col-sm-6">
                                                <label class="col-form-label"> PAN Value <span style="color:red"> *</span></label>
                                                <br />
                                                <input type="text" ng-disabled="!cboStakeholdertype" ng-model="txtpan_number" tabindex="0" class="form-control" required ng-pattern="/^[A-Z]{5}[0-9]{4}[A-Z]{1}$/" maxlength="10" ng-change="getPANbasedGST()" />
                                                <label class="fa fa-check" style="color:green" ng-show="panvalidation"></label>&nbsp;<label style="font-weight:bolder;color:green" ng-show="panvalidation">Verified</label>
                                                <label class="fa fa-warning text-danger" style="font-weight:bolder;" ng-show="Alreadyaddedpanaadhar">This PAN number is already approved, you cannot add.</label>
                                            </div> 
                                        </div>
                                    </div> 
                                    <br/> 
                                <div class="row">
                                    <div class="col-md-12 col-lg-12 col-sm-12">
                                        <div class="col-md-12 col-lg-12 col-sm-12">
                                            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; GST Details </b></p>
                                                        <hr />
                                                        <div class="col-sm-2 col-md-2 col-lg-2">
                                                            <label>GST Registered <span style="color:red"> *</span></label>
                                                            <div class="radio c-radio">
                                                                <label>
                                                                    <input type="radio" id="GST_Registered" name="GST_Registered" value="Yes" ng-model="rdbgstregistered" />
                                                                    &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                </label>
                                                                <label>
                                                                    <input type="radio" id="GST_Registered" name="GST_Registered" value="No" ng-model="rdbgstregistered" />
                                                                    <span class="fa fa-circle"></span>No
                                                                </label>
                                                            </div>
                                                        </div>
                                                        <div class="col-lg-5 col-md-5 col-sm-5">
                                                            <label class="col-form-label"> GST Number <span style="color:red"> *</span></label>
                                                            <br />
                                                            <input type="text" ng-model="txtgst_number" tabindex="0" class="form-control" maxlength="15" ng-pattern="/^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[1-9A-Z]{1}Z[0-9A-Z]{1}$/" ng-change="onchangegst_number()" />
                                                        </div>
                                                        <div class="col-sm-4 col-md-4 col-lg-4">
                                                            <label class="col-form-label"> GST State </label>
                                                            <br />
                                                            <input type="text" ng-model="txtgst_state" tabindex="0" class="form-control" disabled />
                                                        </div>
                                                        <div class="col-lg-1 col-md-1 col-sm-1">
                                                            <br />
                                                            <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="institutiongst_add()"><span class="fa fa-plus" /></a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="table-responsive table-bordered">
                                                                <div style="max-height:200px;overflow:auto;">
                                                                    <table class="table">
                                                                        <thead class="bg-gray text-white">
                                                                            <tr>
                                                                                <th>S.No</th>
                                                                                <th>GST Registered</th>
                                                                                <th>GST State</th>
                                                                                <th>GST Number</th>
                                                                                <th>Action</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody>
                                                                            <tr ng-repeat="list in institutiongst_list">
                                                                                <td>{{$index+1}}</td>
                                                                                <td>{{list.gst_registered}}</td>
                                                                                <td>{{list.gst_state}}</td>
                                                                                <td>{{list.gst_no}}</td>
                                                                                <td class="text-nowrap">
                                                                                    <button title="Delete" ng-click="institutiongst_delete(list.institution2branch_gid)"
                                                                                            class="btn btn-danger btn-xs ">
                                                                                        <em class="fa fa-trash-o"></em>
                                                                                    </button>
                                                                                </td>
                                                                            </tr>
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />

                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label">Certificate of Incorporation <span style="color:red"> *</span></label>
                                            <br />
                                            <p class="input-group">
                                                <input type="text" id="incorporation_date" name="incorporation_date" ng-model="txtincorporation_date" datepicker-popup="{{dp.format}}"
                                                       style="height:36px;" is-open="dp.open6" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                                       close-text="Close" class="form-control" required placeholder="DD-MM-YYYY" ng-change="futuredatecheck(txtincorporation_date)" />
                                                <span class="input-group-btn">
                                                    <button type="button" ng-click="dp.calender6($event)" style="height:36px;" class="btn btn-default">
                                                        <em class="fa fa-calendar"></em>
                                                    </button>
                                                </span>
                                            </p>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label">Business Start Date <span style="color:red"> *</span></label>
                                            <br />
                                            <p class="input-group">
                                                <input type="text" id="businessstart_date" name="businessstart_date" ng-model="txtbusinessstart_date" datepicker-popup="{{dp.format}}"
                                                       style="height:36px;" is-open="dp.open06" min-date="dp.minDate" datepicker-options="dp.dateOptions" ng-change="onchangebusinessstartdate(); futuredatecheck(txtbusinessstart_date);"
                                                       close-text="Close" class="form-control" required placeholder="DD-MM-YYYY" />
                                                <span class="input-group-btn">
                                                    <button type="button" ng-click="dp.calender06($event)" style="height:36px;" class="btn btn-default">
                                                        <em class="fa fa-calendar"></em>
                                                    </button>
                                                </span>
                                            </p>
                                        </div>
                                        <div class="col-lg-2 col-md-2 col-sm-2">
                                            <label class="col-form-label"> Years in Business </label>
                                            <br />
                                            <input type="text" ng-model="txtyearin_business" tabindex="0" class="form-control" disabled required />
                                        </div>
                                        <div class="col-lg-2 col-md-2 col-sm-2">
                                            <label class="col-form-label"> Months in Business </label>
                                            <br />
                                            <input type="text" ng-model="txtmonthsin_business" tabindex="0" class="form-control" disabled required />
                                        </div>
                                    </div>
                                </div>
                                <br />
