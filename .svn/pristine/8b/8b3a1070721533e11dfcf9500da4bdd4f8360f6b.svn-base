<style>
    .space {
        margin: 10px 0px;
    }
</style>
<h3>
    Assigned Details
</h3>
<div ng-controller="MstMarketingUnassignedLeadViewController">
    <div class="panel">
        <div class="panel-heading bg-primary">
            <h5><i class="fa fa-eye"></i> &nbsp; View Unassigned Lead </h5>
        </div>
        <div class="modal-body ng-scope">
            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Assigned Details </b></p>
                <hr class="space" />
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Lead Number</label><br>
                            {{txtticket_refid}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Lead Status</label><br>
                            {{txtcallclosure_status}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Assigned Employee</label><br>
                            {{txtassignemployee_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Assigned Date</label><br>
                            {{txtassign_date}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Entity</label><br>
                            {{txtentity_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Source of Lead</label><br>
                            {{txtsourceofcontact_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Lead Request Type</label><br>
                            {{txtcustomer_type}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Lead Received Date</label><br>
                            {{txtcallreceived_date}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">

                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Lead Name</label><br>
                            {{txtcaller_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Internal Reference</label><br>
                            {{txtinternalreference_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Office Landline Number</label><br>
                            {{txtoffice_landlineno}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Company / Organisation Name</label><br>
                            {{txtcallerassociate_company}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Enquiry Type</label><br>
                            {{txtcalltype_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>TAT in Hrs</label><br>
                            {{txttat_hours}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Base Location</label><br>
                            {{txtbase_location}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Origination</label><br>
                            {{origination}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3" >
                            <label>Industry</label><br>
                            {{txtindustry_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3" ng-show="origination =='Online'">
                            <label>What do you require</label><br>
                            {{txtleadrequire_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3" ng-show="origination =='Millet'">
                            <label>What do you require</label><br>
                            {{txtmilletrequire_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3" ng-show="origination =='Enquiry'">
                            <label>What do you require</label><br>
                            {{txtenquiryrequire_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3" ng-show="origination =='Enquiry'">
                            <label>Stage of the Startup</label><br>
                            {{txtstartuprequire_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3" ng-show="origination =='Enquiry'">
                            <label>Please Describe your Business</label><br>
                            {{txtbusiness_name}}
                        </div>
                    </div>
                </div>
                <br>

                <div class="dd-handle ng-scope">
                    <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Tag Details </b></p>
                    <hr class="space" />
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="table-responsive table-bordered">
                                    <div style="max-height:200px;overflow:auto;">
                                        <table class="table">
                                            <thead class="bg-gray text-white">
                                                <tr width="100%">
                                                    <th align="center" scope="col">S.No</th>
                                                    <th align="center" scope="col">Member Name</th>
                                                    <th align="center" scope="col">Tagged Date/Time</th>
                                                    <th align="center" scope="col">Tagged By</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in ibcalltaggedmember_list" width="100%">
                                                    <td align="left" scope="col" title="S.No">
                                                        {{$index+1}}
                                                    </td>
                                                    <td align="left" scope="col" title="Member Name">
                                                        {{data.taggedmember_name}}
                                                    </td>
                                                    <td align="left" scope="col" title="Tagged Date/Time">
                                                        {{data.tagged_date}}
                                                    </td>
                                                    <td align="left" scope="col" title="Tagged By">
                                                        {{data.tagged_by}}
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="dd-handle ng-scope">
                    <!--ng-show="transfershow"-->
                    <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Transfer Details </b></p>
                    <hr class="space" />
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="table-responsive table-bordered">
                                    <div style="max-height:200px;overflow:auto;">
                                        <table class="table">
                                            <thead class="bg-gray text-white">
                                                <tr>
                                                    <th align="center" scope="col">S.No</th>
                                                    <th align="center" scope="col">Transfer From</th>
                                                    <th align="center" scope="col">Transfer To</th>
                                                    <th align="center" scope="col">Transfer By</th>
                                                    <th align="center" scope="col">Transfer Date</th>
                                                    <th align="center" scope="col">Transfer Remarks</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in ibcalltransfer_list">
                                                    <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                                    <td align="left" scope="col" title="Transfer From">{{data.transferfrom_name}}</td>
                                                    <td align="left" scope="col" title="Transfer To">{{data.transferto_name}}</td>
                                                    <td align="left" scope="col" title="Transfer By">{{data.transfer_by}}</td>
                                                    <td align="left" scope="col" title="Transfer Date">{{data.transfer_date}}</td>
                                                    <td align="left" scope="col" title="Transfer Remarks">{{data.transfer_remarks}}</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="row">
                    <div class="col-lg-8 col-sm-8 col-md-8">
                        <div class="dd-handle ng-scope">
                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Address Details </b></p>
                            <hr class="space" />
                            <div ui-scroll-viewport="" style="height:320px; overflow-y: auto;overflow-x: hidden;" class="row">
                                <div class="row" ng-repeat="data in ibcalladdress_list">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="dd-handle ng-scope" ng-repeat="data in ibcalladdress_list">
