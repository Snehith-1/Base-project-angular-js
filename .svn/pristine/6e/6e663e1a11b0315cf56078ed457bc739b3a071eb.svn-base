<style>
    .spaceline {
        margin: 10px 0px;
    }
</style>
<h3>
    Cheque Management
</h3>
<div ng-controller="AgrMstUDCMakerAddController as dp">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <div id="Basicdetailsdtlid">
                <span class="btn-label">
                    <i class="fa fa-plus"></i>
                </span> Add Cheque - {{txtbasiccustomer_name}} / {{txtapplication_no}}
            </div>
        </div>
        <form novalidate="" name="submitmaker" ng-submit="form.submitmaker()" autocomplete="off" class="form-validate form-horizontal">
            <br />
            <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <div class="dd-handle ng-scope">
                            <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <p class="text-primary" style="font-size:14px;"><b> Applicant Details  </b></p>
                                    <hr class="spaceline" />
                                    <div class="col-sm-4 col-md-4 col-lg-4">
                                        <label> Name of the Applicant / Guarantor / Group / Member </label><span style="color:red">&nbsp;*</span>
                                        <select chosen="" ng-model="cboStakeholder" id="stakeholder_name" name="stakeholder_name" dropzone="" required
                                                class="form-control ng-valid ng-touched ng-valid-parse" ng-change="onChangeStakeholderName(cboStakeholder.stakeholder_gid)"
                                                ng-options="m.stakeholder_name for m in StakeholderList">
                                            <option value="">---- Stakeholder ----</option>
                                        </select>
                                    </div>
                                    <div class="col-sm-4 col-md-4 col-lg-4">
                                        <label>Stakeholder Type</label>
                                        <input type="text" class="form-control" ng-model="txtstakeholder_type" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/]*$/" disabled>
                                    </div>
                                    <div class="col-sm-4 col-md-4 col-lg-4">
                                        <label>Designation</label>
                                        <input type="text" ng-model="txtdesignation" class="form-control" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/]*$/" disabled>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <br>
                        <div class="row" style="margin-bottom:-10px;">
                            <div class="col-lg-8 col-md-8 col-sm-8" style="padding-right: 6px;">
                                <div id="panelDemo20" class="panel">
                                    <div class="panel-heading" style="background:linear-gradient(90deg, rgba(27,156,54,1) 0%, rgba(53,109,171,1) 35%, rgba(161,229,242,1) 100%);color:#ffffff;">General Information</div>
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="dd-handle ng-scope">
                                                    <div class="row">
                                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                                            <div class="col-sm-4 col-md-4 col-lg-4">
                                                                <label>Merged Banking Entity</label><span style="color:red">&nbsp;*</span>
                                                                <select name="mergedbanking_entity" chosen="" id="mergedbanking_entity" ng-model="cbomergedbanking_entity"
                                                                        dropzone="" class="form-control ng-valid ng-touched ng-valid-parse" ng-options="m.bankname_name for m in bankname_list" required>
                                                                    <option value="">---- Merged Banking Entity ----</option>
                                                                </select>
                                                            </div>
                                                            <div class="col-sm-4 col-md-4 col-lg-4">
                                                                <label>Cheque Type</label>
                                                                <select name="" id="" class="form-control" ng-model="cbocheque_type">
                                                                    <option value=""> ---- Cheque Type ---- </option>
                                                                    <option value="PDC"> PDC </option>
                                                                    <option value="UDC"> UDC </option>
                                                                </select>
                                                            </div>
                                                            <div class="col-sm-4 col-md-4 col-lg-4">
                                                                <label>Date of Cheque Type</label><span style="color:red">&nbsp;*</span>
                                                                <p class="input-group">
                                                                    <input type="text" id="enddate" name="date" ng-model="txtdate_chequetype" datepicker-popup="{{dp.format}}" required
                                                                           style="height:36px;" is-open="dp.open1" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                                                           close-text="Close" class="form-control" />
                                                                    <span class="input-group-btn">
                                                                        <button type="button" ng-click="dp.calender1($event)" style="height:36px;" class="btn btn-default">
                                                                            <em class="fa fa-calendar"></em>
                                                                        </button>
                                                                    </span>
                                                                </p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br>
                                                    <div class="row">
                                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                                            <div class="col-sm-4 col-md-4 col-lg-4">
                                                                <label>CTS enabled</label><span style="color:red">&nbsp;*</span>
                                                                <div class="radio c-radio">
                                                                    <label>
                                                                        <input type="radio" id="emailaddress" name="cts_enabled" value="Yes" ng-model="rbocts_enabled" required />
                                                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                    </label>
                                                                    <label>
                                                                        <input type="radio" id="emailaddress" name="cts_enabled" value="No" ng-model="rbocts_enabled" required />
                                                                        <span class="fa fa-circle"></span>No
                                                                    </label>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-4 col-md-4 col-lg-4">
                                                                <label>Date of Cheque Presentation</label><span style="color:red">&nbsp;*</span>
                                                                <p class="input-group">
                                                                    <input type="text" id="enddate" name="date" ng-model="txtdate_chequepresentation" datepicker-popup="{{dp.format}}"
                                                                           style="height:36px;" is-open="dp.open2" min-date="dp.minDate" datepicker-options="dp.dateOptions" required
                                                                           close-text="Close" class="form-control" />
                                                                    <span class="input-group-btn">
                                                                        <button type="button" ng-click="dp.calender2($event)" style="height:36px;" class="btn btn-default">
                                                                            <em class="fa fa-calendar"></em>
                                                                        </button>
                                                                    </span>
                                                                </p>
                                                            </div>
                                                            <div class="col-sm-4 col-md-4 col-lg-4">
                                                                <label>Date of Cheque Clearance/ Return</label><span style="color:red">&nbsp;*</span>
                                                                <p class="input-group">
                                                                    <input type="text" id="enddate" name="date" ng-model="txtdate_chequeclearance" datepicker-popup="{{dp.format}}"
                                                                           style="height:36px;" is-open="dp.open3" min-date="dp.minDate" datepicker-options="dp.dateOptions" required
                                                                           close-text="Close" class="form-control" />
                                                                    <span class="input-group-btn">
                                                                        <button type="button" ng-click="dp.calender3($event)" style="height:36px;" class="btn btn-default">
                                                                            <em class="fa fa-calendar"></em>
                                                                        </button>
                                                                    </span>
                                                                </p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br>
                                                    <div class="row">
                                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                                            <div class="col-sm-12 col-md-12 col-lg-12">
                                                                <label>Status of Cheque Presentation</label><span style="color:red">&nbsp;*</span>
                                                                <textarea name="" id="" rows="5" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/]*$/" required ng-model="txtstatus_chequepresentation" style="resize:vertical;" class="form-control" maxlength="1000"></textarea>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br>
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-sm-12 col-md-12 col-lg-12">
                                                                <label>Status of Cheque Clearance / Return</label><span style="color:red">&nbsp;*</span>
                                                                <textarea rows="5" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/]*$/" ng-model="txtstatus_chequeclearance" required style="resize:vertical;" class="form-control" maxlength="1000"></textarea>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                    <div class="row">
                                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                                            <div class="col-sm-12 col-md-12 col-lg-12">
                                                                <label>Special condition if any in the instrument</label>
                                                                <textarea ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/]*$/" ng-model="txtspecial_condition" style="resize:vertical;" rows="5" class="form-control" maxlength="2000"></textarea>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br>
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-sm-12 col-md-12 col-lg-12">
                                                                <label>General Remarks</label><span style="color:red">&nbsp;*</span>
                                                                <textarea rows="5" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/]*$/" required ng-model="txtgeneral_remarks" style="resize:vertical;" class="form-control" maxlength="1000"></textarea>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                        <br /><br />
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4" style="padding-left:0px;">
                                <div id="panelDemo9" class="panel panel-primary">
                                    <div class="panel-heading" style="background:linear-gradient(90deg, rgba(27,156,54,1) 0%, rgba(53,109,171,1) 35%, rgba(161,229,242,1) 100%);color:#ffffff;"> Cheque Upload </div>
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="col-sm-5 col-md-5 col-lg-5">
                                                    <div class="mda-form-group float-label">
                                                        <strong>Upload Documents <span style="color:red">&nbsp;*</span> : </strong>
                                                    </div>
                                                </div>
                                                <div class="col-sm-7 col-md-7 col-lg-7">
                                                    <div class="mda-form-group float-label">
                                                        <input type="file" id="file" tabindex="5" multiple onchange="angular.element(this).scope().UploadDocument(this.files, angular.element(this).scope().documentupload, this.name)"
                                                               data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <br />
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <ul class="list-inline" style="color: blue;">
                                                    <li ng-repeat="data in chequedocument_list" style="padding:5px">
                                                        <a style="font-size: 14px;" ng-click="downloads(data.document_path,data.document_name)">{{data.document_name}}</a>
                                                        <em class="icon-close" ng-click="delete_document(data.cheque2document_gid)" style="color: blue;"></em>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                        <br />
                                        <div ng-show="chequedocument_list!=null">
                                            <div class="row">
                                                <div class="col-sm-12 col-md-12 col-lg-12">
                                                    <label>Account holder name</label>
                                                    <input type="text" class="form-control" disabled ng-model="txtaccountholder_name">
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-md-12 col-lg-12">
                                                    <label>Account Number</label>
                                                    <input type="text" class="form-control" disabled ng-model="txtaccount_number">
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-md-12 col-lg-12">
                                                    <label>Name of the Bank </label>
                                                    <input type="text" class="form-control" disabled ng-model="txtbank_name">
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-md-12 col-lg-12">
                                                    <label>Cheque Number</label>
                                                    <input type="text" class="form-control" disabled ng-model="txtcheque_no">
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-md-12 col-lg-12">
                                                    <label>IFSC Code</label>
                                                    <input type="text" class="form-control" disabled ng-model="txtifsc_code">
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-md-12 col-lg-12">
                                                    <label>MICR</label>
                                                    <input type="text" class="form-control" disabled ng-model="txtmicr">
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-md-12 col-lg-12">
                                                    <label>Branch Address </label>
                                                    <input type="text" class="form-control" disabled ng-model="txtbranch_address">
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-md-12 col-lg-12">
                                                    <label>Branch name</label>
                                                    <input type="text" class="form-control" disabled ng-model="txtbranch_name">
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-md-12 col-lg-12">
                                                    <label>City</label>
                                                    <input type="text" class="form-control" disabled ng-model="txtcity">
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-md-12 col-lg-12">
                                                    <label>District</label>
                                                    <input type="text" class="form-control" disabled ng-model="txtdistrict">
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12 col-md-12 col-lg-12">
                                                    <label>State</label>
                                                    <input type="text" class="form-control" disabled ng-model="txtstate">
                                                </div>
                                            </div>
                                        </div>
                                        <br />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--<br>
                        <div class="row">
                            <div class="col-sm-12 col-md-12 col-lg-12">
                                <div class="row text-center">
                                    <a class="btn btn-sm btn-default" style="background-color:#09385f;color:white;" ng-click="add_cheque()">&nbsp;&nbsp;<span class="fa fa-plus">&nbsp;&nbsp;Add Cheque&nbsp;&nbsp;</a>
                                </div>
                            </div>
                        </div>
                        <br>-->

                    </div>
                </div>
            </div>
            <div class="row text-center">
                <button type="button" style="background-color:#09385f;" class="btn btn-success btn-md" ng-disabled="submitmaker.$invalid" ng-click="addnext_submit()"><i class="fa fa-plus"></i>&nbsp;&nbsp;Add Next</button>&nbsp;&nbsp;
                <button type="button" class="btn btn-success btn-md" ng-disabled="submitmaker.$invalid" ng-click="submit_borrower()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                <button ng-click="back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
            </div>
            <br />
        </form>
