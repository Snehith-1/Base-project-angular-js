<h3>
    Vendor Consultant Register
</h3>
<div ng-controller="externalRegisterAddcontroller">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <em class="fa fa-plus"></em>&nbsp; Register External Vendor
        </div>
        <div class="panel-body">
            <div class="row">
                <form name="registrationForm" id="registrationForm" novalidate="" class="form-validate mb-lg" autocomplete="off">

                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <fieldset>
                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="form-group float-label">
                                        <label>External Vendor Code <span style="color:red">*</span></label>
                                        <input ng-model="txtexternal_vendorCode" class="form-control ng-pristine ng-valid ng-touched" required>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="form-group float-label">
                                        <label>External Vendor Name <span style="color:red">*</span></label>
                                        <input ng-model="txtexternal_vendorName" class="form-control ng-pristine ng-valid ng-touched" required>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                        <fieldset>
                            <div class="row">
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    <div class="form-group float-label">
                                        <label>Contact Person <span style="color:red">*</span></label>
                                        <input ng-model="txtcontact_person" class="form-control ng-pristine ng-valid ng-touched" required>
                                    </div>
                                </div>
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    <div class="form-group float-label">
                                        <label>Email Address <span style="color:red">*</span></label>
                                        <input ng-model="txtemail_ID" type="email"
                                                class="form-control ng-pristine ng-valid ng-touched" required>
                                    </div>
                                </div>
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    <div class="form-group float-label">
                                        <label>Contact No. <span style="color:red">*</span></label>
                                        <input ng-model="txtcontact_Number" type="number" maxlength="10"
                                                class="form-control ng-pristine ng-valid ng-touched" required>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                        <fieldset>
                            <div class="row">
                                <div class="col-lg-6 col-sm-6 col-md-6 form-group">
                                    <label>Address Line 1 <span style="color:red">*</span></label>
                                    <textarea name="specificcondition" placeholder="Enter Address Line 1..." required
                                              ng-model="txtaddress_line1" class="form-control" rows="4"></textarea>
                                </div>
                                <div class="col-lg-6 col-sm-6 col-md-6 form-group">
                                    <label>Address Line 2 </label>
                                    <textarea name="specificcondition" placeholder="Enter Address Line 2..."
                                              ng-model="txtaddress_line2" class="form-control" rows="4"></textarea>
                                </div>
                            </div>
                        </fieldset>
                        <fieldset>
                            <div class="row">
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <div class="form-group float-label">
                                        <label>State <span style="color:red">*</span></label>
                                        <select id="cbostatename" name="cbostatename" ng-model="cbostategid"
                                                class="form-control ng-valid ng-touched ng-valid-parse" size="1" ng-change="onchangestate(cbostategid)" required>
                                            <option value=""> Select State </option>
                                            <option data-ng-repeat="data in statedtl" value="{{data.state_gid}}"
                                                    style="width:250px;">
                                                {{data.state_name}}
                                            </option>
                                        </select>
                                    </div>
                                </div>

                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <div class="form-group float-label">
                                        <label>District <span style="color:red">*</span></label>
                                        <select id="cbodistrictname" name="cbodistrictname" ng-model="cbodistrictgid"
                                                class="form-control ng-valid ng-touched ng-valid-parse" size="1" ng-change="onchangedistrict(cbodistrictgid)" required>
                                            <option value=""> Select District </option>
                                            <option data-ng-repeat="data in districtdtl" value="{{data.district_gid}}"
                                                    style="width:250px;">
                                                {{data.district_name}}
                                            </option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <div class="form-group float-label">
                                        <label>Country <span style="color:red">*</span></label>
                                        <input ng-model="txtcountry" type="text" class="form-control ng-pristine ng-valid ng-touched" required>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                        <fieldset>
                            <div class="row">
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <div class="form-group float-label">
                                        <label>Postal Code <span style="color:red">*</span></label>
                                        <input ng-model="txtpostalCode" type="number" maxlength="6" class="form-control ng-pristine ng-valid ng-touched" required>
                                    </div>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4" ng-show="upload">
                                    <label>Photo Upload</label>
                                    <input type="file" id="addupload" ng-model="test_document" multiple
                                           onchange="angular.element(this).scope().upload(this.files, angular.element(this).scope().documentupload, this.name)" class="form-control"
                                           data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />

                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4" ng-show="photouploaded">
                                    <label>Photo Upload</label>
                                    <div ng-repeat="data in filename_list">
                                        <p>{{$index+1}} {{data.file_name}}</p>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                        <div class="row">
                            <div class="text-center">
                                <button class="btn btn-success" ng-click="registrationsubmit()" ng-disabled="registrationForm.$invalid">
                                    <em class="fa fa-check"></em>
                                    <span>Submit</span>
                                </button>
                                <a class="btn btn-danger" ng-click="cancel()">
                                    <em class="fa fa-close"></em>
                                    <span>Cancel</span>
                                </a>
                            </div>
                        </div>
                    </div>

                </form>
            </div>
        </div>
    </div>
</div>