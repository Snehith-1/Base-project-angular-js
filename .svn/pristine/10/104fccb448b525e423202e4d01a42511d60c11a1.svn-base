<h3>
    RM Response
</h3>
<div ng-controller="idasTrnResponseDoc">
    <div id="panelDemo13" class="panel panel-default">
        <div class="panel-body table-responsive">

            <p class="lead bb text-primary text-bold text-center">
                {{customerName}}
                <label style="font-weight:bold">/</label>{{customercode}}
            </p>

            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12 ">
                    <p class="lead bb text-purple text-bold" style="font-size:16px;">Customer Details</p>
                    <div class="col-lg-12">
                        <label class="col-form-label">Address</label>
                        <br />
                        <p style="font-size:14px;">
                            {{contactperson}}<br />{{addressline1}}<br />
                            {{addressline2}}
                            <br />{{mobileno}}
                        </p>

                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label"> URN No :</label>
                        <br />
                        <p style="font-size:14px;">{{Customerurn}}</p>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 ">

                    <div class="col-lg-4">
                        <label class="col-form-label">Cluster Manager</label>
                        <br />
                        <p ng-model="ClusterMgr" style="font-size:14px;">{{clusterManager}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Relationship Manager</label>
                        <br />
                        <p style="font-size:14px;">{{relationshipmgmt}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Credit Manager</label>
                        <br />
                        <p style="font-size:14px;">{{creditManager}}</p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12">

                    <div class="col-lg-4">
                        <label class="col-form-label">Zonal Head</label>
                        <br />
                        <p ng-model="zonalHeadName" style="font-size:14px;">{{zonalHeadName}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Business Head</label>
                        <br />
                        <p style="font-size:14px;">{{businessHeadName}}</p>
                    </div>
                </div>
            </div>
            <p class="lead bb text-purple text-bold" style="font-size:16px;">Sanction Details</p>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 ">

                    <div class="col-lg-4">
                        <label class="col-form-label">Sanction Ref No.</label>
                        <br />
                        <p ng-model="sanctionref_no" style="font-size:14px;">{{sanctionrefno}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Sancion Date</label>
                        <br />
                        <p style="font-size:14px;">{{SanctionDate}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Sanction Amount</label>
                        <br />
                        <p ng-model="sanction_amount" style="font-size:14px;">{{SanctionAmount}}</p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 ">

                    <div class="col-lg-4">
                        <label class="col-form-label">Facility </label>
                        <br />
                        <p ng-model="facility_type" style="font-size:14px;">{{FacilityType}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Vertical</label>
                        <br />
                        <p style="font-size:14px;">{{verticalCode}}</p>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label">Collateral Details</label>
                        <br />
                        <p ng-model="collateral_security" style="font-size:14px;">{{collateral_security}}</p>
                    </div>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <p class="lead bb text-purple text-bold" style="font-size:16px;">Common Document Upload</p>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-lg-2 col-md-2 col-sm-2">
                        <button type="button" ng-click="FileShow()" class="btn btn-info btn-xs">
                            <span class="icon icon-cloud-upload"></span>&nbsp;&nbsp;Upload File
                        </button>
                    </div>
                </div>
            </div>
            <br />
            <div ng-show="DivFile">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-2 col-md-2 col-sm-2">
                            <label>Document Title</label>
                        </div>

                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <input type="text" ng-model="txtdocument_title" rows="1" tabindex="0" min="0" aria-required="true"
                                   aria-invalid="true" class="form-control" maxlength="256" />
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-2 col-md-2 col-sm-2">
                            <label>File Upload</label>
                        </div>

                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <input type="file" id="commonupload" ng-model="common_document" ng-disabled="!txtdocument_title"
                                   onchange="angular.element(this).scope().commondocumentupload(this.files, angular.element(this).scope().commondocumentupload, this.name)"
                                   data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select"
                                   accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />
                        </div>
                    </div>
                </div>
                <br />
            </div>

            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="text-center">
                            <div class="dd-handle ng-scope">
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="table-responsive">
                                            <table class="table table-striped table-secondary">
                                                <thead class="bg-gray text-white">
                                                    <tr width="100%">
                                                        <th align="center" scope="col">S.No</th>
                                                        <th align="center" scope="col">Document Name</th>
                                                        <th align="center" scope="col">Document Title</th>
                                                        <th align="center" scope="col">Upload By/On</th>
                                                        <th align="center" scope="col">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in commondocument" width="100%">
                                                        <td scope="col" title="'S.No'">{{$index+1}}</td>
                                                        <td align="left" scope="col" title="'Document Name'">{{data.document_name}}</td>
                                                        <td align="left" scope="col" title="'Document Title'">{{data.document_title}}</td>
                                                        <td align="left" scope="col" title="'Upload By/On'">{{data.created_by}}/{{data.created_date}}</td>
                                                        <td align="left" scope="col" data-title="'Action'">
                                                            <button title="Download Document" ng-click="downloadsdocument(data.document_path,data.document_name)" class="btn btn-info btn-xs">
                                                                <em class="fa fa-download"></em><a download="" href="" id="btnpdf" style="display:none;"></a>
                                                            </button>
                                                            <button title="Delete Document" ng-click="deletedocument(data.uploaddocument_gid)" class="btn btn-danger btn-xs">
                                                                <em class="fa fa-trash-o"></em>
                                                            </button>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>

                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <p class="lead bb text-purple text-bold" style="font-size:16px;">Mail Management</p>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-md-6 col-lg-6 col-sm-6">



                        <button title="Send Mail" ng-click="mailconversation()" class="btn btn-info">
                            <em class="fa fa-send (alias)"></em>
                        </button>
                        &nbsp;&nbsp;

                        <button title="Export Excel" ng-click="export()" class="btn btn-success ">
                            <em class="fa fa-file-excel-o"></em><a download="" href="" id="btnpdf" style="display:none;"></a>
                        </button>
                        &nbsp;&nbsp;
                    </div>



                </div>
            </div>
            <br />
            <div class="row" ng-hide="show">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-md-1 col-lg-1 col-sm-1">
                        <span>File Upload</span>

                    </div>
                    <div class="col-md-2 col-lg-2 col-sm-2">
                        <input type="file" id="addupload" ng-model="test_document"
                               onchange="angular.element(this).scope().uploadallocation(this.files, angular.element(this).scope().documentupload, this.name)"
                               data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select"
                               accept="images/jpg,images/jpeg,images/tif,images/tiff,images/pdf" />
                        &nbsp;
                    </div>
                    <div class="col-md-2 col-lg-2 col-sm-2">
                        <button ng-click="cancel()" class="btn btn-danger">
                            <i class="fa fa-close"></i>&nbsp;&nbsp;Cancel
                        </button>

                    </div>

                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-md-12 col-lg-12 col-sm-12">

                        <div class="table-responsive">
                            <table class="row-border hover" id="statictable">

                                <thead>
                                    <tr class="bg-success text-white">
                                        <th width="3%">S.No</th>
                                        <th width="10%">Sent On</th>
                                        <th width="10%">Sent By</th>
                                        <th width="10%">Subject</th>
                                        <th width="10%">Sent To</th>
                                        <th width="10%">Sent From</th>
                                        <th width="10%">Attachment</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="data in senddata">
                                        <td class="text-center">
                                            {{$index+1}}
                                        </td>
                                        <td>
                                            {{data.created_date}}
                                        </td>
                                        <td>
                                            {{data.created_by}}
                                        </td>

                                        <td>
                                            {{data.subject}}
                                        </td>
                                        <td>
                                            {{data.to_mail}}
                                        </td>
                                        <td>
                                            {{data.from_mail}}
                                        </td>
                                        <td>
                                            <a ng-click="exportattach(data.document_path,data.document_name)">IDAS Document Check List.xls</a>
                                        </td>

                                    </tr>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <p class="lead bb text-purple text-bold" style="font-size:16px;">Scanned Document Details</p>

                </div>
            </div>

            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <!--<input type="text" ng-model="search" class="mb form-control ng-valid ng-dirty ng-valid-parse ng-touched"
                               placeholder="Enter text to search..." />-->
                        <div>
                            <table datatable="ng" class="row-border hover">

                                <thead>
                                    <tr class="bg-primary-light text-white">
                                        <th width="5%">
                                            S.No
                                        </th>
                                        <th width="10%">Document Record ID</th>
                                        <th width="15%">Document Code/Name</th>

                                        <th width="8%">Action</th>
                                    </tr>
                                </thead>
                                <!--</table>
                                <div style="max-height:300px;overflow:auto;">
                                    <table class="table">-->
                                <tbody>
                                    <tr ng-repeat="data in documentation_list | filter:search">
                                        <td width="5%">
                                            {{$index+1}}

                                        </td>

                                        <td width="10%" data-title="Document Record ID">{{data.documentrecord_id}}</td>
                                        <td width="15%" data-title="Document Code/Name">{{data.document_code}}/{{data.document_name}}</td>


                                        <td width="8%" data-title="Action">
                                            <div>
                                                <label ng-if="data.externalquery_count != '0'" class="label label-primary ">New</label>
                                                <button title="Document Conversation" ng-click="rmresponse(data.sanctiondocument_gid)" class="btn btn-pink">
                                                    <em class="icon icon-directions"></em>
                                                </button>

                                                <span ng-if="data.externalconversation_count != '0'" class="badge bg-gray-light ">{{data.externalconversation_count}}</span>
                                            </div>


                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>
            </div>

        </div>
        <br />


        <div id="down" class="ng-scope">
            <div class="text-center">

                <button ng-click="gotoback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
            </div>
        </div>
        <br>
    </div>

    <script type="text/ng-template" id="/mailconversation.html">


        <div class="modal-body ng-scope">
            <h4 id="myModalLabel" class="modal-title">
                <p class="lead bb text-primary text-bold text-center">
                    Document Mail - Conversation
                </p>
            </h4>
            <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="col-lg-6">
                        <label>From :</label> {{rmmail_id}}
                    </div>

                </div>
            </div>
            <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="col-lg-6">
                        <label>To :</label>{{creditmail_id}}
                    </div>
                </div>
            </div>


            <div ng-show="mailbox.mail.cc" class="input-group form-group">
                <span class="input-group-addon text-sm">CC:</span>
                <input type="text" class="form-control" required="" ng-model="cc_mail" />
            </div>
            <div ng-show="mailbox.mail.bcc" class="input-group form-group">
                <span class="input-group-addon text-sm">BCC:</span>
                <input type="text" class="form-control" required="" ng-model="bcc_mail" />
            </div>
            <form novalidate="" name="userform5" ng-submit="form.submitForm1()" autocomplete="off">
                <div class="input-group form-group">

                    <span class="input-group-addon text-sm">Subject:</span>
                    <input type="text" class="form-control" required="" ng-model="subject_mail">

                </div>
            </form>
            <div class="btn-group btn-group-sm mb pull-left">
                <strong>
                    Attachment : <a ng-click="export()">IDAS Document Check List.xls</a>
                </strong>
            </div>
            <div class="btn-group btn-group-sm mb pull-right">
                <button type="button" ng-class="{'active':mailbox.mail.cc}" ng-click="mailbox.mail.cc = !mailbox.mail.cc" href="#" class="btn btn-default btn-sm">CC</button>
                <button type="button" ng-class="{'active':mailbox.mail.bcc}" ng-click="mailbox.mail.bcc = !mailbox.mail.bcc" href="#" class="btn btn-default btn-sm">BCC</button>
            </div>
            <div class="imgcrop-area" style="height:250px;">
                <p ng-bind-html="mailcontent"></p>
            </div>
            <br />
            <div class="text-center">
                <button ng-click="sendMail()" ng-disabled="userform5.$invalid" class="btn btn-success">
                    <i class="fa fa-send (alias)">
                    </i>&nbsp;Send
                </button>
                <button ng-click="ok()" class="btn btn-danger">
                    <i class="fa fa-close (alias)">
                    </i>&nbsp;Cancel
                </button>
            </div>
        </div>
    </script>
</div>
