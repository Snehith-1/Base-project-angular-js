    <script type="text/ng-template" id="/addresstype.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-plus"></em>&nbsp;<label style="font-size:16px">Add Address</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">

                <form novalidate="" name="userform1" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-9 col-md-9 col-sm-9">
                                <label><strong>Address Type </strong> <span style="color:red">*</span></label>
                                <select chosen="" ng-options="m as m.address_type for m in addresstype_list track by m.address_gid"
                                        ng-model="cboaddresstype" class="chosen-select form-control" required>
                                    <option value=""></option>
                                </select>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <label>Primary Status </label> <span style="color:red">*</span>
                                <div class="radio c-radio">
                                    <label>
                                        <input type="radio" id="primary" name="primaryaddress" required value="Yes" ng-model="rdbprimaryaddress" />
                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                    </label>
                                    <label>
                                        <input type="radio" id="primary" name="primaryaddress" required value="No" ng-model="rdbprimaryaddress" />
                                        <span class="fa fa-circle"></span>No
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-sm-12 col-sm-12 col-sm-12">
                                <label><strong>Address Line 1 </strong> <span style="color:red">*</span></label>
                                <input type="text" ng-model="txtaddressline1" tabindex="0" class="form-control" maxlength="300" required/>
                                <label style="font-weight:bolder;color:red" ng-show="geocodingFailed">Invalid Address!</label>
                            </div>
                        </div>
                    </div>
                    <br/>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-sm-12 col-sm-12 col-sm-12">
                                <label><strong>Address Line 2 </strong></label>
                                <input type="text" ng-model="txtaddressline2" tabindex="0" class="form-control" maxlength="300" />
                            </div>
                        </div>
                    </div>
                    
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Land Mark </strong></label>
                                    <input type="text" ng-model="txtLand_Mark" tabindex="0" class="form-control" maxlength="300" />

                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Postal Code </strong> <span style="color:red">*</span></label>
                                    <input type="text" ng-model="txtpostal_code" tabindex="0" class="form-control" required
                                           ng-pattern="/^[1-9]{1}[0-9]{5}$/" ng-change="onchangepostal_code(); getGeoCoding();" maxlength="6" />

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label>City </label>
                                    <input type="text" ng-model="txtcity" tabindex="0" class="form-control" disabled />
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label>Taluka</label>
                                    <input type="text" ng-model="txttaluka" tabindex="0" class="form-control" disabled />

                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label><strong>District</strong></label>
                                    <input type="text" ng-model="txtdistrict" tabindex="0" class="form-control" disabled />
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label>State </label> 
                                    <br />
                                    <input type="text" ng-model="txtstate" tabindex="0" class="form-control" disabled />
                                </div>

                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label>Country</label> <span style="color:red">*</span>
                                    <input type="text" ng-model="txtcountry" tabindex="0" class="form-control" required ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/.,]*$/" />

                                </div>
                            </div>
                        </div>
                    </div>                    
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Latitude </strong></label>
                                    <input type="text" ng-model="txtlatitude" tabindex="0" class="form-control" disabled />

                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Longitude </strong></label>
                                    <input type="text" ng-model="txtlongitude" tabindex="0" class="form-control" disabled />
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="userform1.$invalid" ng-click="addressSubmit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>

                    </div>
                </form>
            </div>
        </div>
    </script>
