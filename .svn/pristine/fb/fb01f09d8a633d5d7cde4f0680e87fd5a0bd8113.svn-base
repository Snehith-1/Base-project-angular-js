                                <select chosen="" ng-options="m as m.livestock_name for m in livestock_list track by m.livestock_gid"
                                        ng-model="cbolivestockname" class="chosen-select form-control" required>
                                    <option value=""></option>
                                </select>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <label>Count</label>
                                <input type="text" ng-model="txtcount" tabindex="0" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <label> Breed </label>
                                <br />
                                <textarea rows="2" placeholder="Enter Breed..." aria-multiline="true" tabindex="0" aria-invalid="false"
                                          style="resize:vertical;" ng-model="txtbreed" class="form-control"></textarea>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <label>Insurance Status </label>
                                <div class="radio c-radio">
                                    <label>
                                        <input type="radio" name="Insurancestatus" value="Yes" ng-model="rdbinsurancestatus" ng-click="insurancestatus_yes()" />
                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                    </label>
                                    <label>
                                        <input type="radio" name="Insurancestatus" value="No" ng-model="rdbinsurancestatus" ng-click="insurancestatus_no()" />
                                        <span class="fa fa-circle"></span>No
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row" ng-show="insurancestatusyesshow">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-sm-12 col-sm-12 col-sm-12">
                                <label> Insurance Details </label>
                                <br />
                                <textarea rows="2" placeholder="Enter Observations..." aria-multiline="true" tabindex="0" aria-invalid="false"
                                          style="resize:vertical;" ng-model="txtlivestockinsurance_details" class="form-control"></textarea>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="livestocadd.$invalid" ng-click="livestock_submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>

                    </div>
                </form>
            </div>
        </div>
    </script>

