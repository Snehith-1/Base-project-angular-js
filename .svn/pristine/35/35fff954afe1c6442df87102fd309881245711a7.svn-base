                                            <strong class="ng-binding w3-text-indigo">
                                                <a class="btn btn-sm btn-default"
                                                   style="background-color:#09385f;color:white;"
                                                   ng-click="address_add()"><span class="fa fa-plus" /> Add Address</a>
                                            </strong>
                                        </div>
                                    </div>
