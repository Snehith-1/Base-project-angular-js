                                                                        <table class="table">
                                                                            <thead class="bg-gray text-white">
                                                                                <tr>
                                                                                    <th>S.No</th>
                                                                                    <th>Mobile No</th>
                                                                                    <th>Primary Status</th>
                                                                                    <th>Whatsapp Number </th>
                                                                                    <th>Action</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                                <tr ng-repeat="list in individualmobileno_list">
                                                                                    <td>{{$index+1}}</td>
