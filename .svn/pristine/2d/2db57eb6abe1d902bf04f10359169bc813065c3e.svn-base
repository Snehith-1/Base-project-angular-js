<h3>
    Live Stock
</h3>
<div ng-controller="MstLiveStockController">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
        <button type="button" title="Add Live Stock" class="btn btn-info btn-sm" ng-hide="lsoneapipage=='OneApi'" ng-click="addLiveStock()">
            <span class="btn-label">
                <i class="fa fa-plus"></i>
            </span>Add Live Stock
        </button>
        </div>
    </div>
    <br />
    <div class="panel">
        <br/>
        <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="table-responsive">
                    <table datatable="ng" class="table table-hover table-striped">
                        <thead>
                            <tr class="text-white" style="background-color:#09385f;">
                                <th style="display:none;"> S.No </th>
                                <th class="" ng-hide="lsoneapipage=='OneApi'"> Created Date </th>
                                <th> One API Code</th>
                                <th> Live Stock </th>
                                <th class="" ng-hide="lsoneapipage=='OneApi'"> LMS Code </th>
                                <th class="" ng-hide="lsoneapipage=='OneApi'"> Bureau Code </th>
                                <th class="" ng-hide="lsoneapipage=='OneApi'"> Created By </th>
                                <th> Status </th>
                                <th class="text-center" ng-hide="lsoneapipage=='OneApi'">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="data in livestock_list">
                                <td data-title="'S.No'" style="display:none;" class="text-center"> {{$index+1}} </td>
                                <td data-title="'Created Date'" class="" ng-hide="lsoneapipage=='OneApi'"> {{data.created_date}} </td>
                                <td data-title="'One API Code'">{{data.api_code}}</td>
                                <td data-title="'LiveStock name'"> {{data.livestock_name}} </td>
                                <td data-title="'LMS Code'" class="" ng-hide="lsoneapipage=='OneApi'"> {{data.lms_code}} </td>
                                <td data-title="'Bureau Code'" class="" ng-hide="lsoneapipage=='OneApi'"> {{data.bureau_code}} </td>
                                <td data-title="'Created By'" class="" ng-hide="lsoneapipage=='OneApi'"> {{data.created_by}} </td>
                                <td data-title="'Status'" ng-switch="data.status">
                                    <label class="label label-success" ng-switch-when='Active' style="font-size:10px;">Active</label>
                                    <label class="label label-warning" ng-switch-when='Inactive' style="font-size:10px;">InActive</label>
                                </td>
                                <td class="nowrap" data-title="'Action'" ng-hide="lsoneapipage=='OneApi'" style="text-align: center;" ng-switch="data.status">
                                    <button title="Edit" ng-click="editlivestock(data.livestock_gid)" class="btn btn-green">
                                        <em class="fa fa-edit"></em>
                                    </button>
                                    <button title="Status" ng-switch-when='Active' ng-click="Status_update(data.livestock_gid)" class="btn btn-success">
                                        <em class="fa fa-thumbs-up"></em>
                                    </button>
                                    <button title="Status" ng-switch-when='Inactive' ng-click="Status_update(data.livestock_gid)" class="btn btn-warning">
                                        <em class="fa fa-thumbs-down"></em>
                                    </button>
                                    <button title="Delete" ng-click="delete(data.livestock_gid)" class="btn btn-danger">
                                        <em class="icon icon-trash "></em>
                                    </button>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <br />
    </div>
    <script type="text/ng-template" id="/addLiveStock.html">
        <div class="modal-header bg-primary ng-scope">
            
            <h4 id="myModalLabel" class="modal-title">
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-plus "></i>
                </span> Add Live Stock
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="Addlivestock" ng-submit="form.Addlivestock()" autocomplete="off" class="form-validate form-horizontal">
                <div class="pd-sm-10">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="row ">
                                <div class="col-lg-12 col-md-12 col-sm-12 ">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtlivestock_name"  maxlength="255" required="" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched" >
                                        <label>Live Stock *</label>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="row ">
                                <div class="col-lg-12 col-md-12 col-sm-12 ">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtlms_code" maxlength="30" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                        <label>LMS Code </label>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="row ">
                                <div class="col-lg-12 col-md-12 col-sm-12 ">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="txtbureau_code"  maxlength="10" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                        <label>Bureau Code </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>                       
                        <div>
                            <br />
                            <div class="text-center">
                                <button type="button" class="btn btn-success btn-md" ng-disabled="Addlivestock.$invalid" ng-click="submit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                                <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                            </div>
                        </div>
                        <br/>
                    </div>
                </div>
            </form>
        </div>
    </script>
    <script type="text/ng-template" id="/editLiveStock.html">
        <div class="modal-header bg-primary ng-scope">

            <h4 id="myModalLabel" class="modal-title">
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-edit "></i>
                </span>Edit Live Stock
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="editlivestock" ng-submit="form.editlivestock()" autocomplete="off" class="form-validate form-horizontal">
                <div class="pd-sm-10">
                    <div class="row ">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="row ">
                                <div class="col-lg-12 col-md-12 col-sm-12 ">
                                    <div class="mda-form-group">
                                        <input ng-model="txteditlivestock_name"  maxlength="255" required tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                        <label>Live Stock *</label>
                                    </div>
                                </div>
                            </div>
                            <div class="row ">
                                <div class="col-lg-12 col-md-12 col-sm-12 ">
                                    <div class="mda-form-group">
                                        <input ng-model="txteditlms_code"  maxlength="30" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                        <label>LMS Code </label>
                                    </div>
                                </div>
                            </div>
                           
                            <div class="row ">
                                <div class="col-lg-12 col-md-12 col-sm-12 ">
                                    <div class="mda-form-group">
                                        <input ng-model="txteditbureau_code"  maxlength="10" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control form-control ng-pristine ng-invalid ng-invalid-required ng-touched">
                                        <label>Bureau Code </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>     
                  
                </div>
                <div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="editlivestock.$invalid" ng-click="update()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
                
            </form>
        </div>
    </script>
    <script type="text/ng-template" id="/statusLiveStock.html">
        <div class="modal-header bg-primary ng-scope">

            <h4 id="myModalLabel" class="modal-title">
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-thumbs-up"></i>
                </span>Status 
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="statuslivestock" ng-submit="form.statuslivestock()" autocomplete="off" class="form-validate form-horizontal">
                <div class="pd-sm-10">
                    <div class="row ">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="row ">
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label> Live Stock </label>
                                </div>
                                <div class="col-lg-8 col-md-8 col-sm-8">
                                    {{txtlivestock_name}}
                                </div>

                            </div>
                            <br/>
                            <div class="row ">
                                <div class="col-lg-4 col-md-4 col-sm-4">                                   
                                    <label>Status  </label>
                                </div>  
                                <div class="col-lg-8 col-md-8 col-sm-8"> 
                                    <div class="radio c-radio">
                                        <label>
                                            <input type="radio" name="rbo_status" value="Y" ng-model="rbo_status" ng-click="status_yes()" />
                                            &nbsp;&nbsp;<span class="fa fa-circle"></span> Active
                                        </label>
                                        <label>
                                            <input type="radio" name="rbo_status" value="N" ng-model="rbo_status" ng-click="status_no()" />
                                            <span class="fa fa-circle"></span> Inactive
                                        </label>
                                    </div>
                                </div>
                           
                        </div>                      
                            <div class="row ">
                                <div class="col-lg-12 col-md-12 col-sm-12 ">
                                    <div class="mda-form-group float-label">
                                        <textarea ng-model="txtremarks" style="resize:vertical;" required rows="4" aria-multiline="true" tabindex="0" aria-invalid="false" class="mda-form-control"></textarea>
                                        <label>Remarks *</label>
                                    </div>  
                                </div>
                            </div>
                        </div>
                    </div>     
                  
                </div>
                <div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="statuslivestock.$invalid" ng-click="update_status()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
                <br/>
                <div class="table-responsive" ng-show="livestockinactivelog_data!=null">
                    <div style="max-height:300px;overflow:auto;" >
                        <table class="table table-striped table-secondary">
                            <thead class="bg-gray text-white">
                                <tr width="100%">
                                    <th align="center" scope="col">S.No</th>
                                    <th align="center" scope="col">Status</th>
                                    <th align="center" scope="col">Updated By</th>
                                    <th align="center" scope="col">Updated Date</th>
                                    <th align="center" scope="col">Remarks</th>
                                    
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in livestockinactivelog_data" width="100%">
                                    <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                    <td align="left" scope="col" title="'Status'">{{data.status}} </td>
                                    <td align="left" scope="col" title="'Updated By'">{{data.updated_by}}</td>
                                    <td align="left" scope="col" title="'Updated Date'">{{data.updated_date}}</td>
                                    <td align="left" scope="col" title="'Remarks'">{{data.remarks}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </form>
        </div>
    </script>
</div>
                
          