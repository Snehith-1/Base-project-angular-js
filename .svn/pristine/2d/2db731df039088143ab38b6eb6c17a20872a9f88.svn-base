<style>
    .space {
        margin: 10px 0px;
    }
</style>
<h3>
    Assigned Lead View
</h3>
<div ng-controller="MstMarketingAssignedFollowupLeadsController as dp">
    <div class="panel">
        <div class="panel-heading bg-primary">
            <h5><i class="fa fa-eye"></i> &nbsp; Assigned Lead View </h5>
        </div>
        <div class="modal-body ng-scope">
            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Assigned Details </b></p>
                <hr class="space" />
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Lead Number</label><br>
                            {{txtticket_refid}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Lead Status</label><br>
                            {{txtcallclosure_status}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Assigned Employee</label><br>
                            {{txtassignemployee_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Assigned Date</label><br>
                            {{txtassign_date}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Entity</label><br>
                            {{txtentity_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Source of Lead</label><br>
                            {{txtsourceofcontact_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Lead Received Date</label><br>
                            {{txtcallreceived_date}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Lead Request Type</label><br>
                            {{txtcustomer_type}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">

                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Lead Name</label><br>
                            {{txtcaller_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Internal Reference</label><br>
                            {{txtinternalreference_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Office Landline Number</label><br>
                            {{txtoffice_landlineno}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label for="">Company / Organisation Name</label><br>
                            {{txtcallerassociate_company}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">

                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Enquiry Type</label><br>
                            {{txtcalltype_name}}
                        </div>

                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>TAT in Hrs</label><br>
                            {{txttat_hours}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Base Location</label><br>
                            {{txtbase_location}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Origination</label><br>
                            {{origination}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-3 col-lg-3 col-sm-3">
                            <label>Industry</label><br>
                            {{txtindustry_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3" ng-show="origination =='Online'">
                            <label>What do you require</label><br>
                            {{txtleadrequire_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3" ng-show="origination =='Millet'">
                            <label>What do you require</label><br>
                            {{txtmilletrequire_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3" ng-show="origination =='Enquiry'">
                            <label>What do you require</label><br>
                            {{txtenquiryrequire_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3" ng-show="origination =='Enquiry'">
                            <label>Stage of the Startup</label><br>
                            {{txtstartuprequire_name}}
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-3" ng-show="origination =='Enquiry'">
                            <label>Please Describe your Business</label><br>
                            {{txtbusiness_name}}
                        </div>
                    </div>
                </div>
                <br>

            </div>

            <div class="dd-handle ng-scope">

                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <label>Lead Description</label><br>
                            {{txtenquiry_description}}
                        </div>
                    </div>
                    <br>
                </div>
            </div>
            <br>
            <div class="row">
                <div class="col-lg-8 col-sm-8 col-md-8">
                    <div class="dd-handle ng-scope">
                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Address Details </b></p>
                        <hr class="space" />
                        <div ui-scroll-viewport="" style="height:320px; overflow-y: auto;overflow-x: hidden;" class="row">
                            <div class="row" ng-repeat="data in ibcalladdress_list">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="dd-handle ng-scope" ng-repeat="data in ibcalladdress_list">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <div class="col-lg-4 col-sm-4 col-md-4" style="color:#3f51b5;font-size:100%;font-weight :bold;">
                                                        <label class="label" style="font-size:12px;background-color:#09385f;">Address Type: {{data.addresstype_name}} </label>
                                                    </div>
                                                    <div class="col-lg-4 col-sm-4 col-md-4" style="color:#3f51b5;font-size:100%;font-weight :bold;">
                                                        Primary Status:  {{data.primary_status}}
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <div class="col-lg-5 col-sm-5 col-md-5">
                                                        <label style="font-weight:bold">Address Line 1</label>
                                                        <br />
                                                        {{data.addressline1}}
                                                    </div>
                                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                                        <label style="font-weight:bold">Address Line 2</label>
                                                        <br />
                                                        {{data.addressline2}}
                                                    </div>
                                                    <div class="col-lg-3 col-sm-3 col-md-3">
                                                        <label style="font-weight:bold">Landmark </label>
                                                        <br />
                                                        {{data.landmark}}
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <div class="col-lg-2 col-sm-2 col-md-2">
                                                        <label style="font-weight:bold">Postal Code </label>
                                                        <br />
                                                        {{data.postal_code}}
                                                    </div>
                                                    <div class="col-lg-2 col-sm-2 col-md-2">
                                                        <label style="font-weight:bold">City </label>
                                                        <br />
                                                        {{data.city}}
                                                    </div>
                                                    <div class="col-lg-2 col-sm-2 col-md-2">
                                                        <label style="font-weight:bold">State </label>
                                                        <br />
                                                        {{data.state}}
                                                    </div>
