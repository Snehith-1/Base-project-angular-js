                <em style="font-size:16px" class="fa fa-eye"></em>&nbsp;<label style="font-size:16px">Warehouse Details</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">

                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <br />
                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Warehouse Details </b></p>
                        <hr />
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <label class="col-form-label">Warehouse agency (WSP) </label>
                            <br />
                            {{cbowarehouseagency}}
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <label class="col-form-label">Warehouse name </label>
                            <br />
                            {{cbowarehouse_name}}
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <label class="col-form-label">Warehouse type </label>
                            <br />
                            {{cbowarehousetype_name}}
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">

                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <label><strong>Total Capacity (In Area)</strong></label>
                            <br />

                            {{txtareacapacity}}
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <label><strong>Capacity Area UOM</strong></label>
                            <br />

                            {{txtareacapacity_uom}}
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <label>Total Capacity (Volume) </label>
                            <br />

                            {{txtcapacity_volume}}
                        </div>

                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <label><strong>Volume UOM</strong> </label>
                            <br />

                            {{txtvolume_uom}}
                        </div>
                        <div class="col-lg-8 col-md-8 col-sm-8">
                            <label class="col-form-label">Warehouse Address </label>
                            <br />

                            {{cbowarehouseaddress}}
                        </div>

                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <label class="col-form-label"> Capacity accepted as per standards state wise (commodity wise) </label>

                            <br />
                            {{txtcapacity_commodity}}

                        </div>

                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">

                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <label class="col-form-label"> Capacity accepted as per standards commodity wise (PAN Ina) </label>
                            <br />

                            {{txtcapacity_panina}}

                        </div>
                    </div>
                </div>
                <br />

                <div class="text-center">

                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>

                </div>

            </div>
        </div>
    </script>
        </div>
