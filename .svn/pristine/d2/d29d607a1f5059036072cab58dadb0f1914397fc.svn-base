<style>
    .formstatic {
        position: fixed;
        width:22%;
    }
    .formstaticbtn:hover {
        background-color: rgb(89, 86, 90);
        color:rgb(31, 26, 102);
        text-decoration: underline;
        font-weight: bold;
    }
    

</style>
<h3>
    Credit Underwriting
</h3>
<div ng-controller="MstCreditGroupDtlAddController as form">
        <div class="panel">
            <form name="form.formValidate" ng-submit="form.submitForm()" novalidate="" class="form-validate form-horizontal">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="panel-heading" style="background-color: #191370;color:#ffffff"><b>Add Credit Group Details</b> 
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-3 col-md-3 col-sm-3" style="padding-left:10px;" >
                                    <div class="">
                                    <div id="panelPortlet13" class="panel">
                                        <div class="panel-heading portlet-handler text-bold text-center  align-middle" style="background-color:#F1EFEF"><h style="font-size:14px" class="m10">Credit Group Add Forms</h></div>
                                        
                                        <div class="panel-body">
                                            <div class="panel widget">
                                                <!--<div class="row row-table formstaticbtn"  ng-click="bureauadd()" style="background-color:#f7f5f5">
                                                    <div class="col-xs-2 text-center  pv-lg">
                                                        <em class="fa fa-plus fa-x"></em>
    
                                                    </div>
                                                    <div class="col-xs-10 pv-lg">
                                                        <div class="m10" style="font-size: 13px;"><b> Add Bureau Details</b></div>
                                                    </div>
                                                </div>-->
                                                <div class="row row-table formstaticbtn"  ng-click="group_bankaccount()" style="background-color:#e9e7e7;" >
                                                    <div class="col-xs-2 text-center  pv-lg">
                                                        <em class="fa fa-plus fa-x"></em>
    
                                                    </div>
                                                    <div class="col-xs-10 pv-lg">
                                                        <div class="m10" style="font-size: 13px;"><b>Add Bank Account Details </b></div>
                                                    </div>
                                                </div>
                                                <div class="row row-table formstaticbtn"  ng-click="group_existingbankaccount()" style="background-color:#f7f5f5;" >
                                                    <div class="col-xs-2 text-center  pv-lg">
                                                        <em class="fa fa-plus fa-x"></em>
    
                                                    </div>
                                                    <div class="col-xs-10 pv-lg">
                                                        <div class="m10" style="font-size: 13px;"><b>Add Existing Bank Facility </b></div>
                                                    </div>
                                                </div>
                                                <div class="row row-table formstaticbtn"  ng-click="group_repayment()" style="background-color:#e9e7e7;" >
                                                    <div class="col-xs-2 text-center  pv-lg">
                                                        <em class="fa fa-plus fa-x"></em>
    
                                                    </div>
                                                    <div class="col-xs-10 pv-lg">
                                                        <div class="m10" style="font-size: 13px;"><b>Add Repayment Track Record </b></div>
                                                    </div>
                                                </div>
                                                <div class="row row-table formstaticbtn"  ng-click="group_PSLdata()" style="background-color:#f7f5f5;" >
                                                    <div class="col-xs-2 text-center  pv-lg">
                                                        <em class="fa fa-plus fa-x"></em>
    
                                                    </div>
                                                    <div class="col-xs-10 pv-lg">
                                                        <div class="m10" style="font-size: 13px;"><b>Add PSL Data Flagging </b></div>
                                                    </div>
                                                </div>
                                                <div class="row row-table formstaticbtn"  ng-click="module()" style="background-color:#e9e7e7;" >
                                                    <div class="col-xs-2 text-center  pv-lg">
                                                        <em class="fa fa-plus fa-x"></em>
    
                                                    </div>
                                                    <div class="col-xs-10 pv-lg">
                                                        <div class="m10" style="font-size: 13px;"><b>Bank Statement Analysis </b></div>
                                                    </div>
                                                </div>
                                                <div class="row row-table formstaticbtn"  ng-click="group_observation()" style="background-color:#f7f5f5;" >
                                                    <div class="col-xs-2 text-center  pv-lg">
                                                        <em class="fa fa-plus fa-x"></em>
    
                                                    </div>
                                                    <div class="col-xs-10 pv-lg">
                                                        <div class="m10" style="font-size: 13px;"><b>Norms Details</b></div>
                                                    </div>
                                                </div>
                                            </div>                                                  
    
                                            </div>
                                        </div>
                                    </div>
                                </div> 
                                <div class="col-lg-9 col-md-9 col-sm-9" style="padding-left:0px;" style="padding-right:32px" >
                                    <div class="panel">
                                        <div class="panel-heading portlet-handler text-bold" style="background-color:#F1EFEF;"><h style="font-size:14px" class="m10">Add Bureau Updates</h></div>
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                            <div class="row">
                                                                    <div class="col-sm-12 col-md-12 col-lg-12">
                                                                        <div class="dd-handle ng-scope">
                                                                            <div class="row"> 
                                                                                <div class="col-sm-12 col-md-12 col-lg-12">
                                                                                <p class="text-primary" style="font-size:14px;"><b> Bureau Details </b></p>
                                                                                <hr />
                                                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                                                    <label class="col-form-label"> Bureau Name <span style="color:red"> *</span></label>
                                                                                    <br />
                                                                                    <select name="BureauName" id="BureauName" chosen="" ng-options="m.geneticcode_name for m in genetic_list" 
                                                                                    type="text" class="form-control parsley-error" required="" ng-model="cboBureauName">
                                                                                        <option value=""> ----- Select Bureau Name -----</option>    
                                                                                    </select>
                                                                                    <span ng-show="form.validateInput('BureauName', 'required')" class="text-danger">This field is required</span>
                                                                                </div>
                                                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                                                    <label class="col-form-label"> Bureau Score <span style="color:red"> *</span></label>
                                                                                    <br />
                                                                                    <input type="text" ng-model="txtBureau_Score" name="BureauScore" id="BureauScore" ng-pattern="/^[0-9+]*$/" tabindex="0" class="form-control" required=""/>
                                                                                    <span ng-show="form.validateInput('BureauScore', 'required')" class="text-danger">This field is required</span>
                                                                                </div>
                                                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                                                    <div ng-controller="MstCreditGroupDtlAddController as dp">
                                                                                        <label class="col-form-label"> Score As On </label>
                                                                                        <br />
                                                                                        <p class="input-group">
                                                                                            <input type="text" id="scoreason" name="scoreason" ng-model="$parent.txtscoreas_on" datepicker-popup="{{dp.format}}"
                                                                                                   style="height:36px;" is-open="dp.open1" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                                                                                   close-text="Close" class="form-control" placeholder="DD-MM-YYYY" required="" />
                                                                                            <span class="input-group-btn">
                                                                                                <button type="button" ng-click="dp.calender1($event)" style="height:36px;" class="btn btn-default">
                                                                                                    <em class="fa fa-calendar"></em>
                                                                                                </button>
                                                                                            </span>
                                                                                        </p>
                                                                                    </div>
                                                                                </div>
                                                                                </div>
                                                                            </div>
                                                                            <br/>
                                                                            <div class="row">
                                                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                                                        <label>Observation(s) </label>
                                                                                        <textarea style="resize:vertical;" rows="4" name="observations" id="observations" placeholder="Enter Observation(s)..." aria-multiline="true" tabindex="0" aria-invalid="false"
                                                                                                  ng-model="txtObservation" class="form-control"  required=""></textarea>
                                                                                        <br />
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <div class="row">
                                                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                                                        <label> Bureau Response </label>
                                                                                        <textarea style="resize:vertical;" rows="4" name="BureauResponse" id="BureauResponse" placeholder="Enter Bureau Response..." aria-multiline="true" tabindex="0" aria-invalid="false"
                                                                                                  ng-model="txtBureau_Response" class="form-control"  required=""></textarea>
                                                                                        <br />
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <div class="row">
                                                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                                                    <div class="col-lg-12 col-md-12 col-sm-12 dd-handle ng-scope">
                                                                                        <div class="row">
                                                                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                                                        <p class="text-primary" style="font-size:14px;"><b> Upload Report </b></p>
                                                                                        <hr />
                                                                                        <div class="col-lg-2 col-sm-2 col-md-2">
                                                                                            <div class="float-label">
                                                                                                <label> Document Upload  : </label>
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="col-lg-9 col-sm-9 col-md-9">
                                                                                            <input type="file" id="chequefilefile" tabindex="5" multiple onchange="angular.element(this).scope().documentUpload(this.files, angular.element(this).scope().documentupload, this.name)"
                                                                                            ng-model="txtreportdoc_name" data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                                                                        </div>
                                                                                        </div>
                                                                                        </div>
                                                                                        <div class="row">
                                                                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                                                                    <br />
                                                                                                    <div class="table-responsive">
                                                                                                        <table class="table table-striped table-secondary">
                                                                                                            <thead class="bg-gray text-white">
                                                                                                                <tr width="100%">
                                                                                                                    <th scope="col">S.No</th>
                                                                                                                    <th scope="col">Document Name</th>
                                                                                                                    <th scope="col">Action</th>
                                                                                                                </tr>
                                                                                                            </thead>
                                                                                                            <tbody>
                                                                                                                <tr ng-repeat="data in institutionbankacct_list">
                                                                                                                    <td title="S.No">{{$index+1}}</td>
                                                                                                                    <td title="Document Name">{{data.document_name}}</td>
                                                                                                                    <td data-title="Action">
                                                                                                                        <button title="Download" class="btn btn-green btn-xs"
                                                                                                                                ng-click="downloads(data.document_path,data.document_name)">
                                                                                                                            <em class="fa fa-download"></em>
                                                                                                                        </button>
                                                                                                                        <a title="Delete" class="btn btn-xs btn-danger" ng-click="uploaddocumentcancel(data.tmpcicdocument_gid)">
                                                                                                                            <span class="fa fa-trash-o"></span>
                                                                                                                        </a>
                                                                                                                    </td>
                                                                                                                </tr>
                                                                                                            </tbody>
                                                                                                        </table>
                                                                                                    </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <br/>
                                                                            <div class="row">
                                                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                                                    <div class="text-center">
                                                                                        <button type="submit" class="btn btn-primary" style="background-color:#09385f;color:white;"
                                                                                                 ng-click="add_bureauDtl()"> <i class="fa fa-plus"></i>&nbsp;&nbsp;Add</button>
                    
                                                                                     </div>
                                                                                </div>
                                                                            </div>
                                                                            <br/>
                                                                            <div class="row">
                                                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                                                    <div class="col-lg-12 col-md-12 col-sm-12 dd-handle ng-scope">
                                                                                        <p class="text-primary" style="font-size:14px;"><b> Bureau Summary </b></p>
                                                                                        <hr />
                                                                                            <div class="table-responsive table-bordered">
                                                                                                <table class="table">
                                                                                                    <thead class="bg-gray text-white">
                                                                                                        <tr>
                                                                                                            <th>S.No</th>
                                                                                                            <th>Bureau Name </th>
                                                                                                            <th>Bureau Score </th>
                                                                                                            <th>Score As On</th>
                                                                                                            <th>Action</th>
                                                                                                        </tr>
                                                                                                    </thead>
                                                                                                    <tbody>
                                                                                                        <tr ng-repeat="data in mstgeneticcode_list">
                                                                                                            <td>{{$index+1}}</td>
                                                                                                            <td>{{data.bureau_name}}</td>
                                                                                                            <td>{{data.bureau_score}}</td>
                                                                                                            <td>{{data.score_on}}</td>
                                                                                                            <td class="text-nowrap">
                                                                                                                <button title="Edit" type="button" ng-click="bureau_edit()" class="btn btn-green btn-xs">
                                                                                                                    <em class="fa fa-edit"></em>
                                                                                                                </button>
                                                                                                                <button title="Delete" type="button" ng-click="bureau_delete()"
                                                                                                                        class="btn btn-danger btn-xs">
                                                                                                                    <em class="fa fa-trash-o"></em>
                                                                                                                </button>
                                                                                                            </td>
                                                                                                        </tr>
                                                                                                    </tbody>
                                                                                                </table>
                                                                                            </div>
                                                                                       
                                                                                    </div>
                                                                                </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                            </div>

                                                            <br/>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="text-center">
                                <br />
                                <button ng-click="Back()" type="button" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                            </div>
                            <br />
                        </div>
                    </div>
                </div>
            </div>
        </form>
        </div>
</div>