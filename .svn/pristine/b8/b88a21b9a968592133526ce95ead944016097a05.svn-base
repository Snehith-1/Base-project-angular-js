<h3>
    View Author Details
</h3>
<div ng-controller="viewAuthorcontroller">
    <div id="panelDemo8" class="panel panel-primary">
        <div class="panel-heading">
            View Author Details
        </div>
        <div class="panel-body">
            <div class="table-responsive">
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">First Name </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{author_details.author_firstnameedit}}
                            </div>
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Last Name </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{author_details.author_lastnameedit}}
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Author Code  </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{author_details.author_codeedit}}
                            </div>
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold"> Email Address  </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{author_details.author_emailidedit}}
                            </div>
                        </div>
                    </div>
                    <br />

                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Mobile No </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{author_details.author_mobnoedit}}
                            </div>
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Address </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{author_details.author_address1edit}}
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                <b>Bank Details :</b>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Account Name </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{author_details.acc_nameedit}}
                            </div>
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Account No</label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{author_details.acc_noedit}}
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">IFSC Code</label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{author_details.ifsc_codeedit}}
                            </div>
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Bank Name</label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{author_details.bank_nameedit}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="panel-footer text-center">
            <div>

                <a class="btn btn-info" ng-click="back()">
                    <i class="fa fa-arrow-left" style="font-size:15px;">
                        &nbsp;Back
                    </i>
                </a>
            </div>
        </div>
    </div>


</div>
