<style>
    .modal-content {
        margin-top: 120px;
    }
</style>
<h3>
    SA Activity Management
</h3>
<div ng-controller="MstSAInstitutionActivityManagementController">

    <br />
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="panel panel-default">


            </div>
            <div id="Code creation pending">
                <p style="color:#09385f;font-size: 17px;"><b>Management Summary</b></p>
                <hr />

                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="institution_completed()"
                            style="width: 200px; height: 50px; border: 1px solid rgb(179, 177, 177); background-color: #84cbdb;">
                        <div class="pull-right label"
                             style="background-color:#838383;padding-top:5px;padding-bottom:5px;">
                            <b>
                                {{institutioninitiated_count}}
                            </b>
                        </div>
                        <b style="font-size: 14px;">Institution</b>
                    </button>
                    <br />
                </div>

                <div class="col-lg-2 col-md-6 col-sm-12">

                    <button type="button" class="btn btn-labeled" ng-click="individual_completed()"
                            style="width:200px;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label"
                             style="background-color:#838383;padding-top:5px;padding-bottom:5px;">
                            <b>
                                {{individualinitiated_count}}
                            </b>
                        </div>
                        <b style="font-size: 14px;">Individual</b>
                    </button>
                    <br />
                </div>
            </div>
        </div>
    </div>

    <br />
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="col-lg-10 col-md-12 col-sm-12">
            </div>
            <div class="col-lg-2 col-md-6 col-sm-12">
                <a style="background-color:#edb9b9;" class="ng-binding ng-scope label label-pink">
                    <em class="fa"></em>
                </a>&nbsp;<label> - Color Indicates Renewed Cases</label>
            </div>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="table-responsive">
                    <table datatable="ng" class="table table-hover table-striped">
                        <thead>
                            <tr class="text-white" style="background-color:#09385f;">
                                <th> S.No </th>
                                <th>SA Application ID </th>
                                <th> Login ID </th>
                                <th>Samunnati Associate Name </th>
                                <th>SA Type</th>
                                <th>SA Entity Type</th>
                                <th> Referred By</th>
                                <!--<th> Approval Status</th>-->
                                <th> Origination</th>
                                <th> Renewal Status</th>
                                <th> RM Name</th>
                                <!--<th>Approval By</th>
    <th>Approval Date</th>
    <th>Allocated By</th>-->
                                <th>Submitted Date</th>

                                <th> Action </th>


                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="data in saOnboardSummary_list">
                                <td>{{$index+1}}</td>
                                <td>{{data.sa_autogeneratedid}}</td>
                                <td>{{data.samagro_code}}</td>
                                <td>{{data.sa_associatename}}</td>
                                <td>{{data.satype_name}}</td>
                                <td>{{data.saentitytype_name}}</td>
                                <td>{{data.referred_by}}</td>
                                <!--<td>{{data.approvalstatus}}</td>-->
                                <td>{{data.recordsource}}</td>
                                <td>{{data.renewal_status}}</td>
                                <td>{{data.created_by}}</td>
                                <!--<td>{{data.approved_by}}</td>-->
                                <!--<td>{{data.approved_date}}</td>-->
                                <!--<td>{{data.sa_updated_by}}</td>-->
                                <td>{{data.sa_updated_date}}</td>
                                <!--<td>{{data.sa_tat}}</td>-->
                                <td class="nowrap" data-title="'Action'" style="text-align: center;">
                                    <button title="View" ng-click="activitymanagement(data.sacontactinstitution_gid,data.samfin_code,data.new_code)" class="btn btn-info">
                                        <em class="fa fa-eye"></em>
                                    </button>

                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <br />
</div>

<script type="text/ng-template" id="/empresetpassowrd.html">
    <div class="modal-header bg-primary ng-scope">

        <h4 id="myModalLabel" class="modal-title">
            &emsp;
            <span class="btn-label bg-primary">
                <i class="fa fa-lock "></i>
            </span>RESET PASSWORD
        </h4>
    </div>
    <div class="modal-body ng-scope">
        <form novalidate="" name="userform" ng-submit="form.submitForm1()" autocomplete="off" class="form-validate form-horizontal">
            <div class="pd-sm-10">

                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">

                        <div class="col-sm-1"></div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <strong class="mt0">Samfin Code / Samagro Code</strong>
                        </div>
                        {{samfin_code}}
                        <!--<div class="col-lg-6 col-md-6 col-sm-6">-->
                        <!--<input class="form-control" type="text" ng-model="employee_code" maxlength="20" name="productgroupcode" tabindex="0" readonly />-->
                        <!--</div>-->


                    </div>
                </div>
                <br />

                <!--<div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-sm-1"></div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <strong class="mt0">Sam Agro Code</strong>
                        </div>
                        {{samagro_code}}-->
                <!--<div class="col-lg-6 col-md-6 col-sm-6">
                    <input class="form-control" type="text" ng-model="employee_name" maxlength="30" name="productgroupname" tabindex="1" readonly />
                </div>-->
                <!--</div>
                </div>-->
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-sm-1"></div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <strong class="mt0">Password</strong><span class="text-danger">*</span>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <input type="text" name="Password" required="" minlength="8" maxlength="20" placeholder="Enter Password"
                                   ng-pattern="/(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[\W])/" ng-model="Password" class="form-control" />
                            <span class="input-group-btn">
                                <button type="button" style="height:35px;" ng-show="eye" ng-click="show()" class="btn btn-default"><em class="fa fa-eye"></em></button>
                                <button type="button" style="height:35px;" ng-show="eyeslash" ng-click="noshow()" class="btn btn-default "><em class="fa fa-eye-slash"></em></button>
                            </span>
                            <span ng-show="userform.Password.$dirty &amp;&amp; userform.Password.$error.required" class="text-danger">This field is required</span>
                            <span ng-show="userform.Password.$dirty &amp;&amp; userform.Password.$error.pattern" class="text-danger">Password should contain: "Atleast one upper case letter, one lower case letter, one special character, one number and minimum eight characters."</span>
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-sm-1"></div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <strong class="mt0">Confirm Password</strong><span class="text-danger">*</span>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <input type="text" name="retypepassword" required="" minlength="8" maxlength="20" placeholder="Confirm Password"
                                   ui-validate="'$value==Password'" ui-validate-watch="'Password'" ng-model="retypepassword" class="form-control" />
                            <span ng-show="userform.retypepassword.$dirty &amp;&amp; userform.retypepassword.$error.validator" class="text-danger">Password does Not match</span>
                        </div>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="userform.$invalid" ng-click="passwordSubmit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
            </div>
        </form>
    </div>


</script>
<script type="text/ng-template" id="/Loginstatus.html">
    <div class="modal-content" style="margin-top:0px">
        <div class="modal-header bg-primary ng-scope">

            <h4 id="myModalLabel" class="modal-title">
                &emsp;
                <span class="btn-label bg-primary">
                    <i class="fa fa-thumbs-up"></i>
                </span>Login Status
            </h4>
        </div>
        <div class="modal-body ng-scope">
            <form novalidate="" name="statusloantype" ng-submit="form.webaccessstatus()" autocomplete="off" class="form-validate form-horizontal">
                <div class="pd-sm-10">
                    <div class="row ">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="row ">
                                <div class="col-sm-4 col-sm-4 col-sm-4">
                                    <label> Samfin / Samagro Code </label>
                                </div>
                                <div class="col-sm-8 col-sm-8 col-sm-8">
                                    {{txtcode}}
                                </div>

                            </div>
                            <br />
                            <div class="row ">
                                <div class="col-sm-4 col-sm-4 col-sm-4">
                                    <label> Login Status  </label>
                                </div>
                                <div class="col-sm-8 col-sm-8 col-sm-8">
                                    <div class="radio c-radio">
                                        <label>
                                            <input type="radio" name="rbo_status" value="Y" ng-model="rbo_status" ng-click="status_yes()" />
                                            &nbsp;&nbsp;<span class="fa fa-circle"></span> Activate
                                        </label>
                                        <label>
                                            <input type="radio" name="rbo_status" value="N" ng-model="rbo_status" ng-click="status_no()" />
                                            <span class="fa fa-circle"></span> DeActivate
                                        </label>
                                    </div>
                                </div>

                            </div>
                            <div class="row ">
                                <div class="col-sm-12 col-sm-12 col-sm-12 ">
                                    <div class="mda-form-group float-label">
                                        <textarea ng-model="txtremarks" style="resize:vertical;" required rows="4" aria-multiline="true" tabindex="0" aria-invalid="false" class="mda-form-control"></textarea>
                                        <label>Remarks *</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success btn-md" ng-disabled="statusloantype.$invalid" ng-click="update_status()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </div>
                <br />
                <div class="table-responsive" ng-show="log_list!=null">
                    <div style="max-height:300px;overflow:auto;">
                        <table class="table table-striped table-secondary">
                            <thead class="bg-gray text-white">
                                <tr width="100%">
                                    <th align="center" scope="col">S.No</th>
                                    <th align="center" scope="col">Status</th>
                                    <th align="center" scope="col">Updated By</th>
                                    <th align="center" scope="col">Updated Date</th>
                                    <th align="center" scope="col">Remarks</th>

                                </tr>
                            </thead>
                            <tbody>
                                <tr width="100%" ng-repeat="data in log_list">
                                    <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                    <td data-title="'Status'" ng-switch="data.status_log">
                                        <label class="label label-success" ng-switch-when='Y' style="font-size:10px;">Active</label>
                                        <label class="label label-warning" ng-switch-when='N' style="font-size:10px;">InActive</label>
                                    </td>
                                    <td align="left" scope="col" title="'Updated By'">{{data.created_by}}</td>
                                    <td align="left" scope="col" title="'Updated Date'">{{data.created_date}}</td>
                                    <td align="left" scope="col" title="'Remarks'">{{data.remarks}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </form>
        </div>
    </div>
</script>
