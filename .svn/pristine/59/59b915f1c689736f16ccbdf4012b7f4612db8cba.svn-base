<h3>
    Buyer / Supplier Onboarding - Rejected 
</h3>
<div ng-controller="AgrMstCustomerOnboardRejectedSummarycontroller">
    
    <div class="card">
        <md-content class="md-padding">
            <md-tabs md-dynamic-height="" md-border-bottom="">
                <md-tab label="Buyer Onboarding - Rejected" md-on-select="GetBuyeronboardingrejectedlist()">
                    <md-tab-label>
                        Buyer Onboarding - Rejected
                        &emsp;<i class='pull-right label label-primary'>{{approvalcount.BuyerRejected}}</i>
                    </md-tab-label>
                    <md-tab-body>
                        <md-content class="md-padding">

                            <div class="">
                                <h4 class="text-pink">
                                    Buyer Onboarding - Rejected
                                </h4>
                                <div class="panel-body">
                                    <div class="table-responsive">
                                        <table datatable="ng" class="table table-hover table-striped">
                                            <thead class="nowrap">
                                                <tr class="text-white" style="background-color:#09385f;">
                                                    <th> S.No </th>
                                                    <th> Buyer ID </th>
                                                    <th> Customer/Buyer Name </th>
                                                    <!--<th> Vertical </th>-->
                                                    <th style="width:10%;"> Applicant Type </th>
                                                    <th> Submitted By </th>
                                                    <th> Submitted Date </th>
                                                    <th> Rejected By </th>
                                                    <th> Rejected Date </th>
                                                    <th class="text-center" style="width:14%">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in buyerapplicationlist">
                                                    <td data-title="'S.No'" class="text-center">{{$index+1}}</td>
                                                    <td data-title="'Buyer ID'">  {{data.application_no}} </td>
                                                    <td data-title="'Applicant Name">  {{data.customer_name}} </td>
                                                    <!--<td data-title="'Vertical">  {{data.vertical_name}} </td>-->
                                                    <td data-title="'Applicant Type" style="width:10%;">  {{data.applicant_type}} </td>
                                                    <td data-title="'Submitted By'">	 {{data.created_by}} </td>
                                                    <td data-title="'Submitted Date'">	 {{data.submittedto_approval}} </td>
                                                    <td data-title="'Rejected By'">	 {{data.onboard_approvedby}} </td>
                                                    <td data-title="'Rejected Date'">
                                                        {{data.onboard_approveddate}}
                                                    </td>
                                                    <td class="nowrap" data-title="'Action'" style="text-align: center;width:14%;" ng-switch="data.application_status">
                                                        <button title="View" ng-click="applcreation_view(data.application_gid)" class="btn btn-info">
                                                            <em class="fa fa-eye"></em>
                                                        </button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                </div>
                            </div>
                        </md-content>
                        <br/>
                        <div class="text-center">
                            <button type="button" ng-click="backbtn()" title="Back" class="btn btn-sm btn-info">
                                <em class="fa fa-arrow-circle-left fa-1x">&nbsp;Back</em>
                            </button>
                        </div>
                        
                    </md-tab-body>
                    
                </md-tab>


                <md-tab md-on-select="Getsupplieronboardingrejectedlist()">
                    <md-tab-label>
                        Supplier Onboarding - Rejected
                        &emsp;<i class='pull-right label label-primary'>{{approvalcount.SupplierRejected}}</i>
                    </md-tab-label>
                    <md-tab-body>
                        <md-content class="md-padding"> 
                            <div class="">
                                <h4 class="text-pink">
                                    Supplier Onboarding - Rejected
                                </h4>
                                <div class="panel-body">
                                    <div class="table-responsive">
                                        <table datatable="ng" class="table table-hover table-striped">
                                            <thead class="nowrap">
                                                <tr class="text-white" style="background-color:#09385f;">
                                                    <th> S.No </th>
                                                    <th> Supplier ID </th>
                                                    <th> Customer/Supplier Name </th>
                                                    <!--<th> Vertical </th>-->
                                                    <th style="width:10%;"> Applicant Type </th>
                                                    <th> Submitted By </th>
                                                    <th> Submitted Date </th>
                                                    <th> Rejected By </th>
                                                    <th> Rejected Date </th>
                                                    <th class="text-center" style="width:14%">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in supplierapplicationlist">
                                                    <td data-title="'S.No'" class="text-center"> {{$index+1}}</td>
                                                    <td data-title="'Buyer ID'">  {{data.application_no}} </td>
                                                    <td data-title="'Applicant Name">  {{data.customer_name}} </td>
                                                    <!--<td data-title="'Vertical">  {{data.vertical_name}} </td>-->
                                                    <td data-title="'Applicant Type" style="width:10%;">  {{data.applicant_type}} </td>
                                                    <td data-title="'Submitted By'">	 {{data.created_by}} </td>
                                                    <td data-title="'Submitted Date'">	 {{data.submittedto_approval}} </td>
                                                    <td data-title="'Rejected By'">	 {{data.onboard_approvedby}} </td>
                                                    <td data-title="'Rejected Date'">
                                                        {{data.onboard_approveddate}}
                                                    </td>
                                                    <td class="nowrap" data-title="'Action'" style="text-align: center;width:14%;" ng-switch="data.application_status">
                                                        <button title="View" ng-click="suprapplcreation_view(data.application_gid)" class="btn btn-info">
                                                            <em class="fa fa-eye"></em>
                                                        </button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>
                        </md-content>
                        <br />
                        <div class="text-center">
                            <button type="button" ng-click="backbtn()" title="Back" class="btn btn-sm btn-info">
                                <em class="fa fa-arrow-circle-left fa-1x">&nbsp;Back</em>
                            </button>
                        </div>

                    </md-tab-body>
                    
                </md-tab>
            </md-tabs>

        </md-content>
    </div>
</div>
