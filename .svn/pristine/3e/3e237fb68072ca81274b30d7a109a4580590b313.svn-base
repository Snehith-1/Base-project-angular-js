<h3>
    View Ticket Details
</h3>
<div ng-controller="viewTicketDetailscontroller">
    <div id="panelDemo8" class="panel panel-primary">
        <div class="panel-heading">
          View Ticket Details 
        </div>
        <div class="panel-body">
            <div class="table-responsive">
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Category </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{ticket_details.category_name}}
                            </div>
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Sub Category </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{ticket_details.subcategory_name}}
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Type  </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{ticket_details.category_name}}
                            </div>
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold"> Ticket Subject </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{ticket_details.complaint_title}}
                            </div>
                        </div>
                    </div>
                    <br />

                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Ticket Description </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{ticket_details.complaint_remarks}}
                            </div>
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Department Approval </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{ticket_details.department_manager}}
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Manager Approval </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{ticket_details.manager}}
                            </div>
                            <div class="col-lg-2 col-sm-2 col-md-2">
                                <label style="font-weight:bold">Service Manager Approval </label>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-md-4">
                                {{ticket_details.service_manager}}
                            </div>
                        </div>
                    </div>
                    <br />
                   
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-sm-3 col-md-3 col-lg-3">
                                <strong class="ng-binding w3-text-indigo">
                                    <em class="fa fa-file text-primary fa-lg"></em>&emsp;
                                    UPLOADED DOCUMENT
                                </strong>
                            </div>
                        </div>
                    </div>
                    <div class="dd-handle ng-scope">
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="table-responsive">
                                    <table class="table table-striped table-secondary">
                                        <thead class="bg-gray text-white">
                                            <tr width="100%">
                                                <th scope="col">S.No</th>
                                                <th scope="col">Document Name</th>
                                                <th scope="col">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in viewDocumentList" width="100%">
                                                <td title="'S.No'">{{$index+1}}</td>
                                                <td title="'Document Name'">{{data.document_name}}</td>
                                                <td data-title="'Action'">
                                                    <span class="fa fa-download" ng-click="downloads(data.document_path,data.document_name)" />
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

                    </div>
                    <br />
                </div>
                <br />
                
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <label style="font-size:14px" class="text-primary">
                                Ticket Approval details
                            </label>
                        </div>
                    </div>
                    <br />
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="table-responsive">
                            <table class="table hover table-striped row-border">
                                <thead>
                                    <tr class="bg-gray text-white">
                                        <th>Approval</th>
                                        <th>Approval Person</th>
                                        <th>Approval Status - Approved Date</th>
                                        <th>Remarks</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td title="Approval">Department Approval</td>
                                        <td title="Approval Person">{{ticket_details.department_approval}}</td>
                                        <td data-title="Approval Status">{{ticket_details.department_approvedstatus}} - {{ticket_details.department_approveddate}}</td>
                                        <td title="Remarks">{{ticket_details.departmentrejected_remarks}}</td>
                                    </tr>
                                    <tr ng-switch="management_approveddate!=''">
                                        <td title="Approval">Management Approval</td>
                                        <td title="Approval Person">{{ticket_details.management_approval}}</td>
                                        <td data-title="Approval Status">{{ticket_details.management_approvedstatus}} - {{ticket_details.management_approveddate}}</td>
                                        <td title="Remarks">{{ticket_details.managementrejected_remarks}}</td>
                                    </tr>
                                    <tr ng-switch="service_approveddate!=''">
                                        <td title="Approval">Service Approval</td>
                                        <td title="Approval Person">{{ticket_details.service_approval}}</td>
                                        <td data-title="Approval Status">{{ticket_details.service_approvedstatus}} - {{ticket_details.service_approveddate}}</td>
                                        <td title="Remarks">{{ticket_details.servicerejected_remarks}}</td>
                                    </tr>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        <br/>
                </div>
        <div class="panel-footer text-center">
            <div>

                <a class="btn btn-info" ng-click="back()">
                    <i class="fa fa-arrow-left" style="font-size:15px;">
                        &nbsp;Back
                    </i>
                </a>
            </div>
        </div>
    </div>
 
   
    </div>
