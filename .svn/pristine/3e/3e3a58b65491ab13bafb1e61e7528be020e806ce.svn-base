<style>
    #changestyle_chosen {
        width: 250px !important;
    }

    #changestyle500_chosen {
        width: 550px !important;
    }
    /* Style the buttons inside the tab */
    .tab button {
        background-color: inherit;
        float: left;
        border: none;
        outline: none;
        cursor: pointer;
        padding: 14px 16px;
        transition: 0.3s;
        font-size: 14px;
        font-weight: bold;
    }

        /* Change background color of buttons on hover */
        .tab button:hover {
            background-color: #B3E1C9;
            color: white;
        }

        /* Create an active/current tablink class */
        .tab button.active {
            background-color: #B3E1C9;
            background-image: linear-gradient(316deg, rgb(251 217 110) 0%, rgb(243 245 144) 74%);
        }

    /* Style the tab content */
    .tabcontent {
        display: none;
        padding: 6px 12px;
        border: 1px solid #fff;
        border-top: none;
    }

    .picColor:hover {
        background-color: transparent;
        box-shadow: none;
        color: #144A6E;
        border: 1px solid #A59F9D;
        transform: translateY(-3px);
        box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2);       
        text-decoration-color: #141478;
    }

    @keyframes glowing {
        0% {
            background-color: #FDECDD;
            box-shadow: 0 0 5px #F9B77F;
        }

        50% {
            background-color: #ffe9a4;
            box-shadow: 0 0 20px #FCE4D0;
        }

        100% {
            background-color: #ffe07c;
            box-shadow: 0 0 5px #FCE4D0;
        }
    }

    .button {
        animation: glowing 2000ms infinite;
    }
</style> 

