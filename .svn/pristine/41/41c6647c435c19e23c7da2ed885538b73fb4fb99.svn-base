<a href="#/app/MstProgram">
    <button type="button" title="Back" class="pull-right btn btn-sm btn-primary">
        <em class="fa fa-arrow-circle-left fa-1x"></em>
    </button>
</a>
<h3>
    Program View
</h3>
<div ng-controller="MstProgramViewController">
    <div id="panelDemo8" class="panel panel-primary ">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-eye"></i>
            </span>View Program
        </div>
        <div class="col-lg-12 col-sm-12 col-md-12 panel">
            <br />
            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">

                    <div class="dd-handle ng-scope">
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12 ">
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12 ">
                                        <b style="color:darkblue;font-size:15px;">
                                            <label>Basic Details</label>
                                        </b>
                                        <hr />
                                    </div>
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold"> Program Ref No </label>
                                                    <p> {{txtprogram_refno}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Short Program Name</label>
                                                    <p> {{txtprogram_name}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Entity</label>
                                                    <p> {{lblentity}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Vertical</label>
                                                    <p> {{cbovertical_edit}} </p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Approved By</label>
                                                    <p> {{cboapproved_edit}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Approved Date </label>
                                                    <p> {{approved_date}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Overall Program Limit (in Rs)</label>
                                                    <p> {{txtprogram_limit}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Maximum Limit per Transaction (in Rs)</label>
                                                    <p> {{txtmax_limit}} </p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">LMS Code </label>
                                                    <p> {{txtlms_code}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Bureau Code </label>
                                                    <p> {{txtbureau_code}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Description</label>
                                                    <p> {{txtdescription}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <div class="panel">
                                <div class="panel-heading">
                                    <p class="text-primary" style="color:darkblue;font-size:15px;"><b>&nbsp;&nbsp; Product Details  </b></p>
                                    <hr />
                                </div>
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="col-lg-12">
                                                <div class="table-responsive table-bordered">
                                                    <div style="max-height:200px;overflow:auto;">
                                                        <table class="table">
                                                            <thead class="bg-gray text-white">
                                                                <tr>
                                                                    <th>SNo</th>
                                                                    <th>Product Name</th>
                                                                    <th>SubProduct Name</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="list in loanproduct_list">
                                                                    <td>{{$index+1}}</td>
                                                                    <td>{{list.loanproduct_name}}</td>
                                                                    <td>{{list.loansubproduct_name}}</td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="dd-handle ng-scope">
                                <p class="text-primary" style="color:darkblue;font-size:15px;"><b>&nbsp;&nbsp;  Upload Documents </b></p>
                                <hr />
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="col-lg-12 col-sm-12 col-md-12">
                                            <div class="table-responsive">
                                                <table class="table table-striped table-secondary">
                                                    <thead class="bg-gray text-white">
                                                        <tr width="100%">
                                                            <th scope="col">S.No</th>
                                                            <th scope="col">Document Name</th>
                                                            <th scope="col">Uploaded Date</th>
                                                            <th align="center" scope="col">
                                                                Action&nbsp;&nbsp;&nbsp;
                                                                <a download="" href="" title="Download All" id="btnpdf1" ng-click="downloadall()">
                                                                    <em class="icon-arrow-down"></em>&nbsp;
                                                                </a>
                                                            </th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="data in upload_list">
                                                            <td title="S.No">{{$index+1}}</td>
                                                            <td title="Document Name">{{data.document_name}}</td>
                                                            <td title="Uploaded Date">{{data.created_date}}</td>
                                                            <td class="nowrap" data-title="'Action'">
                                                                <button title="Download" class="btn btn-green btn-sm"
                                                                        ng-click="downloads(data.document_path,data.document_name)">
                                                                    <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                    <em class="fa fa-download"></em>
                                                                </button>
                                                                <button title="View" class="btn btn-info btn-sm" ng-click="documentviewer(data.document_path,data.document_name)">
                                                                    <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                                </button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <div class="panel">
                                <div class="panel-heading">
                                    <p class="text-primary" style="color:darkblue;font-size:15px;"><b>&nbsp;&nbsp; Program Log  </b></p>
                                    <hr />
                                </div>
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="col-lg-12">
                                                <div class="table-responsive table-bordered">
                                                    <div style="max-height:200px;overflow:auto;">
                                                        <table class="table">
                                                            <thead class="bg-gray text-white">
                                                                <tr>
                                                                    <th>SNo</th>
                                                                    <th>Program</th>
                                                                    <th>Updated_By</th>
                                                                    <th>Updated_Date</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="list in productlog_list">
                                                                    <td>{{$index+1}}</td>
                                                                    <td>{{list.program}}</td>
                                                                    <td>{{list.updated_by}}</td>
                                                                    <td>{{list.updated_date}}</td>
                                                                    
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                </div>
                <div class="text-center">
                    <button type="button" class="btn btn-info btn-md" ng-click="back()"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>&nbsp;&nbsp;
                </div>
                <br />
            </div>
        </div>
    </div>
</div>