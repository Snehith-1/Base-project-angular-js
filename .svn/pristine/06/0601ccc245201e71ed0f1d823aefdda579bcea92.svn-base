<style>
    .nav.nav-pills .active > a {
        background-color: rgb(19,14,89);
        background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);
        color: #ffffff;
    }
</style>
<h3>
    Application Creation
</h3>
<div ng-controller="MstCreditHypothecationEditController as dp">
    <div class="panel">
        <div class="panel-heading" style="background-color:rgb(19,14,89);background: linear-gradient(90deg, rgba(19,14,89,1) 0%, rgba(65,65,185,1) 35%, rgba(0,212,255,1) 100%);color:#ffffff">
            <h5> Edit General Information </h5>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="panel-body">
                        <div class="row">
                            <form novalidate="" name="Hypothecationdtlform" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <br />
                                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Hypothecation Details </b></p>
                                        <hr />
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label><strong>Security Type</strong> </label>
                                            <select name="security_type" id="security_type" ng-options="m.securitytype_gid as m.security_type for m in securitytype_list"
                                                    ng-model="cboeditSecurityType" class="form-control " style="height:37px;" chosen=""
                                                    size="1">
                                                <option value=""> ----- Select Security Type -----</option>
                                            </select>
                                        </div>
                                        <div class="col-lg-8 col-md-8 col-sm-8">
                                            <label class="col-form-label"> Security Description </label>
                                            <br />
                                            <textarea ng-model="txteditsecurity_desc" rows="3" class="form-control" placeholder="Enter Security Description ..." style="resize:vertical;"
                                                      id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                        </div>
        
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Security Value  </label>
                                            <br />
                                            <input id="SecurityValueedit" ng-model="txteditSecurity_Value" class="form-control ng-pristine ng-valid ng-touched"
                                                   ng-change="txtSecurityValuechange()" style="text-align:right;"  pattern="^[\d,]+$">
                                            <span style="font-style:italic;">Number in words:</span>
                                            <span id="words_totalamountedit6" style="color:#003c96fc;" class="text-bold"></span>
                                        </div>
        
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Security Assessed On</label>
                                            <br />
                                            <p class="input-group">
                                                <input type="text" id="txtSecurityAssessededit_date" name="txtSecurityAssessededit_date" ng-model="txtSecurityAssessededit_date" datepicker-popup="{{dp.format}}"
                                                       style="height:36px;" is-open="dp.open1" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                                       close-text="Close" class="form-control" placeholder="DD-MM-YYYY" />
                                                <span class="input-group-btn">
                                                    <button type="button" ng-click="dp.calender1($event)" style="height:36px;" class="btn btn-default">
                                                        <em class="fa fa-calendar"></em>
                                                    </button>
                                                </span>
                                            </p>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> Asset ID </label>
                                            <br />
                                            <input type="text" ng-model="txtassetedit_id" tabindex="0" class="form-control" maxlength="20" />
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> ROC Filling ID </label>
                                            <br />
                                            <input type="text" ng-model="txtrocedit_fillingid" tabindex="0" class="form-control" maxlength="20" />
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <label class="col-form-label"> CERSAI Filling ID </label>
                                            <br />
                                            <input type="text" ng-model="txtCERSAIedit_fillingid" tabindex="0" class="form-control" maxlength="20" />
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4">
        
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <label class="col-form-label"> Observation Summary </label>
                                            <br /> 
                                            <textarea ng-model="txthypoobservationedit_summary" rows="4" class="form-control" placeholder="Type Observation Summary..."  style="resize:vertical;"
                                                      id="input_13" ng-trim="false" aria-multiline="true" tabindex="0"  style="resize:vertical;"aria-invalid="true"></textarea>
                                         
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <label class="col-form-label"> Primary Security </label>
                                            <br />
                                            <textarea ng-model="txtprimaryedit_security" rows="4" class="form-control" placeholder="Enter Primary Security..." style="resize:vertical;"
                                                      id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="true"></textarea>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="dd-handle ng-scope">
                                            <br />
                                            <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp;  Hypothecation Document Upload </b></p>
                                            <hr />
                                            <div class="col-lg-4 col-md-4 col-sm-4">
                                                <label> Document Title </label> <span style="color:red">*</span>
                                                <br />
                                                <select name="DocumentTitle" style="height:37px;"
                                                        ng-model="cbohypodoc_title" class="form-control"
                                                        size="1">
                                                    <option value=""> -----Select Document Title -----</option>
                                                    <option value="Document Observation"> Document Observation </option>
                                                    <option value="Document Hypothecation"> Document Hypothecation </option>
                                                    <option value="Document Legal Opinion"> Document Legal Opinion </option>
                                                </select>
                                            </div>
                                            <div class="col-lg-6 col-sm-6 col-md-6">
                                                <div class="float-label">
                                                    <strong>Upload Documents</strong>
                                                    <br /><br />
                                                    <input type="file" id="file" tabindex="5" multiple onchange="angular.element(this).scope().uploadhypothecationdoc(this.files, angular.element(this).scope().documentupload, this.name)"
                                                           ng-model="txtbureau_document" data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                                </div>
                                            </div>
                                            <div class="col-lg-2 col-sm-2 col-md-2">
                                            </div>
                                            <br /><br />
                                            <div class="row">
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                                        <br />
                                                        <div class="table-responsive">
                                                            <table class="table table-striped table-secondary">
                                                                <thead class="bg-gray text-white">
                                                                    <tr width="100%">
                                                                        <th scope="col">S.No</th>
                                                                        <th scope="col">Document Title</th>
                                                                        <th scope="col">Document Name</th>
                                                                        <th align="center" scope="col">
                                                                            Action&nbsp;&nbsp;&nbsp;
                                                                            <a download="" href="" title="Download All" id="btnpdf2" ng-click="downloadallhyp()">
                                                                                <em class="icon-arrow-down"></em>&nbsp;
                                                                            </a>
                                                                        </th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr ng-repeat="data in DocumentList">
                                                                        <td title="S.No">{{$index+1}}</td>
                                                                        <td title="Document Title">{{data.document_title}}</td>
                                                                        <td title="Document Name">{{data.document_name}}</td>
                                                                        <td data-title="Action">
                                                                            <button title="Download" class="btn btn-green btn-sm"
                                                                                    ng-click="downloads(data.document_path,data.document_name)">
                                                                                <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                                <em class="fa fa-download"></em>
                                                                            </button>
                                                                            <button title="View" class="btn btn-info btn-sm" ng-click="documentviewer(data.document_path,data.document_name)">
                                                                                <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em>
                                                                            </button>
                                                                            <a title="Delete" class="btn btn-sm btn-danger" ng-click="hypodoccancel(data.document_gid, data.application2hypothecation_gid)">
                                                                                <span class="icon icon-trash "></span>
                                                                            </a>
                                                                        </td>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <br /><br />
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="text-center">
                                    <br />
                                    <button type="button" ng-disabled="Hypothecationdtlform.$invalid" class="btn btn-success btn-md" ng-click="update_Hypothecationdtl()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                                    <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                                </div>
                            </form>
                        </div>
                        <br />
                    </div>
                </div>
            </div>
        </div>
    </div>

   

</div>