<h3>
    My Approvals
</h3>
<div ng-controller="osdTrnApprovalViewController">
    <div id="panelDemo8" class="panel panel-primary">
        <div class="panel-heading">
            Approval Details
        </div>
        <form name="feedbackform" id="feedbackform" novalidate="" class="form-validate mb-lg" autocomplete="off">
            <div class="panel-body">
                <div class=".table-responsive">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Request Ref.No</label>
                                </div>
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    {{request_refno}}
                                </div>
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Request Raised On</label>
                                </div>
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    {{created_date}}
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Request Title</label>
                                </div>
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    {{request_title}}
                                </div>
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Request Raised By</label>
                                </div>
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    {{created_by}}
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Approval Requested By</label>
                                </div>
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    {{approvalreq_by}}
                                </div>
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Approval Request Date</label>
                                </div>
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    {{approvalreqdate}}
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <label style="font-weight:bold">Request Remarks</label>
                                    <br />
                                    {{requestapproval_remarks}}
                                </div>
                            </div>
                        </div>
                        <br />

                          <!--
                            Table
                        -->
                        <p class="lead bb text-purple text-bold" style="font-size:16px;">&nbsp; Upload Document</p>

                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="text-center">
                                    <div class="dd-handle ng-scope">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-secondary">
                                                        <thead class="bg-gray text-white">
                                                            <tr width="100%">
                                                                <th align="center" scope="col">S.No</th>
                                                                <th align="center" scope="col">Document Name</th>
                                                                <th align="center" scope="col">Created By</th>
                                                                <th align="center" scope="col">Created Date</th>
                                                                <th align="center" scope="col">
                                                                    Action&nbsp;&nbsp;&nbsp;
                                                                    <a download="" href="" title="Download All" id="btnpdf1" ng-click="downloadall(lblallotfilepath,lblallotfilename)">
                                                                        <em class="icon-arrow-down"></em>&nbsp;
                                                                    </a>
                                                                </th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in alloteddocumentdtl" width="100%">
                                                                <td align="left" scope="col" title="S.No">
                                                                    {{$index+1}}
                                                                </td>
                                                                <td align="left" scope="col" title="Document Name">
                                                                    {{data.document_name}}
                                                                </td>
                                                                <td align="left" scope="col" title="Created By">
                                                                    {{data.created_by}}
                                                                </td>
                                                                <td align="left" scope="col" title="Created Date">
                                                                    {{data.created_date}}
                                                                </td>
                                                                <td align="left" scope="col" data-title="Action">
                                                                    <button title="Download Document"
                                                                            ng-click="downloads(data.document_path,data.document_name)"
                                                                            class="btn btn-green"><a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                        <em class="fa fa-download"></em>
                                                                    </button>
                                                                    <button title="View" class="btn btn-info"  ng-click="documentviewer(data.document_path,data.document_name)">
                                                                        <a download="" href="" style="display:none;"></a>  <em class="fa fa-file"></em> 
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>
                        <br />
                        <!--
                            End
                        -->

                        <p class="lead bb text-purple text-bold" style="font-size:16px;">&nbsp; Approval Details</p>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="text-center">
                                    <div class="dd-handle ng-scope">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-secondary">
                                                        <thead class="bg-gray text-white">
                                                            <tr width="100%">
                                                                <th align="center" scope="col">S.No</th>
                                                                <th align="center" scope="col">Approval Person</th>
                                                                <th align="center" scope="col">Approval Type</th>
                                                                <th align="center" scope="col">Approval Status</th>
                                                                <th align="center" scope="col">Approved / Rejected Date</th>
                                                                <th align="center" scope="col">Approval Initiated By</th>
                                                                <th align="center" scope="col">Approval Remarks</th>
                                                                <th align="center" scope="col">Request Remarks</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in approvaldetails" width="100%">
                                                                <td align="left" scope="col" title="S.No">
                                                                    {{$index+1}}
                                                                </td>
                                                                <td align="left" scope="col" title="Approval Person">
                                                                    {{data.approved_by}}
                                                                </td>
                                                                <td align="left" scope="col" title="Approval Person">
                                                                    {{data.approval_type}}
                                                                </td>
                                                                <td align="left" scope="col" title="Approval Status">
                                                                    {{data.approval_status}}
                                                                </td>
                                                                <td align="left" scope="col" title="Approved / Rejected Date">
                                                                    {{data.approval_date}}
                                                                </td>
                                                                <td align="left" scope="col" title="Approval Initiated By">
                                                                    {{data.approvalinitiated_by}}
                                                                </td>
                                                                <td align="left" scope="col" title="Remarks">
                                                                    {{data.approval_remarks}}
                                                                </td>
                                                                <td title="Request Remarks" align="left" scope="col">
                                                                    <button title="View" ng-click="request_remarks(data.requestapproval_gid)" class="btn btn-info">
                                                                        <em class="fa fa-eye"></em>
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>
                        <br />


                    </div>
                </div>
            </div>
            <!--<div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <label><b>Remarks : <span class="text-danger">*</span></b></label>
                        <textarea ng-model="$parent.txtremarks" class="form-control" />
                    </div>

                </div>
            </div>-->
            <br />
            <div class="row text-center">
                
                <button class="btn btn-oval btn-lg btn-info" ng-click="back()"
                        style="font-size:14px;" type="submit">
                    <i class="fa fa-arrow-circle-left"></i>&nbsp; Back
                </button>
            </div>
            <br />
        </form>

    </div>

    <script type="text/ng-template" id="/RequestRemarks.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-file-text-o"></em>&nbsp;<label style="font-size:16px">Request Remarks</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">

                <form novalidate="" name="userform1" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="dd-handle ng-scope">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <label> Request Remarks</label>
                                        <br />
                                        <p>{{txtrequestapproval_remarks}}</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>
</div>