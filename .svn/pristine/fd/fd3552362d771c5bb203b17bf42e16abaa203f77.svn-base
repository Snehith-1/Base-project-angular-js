<h3>
    Sample Upload View
</h3>
<div ng-controller="AtmTrnAuditTaskSampleViewController">
    <div id="panelDemo8" class="panel panel-primary ">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-eye"></i>
            </span> Sample View
        </div>
        <div class="col-lg-12 col-sm-12 col-md-12 ">
            <div class="row">
                <br />
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <label style="font-weight:bold">Tagged Employee :</label>
                    {{employee_name}}
                </div>
            </div>

            <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                <div class="row">
                    <br />
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">

                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Sample Unique ID </label>
                                    <br />
                                    {{txtaudit_ref_no}}
                                </div>

                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Sample Name</label>
                                    <br />
                                    {{txtsample_name}}
                                </div>
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">SamFin Code</label>
                                    <br />
                                    {{txtsamfin_code}}
                                </div>
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">SamAgro Code </label>
                                    <br />
                                    {{txtsamagro_code}}
                                </div>
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Date of Code Creation </label>
                                    <br />
                                    {{txtcodecreation_date}}
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">

                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Field 1</label>
                                    <br />
                                    {{txtfield1}}
                                </div>

                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Field 2</label>
                                    <br />
                                    {{txtfield2}}
                                </div>
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Field 3</label>
                                    <br />
                                    {{txtfield3}}
                                </div>
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Field 4</label>
                                    <br />
                                    {{txtfield4}}
                                </div>
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Field 5</label>
                                    <br />
                                    {{txtfield5}}
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">

                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Field 6</label>
                                    <br />
                                    {{txtfield6}}
                                </div>

                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Field 7</label>
                                    <br />
                                    {{txtfield7}}
                                </div>
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Field 8</label>
                                    <br />
                                    {{txtfield8}}
                                </div>
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Field 9</label>
                                    <br />
                                    {{txtfield9}}
                                </div>
                                <div class="col-lg-2 col-sm-2 col-md-2">
                                    <label style="font-weight:bold">Field 10</label>
                                    <br />
                                    {{txtfield10}}
                                </div>
                            </div>
                        </div>
                        <br />
                    </div>
                </div>
                <br />
            </div>
            <br />
        </div>
        <div class="ng-scope">
            <div class="text-center">
                <button ng-click="back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
            </div>
        </div>       
        <br />
    </div>
    <br />
</div>
