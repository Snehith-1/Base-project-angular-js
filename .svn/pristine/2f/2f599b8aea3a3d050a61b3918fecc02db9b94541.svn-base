        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-plus"></em>&nbsp;<label style="font-size:16px">Edit Address</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>

        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">

                <form novalidate="" name="edituserform1" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-9 col-md-9 col-sm-9">
                                <label><strong>Address Type </strong> <span style="color:red"> *</span></label>
                                <select name="address_type" id="address_type" required="" ng-options="m.address_gid as m.address_type for m in addresstype_list" style="height:37px;"
                                        ng-model="cboAddressType" class="form-control ng-valid ng-touched ng-valid-parse" style="height:37px;"
                                        size="1">
                                    <option value=""> ----- Select Address Type -----</option>
                                </select>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <label>Primary Status </label> <span style="color:red">*</span>
                                <div class="radio c-radio">
                                    <label>
                                        <input type="radio" id="primary" name="primaryaddress" required value="Yes" ng-model="rdbprimary_status" ng-change="ngchange()" />
                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                    </label>
                                    <label>
                                        <input type="radio" id="primary" name="primaryaddress" required value="No" ng-model="rdbprimary_status" ng-change="ngchange()" />
                                        <span class="fa fa-circle"></span>No
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-sm-12 col-sm-12 col-sm-12">
                                <label><strong>Address Line 1 </strong> <span style="color:red">*</span></label>
                                <input type="text" ng-model="txtaddressline1" tabindex="0" class="form-control" maxlength="300" required />
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-sm-12 col-sm-12 col-sm-12">
                                <label><strong>Address Line 2 </strong></label>
                                <input type="text" ng-model="txtaddressline2" tabindex="0" class="form-control" maxlength="300" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Land Mark </strong> </label>
                                    <input type="text" ng-model="txtlandmark" tabindex="0" class="form-control" maxlength="300" />

                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Postal Code </strong> <span style="color:red"> *</span></label>
                                    <input type="text" ng-model="txtpostal_code" tabindex="0" class="form-control" required maxlength="6" 
                                    ng-pattern="/^[1-9]{1}[0-9]{5}$/" ng-change="onchangepostal_code(); getGeoCoding();" />

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label>City </label>
                                    <input type="text" ng-model="txtcity" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" tabindex="0" class="form-control" disabled />
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label>Taluka</label>
                                    <input type="text" ng-model="txttaluka" tabindex="0" class="form-control" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" disabled />

                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label><strong>District</strong></label>
                                    <input type="text" ng-model="txtdistrict" tabindex="0" class="form-control" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" disabled />
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label>State </label>
                                    <br />
                                    <input type="text" ng-model="txtstate" tabindex="0" class="form-control" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" disabled />
                                </div>

                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <div class="mda-form-group">
                                    <label>Country</label>
                                    <input type="text" ng-model="txtcountry" tabindex="0" class="form-control" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/,.]*$/" />

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Latitude </strong></label>
                                    <input type="text" ng-model="txtlatitude" tabindex="0" class="form-control" disabled />
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="mda-form-group">
                                    <label><strong>Longitude </strong></label>
                                    <input type="text" ng-model="txtlongitude" tabindex="0" class="form-control" disabled />
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="text-center">
                        <button type="button" class="btn btn-success" ng-disabled="edituserform1.$invalid" ng-click="address_update()"><i class="fa fa-check"></i>&nbsp;&nbsp;Update</button>&nbsp;&nbsp;
                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>

                    </div>
                </form>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="/Individualdocuments.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-file-text-o"></em>&nbsp;<label style="font-size:16px">Bureau Documents</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>
        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <form novalidate="" name="userformindividualproof" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">

                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="table-responsive">
                                <table class="table table-striped table-secondary">
                                    <thead class="bg-gray text-white">
                                        <tr width="100%">
                                            <th scope="col">S.No</th>
                                            <th scope="col">Document Name</th>
                                            <th scope="col">Uploaded By / On </th>
                                            <!-- <th scope="col">Action</th> -->
                                            <th align="center" scope="col">
                                                Action&nbsp;&nbsp;&nbsp;
                                                <a download="" href="" title="Download All" id="btnpdf1" ng-click="downloadall_4()">
                                                    <em style="font-size: 1.5em;" class="icon-arrow-down"></em>&nbsp;
                                                </a>
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="data in bureaudoc_list">
                                            <td title="S.No">{{$index+1}}</td>
                                            <td title="Document Name">{{data.document_name}}</td>
                                            <td title="Document Name">{{data.created_by}} / {{data.created_date}} </td>
                                            <td data-title="Action">
                                                <button title="Download" class="btn btn-green btn-sm"
                                                        ng-click="download_bureaudoc(data.document_path,data.document_name)">
                                                    <a download="" href="" id="btnpdf" style="display:none;"></a>

                                                    <em class="fa fa-download"></em>
                                                </button>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>
                    <br />
                    <div class="text-center">

                        <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                    </div>
                </form>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="/StaticMapAndPhotosView.html">
        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <em style="font-size:16px" class="fa fa-image"></em>&nbsp;<label style="font-size:16px">Static Map and Photos</label>
                <span flex></span>
                <button ng-click="ok()" class="pull-right btn btn-primary"><i class="fa fa-close"></i></button>
            </div>
        </md-toolbar>
        <div class="modal-content">
            <div class="modal-body ng-scope">
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            <p class="text-primary" style="font-size:14px;"><b>&nbsp; Static Map </b></p>
                            <img ng-src="{{staticmapImgUrl}}">
                        </div>
                        <div class="col-lg-6 col-sm-6 col-md-6">
                            <p class="text-primary" style="font-size:14px;"><b>&nbsp; Photos </b></p>
                            <div ng-repeat="item in photoUrlList" ng-show="photoFound">
                                <img ng-src="{{item}}">
                                <br />
                                <br />
                            </div>
                            <label style="font-weight:bolder;color:red" ng-show="photoNotFound">Photos not found!</label>
                        </div>
                    </div>
                </div>
                <br />
                <div class="text-center">
                    <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
                </div>
            </div>
        </div>
    </script>

</div>