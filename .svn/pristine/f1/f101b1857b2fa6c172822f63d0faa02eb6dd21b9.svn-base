                                                                <tr ng-repeat="data in MdlTransferLog" width="100%">
                                                                    <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                                                    <td align="left" scope="col" title="Transfer From">{{data.assignedby_name}}</td>
                                                                    <td align="left" scope="col" title="Transfer To'">{{data.transferby_name}}</td>
                                                                    <td align="left" scope="col" title="Allotted By/On'">{{data.created_by}}/{{data.created_date}}</td>
                                                                    <td align="left" scope="col" title="Transfer To'">{{data.transfered_date}}</td>
                                                                    <td align="left" scope="col" title="Transfer To'" style="width:40%">{{data.transfer_remarks}}</td>                                                                
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                      
                                            <br>
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12  ">
                                                    <p class="lead bb text-purple text-bold" style="font-size:16px;">Audit Log</p>
                                
                                                </div>
                                            </div>
                                             
                                                <div class="table-responsive">
                                                    <div style="max-height:300px;overflow:auto;" >
                                                    <table class="table table-striped table-secondary">
                                                        <thead class="bg-gray text-white">
                                                            <tr width="100%">
                                                                <th align="center" scope="col">S.No</th>
                                                                <th align="center" scope="col">Query Response</th>
                                                                <th align="center" scope="col">Done By/On</th>
                                                                
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="data in auditlog_list" width="100%">
                                                                <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                                                <td align="left" scope="col" title="Workitem Response">{{data.action_taken}}</td>
                                                                <td align="left" scope="col" title="Done By/On">{{data.created_by}}/{{data.created_date}}</td>
                                                               
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                    </div>
                                                </div>
                                           
                                    </div>
                                  </div>
                                  </div>
                            </div>
                            
                        </div>
                    </div>
                </div>
               
    
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="btn-group pull-left">
                        <h4 style="color:grey;"><b>Ticket Reference No:{{ticketref_no}}</b></h4>
                    </div>
