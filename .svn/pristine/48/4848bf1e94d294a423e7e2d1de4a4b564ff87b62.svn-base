<h3>
    Customer
</h3>
<div ng-controller="customerMasterAdd">
    <div id="panelDemo9" class="panel panel-success">
        <div class="panel-heading"> &nbsp;
            <span class="btn-label bg-green-200">
                <i class="fa fa-plus "></i>
            </span>ADD CUSTOMER
        </div>
        <div class="panel-body">
            <div class="ng-scope">
                <div class="row" style="display:none;">
                    <div class="col-lg-12 col-md-12 col-sm-12 ">

                        <div class="col-lg-8 col-md-8 col-sm-8 ">
                            <input name="segmentgid" id="segmentgid" style="font-size:12px" placeholder="Enter segment gid"
                                   ng-model="customer_gid" class="form-control" required>
                        </div>
                    </div>
                </div>
                <div class="pd-sm-10">

                    <div class="row ">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-sm-6">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtcustomercode" required="" tabindex="0" aria-required="true"
                                           aria-invalid="true"
                                           class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched"
                                           style="">
                                    <label>Customer ID * </label>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtcustomername" required="" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Customer Name  *</label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row ">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-sm-6">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtcontactperson" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Contact Person </label>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtcontactnumber" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Contact Number </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row ">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-sm-6">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtmobileno" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Mobile No </label>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="mda-form-group float-label">

                                    <input ng-model="txtemail" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Email Address </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row ">
                        <div class="col-lg-12 col-sm-12 col-md-12 ">
                            <div class="col-sm-6">
                                <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                    <label for="input_13">Address Line 1</label>
                                    <textarea ng-model="txtaddress1" rows="3" md-maxlength="150" class="ng-valid md-input ng-valid-md-maxlength ng-touched ng-dirty ng-valid-parse"
                                              id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="false"></textarea>

                                </md-input-container>
                            </div>
                            <div class="col-sm-6">
                                <md-input-container flex="" class="md-default-theme" style="height: auto;">
                                    <label for="input_13">Address Line 2</label>
                                    <textarea ng-model="txtaddress2" rows="3" md-maxlength="150" class="ng-valid md-input ng-valid-md-maxlength ng-touched ng-dirty ng-valid-parse"
                                              id="input_13" ng-trim="false" aria-multiline="true" tabindex="0" aria-invalid="false"></textarea>
                                </md-input-container>
                            </div>
                        </div>
                    </div>
                    <div class="row ">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-sm-6">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtstate" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>State</label>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtregion" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Region </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row ">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-sm-6">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtpostalcode" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Postal Code</label>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="mda-form-group float-label">
                                    <input ng-model="txtcountry" tabindex="0" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                    <label>Country </label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 ">
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <label class="col-form-label"><strong>Zonal Head</strong></label>
                            <select name="zonalHead" dropzone="" ng-model="zonalHead" class="mda-form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched" size="1">
                                <option value="">-----Select Zonal Head----</option>
                                <option data-ng-repeat="data in employee_list" value="{{data.employee_gid}}">{{data.employee_name}}</option>
                            </select>
                        </div>
                        <div class="col-lg-1 col-md-1 col-sm-">

                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <label class="col-form-label"><strong>Business Head </strong></label>
                            <select name="businessHead" dropzone="" ng-model="businessHead" class="mda-form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched" size="1">
                                <option value="">-----Select Business Head----</option>
                                <option data-ng-repeat="data in employee_list" value="{{data.employee_gid}}">{{data.employee_name}}</option>
                            </select>
                        </div>
                    </div>

                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 ">
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <label class="col-form-label"><strong>Cluster Manager</strong></label>
                            <select name="deferralApprover" dropzone="" ng-model="deferralApprover" class="mda-form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched" size="1">
                                <option value="">-----Select Cluster Manager----</option>
                                <option data-ng-repeat="data in employee_list" value="{{data.employee_gid}}">{{data.employee_name}}</option>
                            </select>
                        </div>
                        <div class="col-lg-1 col-md-1 col-sm-">

                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <label class="col-form-label"><strong>Relationship Manager </strong></label>
                            <select name="relationshipMgmt" ng-model="relationshipMgmt" class="mda-form-control ng-pristine ng-invalid-required ng-valid md-default-theme ng-touched" size="1">
                                <option value="">-----Select Relationship Manager----</option>
                                <option data-ng-repeat="data in employee_list" value="{{data.employee_gid}}">{{data.employee_name}}</option>
                            </select>
                        </div>
                    </div>
                </div>
                <br/>
            </div>

        </div>
        <div class="panel-footer">
            <div class="ng-scope">
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-click="customerSubmit()"><i class="fa fa-arrow-circle-right"></i> &nbsp;&nbsp;Submit</button>
                    <button ng-click="backclick()" class="btn btn-info"><i class="fa icon-close"></i>&nbsp;&nbsp;Back</button>
                </div>
            </div>
        </div>
    </div>

</div>