<h3>
    &nbsp;&nbsp;RH Approval 

    <a href="#/app/myApproval">
        <button type="button" title="Back to Home" class="pull-right btn btn-sm btn-primary ">
            <em class="fa fa-arrow-circle-left fa-1x"></em>
        </button>
    </a>
</h3>
<style>
    #bordersty {
        border: 1px solid #ebebeb;
        padding: 10px;
        border-radius: 25px;
        background-color: rgb(225 243 247);
    }
	.buttonstyle {
         border:none;
         background-color: rgb(255, 255, 255);
         color:#141478;

    }
</style>
<div ng-controller="osdTrnRHApprovalViewController">
    <form name="feedbackform" id="feedbackform" novalidate="" class="form-validate mb-lg" autocomplete="off">        
    <div class="panel panel-primary">
        <div class="panel-heading">
            <p style="font-size: 16px;"> &nbsp; RH Approval </p>
        </div>
        <br />
        <div class="panel-body">
            <div class="row">
                <p style="color:#141478;font-size: 18px;" class="text-center"><b> {{lblcustomer_name}} /{{lblcustomer_urn}} </b></p>
            </div>
            <br />
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-8 col-md-8 col-sm-8">
                        <div style="border: 1px solid #EEEEEE;">
                            <br />
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div>
                                    <p style="color:#141478;font-size: 17px;"><b> Customer details </b></p>
                                    <hr />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label style="font-weight:bold">Vertical</label><br />
                                        {{lblvertical}}
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label style="font-weight:bold">constitution</label><br />
                                        {{lblconstitution}}
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label style="font-weight:bold">Contact Person</label><br />
                                        {{lblcontact_person}}
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label style="font-weight:bold">Relationship Manager</label><br />
                                        {{lblrelationshipmanager_name}}
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label style="font-weight:bold">DRM</label><br />
                                        {{lblrm_name}}
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label style="font-weight:bold">Cluster Head</label><br />
                                        {{lblcluster_manager}}
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label style="font-weight:bold">Regional Head</label><br />
                                        {{lblregional_head}}
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label style="font-weight:bold">Zonal Head</label><br />
                                        {{lblzonal_head}}
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label style="font-weight:bold">Business Head</label><br />
                                        {{lblbusiness_head}}
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label style="font-weight:bold">Credit manager</label><br />
                                        {{lblcredit_manager}}
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label style="font-weight:bold">Credit Head Manager</label>
                                        <br />
                                        {{lblcredithead_name}}
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label style="font-weight:bold">National Credit Manager</label><br />
                                        {{lblcreditnationalmanager_name}}
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="col-lg-4 col-sm-4 col-md-4">
                                        <label style="font-weight:bold">Regional Credit Manager</label><br />
                                        {{lblcreditregionalmanager_name}}
                                    </div>
                                </div>
                            </div>
                            <br />
                        </div>
                        <br />
                    </div>
                    <div id="bordersty" ng-show="lblkotakAPI_flag=='N'" class="col-lg-4 col-md-4 col-sm-4">

                        <div class="panel-heading text-center" style="color:#141478;font-size: 17px;">
                            &nbsp;&nbsp;<b>Mail Details </b>
                            <hr />
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label><strong>  Reference  No:</strong></label>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <p> {{lblticketref_no}}</p>
                                </div>

                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label><strong>Mail Sent Date:</strong></label>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <p> {{lblemail_date}}</p>
                                </div>

                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label><strong>Mail Sent By:</strong></label>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <p> {{lblemail_from}}</p>
                                </div>

                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label><strong> Assigned RH:</strong></label>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <p> {{lblregional_head}}</p>
                                </div>

                            </div>

                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label><strong>Status:</strong></label>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <label class="label label-success" style="font-size:11px;"> {{lblallocated_status}}</label>
                                </div>

                            </div>
                        </div>
                        <br />
                    </div>
                    <div id="bordersty" ng-show="lblkotakAPI_flag=='Y'" class="col-lg-4 col-md-4 col-sm-4">
                        <div class="panel-heading text-center" style="color:#141478;font-size: 17px;">
                            &nbsp;&nbsp;<b>E Collection Details </b>
                            <hr />
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label><strong>  Reference  No:</strong></label>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <p> {{lblticketref_no}}</p>
                                </div>

                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label><strong>Details Received at:</strong></label>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <p> {{lbldetailsreceived_at}}</p>
                                </div>

                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label><strong>Source:</strong></label>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <p> {{lblsource}}</p>
                                </div>

                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label><strong> Assigned RM:</strong></label>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <p> {{lblrelationshipmanager_name}}</p>
                                </div>

                            </div>

                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-sm-4 col-md-4 col-lg-4">
                                    <label><strong>Status:</strong></label>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6">
                                    <label class="label label-success" style="font-size:11px;"> {{lblallocated_status}}</label>
                                </div>

                            </div>
                        </div>
                        <br />
                    </div>
                </div>
            </div>
            <br />
            <div style="margin-bottom:6px;" ng-show="lblkotakAPI_flag=='N'">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div style="border: 1px solid #EEEEEE;">
                                <br />
                                <div class="text-bold" style="color:#141478;font-size:17px">
                                    <!--  &nbsp;  <em class="fa fa-envelope-o"></em> --> &nbsp;
                                    <label style="color:#141478;">   Mail details</label>
                                    <hr />
                                </div>

                                <table class="table mb-mails mb-view">
                                    <tbody>
                                        <tr>
                                            <td>
                                                <div class="clearfix">
                                                    <a href="#" class="pull-left">
                                                        <img alt="" ng-src="{{user.picture}}"
                                                             class="media-box-object img-rounded thumb32 mb-mail-avatar" />
                                                    </a>
                                                    <div class="media-box-body">
                                                        <span style="font-size:15px;">
                                                            {{lblfrom_mailaddress}}
                                                        </span><br>
                                                        <span>
                                                            <b>Sent:</b>
                                                        </span>
                                                        <span> {{lblemail_date}} </span><br>
                                                        <span>
                                                            <b>To:</b> {{lblemail_to}}
                                                        </span><br>
                                                        <span>
                                                            <b>Cc:</b> {{lblemail_cc}}
                                                        </span><br>
                                                        <span>
                                                            <b>Bcc:</b> {{lblemail_bcc}}
                                                        </span>
                                                    </div>
                                                </div>
                                                <div class="mb-mail-preview mt-lg">
                                                    <h5>Subject:{{lblemail_subject}}</h5>
                                                </div>
                                                <label>Mail Content : </label>
                                                <p ng-bind-html=trustAsHtml(lblemail_content)></p>
                                                <div class="mb-attachment">
                                                    <ul class="list-inline">
                                                        <li style="padding:5px">
                                                            <button title="Download Document"
                                                                    ng-click="download(lbldocument_path,lbldocument_name)"
                                                                    class="buttonstyle">
                                                                <a download="" href="" id="btnpdf" style="display:none;"></a>
                                                                <em class="icon-arrow-down"></em>&nbsp; {{lbldocument_name}}
                                                            </button>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <br />
                        </div>
                    </div>

                </div>
            </div>
            <div style="margin-bottom:6px;" ng-show="lblkotakAPI_flag=='Y'">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <br />
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <p style="color:#141478;font-size: 16px;"> &nbsp;&nbsp; <b> Transaction Details </b></p>
                                                <hr />
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">Master Account No</label>
                                                            <br />
                                                            {{lblMaster_Acc_No}}
                                                        </div>
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">Remitter Info</label>
                                                            <br />
                                                            {{lblRemitt_Info}}
                                                        </div>
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">Remitter Name</label>
                                                            <br />
                                                            {{lblRemit_Name}}
                                                        </div>
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">Remitter Ifsc</label>
                                                            <br />
                                                            {{lblRemit_Ifsc}}
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">Amount</label>
                                                            <br />
                                                            {{lblAmount}}
                                                        </div>
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">Txn Ref No</label>
                                                            <br />
                                                            {{lblTxn_Ref_No}}
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                                            <label class="col-form-label">Utr No</label>
                                                            <br />
                                                            {{lblUtr_No}}
                                                        </div>
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">Pay Mode</label>
                                                            <br />
                                                            {{lblPay_Mode}}
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">E Collection Account No</label>
                                                            <br />
                                                            {{lblE_Coll_Acc_No}}
                                                        </div>
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">Remitter Account Number</label>
                                                            <br />
                                                            {{lblRemit_Ac_Nmbr}}
                                                        </div>
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">Credit Date and Time</label>
                                                            <br />
                                                            {{lblCreditdateandtime}}
                                                        </div>
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">Txn Date</label>
                                                            <br />
                                                            {{lblTxn_Date}}
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">Bene Cust Account Name</label>
                                                            <br />
                                                            {{lblBene_Cust_Acname}}
                                                        </div>
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">REF1</label>
                                                            <br />
                                                            {{lblREF1}}
                                                        </div>
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">REF2</label>
                                                            <br />
                                                            {{lblREF2}}
                                                        </div>
                                                        <div class="col-lg-3 col-sm-3 col-md-3">
                                                            <label style="font-weight:bold">REF3</label>
                                                            <br />
                                                            {{lblREF3}}
                                                        </div>
                                                    </div>
                                                </div>
                                                <br />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />
                        </div>
                    </div>
                    <br />
                </div>
            </div>
            <br>
            <p class="lead bb text-purple text-bold" style="font-size:16px;">&nbsp; Approval Details</p>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="text-center">
                        <div class="dd-handle ng-scope">
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="table-responsive">
                                        <table class="table table-striped table-secondary">
                                            <thead class="bg-gray text-white">
                                                <tr width="100%">
                                                    <th align="center" scope="col">S.No</th>
                                                    <th align="center" scope="col">Approval Person</th>
                                                    <th align="center" scope="col">Approval Status</th>
                                                    <th align="center" scope="col">Approved Date</th>
                                                    <th align="center" scope="col">Approval Remarks</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in rhapprovaldetails" width="100%">
                                                    <td align="left" scope="col" title="S.No">
                                                        {{$index+1}}
                                                    </td>
                                                    <td align="left" scope="col" title="Approval Person">
                                                        {{data.assignedrh_name}}
                                                    </td>
                                                    <td align="left" scope="col" title="Approval Status">
                                                        {{data.approval_status}}
                                                    </td>
                                                    <td align="left" scope="col" title="Approved / Rejected Date">
                                                        {{data.approval_date}}
                                                    </td>
                                                    <td align="left" scope="col" title="Remarks">
                                                        {{data.rh_remarks}}
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>
                </div>
            </div>
            <br />
            <br />
            <p class="lead bb text-purple text-bold" style="font-size:16px;">&nbsp;RH Rejected Details</p>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="text-center">
                        <div class="dd-handle ng-scope">
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="table-responsive">
                                        <table class="table table-striped table-secondary">
                                            <thead class="bg-gray text-white">
                                                <tr width="100%">
                                                    <th align="center" scope="col">S.No</th>
                                                    <th align="center" scope="col">Rejected Person</th>
                                                    <th align="center" scope="col">Approval Status</th>
                                                    <th align="center" scope="col">Rejected Date</th>
                                                    <th align="center" scope="col">Rejected Remarks</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in rhrejecteddetails" width="100%">
                                                    <td align="left" scope="col" title="S.No">
                                                        {{$index+1}}
                                                    </td>
                                                    <td align="left" scope="col" title="Rejected Person">
                                                        {{data.assignedrh_name}}
                                                    </td>
                                                    <td align="left" scope="col" title="Status">
                                                        {{data.approval_status}}
                                                    </td>
                                                    <td align="left" scope="col" title="Rejected Date">
                                                        {{data.approval_date}}
                                                    </td>
                                                    <td align="left" scope="col" title="Remarks">
                                                        {{data.rh_remarks}}
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>
                </div>
            </div>
            <br />
            <br />
            <div class="dd-handle ng-scope">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <p class="text-primary" style="font-size:14px;"><b> RH Approval </b></p>
                        <hr />
                        <label><b>Remarks : <span class="text-danger">*</span></b></label>
                        <textarea style="resize:vertical" ng-trim="true" ng-model="rh_txtremarks" class="form-control" required></textarea>
                    </div>
                </div>
                <br />
                <div class="row text-center">
                    <button ng-click="rh_approve()" ng-disabled="feedbackform.$invalid" class="btn btn-success">
                        <i class="fa fa-check"></i>&nbsp;&nbsp; Approve &nbsp;&nbsp;
                    </button> &nbsp;&nbsp;
                    <button ng-click="rh_reject()" ng-disabled="feedbackform.$invalid" class="btn btn-danger">
                        <i class="fa icon-close"></i>&nbsp;&nbsp; Reject &nbsp;&nbsp;
                    </button> &nbsp;&nbsp;
                    <button ng-click="back()" class="btn btn-info">
                        <i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp; Back &nbsp;&nbsp;
                    </button> &nbsp;&nbsp;
                </div>
                <br />
            </div>
            <br>
        </div>            
        </form>       
    </div>
    <br />
</div>
 