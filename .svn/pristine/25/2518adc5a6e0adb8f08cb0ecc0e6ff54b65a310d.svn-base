<h3>
    &nbsp;&nbsp;View History
</h3>

<style>
    #bordersty {
        border: 1px solid #ebebeb;
        padding: 10px;
        border-radius: 25px;
    }
</style>
<style>
    .buttonstyle {
        border: none;
        background-color: rgb(255, 255, 255);
        color: #141478;
    }
</style>
<div ng-controller="BrsTrnUnreconBankMatchedViewController">
    <div class="panel">
        <div class="row">
            <div class="col-lg-12 col-sm-12 col-md-12">
                <div class="dd-handle ng-scope">
                    <p class="text-primary" style="font-size:16px;">
                        <b>
                            &nbsp;&nbsp; Unreconciliation View
                        </b>
                        <a>
                            <button type="button" title="Back to Summary" ng-click="Back()" class="pull-right btn btn-sm btn-primary ">
                                <em class="fa fa-arrow-circle-left fa-1x"></em>
                            </button>
                        </a>
                    </p>


                </div>
            </div>


            <div style="margin-bottom:-10px;">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div id="bordersty" style="background-color: #FAF8F8;">
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Reference  Number
                                            </label>
                                            <br>
                                            <span>
                                                {{lblbanktransc_gid}}
                                            </span>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Bank Name
                                            </label>
                                            <br>
                                            <span>
                                                {{lblbank_name}}
                                            </span>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Cheque No.
                                            </label>
                                            <br>
                                            <span>
                                                {{lblchq_no}}
                                            </span>
                                            <!-- <label class="text-primary">
                                                Customer Reference No.
                                            </label>
                                            <br>
                                            <span>
                                                {{lblcustomer_refno}}
                                            </span> -->
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Branch Name
                                            </label>
                                            <br>
                                            <span>
                                                {{lblbranch_name}}
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <br>
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Credit/Debit
                                            </label>
                                            <br>
                                            <span>
                                                {{lblcr_dr}}<br>
                                            </span>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Transaction Value
                                            </label>
                                            <br>
                                            <span>
                                                {{lbltransc_value}}
                                            </span>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <!-- <label class="text-primary">
                                                Transaction Balance
                                            </label>
                                            <br>
                                            <span>
                                                {{lbltransc_balance}}
                                            </span> -->
                                            <label class="text-primary">
                                                Transaction Particulars
                                            </label>
                                            <br>
                                            <span>
                                                {{lbltransact_particulars}}
                                            </span>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Remarks
                                            </label>
                                            <br>
                                            <span>
                                                {{lblremarks}}
                                            </span>
                                        </div>

                                    </div>

                                    <br />
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label class="text-primary">
                                                Remaining Amount
                                            </label>
                                            <br>
                                            <span>
                                                {{lblremainingamount}}
                                            </span>
                                        </div>

                                    </div>
                                </div>
                                <br />
                            </div>


                        </div>
                    </div>
                </div>
                <br>
            </div>
            <!-- <br /> -->

        </div>


        <div class="dd-handle ng-scope">
            <p style="color:#141478;font-size: 16px;"> &nbsp;&nbsp; <b>Transaction Details</b></p>
            <hr class="space" />


            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <div class="table-responsive table-bordered">
                            <table class="table">
                                <thead class="bg-gray text-white">
                                    <tr>
                                        <th>S.No</th>
                                        <th>Customer Name </th>
                                        <th>Department </th>
                                        <th>Updated By </th>
                                        <th>Adjusted Against</th>
                                        <th>Amount</th>
                                        <th>Remaining Amount</th>
                                        <th>Action Name</th>
                                        <th>Remarks</th>
                                        <th> Account Number</th>
                                        <th>Transaction Date</th>
                                        <th>Repayment Date</th>
                                        <th>Transaction Id</th>
                                        <th>Urn Number</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="data in unrecontransactionlist">
                                        <td>{{$index+1}}</td>
                                        <td>{{data.samfincustomer_name}}</td>
                                        <td>{{data.department_name}}</td>
                                        <td>{{data.assignby_name}}</td>
                                        <td>{{data.activity_name}}</td>
                                        <td>{{data.amount}}</td>
                                        <td>{{data.remaining_amount}}</td>
                                        <td>{{data.action_name}}</td>
                                        <td align="center">
                                            <em class="fa fa-info-circle" ng-click=remarks(data.transaction_remarks) style="font-size:24px;align-content:center;"></em>
                                        </td>
                                        <td>{{data.account_number}}</td>
                                        <td>{{data.transaction_date}}</td>
                                        <td>{{data.repayment_date}}</td>
                                        <td>{{data.transaction_id}}</td>
                                        <td>{{data.urn_no}}</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <br />
        </div>
        <br />


        <div class="row">
            <div class="text-center">
                <br />
                <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
            </div>
        </div>
        <br />
        <br />
        <!--<div class="text-center">

            <button type="button" title="Back to Summary" ng-click="Back()" class=" btn btn-sm btn-primary ">
                <em class="fa fa-arrow-circle-left fa-1x"></em>
            </button>
        </div>-->
    </div>
    <script type="text/ng-template" id="/remarks.html">
        <div class="modal-header bg-primary">
            <h4 class="modal-title">
                <span class="btn-label bg-primary">
                    <i class="fa fa-info-circle"></i>
                </span> Remarks
            </h4>
        </div>
        <div class="modal-body">
            <div class="dd-handle ng-scope">
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <label>Remarks : </label>
                        <br />
                        <p> {{transaction_remarks}}  </p>
                    </div>
                </div>
            </div>
            <br />
            <div align="center">
                <button ng-click="ok()" class="btn btn-danger"><i class="fa icon-close"></i>&nbsp;&nbsp;Close</button>
            </div>
            <br />
        </div>
    </script>
</div>

<br />
<br />
