
<h3>
    Individual Document
    <a href="#/app/MstIndividualDocument">
        <button type="button" title="Back" class="pull-right btn btn-sm btn-primary ">
            <em class="fa fa-arrow-circle-left fa-1x"></em>
        </button>
    </a>
</h3>
<div ng-controller="MstIndividualDocumentViewController">
    <div id="panelDemo8" class="panel panel-primary ">
        <div class="panel-heading">
            &nbsp;<span class="btn-label bg-primary">
                <i class="fa fa-eye "></i>
            </span> View Individual Document
        </div>
        <div class="panel-body">
            <div class="dd-handle ng-scope">
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12 ">
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12 ">
                                        <b style="color:darkblue;font-size:15px;">
                                            <label>Basic Details</label>
                                        </b>
                                        <hr />
                                    </div>
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-4 col-sm-4 col-md-4">
                                                    <label style="font-weight:bold">Individual Document Name </label>

                                                    <p> {{lblindividualdocument_name}} </p>
                                                </div>
                                                <div class="col-lg-4 col-sm-4 col-md-4">
                                                    <label style="font-weight:bold">Document Type</label>

                                                    <p> {{lbldocument}} </p>
                                                </div>
                                                <div class="col-lg-4 col-sm-4 col-md-4">
                                                    <label style="font-weight:bold">Document Severity</label>

                                                    <p> {{lbldocumentseverity}} </p>
                                                </div>

                                            </div>
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-4 col-sm-4 col-md-4">
                                                    <label style="font-weight:bold">Covenant</label>

                                                    <p> {{lblCovenanttype}} </p>
                                                </div>
                                                <div class="col-lg-4 col-sm-4 col-md-4">
                                                    <label style="font-weight:bold">LMS Code</label>

                                                    <p> {{lbllms_code}} </p>
                                                </div>
                                                <div class="col-lg-4 col-sm-4 col-md-4">
                                                    <label style="font-weight:bold">Bureau Code </label>

                                                    <p> {{lblbureau_code}} </p>
                                                </div>
                                            </div>
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-4 col-sm-4 col-md-4">
                                                    <label style="font-weight:bold">Created By</label>
                                                    <p> {{lblcreated_by}} </p>
                                                </div>
                                                <div class="col-lg-4 col-sm-4 col-md-4">
                                                    <label style="font-weight:bold">Created On</label>
        
                                                    <p> {{lblcreated_date}} </p>
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label style="font-weight:bold">Log Details</label>
                                                    <br>
                                                    <button class="btn btn-info" ng-click="logdetails()">
                                                        <em class="fa fa-history"></em>
                                                    </button>
                                                </div>                                               
                                            </div>
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-4 col-sm-4 col-md-4">
                                                    <label style="font-weight:bold">Document Code</label>

                                                    <p> {{lbldocumentcode}} </p>
                                                </div>
                                                <div class="col-lg-4 col-sm-4 col-md-4">
                                                    <label style="font-weight:bold">Display Order</label>

                                                    <p> {{lbldisplayorder}} </p>
                                                </div>
                                                <div class="col-lg-4 col-sm-4 col-md-4">
                                                    <label style="font-weight:bold">Program</label>
                                                    <div ng-repeat = "data in CboProgram_list">
                                                        <p> {{$index+1}} | {{data.program}} </p>
                                                    </div>
                                                </div>                                                
                                            </div>
                                            <div class="col-lg-12 col-md-12 col-sm-12">                                                
                                                <div class="col-lg-12 col-sm-12 col-md-12">
                                                    <label style="font-weight:bold">Remarks</label>

                                                    <p> {{lblremarks}} </p>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row" ng-show="IsLogShow">
                                            <div class="col-lg-12 col-sm-12 col-md-12">
                                                <br/>
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12  ">
                                                        <p class="lead bb text-purple text-bold" style="font-size:16px;">Update Log</p>
                                    
                                                    </div>
                                                </div>
                                                     
                                                        <div class="table-responsive">
                                                            <div style="max-height:300px;overflow:auto;" >
                                                              <table class="table table-striped table-secondary">
                                                                  <thead class="bg-gray text-white">
                                                                      <tr width="100%">
                                                                          <th align="center" scope="col">S.No</th>
                                                                          <th align="center" scope="col">Updated By</th>
                                                                          <th align="center" scope="col">Updated Date</th>
                                                                      </tr>
                                                                  </thead>
                                                                  <tbody>
                                                                      <tr ng-repeat="data in documentupdatelog_list" width="100%">
                                                                          <td align="left" scope="col" title="'S.No'">{{$index+1}}</td>
                                                                          <td align="left" scope="col" title="Updated By">{{data.updated_by}}</td>
                                                                          <td align="left" scope="col" title="Updated Date'">{{data.updated_date}}</td>                                                                
                                                                      </tr>
                                                                  </tbody>
                                                              </table>
                                                            </div>
                                                        </div>
                                                  <br>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <br />
                        <div class="col-md-12 col-lg-12 col-sm-12">
                            <div class="panel">
                                <div class="panel-heading">
                                    <p class="text-primary" style="color:darkblue;font-size:15px;"><b>&nbsp;&nbsp; Check List Details  </b></p>
                                    <hr />
                                </div>
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="col-lg-12">
                                                <div class="table-responsive table-bordered">
                                                    <div style="max-height:200px;overflow:auto;">
                                                        <table class="table">
                                                            <thead class="bg-gray text-white">
                                                                <tr>
                                                                    <th>SNo</th>
                                                                    <th>Check List</th>

                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in checklistview_list">
                                                                    <td>{{$index+1}}</td>
                                                                    <td style="white-space: pre-wrap;width:100%; word-wrap: break-word; display: inline-block;">{{data.checklist_name}}</td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="text-center">
                        <br />
                        <button ng-click="Back()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                    </div>
                    <br />
                </div>
            </div>
        </div>