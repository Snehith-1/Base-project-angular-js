<h3>
    Physical Document
</h3>
<div ng-controller="AgrTrnSuprCadPhysicalDocApprovalSummaryController">
    <div class="panel">
        <br>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <p style="color:#09385f;font-size: 17px;"><b>Approval Summary</b></p>
                    <hr />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="maker()" style="width:200px;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;">
                            <b> {{overallCountinfo.MakerPendingCount -- overallCountinfo.MakerFollowUpCount}}</b>
                        </div>
                        <b style="font-size: 14px;"> Maker </b>
                    </button>
                    <br />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="checker()" style="width:200px;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;">
                            <b> {{overallCountinfo.CheckerPendingCount -- overallCountinfo.CheckerApprovalPendingCount -- overallCountinfo.CheckerFollowUpCount}}</b>
                        </div>
                        <b style="font-size: 14px;"> Checker </b>
                    </button>
                    <br />
                </div>
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="approval()" style="width:200px;height:50px;border:1px solid rgb(179, 177, 177);background-color: #84cbdb;">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;">
                            <b> {{overallCountinfo.ApproverPendingCount -- overallCountinfo.ApproverFollowUpCount}}</b>
                        </div>
                        <b style="font-size: 14px;">Approval</b>
                    </button>
                    <br />
                </div>
                <div class="col-lg-4 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="Completed()" style="width:200px;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label" style="background-color:#838383;padding-top:5px;padding-bottom:5px;">
                            <b> {{overallCountinfo.CompletedCount}}</b>
                        </div>
                        <b style="font-size: 14px;">Completed</b>
                    </button>
                    <br />
                </div>
            </div>
        </div>
        <div class="row panel-body">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <tabset justified="true">
                    <tab ng-click="approvalPendingSummary()">
                        <tab-heading>
                            Pending <label style="font-size:11px;" class="label label-info pull-right">{{overallCountinfo.ApproverPendingCount}}</label>
                        </tab-heading>
                        <div class="row">
                            <br>
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="table-responsive">
                                        <table datatable="ng" class="table table-hover table-striped">
                                            <thead>
                                                <tr class="text-white nowrap" style="background-color:#09385f;">
                                                    <th> S.No </th>
                                                    <th> Application Number</th>
                                                    <th> Customer Name </th>
                                                    <th> AC Approved Date </th>
                                                    <th> Approval Status </th>
                                                    <!--<th> Sanction Ref.No </th>-->
                                                    <th> CAD Group </th>
                                                    <th> CAD Accepted Date </th>
                                                    <th> CAD Accepted By </th>
                                                    <th class="text-center">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in scannedapproverpendinglist">
                                                    <td data-title="'S.No'"> {{$index+1}} </td>
                                                    <td data-title="'Application Number'"> {{data.application_no}} </td>
                                                    <td data-title="'Customer Name'"> {{data.customer_name}} </td>
                                                    <td data-title="'AC Approved Date'"> {{data.ccapproved_date}} </td>
                                                    <td data-title="'Approval Status'"> {{data.approval_status}} </td>
                                                    <!--<td data-title="'Sanction Ref.No'"> </td>-->
                                                    <td data-title="'CAD Group'"> {{data.cadgroupname}} </td>
                                                    <td data-title="'CAD Accepted Date'"> {{data.cadaccepted_date}} </td>
                                                    <td data-title="'CAD Accepted By'"> {{data.cadaccepted_by}} </td>
                                                    <td align="center" class="nowrap">
                                                        <button title="View" ng-click="view(data.application_gid)" class="btn btn-info">
                                                            <em class="fa fa-eye"></em>
                                                        </button>
                                                        <button title="Process" ng-click="approver_process(data.application_gid, data.created_by, data.processtypeassign_gid)" style="background-color:#871490;" class="btn btn-pink">
                                                            <em class="fa fa-spinner"></em>
                                                        </button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </tab>
                    <tab ng-click="approvalFollowupSummary()">
                        <tab-heading>
                            Followup <label style="font-size:11px;" class="label label-info pull-right">{{overallCountinfo.ApproverFollowUpCount}}</label>
                        </tab-heading>
                        <div class="row">
                            <br>
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="table-responsive">
                                        <table datatable="ng" class="table table-hover table-striped">
                                            <thead>
                                                <tr class="text-white nowrap" style="background-color:#09385f;">
                                                    <th> S.No </th>
                                                    <th> Application Number</th>
                                                    <th> Customer Name </th>
                                                    <th> AC Approved Date </th>
                                                    <th> Approval Status </th>
                                                    <!--<th> Sanction Ref.No </th>-->
                                                    <th> CAD Group </th>
                                                    <th> CAD Accepted Date </th>
                                                    <th> CAD Accepted By </th>
                                                    <th> Status</th>
                                                    <th class="text-center">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in scannedapproverFollowuplist">
                                                    <td data-title="'S.No'"> {{$index+1}} </td>
                                                    <td data-title="'Application Number'"> {{data.application_no}} </td>
                                                    <td data-title="'Customer Name'"> {{data.customer_name}} </td>
                                                    <td data-title="'AC Approved Date'"> {{data.ccapproved_date}} </td>
                                                    <td data-title="'Approval Status'"> {{data.approval_status}} </td>
                                                    <!--<td data-title="'Sanction Ref.No'"> </td>-->
                                                    <td data-title="'CAD Group'"> {{data.cadgroupname}} </td>
                                                    <td data-title="'CAD Accepted Date'"> {{data.cadaccepted_date}} </td>
                                                    <td data-title="'CAD Accepted By'"> {{data.cadaccepted_by}} </td>
                                                    <td data-title="'Status'" ng-switch="data.overall_approvalstatus">
                                                        <label class="label label-success" style="font-size:11px;" ng-switch-when="Approved">{{data.overall_approvalstatus}}</label>
                                                        <label class="label label-info" style="font-size:11px;" ng-switch-when="Proceed to Checker">{{data.overall_approvalstatus}}</label>
                                                        <label class="label label-info" style="font-size:11px;" ng-switch-when="Proceed To Approval">{{data.overall_approvalstatus}}</label>
                                                    </td>
                                                    <td align="center" class="nowrap">
                                                        <button title="View" ng-click="view(data.application_gid)" class="btn btn-info">
                                                            <em class="fa fa-eye"></em>
                                                        </button>
                                                        <button title="Process" ng-click="approver_process(data.application_gid, data.created_by)" style="background-color:#871490;" class="btn btn-pink">
                                                            <em class="fa fa-spinner"></em>
                                                        </button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </tab>
                </tabset>
            </div>
        </div>


        <br />
    </div>
</div>
