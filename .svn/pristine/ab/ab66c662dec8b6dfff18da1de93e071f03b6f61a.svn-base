<h3>
    Samunnati Associate Onboarding
</h3>
<div ng-controller="MstSAOnboardingInstitutionRenewalEditController as dp">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-plus"></i>
            </span>Add Company/Institution
        </div>


        <form novalidate="" name="submitcomapany" ng-submit="form.submitcomapany()" autocomplete="off"
              class="form-validate">
            <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <md-content class="md-padding">
                        <div class="dd-handle ng-scope">
                            <div class="dd-handle ng-scope">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Basic Details </b></p>
                                        <hr />
                                    </div>
                                </div>
                                <br>
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label>Samunnati Associate Name <span style="color:red">*</span></label><br>
                                            <input type="text" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/]*$/"
                                                   maxlength="100" name="sa_associatename" ng-model="txtsamunnati_associate_name"
                                                   class="form-control" required />
                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label class="col-form-label">RM Reporting To </label><br>
                                            {{txtRM}}
                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label> RM Name  </label><br>
                                            {{lblRM}}
                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label>SA Type </label><br>
                                            Company

                                        </div>
                                    </div>

                                </div>
                                <br>
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label>SA Entity Type <span style="color:red">*</span></label><br>
                                            <select name="saentitytype_name" id="saentitytype_name" chosen=""
                                                    ng-options="m.saentitytype_gid as m.saentitytype_name for m in applicationadd_list"
                                                    ng-model="cbosaentitytype" ng-change="entityname_change(cbosaentitytype)" required class="chosen-select form-control">
                                                <option value=""></option>
                                            </select>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label> Origination </label>
                                            <br />
                                            {{lblorigination}}

                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label>Referred By </label><br>
                                            {{txtreferredby}}

                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3">
                                            <label>Registration Fee-UTR Number  </label><br>
                                            {{txtutrno}}

                                        </div>
                                    </div>

                                </div>
                            </div>

                            <br />

                            <div class="dd-handle ng-scope">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <br />
                                        <p class="text-primary" style="font-size:16px;"><b>&nbsp;&nbsp; Contact Person Details </b></p>
                                        <hr />
                                        <br>
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label> First Name <span style="color:red">*</span></label><br>
                                                    <input type="text" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/]*$/" maxlength="50"
                                                           ng-model="txtsacontact_person_first_name" class="form-control" required />
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label> Middle Name </label><br>
                                                    <input type="text" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/]*$/" maxlength="50"
                                                           ng-model="txtsacontact_person_middle_name" class="form-control" />
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label> Last Name <span style="color:red">*</span></label><br>
                                                    <input type="text" ng-pattern="/^[ A-Za-z0-9)(}{\]\['\\/]*$/" maxlength="50"
                                                           ng-model="txtsacontact_person_last_name" class="form-control" required />
                                                </div>
                                            </div>
                                        </div>
                                        <br />
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <br />
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label>Designation <span style="color:red">*</span></label><br>
                                                    <select name="Designation" id="designation_type" chosen=""
                                                            ng-options="m.designation_gid as m.designation_type for m in designationlist"
                                                            ng-model="cboDesignation" required class="chosen-select form-control">
                                                        <option value=""></option>
                                                    </select>
                                                </div>

                                            </div>
                                        </div>

                                        <br>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <br />
                                    <div class="col-md-12 col-lg-12 col-sm-12">
                                        <div class="col-md-6 col-lg-6 col-sm-6">
                                            <div class="panel">
                                                <div class="panel-heading">
                                                    <p class="text-primary" style="font-size:16px;">
                                                        <b>
                                                            &nbsp;&nbsp;
                                                            Mobile Number
                                                        </b>
                                                    </p>
                                                    <hr />
                                                </div>
                                                <div class="panel-body">
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-sm-5 col-md-5 col-lg-5">
                                                                <label><strong>Mobile Number </strong></label> <span style="color:red">*</span>
                                                                <input ng-model="txtmobile_no" type="text" tabindex="0"
                                                                       maxlength="10" ng-pattern="/^[0-9]*$/"
                                                                       aria-required="true" aria-invalid="true"
                                                                       class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched">

                                                            </div>
                                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                                <label>Primary Status </label> <span style="color:red">*</span>
                                                                <div class="radio c-radio">
                                                                    <label>
                                                                        <input type="radio" id="primarymobileno"
                                                                               name="primarymobile_no" value="Yes"
                                                                               ng-model="rdbprimary_status"
                                                                               ng-change="ngchange()" />
                                                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                    </label>
                                                                    <label>
                                                                        <input type="radio" id="primarymobileno"
                                                                               name="primarymobile_no" value="No"
                                                                               ng-model="rdbprimary_status"
                                                                               ng-change="ngchange()" />
                                                                        <span class="fa fa-circle"></span>No
                                                                    </label>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                                <label>Whatsapp Number </label> <span style="color:red">*</span>
                                                                <div class="radio c-radio">
                                                                    <label>
                                                                        <input type="radio" id="whatsappmobileno"
                                                                               name="whatsappmobile_no" value="Yes"
                                                                               ng-model="rdbwhatsapp_no"
                                                                               ng-change="ngchange()" />
                                                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                    </label>
                                                                    <label>
                                                                        <input type="radio" id="whatsappmobileno"
                                                                               name="whatsappmobile_no" value="No"
                                                                               ng-model="rdbwhatsapp_no"
                                                                               ng-change="ngchange()" />
                                                                        <span class="fa fa-circle"></span>No
                                                                    </label>
                                                                </div>
                                                            </div>

                                                            <div class="col-lg-1 col-md-1 col-sm-1 align-top">

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-lg-5 col-md-5 col-sm-5 ">

                                                            </div>
                                                            <div class="col-lg-2 col-md-2 col-sm-2 align-top">
                                                                <a class="btn btn-sm btn-default"
                                                                   style="background-color:#09385f;color:white;"
                                                                   ng-click="add_mobileno()">
                                                                    <span class="fa fa-plus" />
                                                                    Add
                                                                </a>
                                                            </div>
                                                            <div class="col-lg-5 col-md-5 col-sm-5 ">

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />

                                                    <div class="row">
                                                        <div class="col-lg-12">
                                                            <div class="col-lg-12">
                                                                <div class="table-responsive table-bordered">
                                                                    <div style="max-height:200px;overflow:auto;">
                                                                        <table class="table">
                                                                            <thead class="bg-gray text-white">
                                                                                <tr>
                                                                                    <th>S.No</th>
                                                                                    <th>Mobile No</th>
                                                                                    <th>Primary Status</th>
                                                                                    <th>Whatsapp Number </th>
                                                                                    <th>Action</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>

                                                                                <tr ng-repeat="list in SacontactInstimobileno_list">
                                                                                    <td>{{$index+1}}</td>
                                                                                    <td>{{list.samobile_no}}</td>
                                                                                    <td>{{list.saprimary_status}}</td>
                                                                                    <td>{{list.sawhatsapp_no}}</td>
                                                                                    <td class="text-nowrap">
                                                                                        <button title="Delete"
                                                                                                ng-click="delete_mobileno(list.sainstitution2mobileno_gid)"
                                                                                                class="btn btn-danger btn-xs ">
                                                                                            <em class="fa fa-trash-o"></em>
                                                                                        </button>
                                                                                    </td>
                                                                                </tr>
                                                                            </tbody>

                                                                        </table>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />

                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6 col-lg-6 col-sm-6">
                                            <div class="panel">
                                                <div class="panel-heading">
                                                    <p class="text-primary" style="font-size:16px;">
                                                        <b>
                                                            &nbsp;&nbsp;
                                                            Email Address
                                                        </b>
                                                    </p>
                                                    <hr />
                                                </div>
                                                <div class="panel-body">
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-sm-5 col-md-5 col-lg-5">
                                                                <label><strong>Email Address </strong> </label> <span style="color:red">*</span>
                                                                <input type="text" ng-model="txtemail_address" maxlength="100"
                                                                       pattern="[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,3}$"
                                                                       class="form-control">
                                                            </div>
                                                            <div class="col-sm-4 col-md-4 col-lg-4">
                                                                <label> Mail Type</label> <span style="color:red">*</span>
                                                                <div class="radio c-radio" style="align-content: left;">
                                                                    <label>
                                                                        <input type="radio" id="emailaddress"
                                                                               name="email_type" value="Official"
                                                                               ng-model="rdbemail_type"
                                                                               ng-change="ngchange()" />
                                                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Official
                                                                    </label>
                                                                    <label>
                                                                        &nbsp; <input type="radio" id="emailaddress"
                                                                                      name="email_type" value="Personal"
                                                                                      ng-model="rdbemail_type"
                                                                                      ng-change="ngchange()" />
                                                                        <span class="fa fa-circle"></span>Personal
                                                                    </label>
                                                                </div>

                                                                <!--    <select id="mail_type" name="mail_type" dropzone=""  ng-model="cbomail_type" required="" class="form-control ng-valid ng-touched ng-valid-parse">
                                            <option value="Official">Official</option>
                                            <option value="Personal">Personal</option>
                                        </select> -->
                                                            </div>
                                                            <div class="col-sm-3 col-md-3 col-lg-3">
                                                                <label> Primary Email </label> <span style="color:red">*</span>
                                                                <div class="radio c-radio">
                                                                    <label>
                                                                        <input type="radio" id="emailaddress"
                                                                               name="primaryemail_address" value="Yes"
                                                                               ng-model="rdbprimaryemail_address"
                                                                               ng-change="ngchange()" />
                                                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Yes
                                                                    </label>
                                                                    <label>
                                                                        <input type="radio" id="emailaddress"
                                                                               name="primaryemail_address" value="No"
                                                                               ng-model="rdbprimaryemail_address"
                                                                               ng-change="ngchange()" />
                                                                        <span class="fa fa-circle"></span>No
                                                                    </label>
                                                                </div>
                                                            </div>


                                                        </div>
                                                    </div>
                                                    <br />
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                                            <div class="col-lg-5 col-md-5 col-sm-5 ">

                                                            </div>
                                                            <div class="col-lg-2 col-md-2 col-sm-2 align-top">
                                                                <a class="btn btn-sm btn-default"
                                                                   style="background-color:#09385f;color:white;"
                                                                   ng-click="add_emailaddress()">
                                                                    <span class="fa fa-plus" /> Add
                                                                </a>
                                                            </div>
                                                            <div class="col-lg-5 col-md-5 col-sm-5 ">

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                    <div class="row">
                                                        <div class="col-lg-12">
                                                            <div class="col-lg-12">
                                                                <div class="table-responsive table-bordered">
                                                                    <table class="table">
                                                                        <thead class="bg-gray text-white">
                                                                            <tr>
                                                                                <th>S.No</th>
                                                                                <th>Email Address</th>
                                                                                <th>Mail Type</th>
                                                                                <th>Primary Status</th>
                                                                                <th>Action</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody>
                                                                            <tr ng-repeat="list in saOnboardInstiemailaddress_list">
                                                                                <td>{{$index+1}}</td>
                                                                                <td>{{list.saemail_address}}</td>
                                                                                <td>{{list.samail_type}}</td>
                                                                                <td>{{list.saprimary_status}}</td>
                                                                                <td class="text-nowrap">
                                                                                    <a class="btn btn-xs  btn-danger"
                                                                                       ng-click="delete_emailaddress(list.sainstitution2email_gid)">
                                                                                        <span class="fa fa-trash-o"></span>
                                                                                    </a>

                                                                                </td>
                                                                            </tr>
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="dd-handle ng-scope">
                                <div class="row">
                                    <div class="panel-heading">
                                        <p class="text-primary" style="font-size:16px;">
                                            <b>&nbsp;&nbsp; Company Details</b>
                                        </p>
                                        <hr />
                                    </div>
                                </div>
                                <br>
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-sm-4 col-md-4 col-lg-4">
                                            <label>Date of Incorporation <span style="color:red">*</span></label><br>
                                            <br />
                                            <p class="input-group">
                                                <input type="text" id="Date of incorporation" name="date" ng-model="txtdateofincorporation_date" datepicker-popup="{{dp.format}}"
                                                       style="height:36px;" is-open="dp.open1" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                                       close-text="Close" class="form-control" required placeholder="DD-MM-YYYY" ng-change="futuredatecheck(txtdateofincorporation_date)" />
                                                <span class="input-group-btn">
                                                    <button type="button" ng-click="dp.calender1($event)" style="height:36px;" class="btn btn-default">
                                                        <em class="fa fa-calendar"></em>
                                                    </button>
                                                </span>
                                            </p>
                                        </div>
                                        <div class="col-sm-4 col-md-4 col-lg-4">
                                            <label>Company Start Date <span style="color:red">*</span></label><br>
                                            <br />
                                            <p class="input-group">
                                                <input type="text" id="businessstart_date" name="businessstart_date" ng-model="txtcompanystart_date" datepicker-popup="{{dp.format}}"
                                                       style="height:36px;" is-open="dp.open2" min-date="dp.minDate" datepicker-options="dp.dateOptions" ng-change="onchangebusinessstartdate(); futuredatecheck(txtcompanystart_date);"
                                                       close-text="Close" class="form-control" required placeholder="DD-MM-YYYY" />
                                                <span class="input-group-btn">
                                                    <button type="button" ng-click="dp.calender2($event)" style="height:36px;" class="btn btn-default">
                                                        <em class="fa fa-calendar"></em>
                                                    </button>
                                                </span>
                                            </p>
                                        </div>
                                        <div class="col-lg-2 col-md-2 col-sm-2">
                                            <label class="col-form-label"> Years in Business </label>
                                            <br />
                                            <input type="text" ng-model="txtyearin_business" tabindex="0" class="form-control"
                                                   disabled />
                                        </div>
                                        <div class="col-lg-2 col-md-2 col-sm-2">
                                            <label class="col-form-label"> Months in Business </label>
                                            <br />
                                            <input type="text" ng-model="txtmonthsin_business" tabindex="0" class="form-control"
                                                   disabled />
                                        </div>
                                    </div>
                                </div>
                                <br>
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12">
                                        <div class="col-lg-4 col-md-4 col-sm-4">

                                            <label class="col-form-label"> Company PAN Value <span style="color:red"> *</span></label>
                                            <br />
                                            <input type="text" ng-model="txtsa_pannumber" tabindex="0" class="form-control" required ng-pattern="/^[A-Z]{5}[0-9]{4}[A-Z]{1}$/" maxlength="10" ng-change="getPANbasedGST()" />
                                            <label class="fa fa-check" style="color:green" ng-show="panvalidation"></label>&nbsp;<label style="font-weight:bolder;color:green" ng-show="panvalidation">Verified</label>
                                        </div>
                                        <div class="col-sm-4 col-md-4 col-lg-4">
                                            <label>Annual Turnover <span style="color:red">*</span></label><br>
                                            <input id="annual_turnover" ng-model="txtannual_turnover"
                                                   class="form-control ng-pristine ng-valid ng-touched"
                                                   ng-change="annual_turnoverChange()" required style="text-align:right;"
                                                   maxlength="20">

                                            <span style="font-style:italic;">Number in words:</span>
                                            <span id="words_annualturnover" style="color:#003c96fc;" class="text-bold"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="dd-handle ng-scope">
                                <div class="row">
                                    <div class="panel-heading">
                                        <p class="text-primary" style="font-size:16px;">
                                            <b>&nbsp;&nbsp; Assessment Details</b>
                                        </p>
                                        <hr />
                                    </div>
                                </div>
                                <br>
                                <div class="row">
                                    <div class="col-sm-12 col-sm-12 col-sm-12">
                                        <div class="col-sm-4 col-md-4 col-lg-4">
                                            <label for="">Credit Assessment Agency</label>
                                            <select class="form-control chosen-select" id="assessmentagency_name" chosen="" ng-model="cbosaassessmentagency"
                                                    ng-options="m.assessmentagency_gid as m.assessmentagency_name for m in saassessmentagencylist">
                                                <option value=""> -- Credit Assessment Agency -- </option>
                                                <!--<option value="Test"> Test </option>-->
                                            </select>
                                        </div>
                                        <div class="col-sm-5 col-md-5 col-lg-5">
                                            <label for="">Assessment Rating</label>
                                            <select class="form-control chosen-select" id="assessmentagencyrating_name" chosen="" ng-model="cboassessmentagencyrating"
                                                    ng-options="m.assessmentagencyrating_gid as m.assessmentagencyrating_name for m in assessmentagencyratinglist">
                                                <option value=""> -- Assessment Rating -- </option>
                                            </select>
                                        </div>
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <label for=""> Rating As On </label>
                                            <p class="input-group">
                                                <input type="text" id="ratingason" name="ratingason" ng-model="txtrasteason_date" datepicker-popup="{{dp.format}}"
                                                       style="height:36px;" is-open="dp.open6" min-date="dp.minDate" datepicker-options="dp.dateOptions"
                                                       close-text="Close" class="form-control" />
                                                <span class="input-group-btn">
                                                    <button type="button" ng-click="dp.calender6($event)" style="height:36px;" class="btn btn-default">
                                                        <em class="fa fa-calendar"></em>
                                                    </button>
                                                </span>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />

                            <!--GST Details-->
                            <div class="dd-handle ng-scope">
                                <div class="row">

                                    <div class="col-lg-12 col-md-12 col-sm-12">

                                        <div class="panel-heading">
                                            <p class="text-primary" style="font-size:16px;">
                                                <b>
                                                    &nbsp;&nbsp; GST Details
                                                </b>
                                            </p>
                                            <hr />
                                        </div>

                                        <div>
                                            <div class="col-sm-2 col-md-2 col-lg-2">
                                                <label> GST Registration</label> <span style="color:red">*</span>
                                                <div class="radio c-radio">
                                                    <label>
                                                        <input type="radio" name="rbogst_status" value="Yes" ng-change="onselected_yes()"
                                                               ng-model="rdbgstregister_status" ng-click="" />
                                                        &nbsp;&nbsp;<span class="fa fa-circle"></span> Yes
                                                    </label>
                                                    <label>
                                                        <input type="radio" name="rbogst_status" value="No" ng-change="onselected_yes()"
                                                               ng-model="rdbgstregister_status" ng-click="" />
                                                        <span class="fa fa-circle"></span> No
                                                    </label>

                                                </div>

                                            </div>

                                            <div class="col-lg-5 col-md-5 col-sm-5">
                                                <label class="col-form-label">
                                                    GST Number <span style="color:red">
                                                        *
                                                    </span>
                                                </label>
                                                <br />
                                                <input type="text" ng-model="txtgst_registration_number" tabindex="0"
                                                       class="form-control" maxlength="15"
                                                       ng-pattern="/^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[1-9A-Z]{1}Z[0-9A-Z]{1}$/"
                                                       ng-change="onchangegst_number()" />
                                            </div>
                                            <div class="col-sm-4 col-md-4 col-lg-4">
                                                <label class="col-form-label"> GST State </label>
                                                <br />
                                                <input type="text" ng-model="txtgst_state" tabindex="0" class="form-control"
                                                       disabled />
                                            </div>

                                            <div class="col-sm-1 col-lg-1 col-md-1">
                                                <br />
                                                <a class="btn btn-sm btn-default"
