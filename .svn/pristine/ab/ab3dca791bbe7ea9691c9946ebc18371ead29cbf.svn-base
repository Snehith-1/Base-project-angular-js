<a href="#/app/idasTrnNocAndNdc">
    <button type="button" title="Back" class="pull-right btn btn-sm btn-primary">
        <em class="fa fa-arrow-circle-left fa-1x"></em>
    </button>
</a>
<h3>
    Noc & Ndc View
</h3>
<div ng-controller="idasTrnNocAndNdcViewController">
    <div id="panelDemo8" class="panel panel-primary ">
        <div class="panel-heading">
            <span class="btn-label">
                <i class="fa fa-eye"></i>
            </span>View Noc & Ndc
        </div>
        <div class="col-lg-12 col-sm-12 col-md-12 panel">
            <br />
            <!--<div class="dd-handle ng-scope">-->
            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">

                    <div class="dd-handle ng-scope" >
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12 ">
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12 ">
                                        <b style="color:darkblue;font-size:15px;">
                                            <label>Basic Details</label>
                                        </b>
                                        <hr />
                                    </div>
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Customer name </label>
                                                    
                                                    <p> {{lblcustomer_name}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Vertical</label>
                                                    
                                                    <p> {{cboVertical}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Sanction Ref Number</label>
                                                    
                                                    <p> {{lblsanction_ref_no}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Sanction Date</label>
                                                    
                                                    <p> {{lblsanctiondate}} </p>
                                                </div>
                                            </div>
                                        </div>

                                        <br />
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12">
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">Maker Name</label>
                                                    
                                                    <p> {{cbomaker}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold">  Checker Name </label>
                                                    
                                                    <p> {{cbochecker}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold"> Loan Account Numbers </label>
                                                    
                                                    <p> {{lblloan_account_no}} </p>
                                                </div>
                                                <div class="col-lg-3 col-sm-3 col-md-3">
                                                    <label style="font-weight:bold"> Loan Closure Date </label>                                                   
                                                    <p>{{lblloanclosuredate}}</p>
                                                </div>
                                            </div>
                                            <br />
                                        </div>
                                        <div class="row">
                                            <div class="col-md-12 col-lg-12 col-sm-12">
                                                <br />
                                                <div class="col-lg-3 col-md-3 col-sm-3">
                                                    <label class="col-form-label">  NOC NDC Request Date  </label>
                                                    
                                                   <p> {{lblnocrequestdate}}</p>
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-sm-3">
                                                    <label class="col-form-label"> NOC NDC Issuance Date </label>
                                                    
                                                    <p>{{lblnocissuancedate}}</p>
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-sm-3">

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>
                    <!--<br />-->

                    <div class="dd-handle ng-scope">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="row">
                                    <div class="col-lg-12 col-sm-12 col-md-12 ">
                                        <b style="color:darkblue;font-size:15px;">
                                            <label> Document Upload List</label>
                                        </b>
                                        <hr />
                                    </div>
                                    <div class="col-lg-12 col-sm-12 col-md-12">
                                        <div class="table-responsive table-bordered">
                                            <table class="table m0">
                                                <thead>
                                                    <tr width="100%" class="bg-gray-light text-white">
                                                        <th scope="col">S.No</th>
                                                        <th scope="col">Document Title</th>
                                                        <th scope="col">Document Name</th>
                                                        <th scope="col">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="data in UploadDocumentList">
                                                        <td title="S.No">{{$index+1}}</td>
                                                        <td>{{data.document_name}}</td>
                                                        <td>{{data.file_name}}</td>
                                                        <td class="nowrap" data-title="'Action'">
                                                            <button title="Download" class="btn btn-green btn-sm"
                                                                    ng-click="downloads(data.document_path,data.file_name)">
                                                                <em class="fa fa-download"></em><a download="" href="" id="btnpdf" style="display:none;"></a>
                                                            </button>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />


                </div>
                <div class="text-center">
                    <button type="button" class="btn btn-info btn-md" ng-click="back()"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>&nbsp;&nbsp;
                </div>
                <br />

            </div>
        </div>
    </div>
</div>