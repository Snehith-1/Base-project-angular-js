<h3>
    Sanction MIS Report - Maker
     
 </h3>
 <div ng-controller="MstSanctionMISReportMakerController as dp">
     <div class="panel">
         <br />
         <div class="col-lg-11 col-sm-11 col-md-11 " style="text-align:right;padding-bottom:10px;">
         </div>
         <div class="col-lg-1 col-sm-1 col-md-1 " style="text-align:right;padding-bottom:10px;">
             <button type="button" class="btn btn-success" ng-click="exportsanctionreport()" title="Export Excel"><i class="fa fa-file-excel-o"></i></button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
             <br /><label style="background-color:white;left:90%;"><a download="" href="" id="btnpdf" style="display:none;"></a> Export Excel </label>
             
         </div>
         <div class="row">
             <div class="col-lg-12 col-md-12 col-sm-12">
                 <div class="col-lg-12 col-md-12 col-sm-12">
                     <div class="panel-body">
                         <div class="table-responsive">
                             <table datatable="ng" class="table table-hover table-striped">
                                 <thead>
                                     <tr class="text-white" style="background-color:#09385f;">
                                         <th> S.No </th>
                                         <th> Application Ref No</th>
                                         <th> URN No </th>
                                         <th> Customer Name</th>
                                         <th> Sanction Ref No</th>
                                         <th> Sanction Date</th>                 
                                         <th> Sanction Amount </th>
                                         <th class="text-center">Action</th>
                                     </tr>
                                 </thead>
                                 <tbody>
                                     <tr ng-repeat="data in SanctionMISdtl_list">
                                         <td data-title="'S.No'"> {{$index+1}} </td>
                                         <td data-title="'Application Ref No'">{{data.application_no}}</td>
                                         <td data-title="'URN No'">{{data.customer_urn}}</td>
                                         <td data-title="'Customer Name'">{{data.customer_name}}</td>
                                         <td data-title="'Sanction Ref No'"> {{data.sanction_refno}}</td>
                                         <td data-title="'Sanction Date'"> {{data.sanction_date}}</td>
                                         <td data-title="'Sanction Amount'"> {{data.sanction_amount}} </td>   
                                         <td data-title="'Action'" class="nowrap">
                                             <button title="Sanction 360" class="btn btn-primary btn-sm"
                                                     ng-click="sanctionmisreportview(data.application2sanction_gid,data.application_gid)">
                                                 <em class="fa fa-retweet"></em>
                                             </button>
                                          
                                         </td>
                                     </tr>
                                 </tbody>
                             </table>
                         </div>
                     </div>
                 </div>
             </div>
         </div>
 
     </div>
 
 </div>