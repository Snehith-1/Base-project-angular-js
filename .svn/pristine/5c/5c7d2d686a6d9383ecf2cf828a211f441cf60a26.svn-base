<h3>
    Register Lawyer
</h3>
<div ng-controller="addLawyerController as dpick">
    <div id="panelDemo8" class="panel panel-primary">
        <div class="panel-heading ">
            <span class="btn-label">
                <i class="fa fa-plus"></i>
                Register Lawyer
            </span>
        </div>
        <div class="panel-body">
            <div class="modal-body ng-scope">
                <form novalidate="" name="userform" ng-submit="form.submitLoan()" autocomplete="off" class="form-validate mb-lg">
                    <div class="row" style="display:none;">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="col-lg-8 col-md-8 col-sm-8 ">
                                <label style="font-size:5px;" ng-model="lawyerregister_gid" class="col-form-label w3-text-indigo"><strong></strong></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-1 col-md-1 col-sm-1">
                    </div>
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Lawyer Name <span style="color:red">*</span></label>
                                <input ng-model="txtlawyername" tabindex="0" maxlength="45" required="" aria-required="true" aria-invalid="true"
                                       class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                            </div>
                       
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Enrolment Number <span style="color:red">*</span> </label>
                                <input ng-model="txtenrolment_no" required="" maxlength="68" tabindex="0" aria-required="true" aria-invalid="true"
                                       ng-change="enrolmentno_validation()" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                <div ng-show="validationMsg">
                                    <label style="color:red;"> Enrolment No Already Exists</label>
                                </div>
                            </div>
                          
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>DOB </label>
                                <p class="input-group">
                                    <input type="text" id="dob" name="date" ng-model="dob" datepicker-popup="{{dpick.format}}"
                                           style="height:30px;" is-open="dpick.opened" min-date="dpick.minDate" datepicker-options="dpick.dateOptions"
                                           close-text="Close" class="form-control" />
                                    <span class="input-group-btn">
                                        <button type="button" ng-click="dpick.open($event)" style="height:30px;" class="btn btn-default">
                                            <em class="fa fa-calendar"></em>
                                        </button>
                                    </span>
                                </p>
                            </div>
                           
                            
                        </div>
                    </div>
                    <br/>
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Gender </label>
                                <div class="radio c-radio">
                                    <label>
                                        <input type="radio" id="Gender" name="Gender" value="Male" ng-model="gender" />
                                        &nbsp;&nbsp;<span class="fa fa-circle"></span>Male
                                    </label>
                                    <label>
                                        <input type="radio" id="Gender" name="Gender" value="Female" ng-model="gender" />
                                        <span class="fa fa-circle"></span>Female
                                    </label>

                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Mobile No <span style="color:red">*</span> </label>
                                <input ng-model="txtmobileno" required="" minlength="10" type="number" maxlength="12" tabindex="0"
                                       aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">

                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Telephone No  </label>
                                <input ng-model="txttelephoneno" type="number" maxlength="12" tabindex="0" aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                            </div>


                            </div>
                    </div>
                    <br/>
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Email Address <span style="color:red">*</span> </label>
                                <input ng-model="txtemailaddress" type="email" required="" tabindex="0" aria-required="true" aria-invalid="true"
                                       class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Educational Qualification <span style="color:red">*</span> </label>
                                <input ng-model="txtqualification" required="" maxlength="68" tabindex="0" aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">

                            </div>
                           
                            
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label> Enrolment Date with Bar Council <span style="color:red">*</span></label>
                                <p class="input-group">
                                    <input type="text" name="date" required="" ng-model="txtdateenrol_council" datepicker-popup="{{dpick.format}}"
                                           style="height:30px;" is-open="dpick.closed" min-date="dpick.minDate" datepicker-options="dpick.dateOptions"
                                           close-text="Close" class="form-control" />
                                    <span class="input-group-btn">
                                        <button type="button" ng-click="dpick.close($event)" style="height:30px;" class="btn btn-default">
                                            <em class="fa fa-calendar"></em>
                                        </button>
                                    </span>
                                </p>

                            </div>
                        </div>
                    </div>
                    <br/>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Experience at Bar Council <span style="color:red">*</span></label>
                                <input ng-model="txtexperience" required="" maxlength="15" tabindex="0" aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">


                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label> Place of Empanelment </label>
                                <input ng-model="txtplace_practice" tabindex="0" maxlength="45" aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">


                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>PAN No </label>
                                <input ng-model="txtpanno" tabindex="0" maxlength="10" aria-required="true" aria-invalid="true" onkeyup="this.value = this.value.toUpperCase();" pattern="^[A-Z0-9A-Za-z]+$" ng-pattern-restrict
                                       class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">


                            </div>
                        </div>
                    </div>
                    <br/>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Aadhar No <span style="color:red">*</span></label>
                                <input ng-model="txtaadhar_no" required="" maxlength="14" tabindex="0" aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">


                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label> Bank Name </label>
                                <input ng-model="txtbank_name" tabindex="0" maxlength="68" aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">


                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Account No </label>
                                <input ng-model="txtaccount_no" tabindex="0" maxlength="68" aria-required="true" aria-invalid="true" 
                                       class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">


                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>IFSC Code</label>
                                <input ng-model="txtifsc_code" tabindex="0" maxlength="68" aria-required="true" aria-invalid="true"
                                       class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label><strong>Address Line 1 </strong></label>
                                <textarea rows="2" aria-multiline="true" tabindex="0" aria-invalid="false" ng-model="txtaddress1" class="form-control"></textarea>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label><strong>Address Line 2 </strong></label>
                                <textarea rows="2" aria-multiline="true" tabindex="0" aria-invalid="false" ng-model="txtaddress2" class="form-control"></textarea>

                            </div>
                            
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                            <div class="col-lg-4 col-md-4 col-sm-4">

                                <div ng-show="hidephotodiv">
                                 
                                        <strong>Upload Photo</strong>
                                        <input type="file" filestyle="" id="adduploadphoto" ng-model="upload_photo" onchange="angular.element(this).scope().photo(this.files, angular.element(this).scope().documentupload, this.name)"
                                               class="form-control" data-classbutton="btn btn-default" data-classinput="form-control inline" data-button-text="Select" />
                                 
                                </div>
                                <div ng-show="showphotodiv">
                                    <div class="mda-form-group float-label">
                                        <strong>Upload Photo</strong><br />
                                        Photo Uploaded
                                    </div>
                                </div>

                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>State </label>
                                <input ng-model="txtstate" tabindex="0" maxlength="25" aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">


                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Postal Code </label>
                                <input type="number" maxlength="6" ng-model="txtpostalcode" tabindex="0" aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">

                            </div>
                         
                        </div>
                    </div>
                    <br/>
                    <div class="row">
                        <div class="col-sm-12 col-sm-12 col-sm-12 ">
                           
                            
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <label>Country <span style="color:red">*</span></label>
                                <input ng-model="txtcountry" required="" maxlength="40" tabindex="0" aria-required="true" aria-invalid="true" class="form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">


                            </div>
                        </div>
                    </div>
                    <br/>
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-md-12">
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <strong class="ng-binding w3-text-indigo">
                                    <em class="fa fa-file text-primary fa-lg"></em>&emsp;
                                    Upload Empanelment Certificate
                                </strong>
                            </div>
                        </div>
                    </div>
                    <br/>
                    <div class="dd-handle ng-scope">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="col-lg-5 col-md-5 col-sm-5">
                                    <div class="mda-form-group float-label">
                                        <input ng-model="document_type" tabindex="0" maxlength="45" aria-required="true" aria-invalid="true" class="mda-form-control ng-pristine ng-invalid ng-invalid-required ng-touched" style="">
                                        <label>Document Title </label>
                                    </div>
                                </div>
                                <div class="col-lg-1 col-md-1 col-sm-1">
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                       <strong>Upload Documents</strong>
                                        <input id="addupload" filestyle="" type="file" data-classbutton="btn btn-default" data-classinput="form-control inline"
                                                 ng-model="test_document" onchange="angular.element(this).scope().upload(this.files, angular.element(this).scope().documentupload, this.name)"
                                            data-button-text="Select" />
                                 
                                </div>
                            </div>
                        </div>
                  
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-md-12">
                                <div class="col-lg-12 col-sm-12 col-md-12">
                                    <div class="table-responsive">
                                        <table class="table table-striped table-secondary">
                                            <thead class="bg-gray text-white">
                                                <tr width="100%">
                                                    <th scope="col">S.No</th>
                                                    <th scope="col">Document Title</th>
                                                    <th scope="col">Document Name</th>
                                                    <th scope="col">Uploaded Details</th> 
                                                    <th scope="col">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in filename_list" width="100%">
                                                    <td title="S.No">{{$index+1}}</td>
                                                    <td title="Document Title">{{data.document_type}}</td>
                                                    <td title="Document Name">{{data.document_name}}</td>
                                                    <td title="Uploaded Details">{{data.uploaded_by}} - {{data.updated_date}}</td>
                                                    <td data-title="Action">
                                                        <a class="btn btn-xs btn-danger" ng-click="document_cancelclick(data.document_gid)" title="Delete"><span class="fa fa-close"></span></a>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div>
                <div class="text-center">
                    <button type="button" class="btn btn-success btn-md" ng-disabled="userform.$invalid" ng-click="lawyerSubmit()"><i class="fa fa-arrow-circle-right"></i>&nbsp;&nbsp;Submit</button>&nbsp;&nbsp;
                    <button ng-click="lawyerback()" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>
                </div>
            </div>
        </div>
    </div>
</div>