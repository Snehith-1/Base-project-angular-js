<h3>
    SBA Code Creation
</h3>
<div ng-controller="MstSAOnboardingCodeApprovalInsSummaryController">
    <div class="panel">
        <br />
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <p style="color:#09385f;font-size: 17px;"><b>Institution Summary</b></p>
                    <hr />
                </div>
               
                <div class="col-lg-2 col-md-6 col-sm-12">
                    <button type="button" class="btn btn-labeled" ng-click="institution_approved()"
                            style="width:200px;height:50px;border:1px solid rgb(179, 177, 177);background-color: #84cbdb;">
                        <div class="pull-right label"
                             style="background-color:#838383;padding-top:5px;padding-bottom:5px;">
                            <b>
                                {{institutioninitiated_count}}
                            </b>
                        </div>
                        <b style="font-size: 14px;">Institution approved</b>
                    </button>
                    <br />
                </div>
              
                <div class="col-lg-2 col-md-6 col-sm-12">

                    <button type="button" class="btn btn-labeled" ng-click="individual_approved()"
                            style="width:200px;height:50px;border:1px solid rgb(179, 177, 177);">
                        <div class="pull-right label"
                             style="background-color:#838383;padding-top:5px;padding-bottom:5px;">
                            <b>
                                {{individualinitiated_count}}
                            </b>
                        </div>
                        <b style="font-size: 14px;">Individual approved</b>
                    </button>
                    <br />
                </div>
            </div>
        </div>

        <br />
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="table-responsive">
                        <table datatable="ng" class="table table-hover table-striped">
                            <thead>
                                <tr class="text-white" style="background-color:#09385f;">
                                    <th> S.No </th>
                                    <th>SA-Auto-GeneratedID </th>
                                    <th>Samunnati Associate Name </th>
                                    <th> Status</th>
                                    <th>Approval By</th>
                                    <th>Approval Date</th>
                                    <th>Submitted By</th>
                                    <th>Submitted Date</th>
                                    <th> TAT </th>
                                    <th class="text-center">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in saOnboardSummary_list">
                                    <td>{{$index+1}}</td>
                                    <td>{{data.sa_autogeneratedid}}</td>
                                    <td>{{data.sa_associatename}}</td>
                                    <td>{{data.approvalstatus}}</td>
                                    <td>{{data.approved_by}}</td>
                                    <td>{{data.approved_date}}</td>
                                    <td>{{data.sa_updated_by}}</td>
                                    <td>{{data.sa_updated_date}}</td>
                                    <td>{{data.sa_tat}}</td>
                                    <td class="nowrap" data-title="'Action'" style="text-align: center;">
                                        <button title="Edit" ng-click="saonboardingverification(data.sacontactinstitution_gid)" ng-show="data.code_created_flag == 'N'"
                                                style="text-align: center;" class="btn btn-green">
                                            <em class="fa fa-check-square-o"></em>
                                        </button>
                                        <button title="View" ng-click="viewInstitution(data.sacontactinstitution_gid)" ng-show="data.code_created_flag == 'Y'" class="btn btn-info">
                                            <em class="fa fa-eye"></em>
                                        </button>

                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <br />
    </div>

</div>