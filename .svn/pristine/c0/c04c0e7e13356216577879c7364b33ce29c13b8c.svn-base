<div ng-controller="itDashboardcontroller">
    <div class="panel">
    <div class="row">
        <div class="col-lg-12 col-sm-12 col-md-12">
            <br />
            <h4 style="color:black;" class="text-center animated fadeInDown anim-delay-10"> Service Desk </h4>
            <hr />
            <br /> 
        </div>
    </div>
   
    <div class="row">
        <div class="col-lg-12 col-sm-12 col-md-12">
            <div ng-switch="newticket">
                <div class="col-lg-2 col-sm-6 col-md-3" ng-switch-default>

                    <div class="panel-body text-center" style="background-color:#F0EFEF;">

                        <em style="color:grey;" class="fa fa-ticket fa-2x"></em>
                        <md-tooltip> No Access </md-tooltip>
                        <h5 style="color:grey;"><b>New service Ticket</b></h5>
                    </div>
                </div>
                <div ng-switch-when="Y">
                    <a href='#/app/newServiceTicket'>
                        <div class="col-lg-2 col-sm-6 col-md-3">

                            <div class="panel-body bg-teal-100 text-center">

                                <em class="fa fa-ticket fa-2x" style="color:#0c8a7f;"></em>
                                <md-tooltip>New service Ticket</md-tooltip>
                                <h5 style="color:black;"><b>New service Ticket</b></h5>
                            </div>
                        </div>
                    </a>
                </div>
            </div>
            <div ng-switch="viewticket">
                <div class="col-lg-2 col-sm-6 col-md-3" ng-switch-default>

                    <div class="panel-body text-center" style="background-color:#F0EFEF;">

                        <em style="color:grey;" class="fa fa-eye fa-2x"></em>
                        <md-tooltip> No Access </md-tooltip>
                        <h5 style="color:grey;"><b>View service Ticket</b></h5>
                    </div>
                </div>
                <div ng-switch-when="Y">
                    <a href='#/app/viewServiceTicket'>
                        <div class="col-lg-2 col-sm-6 col-md-3">

                            <div class="panel-body bg-pink-100 text-center">

                                <em class="fa fa-eye fa-2x" style="color:#c33364;"></em>
                                <md-tooltip>View service Ticket</md-tooltip>
                                <h5 style="color:black;"><b>View service Ticket</b></h5>
                            </div>
                        </div>
                    </a>
                </div>
            </div>
       </div>
    </div>
    <div class="col-lg-12 col-sm-6 col-md-12"></div>
    <br /><br /><br /><br /><br /><br /><br /><br /><br />
    <br /><br /><br /><br /><br /><br /><br /><br /><br />
    <br /><br /><br /><br /><br /><br /><br />
</div>
</div>

