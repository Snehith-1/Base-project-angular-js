<h3>
    View Sanction
</h3>
<div ng-controller="sanctionViewcontroller">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <em class="fa fa-eye"></em>&nbsp; View Sanction Details
        </div>
        <div class="panel-body">
            <div class="row">
                <form name="sanctionform" id="sanctionform" novalidate="" class="form-validate mb-lg" autocomplete="off">

                    <div class="col-lg-12 col-sm-12 col-md-12">
                        <fieldset>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12  text-center">
                                    <!--<label>Customer Name :</label>-->
                                    <label class="text-primary" style="font-size:16px;">{{customer_name}} / <span class="text-primary">{{txteditsanctionRefNo}}</span></label>
                                    <br/>
                                    URN No:&nbsp;<label class="text-pink" style="font-size:16px;">{{customer_urn}}</label>
                                </div>
                                <div>
                                   
                                </div>
                                <!--<div class="col-lg-6 col-sm-6 col-md-6">
                                    <div class="form-group float-label">
                                        <label>Sanction Ref.No :</label>
                                        <p>{{txteditsanctionRefNo}}</p>
                                    </div>
                                </div>-->
                            </div>
                        </fieldset>

                        <fieldset>
                            <div class="row">
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    <label>Sanction Date :</label>
                                    <p>{{txteditSanctionDate}}</p>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <div class="form-group float-label">
                                        <label>Sanction Amount :</label>
                                        <p>{{txteditSanctionAmount}}</p>
                                    </div>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <div class="form-group float-label">
                                        <label>Sanction Limit :</label>
                                        <p>{{txteditSanctionLimit}}</p>
                                    </div>
                                </div>
                            </div>
                        </fieldset>

                        <fieldset>
                            <div class="row">
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    <label>Validity :</label>
                                    <p>{{txtEditValidity}}</p>
                                </div>
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    <label>Expiry Date :</label>
                                    <p>{{txtEditExpiryDate}}</p>
                                </div>
                                <div class="col-lg-4 col-sm-4 col-md-4">
                                    <label>Review Date :</label>
                                    <p>{{txtEditReviewDate}}</p>
                                </div>
                            </div>
                        </fieldset>
                        <fieldset>
                            <div class="row">
                                <div class="col-lg-4 col-sm-4 col-md-4 form-group">
                                    <label>Approval Authority :</label>
                                    <p>{{cboeditapproval_authority}}</p>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4 form-group">
                                    <label>Nature of Proposal :</label>
                                    <p>{{cboEditnature_proposal}}</p>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4 form-group">
                                    <label>Constitution :</label>
                                    <p>{{cboEditconstitution}}</p>
                                </div>
                            </div>

                        </fieldset>

                        <fieldset>
                            <div class="row">
                                <div class="col-lg-4 col-md-4 col-sm-4 form-group">
                                    <label>Sanction Letter Authorized Signatory :</label>
                                    <p>{{cboEditauthorizedsignatory}}</p>
                                </div>

                                <div class="col-lg-4 col-md-4 col-sm-4 form-group">
                                    <label>Type Of Facility :</label>
                                    <p>{{cboEditfacilitytype}}</p>
                                </div>
                            </div>

                        </fieldset>

                        <fieldset>
                            <div class="row">
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label>Revisied Limit :</label>
                                    <p>{{txtEditRevisiedLimit}}</p>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <label>Existing Limit :</label>
                                    <p>{{txtEditexistingLimit}}</p>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4">
                                    <div class="form-group">
                                        <label>Escrow Account :</label>
                                        <p>{{cboEditEscrowAccount}}</p>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                        <fieldset>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12 form-group">
                                    <label>Specific Conditions :</label>
                                    <p>{{txtEditspecificcondition}}</p>
                                </div>
                            </div>
                        </fieldset>
                        <fieldset>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-md-12 form-group">
                                    <label>Documentation List :</label>
                                    <div class="table-responsive">
                                        <table class="row-border hover" id="statictable">

                                            <thead>
                                                <tr class="bg-success text-white">
                                                    <th width="3%">S.NO</th>
                                                    <th width="5%">Documentation Ref No.</th>

                                                    <th width="10%">Documentation Name</th>

                                                    <th width="10%">Documentation Type</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in documentationname" class="bg-indigo-50 row-border" style="grid-row:auto" width="100%">
                                                    <td class="text-center">{{$index +1}}</td>
                                                    <td>{{data.documentation_refno}}</td>

                                                    <td>{{data.documentation_name}}</td>

                                                    <td>{{data.documentation_type}}</td>

                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                </div>
                            </div>
                        </fieldset>
                        <div class="row">
                            <div class="text-center">

                                <a class="btn btn-info" ng-click="back()">
                                    <em class="fa fa-arrow-left"></em>
                                    <span>Back</span>
                                </a>
                            </div>
                        </div>
                    </div>

                </form>
            </div>
        </div>
    </div>
</div>