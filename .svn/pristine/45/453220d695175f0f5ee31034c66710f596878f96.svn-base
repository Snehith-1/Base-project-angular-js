
<h3>
    BRE Execution View
</h3>
<style>

    .dataTables_paginate 
{
    display:none
}
    .dataTables_length
    {
        display:none
    }
</style>
<div ng-controller="MstBREExecutionViewController">
    <div class="panel-body">
        <div class="panel panel-primary">
            <div class="panel-heading">
                &nbsp;  BRE Execution View
            </div>     
            <br/>         
                 <div class="panel panel-default">                                                                     
                                <div class="row">
                                    <div class="text-center col-lg-12 col-sm-12 col-md-12">
                                        <div class="dd-handle ng-scope" style="background-color: #f9f8f8;">
                                            <div class="row">
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label style="font-size:12px;" class="text-primary">
                                                        Application Number
                                                    </label>
                                                    <br>
                                                    <span style="font-size:13px;">
                                                        {{application_no}}
                                                    </span>
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <label style="font-size:12px;" class="text-primary">
                                                        Template Name
                                                    </label>
                                                    <br>
                                                    <span style="font-size:13px;">
                                                        {{template_name}}
                                                    </span>
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-sm-4">

                                                    <label style="font-size:12px;" class="text-primary">
                                                        Success percentage
                                                    </label>
                                                    <br>


                                                    <style>
                                                      

                                                        .container {
                                                            background-color: rgb(192, 192, 192);
                                                            width: 12%;
                                                            height: 18px;
                                                            border-radius: 10px;
                                                        }

                                                        .skill {
                                                            background-color: rgb(116, 194, 92);
                                                            color: black;
                                                            padding: 1%;
                                                            text-align: right;
                                                            font-size: 12px;
                                                            border-radius: 8px;
                                                        }

                                                        .html {
                                                            width: 20%;
                                                            border-radius: 15px;
                                                             height: 18px;
                                                        }


                                                       
                                                    </style>                                                                                                                                                              
                                                        <div class="container">
                                                            <div class="skill html">{{percentage}}</div>
                                                        </div>                                                                                                                                        
                                                  
                                                </div>
                                            </div>
                                        </div>
                                        </div>

                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="panel-body">                                             
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <div class="table-responsive">
                                                        <table datatable="ng" class="table table-hover table-striped">
                                                            <thead>
                                                                <tr class="text-white" style="background-color:#09385f;">
                                                                    <th> S.No </th>                                                                 
                                                                    <th> Rule Title </th>
                                                                    <th> Rule Description </th>
                                                                    <th class="text-center"> Status</th>
                                                                    <th class="text-center"> Remarks</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="data in postexecute_list ">
                                                                    <td data-title="'S.No'" class="text-center">{{$index+1}}</td>                                                         
                                                                    <td data-title="'Rule Title'">{{data.rule_title}}</td>
                                                                    <td data-title="'Rule Description'">{{data.param_remarks}}</td>
                                                                    <td data-title="'Result'" class="text-center" ng-switch="data.ruleengine_result">
                                                                        {{data.ruleengine_result}}
                                                                        <label class="label label-green" ng-switch-when='Pass' style="font-size:12px;">
                                                                            <em class="fa fa-thumbs-up"> </em>
                                                                        </label>
                                                                        <label class="label label-danger" ng-switch-when='Failed' style="font-size:12px;">
                                                                            <em class="fa fa-thumbs-down"> </em>
                                                                        </label>
                                                                    </td>
                                                                    <td data-title="'Remarks'" class="text-center">{{data.ruleengineresult_details}}</td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                                <div class="text-center">
                                                    <button type="button" class="btn btn-info btn-md" ng-click="back()"><i class="fa fa-arrow-circle-left"></i>&nbsp;&nbsp;Back</button>&nbsp;&nbsp;
                                                </div>
                                                <br />
                                            </div>
                                        </div>
                                    </div>                 
                    </div>
                </div>
              </div>
    </div>
