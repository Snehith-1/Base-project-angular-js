    <div class="row">
        <div class="col-lg-12 col-sm-12 col-md-12">
            <!-- START panel-->
            <div id="panelDemo13" class="panel panel-default">
                <div class="panel-body">
                    <div class="table-responsive">
                        <table datatable="ng" class="table table-striped row-border hover">
                            <thead>
                                <tr class="text-white" style="background-color:#09385f;">
                                    <th> S.No </th>
                                    <th> Reference Number </th>
                                    <th> Created Date</th>
                                    <th> Employee Name </th>
                                    <th> Role </th>
                                    <th> Department </th>
                                    <th> Amount </th>
                                    <th> Approval Status </th>
                                    <!--<th> Status </th>-->
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="data in Report_list">
                                    <td data-title="'S.No'">{{$index+1}}</td>
                                    <td data-title="'Reference Number'">{{data.request_refno}}</td>
                                    <td data-title="'Created Date'">{{data.created_date}}</td>
                                    <td data-title="'Employee Name'">{{data.employee_name}}</td>
                                    <td data-title="'Employee Role'">{{data.employee_role}}</td>
                                    <td data-title="'Department Name'">{{data.department_name}}</td>
                                    <td data-title="'Amount'">{{data.amount}}</td>
                                    <td data-title="'Request Status'">{{data.request_status}}</td>
                                    <!-- <td data-title="'Status'">{{data.status1}}</td> -->
