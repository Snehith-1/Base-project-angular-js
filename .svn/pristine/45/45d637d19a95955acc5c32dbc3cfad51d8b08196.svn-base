<h3>
    Loan Report
</h3>
<div ng-controller="RptEmployeeLoanReportController">
    <div class="col-lg-12 col-sm-12 col-md-12 " style="text-align:right;padding-bottom:10px;">
        <button type="button" class="btn btn-success" ng-click="Loanreport()" title="Export Excel"><i class="fa fa-file-excel-o"><a download="" href="" id="btnpdf" style="display:none;"></a></i></button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <br /><br /><label style="background-color:white;left:90%;"> Export Excel </label>
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-lg-2 col-md-6 col-sm-12" style="padding:20px;border-radius: 2px;">
                <div class="panel widget bg-white-300" style="border:1px solid rgb(179, 177, 177);">
                    <div class="row row-table">
                        <div class="col-xs-4 text-center bg-white-500 pv-lg" style="padding-top:30px;">
                            <div class="h4 mt0">{{totalcount}}</div>
                        </div>
                        <div class="col-xs-8 pv-lg" style="font-size:14px;">
                            <div><b>Total<br>Request</b></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-2 col-md-6 col-sm-12" style="padding:20px;border-radius: 2px;">
                <div class="panel widget bg-white-300" style="border:1px solid rgb(179, 177, 177);">
                    <div class="row row-table">
                        <div class="col-xs-4 text-center bg-white-500 pv-lg">
                            <div class="h4 mt0">
                                {{pendingcount}}
                            </div>
                        </div>
                        <div class="col-xs-8 pv-lg" style="font-size:14px;">
                            <div><b>Pending Request</b> </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-2 col-md-6 col-sm-12" style="padding:20px;border-radius: 2px;">
                <div class="panel widget bg-white-300" style="border:1px solid rgb(179, 177, 177);">
                    <div class="row row-table">
                        <div class="col-xs-4 text-center bg-white-500 pv-lg" style="padding-top:30px;">
                            <div class="h4 mt0">{{rejectedcount}}</div>
                        </div>
                        <div class="col-xs-8 pv-lg" style="font-size:14px;">
                            <div><b>Rejected Request</b></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-2 col-md-6 col-sm-12" style="padding:20px;border-radius: 2px;">
                <div class="panel widget bg-white-300" style="border:1px solid rgb(179, 177, 177);">
                    <div class="row row-table">
                        <div class="col-xs-4 text-center bg-white-500 pv-lg" style="padding-top:30px;">
                            <div class="h4 mt0">{{completedcount}}</div>
                        </div>
                        <div class="col-xs-8 pv-lg" style="font-size:14px;">
                            <div><b>Completed Request</b></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-2 col-md-6 col-sm-12" style="padding:20px;border-radius: 2px;">
                <div class="panel widget bg-white-300" style="border:1px solid rgb(179, 177, 177);">
                    <div class="row row-table">
                        <div class="col-xs-4 text-center bg-white-500 pv-lg" style="padding-top:30px;">
                            <div class="h4 mt0">{{WithdrawnCount}}</div>
                        </div>
                        <div class="col-xs-8 pv-lg" style="font-size:14px;">
                            <div><b>Withdrawn Request</b></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-2 col-md-6 col-sm-12" style="padding:20px;">
                <div class="panel widget" style="height:60px;width:233px;">
                    <div class="row row-table">
                        <div class="col-xs-6 text-center bg-gray-dark pv-lg">
                            <now format="MMMM" class="text-sm"></now>
                            <now format="d" class="h4 mt0"></now>
                        </div>
                        <div class="col-xs-10 pv-lg bg-gray-500">
                            <now format="EEEE" class="text-sm"></now>
                            <now format="h:mm" class="h4 mt0"></now>
                            <now format="a" class="text-sm"></now>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
