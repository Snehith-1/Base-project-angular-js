    <script type="text/ng-template" id="/CustomerViewMailContent.html">

        <md-toolbar>
            <div class="md-toolbar-tools" style="background-color:rgb(63, 110, 181);">
                <h2><em class="fa fa-eye"></em>&nbsp;View Sent Mail</h2>
                <span flex></span>
                <md-button class="md-icon-button" ng-click="close()" title="Close">
                    <md-icon md-svg-src="app/img/icons/ic_close_24px.svg" aria-label="Close dialog"></md-icon>
                </md-button>
            </div>
        </md-toolbar>

        <div class="modal-body ng-scope">
            <div class="panel-body">
                <form method="post">
                    <div class="input-group form-group">
                        <span class="input-group-addon text-sm">From:</span>
                        <input type="text" ng-model="frommail_view" class="form-control" disabled="" />
                    </div>
                    <div class="input-group form-group">
                        <span class="input-group-addon text-sm">To:</span>
                        <input type="text" ng-model="tomail_view" class="form-control" disabled="" />
                    </div>
                    <div class="input-group form-group">
                        <span class="input-group-addon text-sm">CC:</span>
                        <input type="text" ng-model="ccmail_view" class="form-control" disabled="" />
                    </div>
                    <div class="input-group form-group">
                        <span class="input-group-addon text-sm">BCC:</span>
                        <input type="text" ng-model="bccmail_view" class="form-control" disabled="" />
                    </div>
                    <div class="input-group form-group">
                        <span class="input-group-addon text-sm">Subject:</span>
                        <input type="text" ng-model="mailsubject_view" class="form-control" disabled="" />
                    </div>
                </form>
                <br />
                <div class="row">
                    <div class="col-sm-12">
                        <div class="form-group bg-grey-100">
                            <p ng-bind-html="mailcontent_view"></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="text-center">

            <button ng-click="ok()" class="btn btn-danger">
                <i class="fa fa-close (alias)">
                </i>&nbsp;Close
            </button>
        </div>
        <br />
    </script>
</div>