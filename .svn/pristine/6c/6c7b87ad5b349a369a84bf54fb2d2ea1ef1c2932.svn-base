            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12" >
                    <!-- START panel-->
                    <div id="panelDemo13" class="panel panel-default">
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table datatable="ng" class="table table-striped row-border hover">
                                    <thead>
                                        <tr class="text-white nowrap" style="background-color:#09385f;">
                                            <th>S.No</th>
                                            <th class="nowrap">Application Number</th>
                                            <th class="nowrap">Applicant Name</th>
                                            <th>Vertical</th>
                                            <th class="nowrap">Applicant Type</th>
                                            <th>Status</th>
                                            <th>Created By</th>
                                            <th class="nowrap">Created Date</th>
                                            <th class="nowrap">Updated By</th>
                                            <th class="nowrap">Updated Date</th>
                                            <th class="nowrap">Submitted Date</th>                                           
                                            <th class="nowrap">Overall Limit</th>
                                            </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="data in MstAppReport_list">
                                            <td data-title="'S.No'">{{$index+1}}</td>
                                            <td data-title="'Application Number'">{{data.application_no}}</td>
                                            <td data-title="'Applicant Name'">{{data.customerref_name}}</td>
                                            <td data-title="'Vertical'">{{data.vertical_name}}</td>
                                            <td data-title="'Applicant Type'">{{data.applicant_type}}</td>
                                            <td data-title="'Status'">{{data.approval_status}}</td>
                                            <td data-title="'Created By'">{{data.created_by}}</td>
                                            <td data-title="Created Date">{{data.created_date}}</td>
                                            <td data-title="'Updated By'">{{data.updated_by}}</td>
                                            <td data-title="Updated Date">{{data.updated_date}}</td>
                                            <td data-title="Submitted Date">{{data.submitted_date}}</td>
                                            <td data-title="Overall Limit">{{data.overalllimit_amount}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
