<h3>
    Application Report
</h3>
<div ng-controller="MstApplicationReportController">
    <div class="row">
        <div class="col-lg-3 col-md-3 col-sm-3">
            <div class="panel widget bg-white-300">
                <div class="row row-table">
                    <div class="col-xs-8 pv-lg" style="font-size:14px;">
                        <div><b>Pending Application</b> </div>
                    </div>
                    <div class="col-xs-4 text-center bg-white-500 pv-lg">
                        <div class="label" style="background-color: #141478; padding-top: 4px; font-size: 10px; padding-bottom: 4px; ">{{count_incomplete}}</div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-3">
            <div class="panel widget bg-white-300">
                <div class="row row-table">
                    <div class="col-xs-8 pv-lg" style="font-size:14px;">
                        <div><b>In Credit Underwriting</b></div>
                    </div>
                    <div class="col-xs-4 text-center bg-white-500 pv-lg" style="padding-top:30px;">
                        <div class="label" style="background-color: #141478; padding-top: 4px; font-size: 10px; padding-bottom: 4px; ">{{count_submit}}</div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-3">
            <div class="panel widget bg-white-300">
                <div class="row row-table">
                    <div class="col-xs-8 pv-lg" style="font-size:14px;">
                        <div><b>Total Application</b></div>
                    </div>
                    <div class="col-xs-4 text-center bg-white-500 pv-lg" style="padding-top:30px;">
                        <div class="label" style="background-color: #141478; padding-top: 4px; font-size: 10px; padding-bottom: 4px; ">{{count_Report}}</div>
                    </div>
                </div>
            </div>
        </div>
